<?xml version="1.0" encoding="UTF-8"?>
<tns:fragment id="SMC_test_fragment" xmlns:tns="http://das.fbk.eu/Fragment"
	xmlns:tns1="http://das.fbk.eu/Annotation" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://das.fbk.eu/Fragment Fragment.xsd ">

	<tns:state isInitial="true" name="ST0" />
	<tns:state name="ST1" />

	<tns:action name="testInput" actionType="input">
		<tns:effect>
			<tns1:event dpName="testDp" eventName="endend"></tns1:event>
		</tns:effect>
	</tns:action>

	<tns:transition>
		<tns:initialState>ST0</tns:initialState>
		<tns:finalState>ST1</tns:finalState>
		<tns:action name="testInput" actionType="input" />
	</tns:transition>

</tns:fragment>
