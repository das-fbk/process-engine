-------------------------------
--   C O M P O S I T I O N   --
-------------------------------
MODULE dn_main

---------------------
--   I N P U T S   --
---------------------
IVAR input:
{
UNDEF, service20_UMS_ManagementTrip, service20_UMS_TripManagementRequest, service20_RESET, service21_UMS_TripRequest, service21_RESET
};

-----------------------
--   O U T P U T S   --
-----------------------
IVAR output:
{
UNDEF, service20_UMS_concrete4, service20_UMS_concrete3, service20_UMS_concrete2, service20_UMS_concrete1, service20_UMS_TripManagementReply, service20_UMS_concrete5, service21_UMS_TripAlternatives, service21_UMS_concrete2, service21_UMS_concrete1
};

-------------------------
--   S E R V I C E S   --
-------------------------
--	 service20
VAR service20: {ST2, ST1, ST4, ST3, ST6, ST5, ST8, ST7, ST0};
ASSIGN
next(service20) :=
case
(service20 = ST3 & input = service20_RESET): ST0;
(service20 = ST7 & input = service20_RESET): ST0;
(service20 = ST5 & output = service20_UMS_concrete5): ST6;
(service20 = ST3 & output = service20_UMS_concrete3): ST4;
(service20 = ST4 & output = service20_UMS_concrete4): ST5;
(service20 = ST2 & input = service20_RESET): ST0;
(service20 = ST4 & input = service20_RESET): ST0;
(service20 = ST1 & input = service20_RESET): ST0;
(service20 = ST8 & input = service20_RESET): ST0;
(service20 = ST6 & input = service20_UMS_ManagementTrip): ST7;
(service20 = ST7 & output = service20_UMS_TripManagementReply): ST8;
(service20 = ST1 & output = service20_UMS_concrete1): ST2;
(service20 = ST6 & input = service20_RESET): ST0;
(service20 = ST5 & input = service20_RESET): ST0;
(service20 = ST2 & output = service20_UMS_concrete2): ST3;
(service20 = ST0 & input = service20_UMS_TripManagementRequest): ST1;
1:service20;
esac;
TRANS ( input = service20_UMS_ManagementTrip) -> (service20 = ST6)
TRANS ( output = service20_UMS_concrete4) -> (service20 = ST4)
TRANS ( input = service20_UMS_TripManagementRequest) -> (service20 = ST0)
TRANS ( output = service20_UMS_concrete3) -> (service20 = ST3)
TRANS ( output = service20_UMS_concrete2) -> (service20 = ST2)
TRANS ( input = service20_RESET) -> (service20 = ST2 | service20 = ST1 | service20 = ST4 | service20 = ST3 | service20 = ST6 | service20 = ST5 | service20 = ST8 | service20 = ST7)
TRANS ( output = service20_UMS_concrete1) -> (service20 = ST1)
TRANS ( output = service20_UMS_TripManagementReply) -> (service20 = ST7)
TRANS ( output = service20_UMS_concrete5) -> (service20 = ST5)

--	 service21
VAR service21: {ST2, ST1, ST4, ST3, ST0};
ASSIGN
next(service21) :=
case
(service21 = ST3 & input = service21_RESET): ST0;
(service21 = ST0 & input = service21_UMS_TripRequest): ST1;
(service21 = ST1 & output = service21_UMS_concrete1): ST2;
(service21 = ST2 & input = service21_RESET): ST0;
(service21 = ST1 & input = service21_RESET): ST0;
(service21 = ST4 & input = service21_RESET): ST0;
(service21 = ST2 & output = service21_UMS_concrete2): ST3;
(service21 = ST3 & output = service21_UMS_TripAlternatives): ST4;
1:service21;
esac;
TRANS ( input = service21_UMS_TripRequest) -> (service21 = ST0)
TRANS ( output = service21_UMS_TripAlternatives) -> (service21 = ST3)
TRANS ( output = service21_UMS_concrete2) -> (service21 = ST2)
TRANS ( input = service21_RESET) -> (service21 = ST2 | service21 = ST1 | service21 = ST4 | service21 = ST3)
TRANS ( output = service21_UMS_concrete1) -> (service21 = ST1)

-----------------------
--   O B J E C T S   --
-----------------------
--	 SetupTrip
VAR SetupTrip: {ORGANIZED, INITIAL};
ASSIGN
next(SetupTrip) :=
case
(input = service20_UMS_ManagementTrip):{(ORGANIZED)};
1:SetupTrip;
esac;
--	 ProvideTrip
VAR ProvideTrip: {TRIP_REQUEST_ACCOMPLISHED, TRIP_MANAGEMENT_ACCOMPLISHED, TRIP_MANAGE_REQUEST_LOADED, TRIP_REQUEST_LOADED, INITIAL};
ASSIGN
next(ProvideTrip) :=
case
(ProvideTrip = TRIP_REQUEST_LOADED & output = service21_UMS_TripAlternatives): TRIP_REQUEST_ACCOMPLISHED;
(ProvideTrip = INITIAL & input = service21_UMS_TripRequest): TRIP_REQUEST_LOADED;
(ProvideTrip = TRIP_REQUEST_ACCOMPLISHED & input = service20_UMS_TripManagementRequest): TRIP_MANAGE_REQUEST_LOADED;
(ProvideTrip = TRIP_MANAGE_REQUEST_LOADED & output = service20_UMS_TripManagementReply): TRIP_MANAGEMENT_ACCOMPLISHED;
1:ProvideTrip;
esac;
-----------------------------------
--   P R E C O N D I T I O N S   --
-----------------------------------
TRANS (input = service20_UMS_TripManagementRequest) -> ((((ProvideTrip = TRIP_REQUEST_ACCOMPLISHED))))
TRANS (output = service20_UMS_TripManagementReply) -> ((((ProvideTrip = TRIP_MANAGE_REQUEST_LOADED))))
TRANS (input = service21_UMS_TripRequest) -> ((((ProvideTrip = INITIAL))))
TRANS (output = service21_UMS_TripAlternatives) -> ((((ProvideTrip = TRIP_REQUEST_LOADED))))
-----------------------------
--   I N V A R I A N T S   --
-----------------------------
--	 Synchronicity
TRANS (1-(input = UNDEF)) + (1-(output = UNDEF)) = 1

---------------------------------------
--   S Y N C H R O N I Z A T I O N   --
---------------------------------------
DEFINE SYNC0 := ((ProvideTrip in {TRIP_REQUEST_ACCOMPLISHED}));
--	 Synchronicity enforcement
TRANS (service20 in {ST2, ST1, ST4, ST3, ST5, ST7}) -> (next(service20) != service20)
TRANS (service21 in {ST2, ST1, ST3}) -> (next(service21) != service21)
-------------------------------
--   C O N T R O L L E R S   --
-------------------------------
--	 Fault Controller
VAR FAULT_CONTROLLER: { START, STOP };
INIT FAULT_CONTROLLER = START

ASSIGN
next(FAULT_CONTROLLER) := 
case
(FAULT_CONTROLLER = START & FALSE): STOP;
1: FAULT_CONTROLLER;
esac;

TRANS (output != UNDEF | input != UNDEF) -> (FAULT_CONTROLLER != STOP)

-------------------------------------
--   I N I T I A L I Z A T I O N   --
-------------------------------------
INIT ((SetupTrip = INITIAL) & (ProvideTrip = INITIAL))
INIT ((service20 = ST0) & (service21 = ST0))

-----------------
--   G O A L   --
-----------------
MODULE main
VAR dn_main : dn_main;

GOAL ONEOF(
(dn_main.SYNC0), 1;
(dn_main.FAULT_CONTROLLER = STOP), 0

)