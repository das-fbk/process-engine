-------------------------------
--   C O M P O S I T I O N   --
-------------------------------
MODULE dn_main

---------------------
--   I N P U T S   --
---------------------
IVAR input:
{
UNDEF, service70_UMS_ManagementTrip, service70_UMS_TripManagementRequest, service70_RESET, service72_UMS_TripRequest, service72_RESET
};

-----------------------
--   O U T P U T S   --
-----------------------
IVAR output:
{
UNDEF, service70_UMS_concrete4, service70_UMS_concrete3, service70_UMS_concrete2, service70_UMS_concrete1, service70_UMS_TripManagementReply, service70_UMS_concrete5, service72_UMS_TripAlternatives, service72_UMS_concrete2, service72_UMS_concrete1
};

-------------------------
--   S E R V I C E S   --
-------------------------
--	 service70
VAR service70: {ST2, ST1, ST4, ST3, ST6, ST5, ST8, ST7, ST0};
ASSIGN
next(service70) :=
case
(service70 = ST2 & input = service70_RESET): ST0;
(service70 = ST5 & input = service70_RESET): ST0;
(service70 = ST8 & input = service70_RESET): ST0;
(service70 = ST6 & input = service70_UMS_ManagementTrip): ST7;
(service70 = ST7 & output = service70_UMS_TripManagementReply): ST8;
(service70 = ST4 & input = service70_RESET): ST0;
(service70 = ST1 & input = service70_RESET): ST0;
(service70 = ST5 & output = service70_UMS_concrete5): ST6;
(service70 = ST6 & input = service70_RESET): ST0;
(service70 = ST4 & output = service70_UMS_concrete4): ST5;
(service70 = ST3 & output = service70_UMS_concrete3): ST4;
(service70 = ST7 & input = service70_RESET): ST0;
(service70 = ST0 & input = service70_UMS_TripManagementRequest): ST1;
(service70 = ST2 & output = service70_UMS_concrete2): ST3;
(service70 = ST1 & output = service70_UMS_concrete1): ST2;
(service70 = ST3 & input = service70_RESET): ST0;
1:service70;
esac;
TRANS ( input = service70_UMS_ManagementTrip) -> (service70 = ST6)
TRANS ( output = service70_UMS_concrete4) -> (service70 = ST4)
TRANS ( input = service70_UMS_TripManagementRequest) -> (service70 = ST0)
TRANS ( output = service70_UMS_concrete3) -> (service70 = ST3)
TRANS ( output = service70_UMS_concrete2) -> (service70 = ST2)
TRANS ( input = service70_RESET) -> (service70 = ST2 | service70 = ST1 | service70 = ST4 | service70 = ST3 | service70 = ST6 | service70 = ST5 | service70 = ST8 | service70 = ST7)
TRANS ( output = service70_UMS_concrete1) -> (service70 = ST1)
TRANS ( output = service70_UMS_TripManagementReply) -> (service70 = ST7)
TRANS ( output = service70_UMS_concrete5) -> (service70 = ST5)

--	 service72
VAR service72: {ST2, ST1, ST4, ST3, ST0};
ASSIGN
next(service72) :=
case
(service72 = ST0 & input = service72_UMS_TripRequest): ST1;
(service72 = ST2 & output = service72_UMS_concrete2): ST3;
(service72 = ST4 & input = service72_RESET): ST0;
(service72 = ST2 & input = service72_RESET): ST0;
(service72 = ST1 & input = service72_RESET): ST0;
(service72 = ST3 & output = service72_UMS_TripAlternatives): ST4;
(service72 = ST3 & input = service72_RESET): ST0;
(service72 = ST1 & output = service72_UMS_concrete1): ST2;
1:service72;
esac;
TRANS ( input = service72_UMS_TripRequest) -> (service72 = ST0)
TRANS ( output = service72_UMS_TripAlternatives) -> (service72 = ST3)
TRANS ( output = service72_UMS_concrete2) -> (service72 = ST2)
TRANS ( input = service72_RESET) -> (service72 = ST2 | service72 = ST1 | service72 = ST4 | service72 = ST3)
TRANS ( output = service72_UMS_concrete1) -> (service72 = ST1)

-----------------------
--   O B J E C T S   --
-----------------------
--	 SetupTrip
VAR SetupTrip: {ORGANIZED, INITIAL};
ASSIGN
next(SetupTrip) :=
case
(input = service70_UMS_ManagementTrip):{(ORGANIZED)};
1:SetupTrip;
esac;
--	 ProvideTrip
VAR ProvideTrip: {TRIP_REQUEST_ACCOMPLISHED, TRIP_MANAGEMENT_ACCOMPLISHED, TRIP_MANAGE_REQUEST_LOADED, TRIP_REQUEST_LOADED, INITIAL};
ASSIGN
next(ProvideTrip) :=
case
(ProvideTrip = TRIP_MANAGE_REQUEST_LOADED & output = service70_UMS_TripManagementReply): TRIP_MANAGEMENT_ACCOMPLISHED;
(ProvideTrip = TRIP_REQUEST_LOADED & output = service72_UMS_TripAlternatives): TRIP_REQUEST_ACCOMPLISHED;
(ProvideTrip = INITIAL & input = service72_UMS_TripRequest): TRIP_REQUEST_LOADED;
(ProvideTrip = TRIP_REQUEST_ACCOMPLISHED & input = service70_UMS_TripManagementRequest): TRIP_MANAGE_REQUEST_LOADED;
1:ProvideTrip;
esac;
-----------------------------------
--   P R E C O N D I T I O N S   --
-----------------------------------
TRANS (input = service70_UMS_TripManagementRequest) -> ((((ProvideTrip = TRIP_REQUEST_ACCOMPLISHED))))
TRANS (output = service70_UMS_TripManagementReply) -> ((((ProvideTrip = TRIP_MANAGE_REQUEST_LOADED))))
TRANS (input = service72_UMS_TripRequest) -> ((((ProvideTrip = INITIAL))))
TRANS (output = service72_UMS_TripAlternatives) -> ((((ProvideTrip = TRIP_REQUEST_LOADED))))
-----------------------------
--   I N V A R I A N T S   --
-----------------------------
--	 Synchronicity
TRANS (1-(input = UNDEF)) + (1-(output = UNDEF)) = 1

---------------------------------------
--   S Y N C H R O N I Z A T I O N   --
---------------------------------------
DEFINE SYNC0 := ((ProvideTrip in {TRIP_REQUEST_ACCOMPLISHED}));
--	 Synchronicity enforcement
TRANS (service70 in {ST2, ST1, ST4, ST3, ST5, ST7}) -> (next(service70) != service70)
TRANS (service72 in {ST2, ST1, ST3}) -> (next(service72) != service72)
-------------------------------
--   C O N T R O L L E R S   --
-------------------------------
--	 Fault Controller
VAR FAULT_CONTROLLER: { START, STOP };
INIT FAULT_CONTROLLER = START

ASSIGN
next(FAULT_CONTROLLER) := 
case
(FAULT_CONTROLLER = START & FALSE): STOP;
1: FAULT_CONTROLLER;
esac;

TRANS (output != UNDEF | input != UNDEF) -> (FAULT_CONTROLLER != STOP)

-------------------------------------
--   I N I T I A L I Z A T I O N   --
-------------------------------------
INIT ((SetupTrip = INITIAL) & (ProvideTrip = INITIAL))
INIT ((service70 = ST0) & (service72 = ST0))

-----------------
--   G O A L   --
-----------------
MODULE main
VAR dn_main : dn_main;

GOAL ONEOF(
(dn_main.SYNC0), 1;
(dn_main.FAULT_CONTROLLER = STOP), 0

)