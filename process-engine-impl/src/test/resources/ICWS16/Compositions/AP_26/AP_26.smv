-------------------------------
--   C O M P O S I T I O N   --
-------------------------------
MODULE dn_main

---------------------
--   I N P U T S   --
---------------------
IVAR input:
{
UNDEF, service55_UMS_ManagementTrip, service55_UMS_TripManagementRequest, service55_RESET, service56_UMS_TripRequest, service56_RESET
};

-----------------------
--   O U T P U T S   --
-----------------------
IVAR output:
{
UNDEF, service55_UMS_concrete4, service55_UMS_concrete3, service55_UMS_concrete2, service55_UMS_concrete1, service55_UMS_TripManagementReply, service55_UMS_concrete5, service56_UMS_TripAlternatives, service56_UMS_concrete2, service56_UMS_concrete1
};

-------------------------
--   S E R V I C E S   --
-------------------------
--	 service55
VAR service55: {ST2, ST1, ST4, ST3, ST6, ST5, ST8, ST7, ST0};
ASSIGN
next(service55) :=
case
(service55 = ST6 & input = service55_UMS_ManagementTrip): ST7;
(service55 = ST1 & input = service55_RESET): ST0;
(service55 = ST6 & input = service55_RESET): ST0;
(service55 = ST7 & input = service55_RESET): ST0;
(service55 = ST4 & output = service55_UMS_concrete4): ST5;
(service55 = ST2 & output = service55_UMS_concrete2): ST3;
(service55 = ST7 & output = service55_UMS_TripManagementReply): ST8;
(service55 = ST3 & output = service55_UMS_concrete3): ST4;
(service55 = ST2 & input = service55_RESET): ST0;
(service55 = ST5 & output = service55_UMS_concrete5): ST6;
(service55 = ST4 & input = service55_RESET): ST0;
(service55 = ST3 & input = service55_RESET): ST0;
(service55 = ST1 & output = service55_UMS_concrete1): ST2;
(service55 = ST0 & input = service55_UMS_TripManagementRequest): ST1;
(service55 = ST8 & input = service55_RESET): ST0;
(service55 = ST5 & input = service55_RESET): ST0;
1:service55;
esac;
TRANS ( input = service55_UMS_ManagementTrip) -> (service55 = ST6)
TRANS ( output = service55_UMS_concrete4) -> (service55 = ST4)
TRANS ( input = service55_UMS_TripManagementRequest) -> (service55 = ST0)
TRANS ( output = service55_UMS_concrete3) -> (service55 = ST3)
TRANS ( output = service55_UMS_concrete2) -> (service55 = ST2)
TRANS ( input = service55_RESET) -> (service55 = ST2 | service55 = ST1 | service55 = ST4 | service55 = ST3 | service55 = ST6 | service55 = ST5 | service55 = ST8 | service55 = ST7)
TRANS ( output = service55_UMS_concrete1) -> (service55 = ST1)
TRANS ( output = service55_UMS_TripManagementReply) -> (service55 = ST7)
TRANS ( output = service55_UMS_concrete5) -> (service55 = ST5)

--	 service56
VAR service56: {ST2, ST1, ST4, ST3, ST0};
ASSIGN
next(service56) :=
case
(service56 = ST2 & input = service56_RESET): ST0;
(service56 = ST1 & input = service56_RESET): ST0;
(service56 = ST2 & output = service56_UMS_concrete2): ST3;
(service56 = ST3 & output = service56_UMS_TripAlternatives): ST4;
(service56 = ST4 & input = service56_RESET): ST0;
(service56 = ST3 & input = service56_RESET): ST0;
(service56 = ST1 & output = service56_UMS_concrete1): ST2;
(service56 = ST0 & input = service56_UMS_TripRequest): ST1;
1:service56;
esac;
TRANS ( input = service56_UMS_TripRequest) -> (service56 = ST0)
TRANS ( output = service56_UMS_TripAlternatives) -> (service56 = ST3)
TRANS ( output = service56_UMS_concrete2) -> (service56 = ST2)
TRANS ( input = service56_RESET) -> (service56 = ST2 | service56 = ST1 | service56 = ST4 | service56 = ST3)
TRANS ( output = service56_UMS_concrete1) -> (service56 = ST1)

-----------------------
--   O B J E C T S   --
-----------------------
--	 SetupTrip
VAR SetupTrip: {ORGANIZED, INITIAL};
ASSIGN
next(SetupTrip) :=
case
(input = service55_UMS_ManagementTrip):{(ORGANIZED)};
1:SetupTrip;
esac;
--	 ProvideTrip
VAR ProvideTrip: {TRIP_REQUEST_ACCOMPLISHED, TRIP_MANAGEMENT_ACCOMPLISHED, TRIP_MANAGE_REQUEST_LOADED, TRIP_REQUEST_LOADED, INITIAL};
ASSIGN
next(ProvideTrip) :=
case
(ProvideTrip = TRIP_MANAGE_REQUEST_LOADED & output = service55_UMS_TripManagementReply): TRIP_MANAGEMENT_ACCOMPLISHED;
(ProvideTrip = TRIP_REQUEST_LOADED & output = service56_UMS_TripAlternatives): TRIP_REQUEST_ACCOMPLISHED;
(ProvideTrip = INITIAL & input = service56_UMS_TripRequest): TRIP_REQUEST_LOADED;
(ProvideTrip = TRIP_REQUEST_ACCOMPLISHED & input = service55_UMS_TripManagementRequest): TRIP_MANAGE_REQUEST_LOADED;
1:ProvideTrip;
esac;
-----------------------------------
--   P R E C O N D I T I O N S   --
-----------------------------------
TRANS (input = service55_UMS_TripManagementRequest) -> ((((ProvideTrip = TRIP_REQUEST_ACCOMPLISHED))))
TRANS (output = service55_UMS_TripManagementReply) -> ((((ProvideTrip = TRIP_MANAGE_REQUEST_LOADED))))
TRANS (input = service56_UMS_TripRequest) -> ((((ProvideTrip = INITIAL))))
TRANS (output = service56_UMS_TripAlternatives) -> ((((ProvideTrip = TRIP_REQUEST_LOADED))))
-----------------------------
--   I N V A R I A N T S   --
-----------------------------
--	 Synchronicity
TRANS (1-(input = UNDEF)) + (1-(output = UNDEF)) = 1

---------------------------------------
--   S Y N C H R O N I Z A T I O N   --
---------------------------------------
DEFINE SYNC0 := ((ProvideTrip in {TRIP_REQUEST_ACCOMPLISHED}));
--	 Synchronicity enforcement
TRANS (service55 in {ST2, ST1, ST4, ST3, ST5, ST7}) -> (next(service55) != service55)
TRANS (service56 in {ST2, ST1, ST3}) -> (next(service56) != service56)
-------------------------------
--   C O N T R O L L E R S   --
-------------------------------
--	 Fault Controller
VAR FAULT_CONTROLLER: { START, STOP };
INIT FAULT_CONTROLLER = START

ASSIGN
next(FAULT_CONTROLLER) := 
case
(FAULT_CONTROLLER = START & FALSE): STOP;
1: FAULT_CONTROLLER;
esac;

TRANS (output != UNDEF | input != UNDEF) -> (FAULT_CONTROLLER != STOP)

-------------------------------------
--   I N I T I A L I Z A T I O N   --
-------------------------------------
INIT ((SetupTrip = INITIAL) & (ProvideTrip = INITIAL))
INIT ((service55 = ST0) & (service56 = ST0))

-----------------
--   G O A L   --
-----------------
MODULE main
VAR dn_main : dn_main;

GOAL ONEOF(
(dn_main.SYNC0), 1;
(dn_main.FAULT_CONTROLLER = STOP), 0

)