-------------------------------
--   C O M P O S I T I O N   --
-------------------------------
MODULE dn_main

---------------------
--   I N P U T S   --
---------------------
IVAR input:
{
UNDEF, service1_UMS_ManagementTrip, service1_UMS_TripManagementRequest, service1_RESET, service3_UMS_TripRequest, service3_RESET
};

-----------------------
--   O U T P U T S   --
-----------------------
IVAR output:
{
UNDEF, service1_UMS_concrete4, service1_UMS_concrete3, service1_UMS_concrete2, service1_UMS_concrete1, service1_UMS_TripManagementReply, service1_UMS_concrete5, service3_UMS_TripAlternatives, service3_UMS_concrete2, service3_UMS_concrete1
};

-------------------------
--   S E R V I C E S   --
-------------------------
--	 service1
VAR service1: {ST2, ST1, ST4, ST3, ST6, ST5, ST8, ST7, ST0};
ASSIGN
next(service1) :=
case
(service1 = ST3 & output = service1_UMS_concrete3): ST4;
(service1 = ST5 & output = service1_UMS_concrete5): ST6;
(service1 = ST4 & input = service1_RESET): ST0;
(service1 = ST4 & output = service1_UMS_concrete4): ST5;
(service1 = ST5 & input = service1_RESET): ST0;
(service1 = ST2 & input = service1_RESET): ST0;
(service1 = ST7 & output = service1_UMS_TripManagementReply): ST8;
(service1 = ST8 & input = service1_RESET): ST0;
(service1 = ST0 & input = service1_UMS_TripManagementRequest): ST1;
(service1 = ST2 & output = service1_UMS_concrete2): ST3;
(service1 = ST1 & output = service1_UMS_concrete1): ST2;
(service1 = ST6 & input = service1_UMS_ManagementTrip): ST7;
(service1 = ST1 & input = service1_RESET): ST0;
(service1 = ST3 & input = service1_RESET): ST0;
(service1 = ST6 & input = service1_RESET): ST0;
(service1 = ST7 & input = service1_RESET): ST0;
1:service1;
esac;
TRANS ( input = service1_UMS_ManagementTrip) -> (service1 = ST6)
TRANS ( output = service1_UMS_concrete4) -> (service1 = ST4)
TRANS ( input = service1_UMS_TripManagementRequest) -> (service1 = ST0)
TRANS ( output = service1_UMS_concrete3) -> (service1 = ST3)
TRANS ( output = service1_UMS_concrete2) -> (service1 = ST2)
TRANS ( input = service1_RESET) -> (service1 = ST2 | service1 = ST1 | service1 = ST4 | service1 = ST3 | service1 = ST6 | service1 = ST5 | service1 = ST8 | service1 = ST7)
TRANS ( output = service1_UMS_concrete1) -> (service1 = ST1)
TRANS ( output = service1_UMS_TripManagementReply) -> (service1 = ST7)
TRANS ( output = service1_UMS_concrete5) -> (service1 = ST5)

--	 service3
VAR service3: {ST2, ST1, ST4, ST3, ST0};
ASSIGN
next(service3) :=
case
(service3 = ST1 & output = service3_UMS_concrete1): ST2;
(service3 = ST2 & output = service3_UMS_concrete2): ST3;
(service3 = ST3 & output = service3_UMS_TripAlternatives): ST4;
(service3 = ST3 & input = service3_RESET): ST0;
(service3 = ST4 & input = service3_RESET): ST0;
(service3 = ST1 & input = service3_RESET): ST0;
(service3 = ST0 & input = service3_UMS_TripRequest): ST1;
(service3 = ST2 & input = service3_RESET): ST0;
1:service3;
esac;
TRANS ( input = service3_UMS_TripRequest) -> (service3 = ST0)
TRANS ( output = service3_UMS_TripAlternatives) -> (service3 = ST3)
TRANS ( output = service3_UMS_concrete2) -> (service3 = ST2)
TRANS ( input = service3_RESET) -> (service3 = ST2 | service3 = ST1 | service3 = ST4 | service3 = ST3)
TRANS ( output = service3_UMS_concrete1) -> (service3 = ST1)

-----------------------
--   O B J E C T S   --
-----------------------
--	 SetupTrip
VAR SetupTrip: {ORGANIZED, INITIAL};
ASSIGN
next(SetupTrip) :=
case
(input = service1_UMS_ManagementTrip):{(ORGANIZED)};
1:SetupTrip;
esac;
--	 ProvideTrip
VAR ProvideTrip: {TRIP_REQUEST_ACCOMPLISHED, TRIP_MANAGEMENT_ACCOMPLISHED, TRIP_MANAGE_REQUEST_LOADED, TRIP_REQUEST_LOADED, INITIAL};
ASSIGN
next(ProvideTrip) :=
case
(ProvideTrip = TRIP_MANAGE_REQUEST_LOADED & output = service1_UMS_TripManagementReply): TRIP_MANAGEMENT_ACCOMPLISHED;
(ProvideTrip = TRIP_REQUEST_LOADED & output = service3_UMS_TripAlternatives): TRIP_REQUEST_ACCOMPLISHED;
(ProvideTrip = INITIAL & input = service3_UMS_TripRequest): TRIP_REQUEST_LOADED;
(ProvideTrip = TRIP_REQUEST_ACCOMPLISHED & input = service1_UMS_TripManagementRequest): TRIP_MANAGE_REQUEST_LOADED;
1:ProvideTrip;
esac;
-----------------------------------
--   P R E C O N D I T I O N S   --
-----------------------------------
TRANS (input = service1_UMS_TripManagementRequest) -> ((((ProvideTrip = TRIP_REQUEST_ACCOMPLISHED))))
TRANS (output = service1_UMS_TripManagementReply) -> ((((ProvideTrip = TRIP_MANAGE_REQUEST_LOADED))))
TRANS (input = service3_UMS_TripRequest) -> ((((ProvideTrip = INITIAL))))
TRANS (output = service3_UMS_TripAlternatives) -> ((((ProvideTrip = TRIP_REQUEST_LOADED))))
-----------------------------
--   I N V A R I A N T S   --
-----------------------------
--	 Synchronicity
TRANS (1-(input = UNDEF)) + (1-(output = UNDEF)) = 1

---------------------------------------
--   S Y N C H R O N I Z A T I O N   --
---------------------------------------
DEFINE SYNC0 := ((ProvideTrip in {TRIP_REQUEST_ACCOMPLISHED}));
--	 Synchronicity enforcement
TRANS (service1 in {ST2, ST1, ST4, ST3, ST5, ST7}) -> (next(service1) != service1)
TRANS (service3 in {ST2, ST1, ST3}) -> (next(service3) != service3)
-------------------------------
--   C O N T R O L L E R S   --
-------------------------------
--	 Fault Controller
VAR FAULT_CONTROLLER: { START, STOP };
INIT FAULT_CONTROLLER = START

ASSIGN
next(FAULT_CONTROLLER) := 
case
(FAULT_CONTROLLER = START & FALSE): STOP;
1: FAULT_CONTROLLER;
esac;

TRANS (output != UNDEF | input != UNDEF) -> (FAULT_CONTROLLER != STOP)

-------------------------------------
--   I N I T I A L I Z A T I O N   --
-------------------------------------
INIT ((SetupTrip = INITIAL) & (ProvideTrip = INITIAL))
INIT ((service1 = ST0) & (service3 = ST0))

-----------------
--   G O A L   --
-----------------
MODULE main
VAR dn_main : dn_main;

GOAL ONEOF(
(dn_main.SYNC0), 1;
(dn_main.FAULT_CONTROLLER = STOP), 0

)