-------------------------------
--   C O M P O S I T I O N   --
-------------------------------
MODULE dn_main

---------------------
--   I N P U T S   --
---------------------
IVAR input:
{
UNDEF, service216_UMS_ManagementTrip, service216_UMS_TripManagementRequest, service216_RESET, service217_UMS_TripRequest, service217_RESET
};

-----------------------
--   O U T P U T S   --
-----------------------
IVAR output:
{
UNDEF, service216_UMS_concrete4, service216_UMS_concrete3, service216_UMS_concrete2, service216_UMS_concrete1, service216_UMS_TripManagementReply, service216_UMS_concrete5, service217_UMS_TripAlternatives, service217_UMS_concrete2, service217_UMS_concrete1
};

-------------------------
--   S E R V I C E S   --
-------------------------
--	 service216
VAR service216: {ST2, ST1, ST4, ST3, ST6, ST5, ST8, ST7, ST0};
ASSIGN
next(service216) :=
case
(service216 = ST2 & output = service216_UMS_concrete2): ST3;
(service216 = ST1 & input = service216_RESET): ST0;
(service216 = ST7 & output = service216_UMS_TripManagementReply): ST8;
(service216 = ST1 & output = service216_UMS_concrete1): ST2;
(service216 = ST2 & input = service216_RESET): ST0;
(service216 = ST5 & input = service216_RESET): ST0;
(service216 = ST3 & input = service216_RESET): ST0;
(service216 = ST0 & input = service216_UMS_TripManagementRequest): ST1;
(service216 = ST6 & input = service216_UMS_ManagementTrip): ST7;
(service216 = ST5 & output = service216_UMS_concrete5): ST6;
(service216 = ST4 & output = service216_UMS_concrete4): ST5;
(service216 = ST6 & input = service216_RESET): ST0;
(service216 = ST3 & output = service216_UMS_concrete3): ST4;
(service216 = ST8 & input = service216_RESET): ST0;
(service216 = ST4 & input = service216_RESET): ST0;
(service216 = ST7 & input = service216_RESET): ST0;
1:service216;
esac;
TRANS ( input = service216_UMS_ManagementTrip) -> (service216 = ST6)
TRANS ( output = service216_UMS_concrete4) -> (service216 = ST4)
TRANS ( input = service216_UMS_TripManagementRequest) -> (service216 = ST0)
TRANS ( output = service216_UMS_concrete3) -> (service216 = ST3)
TRANS ( output = service216_UMS_concrete2) -> (service216 = ST2)
TRANS ( input = service216_RESET) -> (service216 = ST2 | service216 = ST1 | service216 = ST4 | service216 = ST3 | service216 = ST6 | service216 = ST5 | service216 = ST8 | service216 = ST7)
TRANS ( output = service216_UMS_concrete1) -> (service216 = ST1)
TRANS ( output = service216_UMS_TripManagementReply) -> (service216 = ST7)
TRANS ( output = service216_UMS_concrete5) -> (service216 = ST5)

--	 service217
VAR service217: {ST2, ST1, ST4, ST3, ST0};
ASSIGN
next(service217) :=
case
(service217 = ST0 & input = service217_UMS_TripRequest): ST1;
(service217 = ST3 & output = service217_UMS_TripAlternatives): ST4;
(service217 = ST2 & output = service217_UMS_concrete2): ST3;
(service217 = ST1 & input = service217_RESET): ST0;
(service217 = ST3 & input = service217_RESET): ST0;
(service217 = ST1 & output = service217_UMS_concrete1): ST2;
(service217 = ST2 & input = service217_RESET): ST0;
(service217 = ST4 & input = service217_RESET): ST0;
1:service217;
esac;
TRANS ( input = service217_UMS_TripRequest) -> (service217 = ST0)
TRANS ( output = service217_UMS_TripAlternatives) -> (service217 = ST3)
TRANS ( output = service217_UMS_concrete2) -> (service217 = ST2)
TRANS ( input = service217_RESET) -> (service217 = ST2 | service217 = ST1 | service217 = ST4 | service217 = ST3)
TRANS ( output = service217_UMS_concrete1) -> (service217 = ST1)

-----------------------
--   O B J E C T S   --
-----------------------
--	 SetupTrip
VAR SetupTrip: {ORGANIZED, INITIAL};
ASSIGN
next(SetupTrip) :=
case
(input = service216_UMS_ManagementTrip):{(ORGANIZED)};
1:SetupTrip;
esac;
--	 ProvideTrip
VAR ProvideTrip: {TRIP_REQUEST_ACCOMPLISHED, TRIP_MANAGEMENT_ACCOMPLISHED, TRIP_MANAGE_REQUEST_LOADED, TRIP_REQUEST_LOADED, INITIAL};
ASSIGN
next(ProvideTrip) :=
case
(ProvideTrip = TRIP_MANAGE_REQUEST_LOADED & output = service216_UMS_TripManagementReply): TRIP_MANAGEMENT_ACCOMPLISHED;
(ProvideTrip = INITIAL & input = service217_UMS_TripRequest): TRIP_REQUEST_LOADED;
(ProvideTrip = TRIP_REQUEST_ACCOMPLISHED & input = service216_UMS_TripManagementRequest): TRIP_MANAGE_REQUEST_LOADED;
(ProvideTrip = TRIP_REQUEST_LOADED & output = service217_UMS_TripAlternatives): TRIP_REQUEST_ACCOMPLISHED;
1:ProvideTrip;
esac;
-----------------------------------
--   P R E C O N D I T I O N S   --
-----------------------------------
TRANS (input = service216_UMS_TripManagementRequest) -> ((((ProvideTrip = TRIP_REQUEST_ACCOMPLISHED))))
TRANS (output = service216_UMS_TripManagementReply) -> ((((ProvideTrip = TRIP_MANAGE_REQUEST_LOADED))))
TRANS (input = service217_UMS_TripRequest) -> ((((ProvideTrip = INITIAL))))
TRANS (output = service217_UMS_TripAlternatives) -> ((((ProvideTrip = TRIP_REQUEST_LOADED))))
-----------------------------
--   I N V A R I A N T S   --
-----------------------------
--	 Synchronicity
TRANS (1-(input = UNDEF)) + (1-(output = UNDEF)) = 1

---------------------------------------
--   S Y N C H R O N I Z A T I O N   --
---------------------------------------
DEFINE SYNC0 := ((ProvideTrip in {TRIP_MANAGEMENT_ACCOMPLISHED}));
--	 Synchronicity enforcement
TRANS (service216 in {ST2, ST1, ST4, ST3, ST5, ST7}) -> (next(service216) != service216)
TRANS (service217 in {ST2, ST1, ST3}) -> (next(service217) != service217)
-------------------------------
--   C O N T R O L L E R S   --
-------------------------------
--	 Fault Controller
VAR FAULT_CONTROLLER: { START, STOP };
INIT FAULT_CONTROLLER = START

ASSIGN
next(FAULT_CONTROLLER) := 
case
(FAULT_CONTROLLER = START & FALSE): STOP;
1: FAULT_CONTROLLER;
esac;

TRANS (output != UNDEF | input != UNDEF) -> (FAULT_CONTROLLER != STOP)

-------------------------------------
--   I N I T I A L I Z A T I O N   --
-------------------------------------
INIT ((SetupTrip = INITIAL) & (ProvideTrip = TRIP_REQUEST_ACCOMPLISHED))
INIT ((service216 = ST0) & (service217 = ST0))

-----------------
--   G O A L   --
-----------------
MODULE main
VAR dn_main : dn_main;

GOAL ONEOF(
(dn_main.SYNC0), 1;
(dn_main.FAULT_CONTROLLER = STOP), 0

)