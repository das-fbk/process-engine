-------------------------------
--   C O M P O S I T I O N   --
-------------------------------
MODULE dn_main

---------------------
--   I N P U T S   --
---------------------
IVAR input:
{
UNDEF, service66_UMS_ManagementTrip, service66_UMS_TripManagementRequest, service66_RESET, service67_UMS_TripRequest, service67_RESET
};

-----------------------
--   O U T P U T S   --
-----------------------
IVAR output:
{
UNDEF, service66_UMS_concrete4, service66_UMS_concrete3, service66_UMS_concrete2, service66_UMS_concrete1, service66_UMS_TripManagementReply, service66_UMS_concrete5, service67_UMS_TripAlternatives, service67_UMS_concrete2, service67_UMS_concrete1
};

-------------------------
--   S E R V I C E S   --
-------------------------
--	 service66
VAR service66: {ST2, ST1, ST4, ST3, ST6, ST5, ST8, ST7, ST0};
ASSIGN
next(service66) :=
case
(service66 = ST4 & output = service66_UMS_concrete4): ST5;
(service66 = ST5 & input = service66_RESET): ST0;
(service66 = ST3 & output = service66_UMS_concrete3): ST4;
(service66 = ST5 & output = service66_UMS_concrete5): ST6;
(service66 = ST7 & output = service66_UMS_TripManagementReply): ST8;
(service66 = ST1 & output = service66_UMS_concrete1): ST2;
(service66 = ST2 & input = service66_RESET): ST0;
(service66 = ST8 & input = service66_RESET): ST0;
(service66 = ST7 & input = service66_RESET): ST0;
(service66 = ST6 & input = service66_RESET): ST0;
(service66 = ST0 & input = service66_UMS_TripManagementRequest): ST1;
(service66 = ST4 & input = service66_RESET): ST0;
(service66 = ST6 & input = service66_UMS_ManagementTrip): ST7;
(service66 = ST1 & input = service66_RESET): ST0;
(service66 = ST3 & input = service66_RESET): ST0;
(service66 = ST2 & output = service66_UMS_concrete2): ST3;
1:service66;
esac;
TRANS ( input = service66_UMS_ManagementTrip) -> (service66 = ST6)
TRANS ( output = service66_UMS_concrete4) -> (service66 = ST4)
TRANS ( input = service66_UMS_TripManagementRequest) -> (service66 = ST0)
TRANS ( output = service66_UMS_concrete3) -> (service66 = ST3)
TRANS ( output = service66_UMS_concrete2) -> (service66 = ST2)
TRANS ( input = service66_RESET) -> (service66 = ST2 | service66 = ST1 | service66 = ST4 | service66 = ST3 | service66 = ST6 | service66 = ST5 | service66 = ST8 | service66 = ST7)
TRANS ( output = service66_UMS_concrete1) -> (service66 = ST1)
TRANS ( output = service66_UMS_TripManagementReply) -> (service66 = ST7)
TRANS ( output = service66_UMS_concrete5) -> (service66 = ST5)

--	 service67
VAR service67: {ST2, ST1, ST4, ST3, ST0};
ASSIGN
next(service67) :=
case
(service67 = ST1 & output = service67_UMS_concrete1): ST2;
(service67 = ST4 & input = service67_RESET): ST0;
(service67 = ST2 & input = service67_RESET): ST0;
(service67 = ST0 & input = service67_UMS_TripRequest): ST1;
(service67 = ST3 & input = service67_RESET): ST0;
(service67 = ST2 & output = service67_UMS_concrete2): ST3;
(service67 = ST1 & input = service67_RESET): ST0;
(service67 = ST3 & output = service67_UMS_TripAlternatives): ST4;
1:service67;
esac;
TRANS ( input = service67_UMS_TripRequest) -> (service67 = ST0)
TRANS ( output = service67_UMS_TripAlternatives) -> (service67 = ST3)
TRANS ( output = service67_UMS_concrete2) -> (service67 = ST2)
TRANS ( input = service67_RESET) -> (service67 = ST2 | service67 = ST1 | service67 = ST4 | service67 = ST3)
TRANS ( output = service67_UMS_concrete1) -> (service67 = ST1)

-----------------------
--   O B J E C T S   --
-----------------------
--	 SetupTrip
VAR SetupTrip: {ORGANIZED, INITIAL};
ASSIGN
next(SetupTrip) :=
case
(input = service66_UMS_ManagementTrip):{(ORGANIZED)};
1:SetupTrip;
esac;
--	 ProvideTrip
VAR ProvideTrip: {TRIP_REQUEST_ACCOMPLISHED, TRIP_MANAGEMENT_ACCOMPLISHED, TRIP_MANAGE_REQUEST_LOADED, TRIP_REQUEST_LOADED, INITIAL};
ASSIGN
next(ProvideTrip) :=
case
(ProvideTrip = TRIP_REQUEST_LOADED & output = service67_UMS_TripAlternatives): TRIP_REQUEST_ACCOMPLISHED;
(ProvideTrip = TRIP_REQUEST_ACCOMPLISHED & input = service66_UMS_TripManagementRequest): TRIP_MANAGE_REQUEST_LOADED;
(ProvideTrip = TRIP_MANAGE_REQUEST_LOADED & output = service66_UMS_TripManagementReply): TRIP_MANAGEMENT_ACCOMPLISHED;
(ProvideTrip = INITIAL & input = service67_UMS_TripRequest): TRIP_REQUEST_LOADED;
1:ProvideTrip;
esac;
-----------------------------------
--   P R E C O N D I T I O N S   --
-----------------------------------
TRANS (input = service66_UMS_TripManagementRequest) -> ((((ProvideTrip = TRIP_REQUEST_ACCOMPLISHED))))
TRANS (output = service66_UMS_TripManagementReply) -> ((((ProvideTrip = TRIP_MANAGE_REQUEST_LOADED))))
TRANS (input = service67_UMS_TripRequest) -> ((((ProvideTrip = INITIAL))))
TRANS (output = service67_UMS_TripAlternatives) -> ((((ProvideTrip = TRIP_REQUEST_LOADED))))
-----------------------------
--   I N V A R I A N T S   --
-----------------------------
--	 Synchronicity
TRANS (1-(input = UNDEF)) + (1-(output = UNDEF)) = 1

---------------------------------------
--   S Y N C H R O N I Z A T I O N   --
---------------------------------------
DEFINE SYNC0 := ((ProvideTrip in {TRIP_REQUEST_ACCOMPLISHED}));
--	 Synchronicity enforcement
TRANS (service66 in {ST2, ST1, ST4, ST3, ST5, ST7}) -> (next(service66) != service66)
TRANS (service67 in {ST2, ST1, ST3}) -> (next(service67) != service67)
-------------------------------
--   C O N T R O L L E R S   --
-------------------------------
--	 Fault Controller
VAR FAULT_CONTROLLER: { START, STOP };
INIT FAULT_CONTROLLER = START

ASSIGN
next(FAULT_CONTROLLER) := 
case
(FAULT_CONTROLLER = START & FALSE): STOP;
1: FAULT_CONTROLLER;
esac;

TRANS (output != UNDEF | input != UNDEF) -> (FAULT_CONTROLLER != STOP)

-------------------------------------
--   I N I T I A L I Z A T I O N   --
-------------------------------------
INIT ((SetupTrip = INITIAL) & (ProvideTrip = INITIAL))
INIT ((service66 = ST0) & (service67 = ST0))

-----------------
--   G O A L   --
-----------------
MODULE main
VAR dn_main : dn_main;

GOAL ONEOF(
(dn_main.SYNC0), 1;
(dn_main.FAULT_CONTROLLER = STOP), 0

)