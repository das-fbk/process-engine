-------------------------------
--   C O M P O S I T I O N   --
-------------------------------
MODULE dn_main

---------------------
--   I N P U T S   --
---------------------
IVAR input:
{
UNDEF, service127_UMS_ManagementTrip, service127_UMS_TripManagementRequest, service127_RESET, service129_UMS_TripRequest, service129_RESET
};

-----------------------
--   O U T P U T S   --
-----------------------
IVAR output:
{
UNDEF, service127_UMS_concrete4, service127_UMS_concrete3, service127_UMS_concrete2, service127_UMS_concrete1, service127_UMS_TripManagementReply, service127_UMS_concrete5, service129_UMS_TripAlternatives, service129_UMS_concrete2, service129_UMS_concrete1
};

-------------------------
--   S E R V I C E S   --
-------------------------
--	 service127
VAR service127: {ST2, ST1, ST4, ST3, ST6, ST5, ST8, ST7, ST0};
ASSIGN
next(service127) :=
case
(service127 = ST3 & output = service127_UMS_concrete3): ST4;
(service127 = ST2 & input = service127_RESET): ST0;
(service127 = ST7 & output = service127_UMS_TripManagementReply): ST8;
(service127 = ST5 & output = service127_UMS_concrete5): ST6;
(service127 = ST2 & output = service127_UMS_concrete2): ST3;
(service127 = ST6 & input = service127_RESET): ST0;
(service127 = ST4 & input = service127_RESET): ST0;
(service127 = ST7 & input = service127_RESET): ST0;
(service127 = ST0 & input = service127_UMS_TripManagementRequest): ST1;
(service127 = ST3 & input = service127_RESET): ST0;
(service127 = ST1 & input = service127_RESET): ST0;
(service127 = ST8 & input = service127_RESET): ST0;
(service127 = ST6 & input = service127_UMS_ManagementTrip): ST7;
(service127 = ST1 & output = service127_UMS_concrete1): ST2;
(service127 = ST4 & output = service127_UMS_concrete4): ST5;
(service127 = ST5 & input = service127_RESET): ST0;
1:service127;
esac;
TRANS ( input = service127_UMS_ManagementTrip) -> (service127 = ST6)
TRANS ( output = service127_UMS_concrete4) -> (service127 = ST4)
TRANS ( input = service127_UMS_TripManagementRequest) -> (service127 = ST0)
TRANS ( output = service127_UMS_concrete3) -> (service127 = ST3)
TRANS ( output = service127_UMS_concrete2) -> (service127 = ST2)
TRANS ( input = service127_RESET) -> (service127 = ST2 | service127 = ST1 | service127 = ST4 | service127 = ST3 | service127 = ST6 | service127 = ST5 | service127 = ST8 | service127 = ST7)
TRANS ( output = service127_UMS_concrete1) -> (service127 = ST1)
TRANS ( output = service127_UMS_TripManagementReply) -> (service127 = ST7)
TRANS ( output = service127_UMS_concrete5) -> (service127 = ST5)

--	 service129
VAR service129: {ST2, ST1, ST4, ST3, ST0};
ASSIGN
next(service129) :=
case
(service129 = ST2 & output = service129_UMS_concrete2): ST3;
(service129 = ST1 & input = service129_RESET): ST0;
(service129 = ST3 & input = service129_RESET): ST0;
(service129 = ST0 & input = service129_UMS_TripRequest): ST1;
(service129 = ST4 & input = service129_RESET): ST0;
(service129 = ST2 & input = service129_RESET): ST0;
(service129 = ST3 & output = service129_UMS_TripAlternatives): ST4;
(service129 = ST1 & output = service129_UMS_concrete1): ST2;
1:service129;
esac;
TRANS ( input = service129_UMS_TripRequest) -> (service129 = ST0)
TRANS ( output = service129_UMS_TripAlternatives) -> (service129 = ST3)
TRANS ( output = service129_UMS_concrete2) -> (service129 = ST2)
TRANS ( input = service129_RESET) -> (service129 = ST2 | service129 = ST1 | service129 = ST4 | service129 = ST3)
TRANS ( output = service129_UMS_concrete1) -> (service129 = ST1)

-----------------------
--   O B J E C T S   --
-----------------------
--	 SetupTrip
VAR SetupTrip: {ORGANIZED, INITIAL};
ASSIGN
next(SetupTrip) :=
case
(input = service127_UMS_ManagementTrip):{(ORGANIZED)};
1:SetupTrip;
esac;
--	 ProvideTrip
VAR ProvideTrip: {TRIP_REQUEST_ACCOMPLISHED, TRIP_MANAGEMENT_ACCOMPLISHED, TRIP_MANAGE_REQUEST_LOADED, TRIP_REQUEST_LOADED, INITIAL};
ASSIGN
next(ProvideTrip) :=
case
(ProvideTrip = TRIP_MANAGE_REQUEST_LOADED & output = service127_UMS_TripManagementReply): TRIP_MANAGEMENT_ACCOMPLISHED;
(ProvideTrip = INITIAL & input = service129_UMS_TripRequest): TRIP_REQUEST_LOADED;
(ProvideTrip = TRIP_REQUEST_ACCOMPLISHED & input = service127_UMS_TripManagementRequest): TRIP_MANAGE_REQUEST_LOADED;
(ProvideTrip = TRIP_REQUEST_LOADED & output = service129_UMS_TripAlternatives): TRIP_REQUEST_ACCOMPLISHED;
1:ProvideTrip;
esac;
-----------------------------------
--   P R E C O N D I T I O N S   --
-----------------------------------
TRANS (input = service127_UMS_TripManagementRequest) -> ((((ProvideTrip = TRIP_REQUEST_ACCOMPLISHED))))
TRANS (output = service127_UMS_TripManagementReply) -> ((((ProvideTrip = TRIP_MANAGE_REQUEST_LOADED))))
TRANS (input = service129_UMS_TripRequest) -> ((((ProvideTrip = INITIAL))))
TRANS (output = service129_UMS_TripAlternatives) -> ((((ProvideTrip = TRIP_REQUEST_LOADED))))
-----------------------------
--   I N V A R I A N T S   --
-----------------------------
--	 Synchronicity
TRANS (1-(input = UNDEF)) + (1-(output = UNDEF)) = 1

---------------------------------------
--   S Y N C H R O N I Z A T I O N   --
---------------------------------------
DEFINE SYNC0 := ((ProvideTrip in {TRIP_MANAGEMENT_ACCOMPLISHED}));
--	 Synchronicity enforcement
TRANS (service127 in {ST2, ST1, ST4, ST3, ST5, ST7}) -> (next(service127) != service127)
TRANS (service129 in {ST2, ST1, ST3}) -> (next(service129) != service129)
-------------------------------
--   C O N T R O L L E R S   --
-------------------------------
--	 Fault Controller
VAR FAULT_CONTROLLER: { START, STOP };
INIT FAULT_CONTROLLER = START

ASSIGN
next(FAULT_CONTROLLER) := 
case
(FAULT_CONTROLLER = START & FALSE): STOP;
1: FAULT_CONTROLLER;
esac;

TRANS (output != UNDEF | input != UNDEF) -> (FAULT_CONTROLLER != STOP)

-------------------------------------
--   I N I T I A L I Z A T I O N   --
-------------------------------------
INIT ((SetupTrip = INITIAL) & (ProvideTrip = TRIP_REQUEST_ACCOMPLISHED))
INIT ((service127 = ST0) & (service129 = ST0))

-----------------
--   G O A L   --
-----------------
MODULE main
VAR dn_main : dn_main;

GOAL ONEOF(
(dn_main.SYNC0), 1;
(dn_main.FAULT_CONTROLLER = STOP), 0

)