-------------------------------
--   C O M P O S I T I O N   --
-------------------------------
MODULE dn_main

---------------------
--   I N P U T S   --
---------------------
IVAR input:
{
UNDEF, service289_BS_RegistrationRequest, service289_BS_UpdateSmartMobilityCard, service289_RESET, service291_BS_PlanBikeRoute, service291_BS_ExecuteRouteRequest, service291_RESET
};

-----------------------
--   O U T P U T S   --
-----------------------
IVAR output:
{
UNDEF, service289_BS_RegistrationAck, service291_BS_ExecuteRouteAck
};

-------------------------
--   S E R V I C E S   --
-------------------------
--	 service289
VAR service289: {ST2, ST1, ST3, ST0};
ASSIGN
next(service289) :=
case
(service289 = ST2 & input = service289_RESET): ST0;
(service289 = ST3 & input = service289_RESET): ST0;
(service289 = ST2 & output = service289_BS_RegistrationAck): ST3;
(service289 = ST0 & input = service289_BS_RegistrationRequest): ST1;
(service289 = ST1 & input = service289_RESET): ST0;
(service289 = ST1 & input = service289_BS_UpdateSmartMobilityCard): ST2;
1:service289;
esac;
TRANS ( input = service289_BS_RegistrationRequest) -> (service289 = ST0)
TRANS ( output = service289_BS_RegistrationAck) -> (service289 = ST2)
TRANS ( input = service289_BS_UpdateSmartMobilityCard) -> (service289 = ST1)
TRANS ( input = service289_RESET) -> (service289 = ST2 | service289 = ST1 | service289 = ST3)

--	 service291
VAR service291: {ST2, ST1, ST3, ST0};
ASSIGN
next(service291) :=
case
(service291 = ST2 & output = service291_BS_ExecuteRouteAck): ST3;
(service291 = ST0 & input = service291_BS_ExecuteRouteRequest): ST1;
(service291 = ST1 & input = service291_BS_PlanBikeRoute): ST2;
(service291 = ST1 & input = service291_RESET): ST0;
(service291 = ST2 & input = service291_RESET): ST0;
(service291 = ST3 & input = service291_RESET): ST0;
1:service291;
esac;
TRANS ( input = service291_BS_PlanBikeRoute) -> (service291 = ST1)
TRANS ( input = service291_BS_ExecuteRouteRequest) -> (service291 = ST0)
TRANS ( input = service291_RESET) -> (service291 = ST2 | service291 = ST1 | service291 = ST3)
TRANS ( output = service291_BS_ExecuteRouteAck) -> (service291 = ST2)

-----------------------
--   O B J E C T S   --
-----------------------
--	 BikeRoutePlan
VAR BikeRoutePlan: {ROUTE_PLANNED, INITIAL};
ASSIGN
next(BikeRoutePlan) :=
case
(input = service291_BS_PlanBikeRoute):{(ROUTE_PLANNED)};
1:BikeRoutePlan;
esac;
--	 BikeSharingTrip
VAR BikeSharingTrip: {RACK_REACHED, INITIAL, BIKE_ROUTE_PLANNED, USER_REGISTERED};
ASSIGN
next(BikeSharingTrip) :=
case
(BikeSharingTrip = USER_REGISTERED & input = service291_BS_PlanBikeRoute): BIKE_ROUTE_PLANNED;
(BikeSharingTrip = BIKE_ROUTE_PLANNED & output = service291_BS_ExecuteRouteAck): RACK_REACHED;
(BikeSharingTrip = INITIAL & output = service289_BS_RegistrationAck): USER_REGISTERED;
1:BikeSharingTrip;
esac;
--	 HandleSmartMobilityCard
VAR HandleSmartMobilityCard: {INITIAL, SUBSCRIPTION, CARD_RELEASED, SERVICE_ADDED};
ASSIGN
next(HandleSmartMobilityCard) :=
case
(HandleSmartMobilityCard = SERVICE_ADDED & input = service289_BS_UpdateSmartMobilityCard): CARD_RELEASED;
(input = service289_BS_UpdateSmartMobilityCard):{(SERVICE_ADDED)};
1:HandleSmartMobilityCard;
esac;
-----------------------------------
--   P R E C O N D I T I O N S   --
-----------------------------------
TRANS (input = service289_BS_RegistrationRequest) -> ((((BikeSharingTrip = INITIAL))))
TRANS (input = service291_BS_ExecuteRouteRequest) -> ((((BikeSharingTrip = USER_REGISTERED))))
-----------------------------
--   I N V A R I A N T S   --
-----------------------------
--	 Synchronicity
TRANS (1-(input = UNDEF)) + (1-(output = UNDEF)) = 1

---------------------------------------
--   S Y N C H R O N I Z A T I O N   --
---------------------------------------
DEFINE SYNC0 := ((BikeSharingTrip in {USER_REGISTERED}));
--	 Synchronicity enforcement
TRANS (service289 in {ST2}) -> (next(service289) != service289)
TRANS (service291 in {ST2}) -> (next(service291) != service291)
-------------------------------
--   C O N T R O L L E R S   --
-------------------------------
--	 Fault Controller
VAR FAULT_CONTROLLER: { START, STOP };
INIT FAULT_CONTROLLER = START

ASSIGN
next(FAULT_CONTROLLER) := 
case
(FAULT_CONTROLLER = START & FALSE): STOP;
1: FAULT_CONTROLLER;
esac;

TRANS (output != UNDEF | input != UNDEF) -> (FAULT_CONTROLLER != STOP)

-------------------------------------
--   I N I T I A L I Z A T I O N   --
-------------------------------------
INIT ((BikeRoutePlan = INITIAL) & (BikeSharingTrip = INITIAL) & (HandleSmartMobilityCard = INITIAL))
INIT ((service289 = ST0) & (service291 = ST0))

-----------------
--   G O A L   --
-----------------
MODULE main
VAR dn_main : dn_main;

GOAL ONEOF(
(dn_main.SYNC0), 1;
(dn_main.FAULT_CONTROLLER = STOP), 0

)