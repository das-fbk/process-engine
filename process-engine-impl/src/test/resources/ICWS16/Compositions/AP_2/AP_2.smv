-------------------------------
--   C O M P O S I T I O N   --
-------------------------------
MODULE dn_main

---------------------
--   I N P U T S   --
---------------------
IVAR input:
{
UNDEF, service8_UMS_ManagementTrip, service8_UMS_TripManagementRequest, service8_RESET, service9_UMS_TripRequest, service9_RESET
};

-----------------------
--   O U T P U T S   --
-----------------------
IVAR output:
{
UNDEF, service8_UMS_concrete4, service8_UMS_concrete3, service8_UMS_concrete2, service8_UMS_concrete1, service8_UMS_TripManagementReply, service8_UMS_concrete5, service9_UMS_TripAlternatives, service9_UMS_concrete2, service9_UMS_concrete1
};

-------------------------
--   S E R V I C E S   --
-------------------------
--	 service8
VAR service8: {ST2, ST1, ST4, ST3, ST6, ST5, ST8, ST7, ST0};
ASSIGN
next(service8) :=
case
(service8 = ST7 & output = service8_UMS_TripManagementReply): ST8;
(service8 = ST3 & output = service8_UMS_concrete3): ST4;
(service8 = ST5 & output = service8_UMS_concrete5): ST6;
(service8 = ST0 & input = service8_UMS_TripManagementRequest): ST1;
(service8 = ST1 & output = service8_UMS_concrete1): ST2;
(service8 = ST6 & input = service8_UMS_ManagementTrip): ST7;
(service8 = ST5 & input = service8_RESET): ST0;
(service8 = ST7 & input = service8_RESET): ST0;
(service8 = ST2 & output = service8_UMS_concrete2): ST3;
(service8 = ST6 & input = service8_RESET): ST0;
(service8 = ST3 & input = service8_RESET): ST0;
(service8 = ST8 & input = service8_RESET): ST0;
(service8 = ST2 & input = service8_RESET): ST0;
(service8 = ST4 & input = service8_RESET): ST0;
(service8 = ST1 & input = service8_RESET): ST0;
(service8 = ST4 & output = service8_UMS_concrete4): ST5;
1:service8;
esac;
TRANS ( input = service8_UMS_ManagementTrip) -> (service8 = ST6)
TRANS ( output = service8_UMS_concrete4) -> (service8 = ST4)
TRANS ( input = service8_UMS_TripManagementRequest) -> (service8 = ST0)
TRANS ( output = service8_UMS_concrete3) -> (service8 = ST3)
TRANS ( output = service8_UMS_concrete2) -> (service8 = ST2)
TRANS ( input = service8_RESET) -> (service8 = ST2 | service8 = ST1 | service8 = ST4 | service8 = ST3 | service8 = ST6 | service8 = ST5 | service8 = ST8 | service8 = ST7)
TRANS ( output = service8_UMS_concrete1) -> (service8 = ST1)
TRANS ( output = service8_UMS_TripManagementReply) -> (service8 = ST7)
TRANS ( output = service8_UMS_concrete5) -> (service8 = ST5)

--	 service9
VAR service9: {ST2, ST1, ST4, ST3, ST0};
ASSIGN
next(service9) :=
case
(service9 = ST2 & input = service9_RESET): ST0;
(service9 = ST1 & input = service9_RESET): ST0;
(service9 = ST4 & input = service9_RESET): ST0;
(service9 = ST3 & input = service9_RESET): ST0;
(service9 = ST2 & output = service9_UMS_concrete2): ST3;
(service9 = ST0 & input = service9_UMS_TripRequest): ST1;
(service9 = ST1 & output = service9_UMS_concrete1): ST2;
(service9 = ST3 & output = service9_UMS_TripAlternatives): ST4;
1:service9;
esac;
TRANS ( input = service9_UMS_TripRequest) -> (service9 = ST0)
TRANS ( output = service9_UMS_TripAlternatives) -> (service9 = ST3)
TRANS ( output = service9_UMS_concrete2) -> (service9 = ST2)
TRANS ( input = service9_RESET) -> (service9 = ST2 | service9 = ST1 | service9 = ST4 | service9 = ST3)
TRANS ( output = service9_UMS_concrete1) -> (service9 = ST1)

-----------------------
--   O B J E C T S   --
-----------------------
--	 SetupTrip
VAR SetupTrip: {ORGANIZED, INITIAL};
ASSIGN
next(SetupTrip) :=
case
(input = service8_UMS_ManagementTrip):{(ORGANIZED)};
1:SetupTrip;
esac;
--	 ProvideTrip
VAR ProvideTrip: {TRIP_REQUEST_ACCOMPLISHED, TRIP_MANAGEMENT_ACCOMPLISHED, TRIP_MANAGE_REQUEST_LOADED, TRIP_REQUEST_LOADED, INITIAL};
ASSIGN
next(ProvideTrip) :=
case
(ProvideTrip = TRIP_REQUEST_LOADED & output = service9_UMS_TripAlternatives): TRIP_REQUEST_ACCOMPLISHED;
(ProvideTrip = INITIAL & input = service9_UMS_TripRequest): TRIP_REQUEST_LOADED;
(ProvideTrip = TRIP_REQUEST_ACCOMPLISHED & input = service8_UMS_TripManagementRequest): TRIP_MANAGE_REQUEST_LOADED;
(ProvideTrip = TRIP_MANAGE_REQUEST_LOADED & output = service8_UMS_TripManagementReply): TRIP_MANAGEMENT_ACCOMPLISHED;
1:ProvideTrip;
esac;
-----------------------------------
--   P R E C O N D I T I O N S   --
-----------------------------------
TRANS (input = service8_UMS_TripManagementRequest) -> ((((ProvideTrip = TRIP_REQUEST_ACCOMPLISHED))))
TRANS (output = service8_UMS_TripManagementReply) -> ((((ProvideTrip = TRIP_MANAGE_REQUEST_LOADED))))
TRANS (input = service9_UMS_TripRequest) -> ((((ProvideTrip = INITIAL))))
TRANS (output = service9_UMS_TripAlternatives) -> ((((ProvideTrip = TRIP_REQUEST_LOADED))))
-----------------------------
--   I N V A R I A N T S   --
-----------------------------
--	 Synchronicity
TRANS (1-(input = UNDEF)) + (1-(output = UNDEF)) = 1

---------------------------------------
--   S Y N C H R O N I Z A T I O N   --
---------------------------------------
DEFINE SYNC0 := ((ProvideTrip in {TRIP_REQUEST_ACCOMPLISHED}));
--	 Synchronicity enforcement
TRANS (service8 in {ST2, ST1, ST4, ST3, ST5, ST7}) -> (next(service8) != service8)
TRANS (service9 in {ST2, ST1, ST3}) -> (next(service9) != service9)
-------------------------------
--   C O N T R O L L E R S   --
-------------------------------
--	 Fault Controller
VAR FAULT_CONTROLLER: { START, STOP };
INIT FAULT_CONTROLLER = START

ASSIGN
next(FAULT_CONTROLLER) := 
case
(FAULT_CONTROLLER = START & FALSE): STOP;
1: FAULT_CONTROLLER;
esac;

TRANS (output != UNDEF | input != UNDEF) -> (FAULT_CONTROLLER != STOP)

-------------------------------------
--   I N I T I A L I Z A T I O N   --
-------------------------------------
INIT ((SetupTrip = INITIAL) & (ProvideTrip = INITIAL))
INIT ((service8 = ST0) & (service9 = ST0))

-----------------
--   G O A L   --
-----------------
MODULE main
VAR dn_main : dn_main;

GOAL ONEOF(
(dn_main.SYNC0), 1;
(dn_main.FAULT_CONTROLLER = STOP), 0

)