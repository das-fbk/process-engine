-------------------------------
--   C O M P O S I T I O N   --
-------------------------------
MODULE dn_main

---------------------
--   I N P U T S   --
---------------------
IVAR input:
{
UNDEF, service51_UMS_ManagementTrip, service51_UMS_TripManagementRequest, service51_RESET, service52_UMS_TripRequest, service52_RESET
};

-----------------------
--   O U T P U T S   --
-----------------------
IVAR output:
{
UNDEF, service51_UMS_concrete4, service51_UMS_concrete3, service51_UMS_concrete2, service51_UMS_concrete1, service51_UMS_TripManagementReply, service51_UMS_concrete5, service52_UMS_TripAlternatives, service52_UMS_concrete2, service52_UMS_concrete1
};

-------------------------
--   S E R V I C E S   --
-------------------------
--	 service51
VAR service51: {ST2, ST1, ST4, ST3, ST6, ST5, ST8, ST7, ST0};
ASSIGN
next(service51) :=
case
(service51 = ST0 & input = service51_UMS_TripManagementRequest): ST1;
(service51 = ST2 & input = service51_RESET): ST0;
(service51 = ST6 & input = service51_RESET): ST0;
(service51 = ST3 & output = service51_UMS_concrete3): ST4;
(service51 = ST5 & output = service51_UMS_concrete5): ST6;
(service51 = ST5 & input = service51_RESET): ST0;
(service51 = ST1 & output = service51_UMS_concrete1): ST2;
(service51 = ST7 & output = service51_UMS_TripManagementReply): ST8;
(service51 = ST6 & input = service51_UMS_ManagementTrip): ST7;
(service51 = ST8 & input = service51_RESET): ST0;
(service51 = ST4 & output = service51_UMS_concrete4): ST5;
(service51 = ST1 & input = service51_RESET): ST0;
(service51 = ST7 & input = service51_RESET): ST0;
(service51 = ST2 & output = service51_UMS_concrete2): ST3;
(service51 = ST4 & input = service51_RESET): ST0;
(service51 = ST3 & input = service51_RESET): ST0;
1:service51;
esac;
TRANS ( input = service51_UMS_ManagementTrip) -> (service51 = ST6)
TRANS ( output = service51_UMS_concrete4) -> (service51 = ST4)
TRANS ( input = service51_UMS_TripManagementRequest) -> (service51 = ST0)
TRANS ( output = service51_UMS_concrete3) -> (service51 = ST3)
TRANS ( output = service51_UMS_concrete2) -> (service51 = ST2)
TRANS ( input = service51_RESET) -> (service51 = ST2 | service51 = ST1 | service51 = ST4 | service51 = ST3 | service51 = ST6 | service51 = ST5 | service51 = ST8 | service51 = ST7)
TRANS ( output = service51_UMS_concrete1) -> (service51 = ST1)
TRANS ( output = service51_UMS_TripManagementReply) -> (service51 = ST7)
TRANS ( output = service51_UMS_concrete5) -> (service51 = ST5)

--	 service52
VAR service52: {ST2, ST1, ST4, ST3, ST0};
ASSIGN
next(service52) :=
case
(service52 = ST2 & output = service52_UMS_concrete2): ST3;
(service52 = ST3 & output = service52_UMS_TripAlternatives): ST4;
(service52 = ST1 & output = service52_UMS_concrete1): ST2;
(service52 = ST0 & input = service52_UMS_TripRequest): ST1;
(service52 = ST4 & input = service52_RESET): ST0;
(service52 = ST3 & input = service52_RESET): ST0;
(service52 = ST2 & input = service52_RESET): ST0;
(service52 = ST1 & input = service52_RESET): ST0;
1:service52;
esac;
TRANS ( input = service52_UMS_TripRequest) -> (service52 = ST0)
TRANS ( output = service52_UMS_TripAlternatives) -> (service52 = ST3)
TRANS ( output = service52_UMS_concrete2) -> (service52 = ST2)
TRANS ( input = service52_RESET) -> (service52 = ST2 | service52 = ST1 | service52 = ST4 | service52 = ST3)
TRANS ( output = service52_UMS_concrete1) -> (service52 = ST1)

-----------------------
--   O B J E C T S   --
-----------------------
--	 SetupTrip
VAR SetupTrip: {ORGANIZED, INITIAL};
ASSIGN
next(SetupTrip) :=
case
(input = service51_UMS_ManagementTrip):{(ORGANIZED)};
1:SetupTrip;
esac;
--	 ProvideTrip
VAR ProvideTrip: {TRIP_REQUEST_ACCOMPLISHED, TRIP_MANAGEMENT_ACCOMPLISHED, TRIP_MANAGE_REQUEST_LOADED, TRIP_REQUEST_LOADED, INITIAL};
ASSIGN
next(ProvideTrip) :=
case
(ProvideTrip = TRIP_REQUEST_ACCOMPLISHED & input = service51_UMS_TripManagementRequest): TRIP_MANAGE_REQUEST_LOADED;
(ProvideTrip = TRIP_MANAGE_REQUEST_LOADED & output = service51_UMS_TripManagementReply): TRIP_MANAGEMENT_ACCOMPLISHED;
(ProvideTrip = TRIP_REQUEST_LOADED & output = service52_UMS_TripAlternatives): TRIP_REQUEST_ACCOMPLISHED;
(ProvideTrip = INITIAL & input = service52_UMS_TripRequest): TRIP_REQUEST_LOADED;
1:ProvideTrip;
esac;
-----------------------------------
--   P R E C O N D I T I O N S   --
-----------------------------------
TRANS (input = service51_UMS_TripManagementRequest) -> ((((ProvideTrip = TRIP_REQUEST_ACCOMPLISHED))))
TRANS (output = service51_UMS_TripManagementReply) -> ((((ProvideTrip = TRIP_MANAGE_REQUEST_LOADED))))
TRANS (input = service52_UMS_TripRequest) -> ((((ProvideTrip = INITIAL))))
TRANS (output = service52_UMS_TripAlternatives) -> ((((ProvideTrip = TRIP_REQUEST_LOADED))))
-----------------------------
--   I N V A R I A N T S   --
-----------------------------
--	 Synchronicity
TRANS (1-(input = UNDEF)) + (1-(output = UNDEF)) = 1

---------------------------------------
--   S Y N C H R O N I Z A T I O N   --
---------------------------------------
DEFINE SYNC0 := ((ProvideTrip in {TRIP_REQUEST_ACCOMPLISHED}));
--	 Synchronicity enforcement
TRANS (service51 in {ST2, ST1, ST4, ST3, ST5, ST7}) -> (next(service51) != service51)
TRANS (service52 in {ST2, ST1, ST3}) -> (next(service52) != service52)
-------------------------------
--   C O N T R O L L E R S   --
-------------------------------
--	 Fault Controller
VAR FAULT_CONTROLLER: { START, STOP };
INIT FAULT_CONTROLLER = START

ASSIGN
next(FAULT_CONTROLLER) := 
case
(FAULT_CONTROLLER = START & FALSE): STOP;
1: FAULT_CONTROLLER;
esac;

TRANS (output != UNDEF | input != UNDEF) -> (FAULT_CONTROLLER != STOP)

-------------------------------------
--   I N I T I A L I Z A T I O N   --
-------------------------------------
INIT ((SetupTrip = INITIAL) & (ProvideTrip = INITIAL))
INIT ((service51 = ST0) & (service52 = ST0))

-----------------
--   G O A L   --
-----------------
MODULE main
VAR dn_main : dn_main;

GOAL ONEOF(
(dn_main.SYNC0), 1;
(dn_main.FAULT_CONTROLLER = STOP), 0

)