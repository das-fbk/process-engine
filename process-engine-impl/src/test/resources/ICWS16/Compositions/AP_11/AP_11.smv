-------------------------------
--   C O M P O S I T I O N   --
-------------------------------
MODULE dn_main

---------------------
--   I N P U T S   --
---------------------
IVAR input:
{
UNDEF, service26_UMS_ManagementTrip, service26_UMS_TripManagementRequest, service26_RESET, service27_UMS_TripRequest, service27_RESET
};

-----------------------
--   O U T P U T S   --
-----------------------
IVAR output:
{
UNDEF, service26_UMS_concrete4, service26_UMS_concrete3, service26_UMS_concrete2, service26_UMS_concrete1, service26_UMS_TripManagementReply, service26_UMS_concrete5, service27_UMS_TripAlternatives, service27_UMS_concrete2, service27_UMS_concrete1
};

-------------------------
--   S E R V I C E S   --
-------------------------
--	 service26
VAR service26: {ST2, ST1, ST4, ST3, ST6, ST5, ST8, ST7, ST0};
ASSIGN
next(service26) :=
case
(service26 = ST2 & output = service26_UMS_concrete2): ST3;
(service26 = ST4 & input = service26_RESET): ST0;
(service26 = ST2 & input = service26_RESET): ST0;
(service26 = ST3 & output = service26_UMS_concrete3): ST4;
(service26 = ST4 & output = service26_UMS_concrete4): ST5;
(service26 = ST6 & input = service26_RESET): ST0;
(service26 = ST1 & input = service26_RESET): ST0;
(service26 = ST5 & input = service26_RESET): ST0;
(service26 = ST8 & input = service26_RESET): ST0;
(service26 = ST5 & output = service26_UMS_concrete5): ST6;
(service26 = ST1 & output = service26_UMS_concrete1): ST2;
(service26 = ST7 & input = service26_RESET): ST0;
(service26 = ST3 & input = service26_RESET): ST0;
(service26 = ST7 & output = service26_UMS_TripManagementReply): ST8;
(service26 = ST0 & input = service26_UMS_TripManagementRequest): ST1;
(service26 = ST6 & input = service26_UMS_ManagementTrip): ST7;
1:service26;
esac;
TRANS ( input = service26_UMS_ManagementTrip) -> (service26 = ST6)
TRANS ( output = service26_UMS_concrete4) -> (service26 = ST4)
TRANS ( input = service26_UMS_TripManagementRequest) -> (service26 = ST0)
TRANS ( output = service26_UMS_concrete3) -> (service26 = ST3)
TRANS ( output = service26_UMS_concrete2) -> (service26 = ST2)
TRANS ( input = service26_RESET) -> (service26 = ST2 | service26 = ST1 | service26 = ST4 | service26 = ST3 | service26 = ST6 | service26 = ST5 | service26 = ST8 | service26 = ST7)
TRANS ( output = service26_UMS_concrete1) -> (service26 = ST1)
TRANS ( output = service26_UMS_TripManagementReply) -> (service26 = ST7)
TRANS ( output = service26_UMS_concrete5) -> (service26 = ST5)

--	 service27
VAR service27: {ST2, ST1, ST4, ST3, ST0};
ASSIGN
next(service27) :=
case
(service27 = ST0 & input = service27_UMS_TripRequest): ST1;
(service27 = ST4 & input = service27_RESET): ST0;
(service27 = ST1 & output = service27_UMS_concrete1): ST2;
(service27 = ST2 & input = service27_RESET): ST0;
(service27 = ST3 & output = service27_UMS_TripAlternatives): ST4;
(service27 = ST2 & output = service27_UMS_concrete2): ST3;
(service27 = ST1 & input = service27_RESET): ST0;
(service27 = ST3 & input = service27_RESET): ST0;
1:service27;
esac;
TRANS ( input = service27_UMS_TripRequest) -> (service27 = ST0)
TRANS ( output = service27_UMS_TripAlternatives) -> (service27 = ST3)
TRANS ( output = service27_UMS_concrete2) -> (service27 = ST2)
TRANS ( input = service27_RESET) -> (service27 = ST2 | service27 = ST1 | service27 = ST4 | service27 = ST3)
TRANS ( output = service27_UMS_concrete1) -> (service27 = ST1)

-----------------------
--   O B J E C T S   --
-----------------------
--	 SetupTrip
VAR SetupTrip: {ORGANIZED, INITIAL};
ASSIGN
next(SetupTrip) :=
case
(input = service26_UMS_ManagementTrip):{(ORGANIZED)};
1:SetupTrip;
esac;
--	 ProvideTrip
VAR ProvideTrip: {TRIP_REQUEST_ACCOMPLISHED, TRIP_MANAGEMENT_ACCOMPLISHED, TRIP_MANAGE_REQUEST_LOADED, TRIP_REQUEST_LOADED, INITIAL};
ASSIGN
next(ProvideTrip) :=
case
(ProvideTrip = INITIAL & input = service27_UMS_TripRequest): TRIP_REQUEST_LOADED;
(ProvideTrip = TRIP_MANAGE_REQUEST_LOADED & output = service26_UMS_TripManagementReply): TRIP_MANAGEMENT_ACCOMPLISHED;
(ProvideTrip = TRIP_REQUEST_LOADED & output = service27_UMS_TripAlternatives): TRIP_REQUEST_ACCOMPLISHED;
(ProvideTrip = TRIP_REQUEST_ACCOMPLISHED & input = service26_UMS_TripManagementRequest): TRIP_MANAGE_REQUEST_LOADED;
1:ProvideTrip;
esac;
-----------------------------------
--   P R E C O N D I T I O N S   --
-----------------------------------
TRANS (input = service26_UMS_TripManagementRequest) -> ((((ProvideTrip = TRIP_REQUEST_ACCOMPLISHED))))
TRANS (output = service26_UMS_TripManagementReply) -> ((((ProvideTrip = TRIP_MANAGE_REQUEST_LOADED))))
TRANS (input = service27_UMS_TripRequest) -> ((((ProvideTrip = INITIAL))))
TRANS (output = service27_UMS_TripAlternatives) -> ((((ProvideTrip = TRIP_REQUEST_LOADED))))
-----------------------------
--   I N V A R I A N T S   --
-----------------------------
--	 Synchronicity
TRANS (1-(input = UNDEF)) + (1-(output = UNDEF)) = 1

---------------------------------------
--   S Y N C H R O N I Z A T I O N   --
---------------------------------------
DEFINE SYNC0 := ((ProvideTrip in {TRIP_REQUEST_ACCOMPLISHED}));
--	 Synchronicity enforcement
TRANS (service26 in {ST2, ST1, ST4, ST3, ST5, ST7}) -> (next(service26) != service26)
TRANS (service27 in {ST2, ST1, ST3}) -> (next(service27) != service27)
-------------------------------
--   C O N T R O L L E R S   --
-------------------------------
--	 Fault Controller
VAR FAULT_CONTROLLER: { START, STOP };
INIT FAULT_CONTROLLER = START

ASSIGN
next(FAULT_CONTROLLER) := 
case
(FAULT_CONTROLLER = START & FALSE): STOP;
1: FAULT_CONTROLLER;
esac;

TRANS (output != UNDEF | input != UNDEF) -> (FAULT_CONTROLLER != STOP)

-------------------------------------
--   I N I T I A L I Z A T I O N   --
-------------------------------------
INIT ((SetupTrip = INITIAL) & (ProvideTrip = INITIAL))
INIT ((service26 = ST0) & (service27 = ST0))

-----------------
--   G O A L   --
-----------------
MODULE main
VAR dn_main : dn_main;

GOAL ONEOF(
(dn_main.SYNC0), 1;
(dn_main.FAULT_CONTROLLER = STOP), 0

)