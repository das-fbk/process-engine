-------------------------------
--   C O M P O S I T I O N   --
-------------------------------
MODULE dn_main

---------------------
--   I N P U T S   --
---------------------
IVAR input:
{
UNDEF, service172_UMS_ManagementTrip, service172_UMS_TripManagementRequest, service172_RESET, service174_UMS_TripRequest, service174_RESET
};

-----------------------
--   O U T P U T S   --
-----------------------
IVAR output:
{
UNDEF, service172_UMS_concrete4, service172_UMS_concrete3, service172_UMS_concrete2, service172_UMS_concrete1, service172_UMS_TripManagementReply, service172_UMS_concrete5, service174_UMS_TripAlternatives, service174_UMS_concrete2, service174_UMS_concrete1
};

-------------------------
--   S E R V I C E S   --
-------------------------
--	 service172
VAR service172: {ST2, ST1, ST4, ST3, ST6, ST5, ST8, ST7, ST0};
ASSIGN
next(service172) :=
case
(service172 = ST2 & output = service172_UMS_concrete2): ST3;
(service172 = ST4 & output = service172_UMS_concrete4): ST5;
(service172 = ST4 & input = service172_RESET): ST0;
(service172 = ST1 & output = service172_UMS_concrete1): ST2;
(service172 = ST6 & input = service172_RESET): ST0;
(service172 = ST8 & input = service172_RESET): ST0;
(service172 = ST0 & input = service172_UMS_TripManagementRequest): ST1;
(service172 = ST3 & output = service172_UMS_concrete3): ST4;
(service172 = ST1 & input = service172_RESET): ST0;
(service172 = ST6 & input = service172_UMS_ManagementTrip): ST7;
(service172 = ST5 & input = service172_RESET): ST0;
(service172 = ST5 & output = service172_UMS_concrete5): ST6;
(service172 = ST7 & output = service172_UMS_TripManagementReply): ST8;
(service172 = ST2 & input = service172_RESET): ST0;
(service172 = ST3 & input = service172_RESET): ST0;
(service172 = ST7 & input = service172_RESET): ST0;
1:service172;
esac;
TRANS ( input = service172_UMS_ManagementTrip) -> (service172 = ST6)
TRANS ( output = service172_UMS_concrete4) -> (service172 = ST4)
TRANS ( input = service172_UMS_TripManagementRequest) -> (service172 = ST0)
TRANS ( output = service172_UMS_concrete3) -> (service172 = ST3)
TRANS ( output = service172_UMS_concrete2) -> (service172 = ST2)
TRANS ( input = service172_RESET) -> (service172 = ST2 | service172 = ST1 | service172 = ST4 | service172 = ST3 | service172 = ST6 | service172 = ST5 | service172 = ST8 | service172 = ST7)
TRANS ( output = service172_UMS_concrete1) -> (service172 = ST1)
TRANS ( output = service172_UMS_TripManagementReply) -> (service172 = ST7)
TRANS ( output = service172_UMS_concrete5) -> (service172 = ST5)

--	 service174
VAR service174: {ST2, ST1, ST4, ST3, ST0};
ASSIGN
next(service174) :=
case
(service174 = ST2 & input = service174_RESET): ST0;
(service174 = ST1 & input = service174_RESET): ST0;
(service174 = ST0 & input = service174_UMS_TripRequest): ST1;
(service174 = ST2 & output = service174_UMS_concrete2): ST3;
(service174 = ST4 & input = service174_RESET): ST0;
(service174 = ST3 & input = service174_RESET): ST0;
(service174 = ST1 & output = service174_UMS_concrete1): ST2;
(service174 = ST3 & output = service174_UMS_TripAlternatives): ST4;
1:service174;
esac;
TRANS ( input = service174_UMS_TripRequest) -> (service174 = ST0)
TRANS ( output = service174_UMS_TripAlternatives) -> (service174 = ST3)
TRANS ( output = service174_UMS_concrete2) -> (service174 = ST2)
TRANS ( input = service174_RESET) -> (service174 = ST2 | service174 = ST1 | service174 = ST4 | service174 = ST3)
TRANS ( output = service174_UMS_concrete1) -> (service174 = ST1)

-----------------------
--   O B J E C T S   --
-----------------------
--	 SetupTrip
VAR SetupTrip: {ORGANIZED, INITIAL};
ASSIGN
next(SetupTrip) :=
case
(input = service172_UMS_ManagementTrip):{(ORGANIZED)};
1:SetupTrip;
esac;
--	 ProvideTrip
VAR ProvideTrip: {TRIP_REQUEST_ACCOMPLISHED, TRIP_MANAGEMENT_ACCOMPLISHED, TRIP_MANAGE_REQUEST_LOADED, TRIP_REQUEST_LOADED, INITIAL};
ASSIGN
next(ProvideTrip) :=
case
(ProvideTrip = TRIP_REQUEST_ACCOMPLISHED & input = service172_UMS_TripManagementRequest): TRIP_MANAGE_REQUEST_LOADED;
(ProvideTrip = INITIAL & input = service174_UMS_TripRequest): TRIP_REQUEST_LOADED;
(ProvideTrip = TRIP_MANAGE_REQUEST_LOADED & output = service172_UMS_TripManagementReply): TRIP_MANAGEMENT_ACCOMPLISHED;
(ProvideTrip = TRIP_REQUEST_LOADED & output = service174_UMS_TripAlternatives): TRIP_REQUEST_ACCOMPLISHED;
1:ProvideTrip;
esac;
-----------------------------------
--   P R E C O N D I T I O N S   --
-----------------------------------
TRANS (input = service172_UMS_TripManagementRequest) -> ((((ProvideTrip = TRIP_REQUEST_ACCOMPLISHED))))
TRANS (output = service172_UMS_TripManagementReply) -> ((((ProvideTrip = TRIP_MANAGE_REQUEST_LOADED))))
TRANS (input = service174_UMS_TripRequest) -> ((((ProvideTrip = INITIAL))))
TRANS (output = service174_UMS_TripAlternatives) -> ((((ProvideTrip = TRIP_REQUEST_LOADED))))
-----------------------------
--   I N V A R I A N T S   --
-----------------------------
--	 Synchronicity
TRANS (1-(input = UNDEF)) + (1-(output = UNDEF)) = 1

---------------------------------------
--   S Y N C H R O N I Z A T I O N   --
---------------------------------------
DEFINE SYNC0 := ((ProvideTrip in {TRIP_MANAGEMENT_ACCOMPLISHED}));
--	 Synchronicity enforcement
TRANS (service172 in {ST2, ST1, ST4, ST3, ST5, ST7}) -> (next(service172) != service172)
TRANS (service174 in {ST2, ST1, ST3}) -> (next(service174) != service174)
-------------------------------
--   C O N T R O L L E R S   --
-------------------------------
--	 Fault Controller
VAR FAULT_CONTROLLER: { START, STOP };
INIT FAULT_CONTROLLER = START

ASSIGN
next(FAULT_CONTROLLER) := 
case
(FAULT_CONTROLLER = START & FALSE): STOP;
1: FAULT_CONTROLLER;
esac;

TRANS (output != UNDEF | input != UNDEF) -> (FAULT_CONTROLLER != STOP)

-------------------------------------
--   I N I T I A L I Z A T I O N   --
-------------------------------------
INIT ((SetupTrip = INITIAL) & (ProvideTrip = TRIP_REQUEST_ACCOMPLISHED))
INIT ((service172 = ST0) & (service174 = ST0))

-----------------
--   G O A L   --
-----------------
MODULE main
VAR dn_main : dn_main;

GOAL ONEOF(
(dn_main.SYNC0), 1;
(dn_main.FAULT_CONTROLLER = STOP), 0

)