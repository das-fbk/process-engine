<?xml version="1.0" encoding="UTF-8"?>
<tns:domainProperty id="ParkAndRideTrip" xmlns:tns="http://das.fbk.eu/Process" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://das.fbk.eu/Process ../../../../../../../schema-allow-ensembles/schema/allow-ensembles/DomainProperty.xsd ">
  
  <tns:state isInitial="true">INITIAL</tns:state>
  <tns:state>USER_REGISTERED</tns:state>
  <tns:state>CAR_ROUTE_PLANNED</tns:state>
  <tns:state>PARK_REACHED</tns:state>
  <tns:state>SHUTTLE_BOOKED</tns:state>
  <tns:state>CHECK_IN_EXECUTED</tns:state>
  
  <tns:event>userRegistration</tns:event>
  <tns:event>planRoute</tns:event>
  <tns:event>reachPark</tns:event>
  <tns:event>bookShuttle</tns:event>
  <tns:event>checkInOnShuttle</tns:event>
  
  <tns:transition isControllable="true">
    <tns:from>INITIAL</tns:from>
    <tns:to>USER_REGISTERED</tns:to>
    <tns:event>userRegistration</tns:event>
  </tns:transition>
  
  <tns:transition isControllable="true">
    <tns:from>USER_REGISTERED</tns:from>
    <tns:to>CAR_ROUTE_PLANNED</tns:to>
    <tns:event>planRoute</tns:event>
  </tns:transition>
  
  <tns:transition isControllable="true">
    <tns:from>CAR_ROUTE_PLANNED</tns:from>
    <tns:to>PARK_REACHED</tns:to>
    <tns:event>reachPark</tns:event>
  </tns:transition>
  
  <tns:transition isControllable="true">
    <tns:from>PARK_REACHED</tns:from>
    <tns:to>SHUTTLE_BOOKED</tns:to>
    <tns:event>bookShuttle</tns:event>
  </tns:transition>
  
  <tns:transition isControllable="true">
    <tns:from>SHUTTLE_BOOKED</tns:from>
    <tns:to>CHECK_IN_EXECUTED</tns:to>
    <tns:event>checkInOnShuttle</tns:event>
  </tns:transition>
  
</tns:domainProperty>
