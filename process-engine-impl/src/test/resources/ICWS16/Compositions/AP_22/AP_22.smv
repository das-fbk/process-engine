-------------------------------
--   C O M P O S I T I O N   --
-------------------------------
MODULE dn_main

---------------------
--   I N P U T S   --
---------------------
IVAR input:
{
UNDEF, service48_UMS_ManagementTrip, service48_UMS_TripManagementRequest, service48_RESET, service49_UMS_TripRequest, service49_RESET
};

-----------------------
--   O U T P U T S   --
-----------------------
IVAR output:
{
UNDEF, service48_UMS_concrete4, service48_UMS_concrete3, service48_UMS_concrete2, service48_UMS_concrete1, service48_UMS_TripManagementReply, service48_UMS_concrete5, service49_UMS_TripAlternatives, service49_UMS_concrete2, service49_UMS_concrete1
};

-------------------------
--   S E R V I C E S   --
-------------------------
--	 service48
VAR service48: {ST2, ST1, ST4, ST3, ST6, ST5, ST8, ST7, ST0};
ASSIGN
next(service48) :=
case
(service48 = ST2 & input = service48_RESET): ST0;
(service48 = ST2 & output = service48_UMS_concrete2): ST3;
(service48 = ST6 & input = service48_UMS_ManagementTrip): ST7;
(service48 = ST6 & input = service48_RESET): ST0;
(service48 = ST3 & output = service48_UMS_concrete3): ST4;
(service48 = ST5 & input = service48_RESET): ST0;
(service48 = ST5 & output = service48_UMS_concrete5): ST6;
(service48 = ST4 & input = service48_RESET): ST0;
(service48 = ST1 & input = service48_RESET): ST0;
(service48 = ST0 & input = service48_UMS_TripManagementRequest): ST1;
(service48 = ST3 & input = service48_RESET): ST0;
(service48 = ST4 & output = service48_UMS_concrete4): ST5;
(service48 = ST8 & input = service48_RESET): ST0;
(service48 = ST7 & input = service48_RESET): ST0;
(service48 = ST1 & output = service48_UMS_concrete1): ST2;
(service48 = ST7 & output = service48_UMS_TripManagementReply): ST8;
1:service48;
esac;
TRANS ( input = service48_UMS_ManagementTrip) -> (service48 = ST6)
TRANS ( output = service48_UMS_concrete4) -> (service48 = ST4)
TRANS ( input = service48_UMS_TripManagementRequest) -> (service48 = ST0)
TRANS ( output = service48_UMS_concrete3) -> (service48 = ST3)
TRANS ( output = service48_UMS_concrete2) -> (service48 = ST2)
TRANS ( input = service48_RESET) -> (service48 = ST2 | service48 = ST1 | service48 = ST4 | service48 = ST3 | service48 = ST6 | service48 = ST5 | service48 = ST8 | service48 = ST7)
TRANS ( output = service48_UMS_concrete1) -> (service48 = ST1)
TRANS ( output = service48_UMS_TripManagementReply) -> (service48 = ST7)
TRANS ( output = service48_UMS_concrete5) -> (service48 = ST5)

--	 service49
VAR service49: {ST2, ST1, ST4, ST3, ST0};
ASSIGN
next(service49) :=
case
(service49 = ST1 & output = service49_UMS_concrete1): ST2;
(service49 = ST2 & output = service49_UMS_concrete2): ST3;
(service49 = ST3 & output = service49_UMS_TripAlternatives): ST4;
(service49 = ST4 & input = service49_RESET): ST0;
(service49 = ST0 & input = service49_UMS_TripRequest): ST1;
(service49 = ST2 & input = service49_RESET): ST0;
(service49 = ST3 & input = service49_RESET): ST0;
(service49 = ST1 & input = service49_RESET): ST0;
1:service49;
esac;
TRANS ( input = service49_UMS_TripRequest) -> (service49 = ST0)
TRANS ( output = service49_UMS_TripAlternatives) -> (service49 = ST3)
TRANS ( output = service49_UMS_concrete2) -> (service49 = ST2)
TRANS ( input = service49_RESET) -> (service49 = ST2 | service49 = ST1 | service49 = ST4 | service49 = ST3)
TRANS ( output = service49_UMS_concrete1) -> (service49 = ST1)

-----------------------
--   O B J E C T S   --
-----------------------
--	 SetupTrip
VAR SetupTrip: {ORGANIZED, INITIAL};
ASSIGN
next(SetupTrip) :=
case
(input = service48_UMS_ManagementTrip):{(ORGANIZED)};
1:SetupTrip;
esac;
--	 ProvideTrip
VAR ProvideTrip: {TRIP_REQUEST_ACCOMPLISHED, TRIP_MANAGEMENT_ACCOMPLISHED, TRIP_MANAGE_REQUEST_LOADED, TRIP_REQUEST_LOADED, INITIAL};
ASSIGN
next(ProvideTrip) :=
case
(ProvideTrip = TRIP_MANAGE_REQUEST_LOADED & output = service48_UMS_TripManagementReply): TRIP_MANAGEMENT_ACCOMPLISHED;
(ProvideTrip = TRIP_REQUEST_ACCOMPLISHED & input = service48_UMS_TripManagementRequest): TRIP_MANAGE_REQUEST_LOADED;
(ProvideTrip = INITIAL & input = service49_UMS_TripRequest): TRIP_REQUEST_LOADED;
(ProvideTrip = TRIP_REQUEST_LOADED & output = service49_UMS_TripAlternatives): TRIP_REQUEST_ACCOMPLISHED;
1:ProvideTrip;
esac;
-----------------------------------
--   P R E C O N D I T I O N S   --
-----------------------------------
TRANS (input = service48_UMS_TripManagementRequest) -> ((((ProvideTrip = TRIP_REQUEST_ACCOMPLISHED))))
TRANS (output = service48_UMS_TripManagementReply) -> ((((ProvideTrip = TRIP_MANAGE_REQUEST_LOADED))))
TRANS (input = service49_UMS_TripRequest) -> ((((ProvideTrip = INITIAL))))
TRANS (output = service49_UMS_TripAlternatives) -> ((((ProvideTrip = TRIP_REQUEST_LOADED))))
-----------------------------
--   I N V A R I A N T S   --
-----------------------------
--	 Synchronicity
TRANS (1-(input = UNDEF)) + (1-(output = UNDEF)) = 1

---------------------------------------
--   S Y N C H R O N I Z A T I O N   --
---------------------------------------
DEFINE SYNC0 := ((ProvideTrip in {TRIP_REQUEST_ACCOMPLISHED}));
--	 Synchronicity enforcement
TRANS (service48 in {ST2, ST1, ST4, ST3, ST5, ST7}) -> (next(service48) != service48)
TRANS (service49 in {ST2, ST1, ST3}) -> (next(service49) != service49)
-------------------------------
--   C O N T R O L L E R S   --
-------------------------------
--	 Fault Controller
VAR FAULT_CONTROLLER: { START, STOP };
INIT FAULT_CONTROLLER = START

ASSIGN
next(FAULT_CONTROLLER) := 
case
(FAULT_CONTROLLER = START & FALSE): STOP;
1: FAULT_CONTROLLER;
esac;

TRANS (output != UNDEF | input != UNDEF) -> (FAULT_CONTROLLER != STOP)

-------------------------------------
--   I N I T I A L I Z A T I O N   --
-------------------------------------
INIT ((SetupTrip = INITIAL) & (ProvideTrip = INITIAL))
INIT ((service48 = ST0) & (service49 = ST0))

-----------------
--   G O A L   --
-----------------
MODULE main
VAR dn_main : dn_main;

GOAL ONEOF(
(dn_main.SYNC0), 1;
(dn_main.FAULT_CONTROLLER = STOP), 0

)