-------------------------------
--   C O M P O S I T I O N   --
-------------------------------
MODULE dn_main

---------------------
--   I N P U T S   --
---------------------
IVAR input:
{
UNDEF, service132_UMS_ManagementTrip, service132_UMS_TripManagementRequest, service132_RESET, service134_UMS_TripRequest, service134_RESET
};

-----------------------
--   O U T P U T S   --
-----------------------
IVAR output:
{
UNDEF, service132_UMS_concrete4, service132_UMS_concrete3, service132_UMS_concrete2, service132_UMS_concrete1, service132_UMS_TripManagementReply, service132_UMS_concrete5, service134_UMS_TripAlternatives, service134_UMS_concrete2, service134_UMS_concrete1
};

-------------------------
--   S E R V I C E S   --
-------------------------
--	 service132
VAR service132: {ST2, ST1, ST4, ST3, ST6, ST5, ST8, ST7, ST0};
ASSIGN
next(service132) :=
case
(service132 = ST2 & output = service132_UMS_concrete2): ST3;
(service132 = ST1 & input = service132_RESET): ST0;
(service132 = ST5 & output = service132_UMS_concrete5): ST6;
(service132 = ST3 & output = service132_UMS_concrete3): ST4;
(service132 = ST7 & output = service132_UMS_TripManagementReply): ST8;
(service132 = ST6 & input = service132_UMS_ManagementTrip): ST7;
(service132 = ST5 & input = service132_RESET): ST0;
(service132 = ST2 & input = service132_RESET): ST0;
(service132 = ST4 & input = service132_RESET): ST0;
(service132 = ST7 & input = service132_RESET): ST0;
(service132 = ST8 & input = service132_RESET): ST0;
(service132 = ST6 & input = service132_RESET): ST0;
(service132 = ST0 & input = service132_UMS_TripManagementRequest): ST1;
(service132 = ST1 & output = service132_UMS_concrete1): ST2;
(service132 = ST3 & input = service132_RESET): ST0;
(service132 = ST4 & output = service132_UMS_concrete4): ST5;
1:service132;
esac;
TRANS ( input = service132_UMS_ManagementTrip) -> (service132 = ST6)
TRANS ( output = service132_UMS_concrete4) -> (service132 = ST4)
TRANS ( input = service132_UMS_TripManagementRequest) -> (service132 = ST0)
TRANS ( output = service132_UMS_concrete3) -> (service132 = ST3)
TRANS ( output = service132_UMS_concrete2) -> (service132 = ST2)
TRANS ( input = service132_RESET) -> (service132 = ST2 | service132 = ST1 | service132 = ST4 | service132 = ST3 | service132 = ST6 | service132 = ST5 | service132 = ST8 | service132 = ST7)
TRANS ( output = service132_UMS_concrete1) -> (service132 = ST1)
TRANS ( output = service132_UMS_TripManagementReply) -> (service132 = ST7)
TRANS ( output = service132_UMS_concrete5) -> (service132 = ST5)

--	 service134
VAR service134: {ST2, ST1, ST4, ST3, ST0};
ASSIGN
next(service134) :=
case
(service134 = ST0 & input = service134_UMS_TripRequest): ST1;
(service134 = ST2 & input = service134_RESET): ST0;
(service134 = ST4 & input = service134_RESET): ST0;
(service134 = ST1 & input = service134_RESET): ST0;
(service134 = ST3 & output = service134_UMS_TripAlternatives): ST4;
(service134 = ST3 & input = service134_RESET): ST0;
(service134 = ST1 & output = service134_UMS_concrete1): ST2;
(service134 = ST2 & output = service134_UMS_concrete2): ST3;
1:service134;
esac;
TRANS ( input = service134_UMS_TripRequest) -> (service134 = ST0)
TRANS ( output = service134_UMS_TripAlternatives) -> (service134 = ST3)
TRANS ( output = service134_UMS_concrete2) -> (service134 = ST2)
TRANS ( input = service134_RESET) -> (service134 = ST2 | service134 = ST1 | service134 = ST4 | service134 = ST3)
TRANS ( output = service134_UMS_concrete1) -> (service134 = ST1)

-----------------------
--   O B J E C T S   --
-----------------------
--	 SetupTrip
VAR SetupTrip: {ORGANIZED, INITIAL};
ASSIGN
next(SetupTrip) :=
case
(input = service132_UMS_ManagementTrip):{(ORGANIZED)};
1:SetupTrip;
esac;
--	 ProvideTrip
VAR ProvideTrip: {TRIP_REQUEST_ACCOMPLISHED, TRIP_MANAGEMENT_ACCOMPLISHED, TRIP_MANAGE_REQUEST_LOADED, TRIP_REQUEST_LOADED, INITIAL};
ASSIGN
next(ProvideTrip) :=
case
(ProvideTrip = TRIP_MANAGE_REQUEST_LOADED & output = service132_UMS_TripManagementReply): TRIP_MANAGEMENT_ACCOMPLISHED;
(ProvideTrip = INITIAL & input = service134_UMS_TripRequest): TRIP_REQUEST_LOADED;
(ProvideTrip = TRIP_REQUEST_LOADED & output = service134_UMS_TripAlternatives): TRIP_REQUEST_ACCOMPLISHED;
(ProvideTrip = TRIP_REQUEST_ACCOMPLISHED & input = service132_UMS_TripManagementRequest): TRIP_MANAGE_REQUEST_LOADED;
1:ProvideTrip;
esac;
-----------------------------------
--   P R E C O N D I T I O N S   --
-----------------------------------
TRANS (input = service132_UMS_TripManagementRequest) -> ((((ProvideTrip = TRIP_REQUEST_ACCOMPLISHED))))
TRANS (output = service132_UMS_TripManagementReply) -> ((((ProvideTrip = TRIP_MANAGE_REQUEST_LOADED))))
TRANS (input = service134_UMS_TripRequest) -> ((((ProvideTrip = INITIAL))))
TRANS (output = service134_UMS_TripAlternatives) -> ((((ProvideTrip = TRIP_REQUEST_LOADED))))
-----------------------------
--   I N V A R I A N T S   --
-----------------------------
--	 Synchronicity
TRANS (1-(input = UNDEF)) + (1-(output = UNDEF)) = 1

---------------------------------------
--   S Y N C H R O N I Z A T I O N   --
---------------------------------------
DEFINE SYNC0 := ((ProvideTrip in {TRIP_MANAGEMENT_ACCOMPLISHED}));
--	 Synchronicity enforcement
TRANS (service132 in {ST2, ST1, ST4, ST3, ST5, ST7}) -> (next(service132) != service132)
TRANS (service134 in {ST2, ST1, ST3}) -> (next(service134) != service134)
-------------------------------
--   C O N T R O L L E R S   --
-------------------------------
--	 Fault Controller
VAR FAULT_CONTROLLER: { START, STOP };
INIT FAULT_CONTROLLER = START

ASSIGN
next(FAULT_CONTROLLER) := 
case
(FAULT_CONTROLLER = START & FALSE): STOP;
1: FAULT_CONTROLLER;
esac;

TRANS (output != UNDEF | input != UNDEF) -> (FAULT_CONTROLLER != STOP)

-------------------------------------
--   I N I T I A L I Z A T I O N   --
-------------------------------------
INIT ((SetupTrip = INITIAL) & (ProvideTrip = TRIP_REQUEST_ACCOMPLISHED))
INIT ((service132 = ST0) & (service134 = ST0))

-----------------
--   G O A L   --
-----------------
MODULE main
VAR dn_main : dn_main;

GOAL ONEOF(
(dn_main.SYNC0), 1;
(dn_main.FAULT_CONTROLLER = STOP), 0

)