-------------------------------
--   C O M P O S I T I O N   --
-------------------------------
MODULE dn_main

---------------------
--   I N P U T S   --
---------------------
IVAR input:
{
UNDEF, service14_RESET, service14_PP_PaymentRequest
};

-----------------------
--   O U T P U T S   --
-----------------------
IVAR output:
{
UNDEF, service14_PP_PaymentReply
};

-------------------------
--   S E R V I C E S   --
-------------------------
--	 service14
VAR service14: {ST2, ST1, ST0};
ASSIGN
next(service14) :=
case
(service14 = ST1 & output = service14_PP_PaymentReply): ST2;
(service14 = ST0 & input = service14_PP_PaymentRequest): ST1;
(service14 = ST2 & input = service14_RESET): ST0;
(service14 = ST1 & input = service14_RESET): ST0;
1:service14;
esac;
TRANS ( input = service14_RESET) -> (service14 = ST2 | service14 = ST1)
TRANS ( input = service14_PP_PaymentRequest) -> (service14 = ST0)
TRANS ( output = service14_PP_PaymentReply) -> (service14 = ST1)

-----------------------
--   O B J E C T S   --
-----------------------
--	 PayPalPayment
VAR PayPalPayment: {INITIAL, PP_PAYMENT_EXECUTED};
ASSIGN
next(PayPalPayment) :=
case
(PayPalPayment = INITIAL & output = service14_PP_PaymentReply): PP_PAYMENT_EXECUTED;
1:PayPalPayment;
esac;
-----------------------------------
--   P R E C O N D I T I O N S   --
-----------------------------------
TRANS (input = service14_PP_PaymentRequest) -> ((((PayPalPayment = INITIAL))))
-----------------------------
--   I N V A R I A N T S   --
-----------------------------
--	 Synchronicity
TRANS (1-(input = UNDEF)) + (1-(output = UNDEF)) = 1

---------------------------------------
--   S Y N C H R O N I Z A T I O N   --
---------------------------------------
DEFINE SYNC0 := ((PayPalPayment in {PP_PAYMENT_EXECUTED}));
--	 Synchronicity enforcement
TRANS (service14 in {ST1}) -> (next(service14) != service14)
-------------------------------
--   C O N T R O L L E R S   --
-------------------------------
--	 Fault Controller
VAR FAULT_CONTROLLER: { START, STOP };
INIT FAULT_CONTROLLER = START

ASSIGN
next(FAULT_CONTROLLER) := 
case
(FAULT_CONTROLLER = START & FALSE): STOP;
1: FAULT_CONTROLLER;
esac;

TRANS (output != UNDEF | input != UNDEF) -> (FAULT_CONTROLLER != STOP)

-------------------------------------
--   I N I T I A L I Z A T I O N   --
-------------------------------------
INIT ((PayPalPayment = INITIAL))
INIT ((service14 = ST0))

-----------------
--   G O A L   --
-----------------
MODULE main
VAR dn_main : dn_main;

GOAL ONEOF(
(dn_main.SYNC0), 1;
(dn_main.FAULT_CONTROLLER = STOP), 0

)