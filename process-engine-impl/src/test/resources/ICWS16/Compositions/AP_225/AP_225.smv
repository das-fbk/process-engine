-------------------------------
--   C O M P O S I T I O N   --
-------------------------------
MODULE dn_main

---------------------
--   I N P U T S   --
---------------------
IVAR input:
{
UNDEF, service438_RESET, service438_PP_PaymentRequest
};

-----------------------
--   O U T P U T S   --
-----------------------
IVAR output:
{
UNDEF, service438_PP_concrete1, service438_PP_concrete3, service438_PP_concrete2, service438_PP_PaymentReply
};

-------------------------
--   S E R V I C E S   --
-------------------------
--	 service438
VAR service438: {ST2, ST1, ST4, ST3, ST5, ST0};
ASSIGN
next(service438) :=
case
(service438 = ST1 & output = service438_PP_concrete1): ST2;
(service438 = ST2 & output = service438_PP_concrete2): ST3;
(service438 = ST4 & input = service438_RESET): ST0;
(service438 = ST3 & output = service438_PP_concrete3): ST4;
(service438 = ST0 & input = service438_PP_PaymentRequest): ST1;
(service438 = ST4 & output = service438_PP_PaymentReply): ST5;
(service438 = ST1 & input = service438_RESET): ST0;
(service438 = ST2 & input = service438_RESET): ST0;
(service438 = ST5 & input = service438_RESET): ST0;
(service438 = ST3 & input = service438_RESET): ST0;
1:service438;
esac;
TRANS ( output = service438_PP_concrete1) -> (service438 = ST1)
TRANS ( output = service438_PP_concrete3) -> (service438 = ST3)
TRANS ( output = service438_PP_concrete2) -> (service438 = ST2)
TRANS ( input = service438_RESET) -> (service438 = ST2 | service438 = ST1 | service438 = ST4 | service438 = ST3 | service438 = ST5)
TRANS ( input = service438_PP_PaymentRequest) -> (service438 = ST0)
TRANS ( output = service438_PP_PaymentReply) -> (service438 = ST4)

-----------------------
--   O B J E C T S   --
-----------------------
--	 PayPalPayment
VAR PayPalPayment: {INITIAL, PP_PAYMENT_EXECUTED};
ASSIGN
next(PayPalPayment) :=
case
(PayPalPayment = INITIAL & output = service438_PP_PaymentReply): PP_PAYMENT_EXECUTED;
1:PayPalPayment;
esac;
-----------------------------------
--   P R E C O N D I T I O N S   --
-----------------------------------
TRANS (input = service438_PP_PaymentRequest) -> ((((PayPalPayment = INITIAL))))
-----------------------------
--   I N V A R I A N T S   --
-----------------------------
--	 Synchronicity
TRANS (1-(input = UNDEF)) + (1-(output = UNDEF)) = 1

---------------------------------------
--   S Y N C H R O N I Z A T I O N   --
---------------------------------------
DEFINE SYNC0 := ((PayPalPayment in {PP_PAYMENT_EXECUTED}));
--	 Synchronicity enforcement
TRANS (service438 in {ST2, ST1, ST4, ST3}) -> (next(service438) != service438)
-------------------------------
--   C O N T R O L L E R S   --
-------------------------------
--	 Fault Controller
VAR FAULT_CONTROLLER: { START, STOP };
INIT FAULT_CONTROLLER = START

ASSIGN
next(FAULT_CONTROLLER) := 
case
(FAULT_CONTROLLER = START & FALSE): STOP;
1: FAULT_CONTROLLER;
esac;

TRANS (output != UNDEF | input != UNDEF) -> (FAULT_CONTROLLER != STOP)

-------------------------------------
--   I N I T I A L I Z A T I O N   --
-------------------------------------
INIT ((PayPalPayment = INITIAL))
INIT ((service438 = ST0))

-----------------
--   G O A L   --
-----------------
MODULE main
VAR dn_main : dn_main;

GOAL ONEOF(
(dn_main.SYNC0), 1;
(dn_main.FAULT_CONTROLLER = STOP), 0

)