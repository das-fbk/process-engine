-------------------------------
--   C O M P O S I T I O N   --
-------------------------------
MODULE dn_main

---------------------
--   I N P U T S   --
---------------------
IVAR input:
{
UNDEF, service128_UMS_ManagementTrip, service128_UMS_TripManagementRequest, service128_RESET, service130_UMS_TripRequest, service130_RESET
};

-----------------------
--   O U T P U T S   --
-----------------------
IVAR output:
{
UNDEF, service128_UMS_concrete4, service128_UMS_concrete3, service128_UMS_concrete2, service128_UMS_concrete1, service128_UMS_TripManagementReply, service128_UMS_concrete5, service130_UMS_TripAlternatives, service130_UMS_concrete2, service130_UMS_concrete1
};

-------------------------
--   S E R V I C E S   --
-------------------------
--	 service128
VAR service128: {ST2, ST1, ST4, ST3, ST6, ST5, ST8, ST7, ST0};
ASSIGN
next(service128) :=
case
(service128 = ST6 & input = service128_RESET): ST0;
(service128 = ST3 & output = service128_UMS_concrete3): ST4;
(service128 = ST8 & input = service128_RESET): ST0;
(service128 = ST1 & input = service128_RESET): ST0;
(service128 = ST5 & output = service128_UMS_concrete5): ST6;
(service128 = ST0 & input = service128_UMS_TripManagementRequest): ST1;
(service128 = ST1 & output = service128_UMS_concrete1): ST2;
(service128 = ST2 & input = service128_RESET): ST0;
(service128 = ST7 & input = service128_RESET): ST0;
(service128 = ST4 & output = service128_UMS_concrete4): ST5;
(service128 = ST6 & input = service128_UMS_ManagementTrip): ST7;
(service128 = ST4 & input = service128_RESET): ST0;
(service128 = ST3 & input = service128_RESET): ST0;
(service128 = ST7 & output = service128_UMS_TripManagementReply): ST8;
(service128 = ST2 & output = service128_UMS_concrete2): ST3;
(service128 = ST5 & input = service128_RESET): ST0;
1:service128;
esac;
TRANS ( input = service128_UMS_ManagementTrip) -> (service128 = ST6)
TRANS ( output = service128_UMS_concrete4) -> (service128 = ST4)
TRANS ( input = service128_UMS_TripManagementRequest) -> (service128 = ST0)
TRANS ( output = service128_UMS_concrete3) -> (service128 = ST3)
TRANS ( output = service128_UMS_concrete2) -> (service128 = ST2)
TRANS ( input = service128_RESET) -> (service128 = ST2 | service128 = ST1 | service128 = ST4 | service128 = ST3 | service128 = ST6 | service128 = ST5 | service128 = ST8 | service128 = ST7)
TRANS ( output = service128_UMS_concrete1) -> (service128 = ST1)
TRANS ( output = service128_UMS_TripManagementReply) -> (service128 = ST7)
TRANS ( output = service128_UMS_concrete5) -> (service128 = ST5)

--	 service130
VAR service130: {ST2, ST1, ST4, ST3, ST0};
ASSIGN
next(service130) :=
case
(service130 = ST3 & output = service130_UMS_TripAlternatives): ST4;
(service130 = ST2 & input = service130_RESET): ST0;
(service130 = ST1 & input = service130_RESET): ST0;
(service130 = ST3 & input = service130_RESET): ST0;
(service130 = ST0 & input = service130_UMS_TripRequest): ST1;
(service130 = ST1 & output = service130_UMS_concrete1): ST2;
(service130 = ST2 & output = service130_UMS_concrete2): ST3;
(service130 = ST4 & input = service130_RESET): ST0;
1:service130;
esac;
TRANS ( input = service130_UMS_TripRequest) -> (service130 = ST0)
TRANS ( output = service130_UMS_TripAlternatives) -> (service130 = ST3)
TRANS ( output = service130_UMS_concrete2) -> (service130 = ST2)
TRANS ( input = service130_RESET) -> (service130 = ST2 | service130 = ST1 | service130 = ST4 | service130 = ST3)
TRANS ( output = service130_UMS_concrete1) -> (service130 = ST1)

-----------------------
--   O B J E C T S   --
-----------------------
--	 SetupTrip
VAR SetupTrip: {ORGANIZED, INITIAL};
ASSIGN
next(SetupTrip) :=
case
(input = service128_UMS_ManagementTrip):{(ORGANIZED)};
1:SetupTrip;
esac;
--	 ProvideTrip
VAR ProvideTrip: {TRIP_REQUEST_ACCOMPLISHED, TRIP_MANAGEMENT_ACCOMPLISHED, TRIP_MANAGE_REQUEST_LOADED, TRIP_REQUEST_LOADED, INITIAL};
ASSIGN
next(ProvideTrip) :=
case
(ProvideTrip = TRIP_REQUEST_ACCOMPLISHED & input = service128_UMS_TripManagementRequest): TRIP_MANAGE_REQUEST_LOADED;
(ProvideTrip = TRIP_MANAGE_REQUEST_LOADED & output = service128_UMS_TripManagementReply): TRIP_MANAGEMENT_ACCOMPLISHED;
(ProvideTrip = INITIAL & input = service130_UMS_TripRequest): TRIP_REQUEST_LOADED;
(ProvideTrip = TRIP_REQUEST_LOADED & output = service130_UMS_TripAlternatives): TRIP_REQUEST_ACCOMPLISHED;
1:ProvideTrip;
esac;
-----------------------------------
--   P R E C O N D I T I O N S   --
-----------------------------------
TRANS (input = service128_UMS_TripManagementRequest) -> ((((ProvideTrip = TRIP_REQUEST_ACCOMPLISHED))))
TRANS (output = service128_UMS_TripManagementReply) -> ((((ProvideTrip = TRIP_MANAGE_REQUEST_LOADED))))
TRANS (input = service130_UMS_TripRequest) -> ((((ProvideTrip = INITIAL))))
TRANS (output = service130_UMS_TripAlternatives) -> ((((ProvideTrip = TRIP_REQUEST_LOADED))))
-----------------------------
--   I N V A R I A N T S   --
-----------------------------
--	 Synchronicity
TRANS (1-(input = UNDEF)) + (1-(output = UNDEF)) = 1

---------------------------------------
--   S Y N C H R O N I Z A T I O N   --
---------------------------------------
DEFINE SYNC0 := ((ProvideTrip in {TRIP_MANAGEMENT_ACCOMPLISHED}));
--	 Synchronicity enforcement
TRANS (service128 in {ST2, ST1, ST4, ST3, ST5, ST7}) -> (next(service128) != service128)
TRANS (service130 in {ST2, ST1, ST3}) -> (next(service130) != service130)
-------------------------------
--   C O N T R O L L E R S   --
-------------------------------
--	 Fault Controller
VAR FAULT_CONTROLLER: { START, STOP };
INIT FAULT_CONTROLLER = START

ASSIGN
next(FAULT_CONTROLLER) := 
case
(FAULT_CONTROLLER = START & FALSE): STOP;
1: FAULT_CONTROLLER;
esac;

TRANS (output != UNDEF | input != UNDEF) -> (FAULT_CONTROLLER != STOP)

-------------------------------------
--   I N I T I A L I Z A T I O N   --
-------------------------------------
INIT ((SetupTrip = INITIAL) & (ProvideTrip = TRIP_REQUEST_ACCOMPLISHED))
INIT ((service128 = ST0) & (service130 = ST0))

-----------------
--   G O A L   --
-----------------
MODULE main
VAR dn_main : dn_main;

GOAL ONEOF(
(dn_main.SYNC0), 1;
(dn_main.FAULT_CONTROLLER = STOP), 0

)