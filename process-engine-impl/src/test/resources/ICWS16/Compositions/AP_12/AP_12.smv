-------------------------------
--   C O M P O S I T I O N   --
-------------------------------
MODULE dn_main

---------------------
--   I N P U T S   --
---------------------
IVAR input:
{
UNDEF, service28_UMS_ManagementTrip, service28_UMS_TripManagementRequest, service28_RESET, service29_UMS_TripRequest, service29_RESET
};

-----------------------
--   O U T P U T S   --
-----------------------
IVAR output:
{
UNDEF, service28_UMS_concrete4, service28_UMS_concrete3, service28_UMS_concrete2, service28_UMS_concrete1, service28_UMS_TripManagementReply, service28_UMS_concrete5, service29_UMS_TripAlternatives, service29_UMS_concrete2, service29_UMS_concrete1
};

-------------------------
--   S E R V I C E S   --
-------------------------
--	 service28
VAR service28: {ST2, ST1, ST4, ST3, ST6, ST5, ST8, ST7, ST0};
ASSIGN
next(service28) :=
case
(service28 = ST1 & input = service28_RESET): ST0;
(service28 = ST5 & input = service28_RESET): ST0;
(service28 = ST7 & input = service28_RESET): ST0;
(service28 = ST5 & output = service28_UMS_concrete5): ST6;
(service28 = ST2 & input = service28_RESET): ST0;
(service28 = ST3 & input = service28_RESET): ST0;
(service28 = ST3 & output = service28_UMS_concrete3): ST4;
(service28 = ST4 & input = service28_RESET): ST0;
(service28 = ST8 & input = service28_RESET): ST0;
(service28 = ST0 & input = service28_UMS_TripManagementRequest): ST1;
(service28 = ST7 & output = service28_UMS_TripManagementReply): ST8;
(service28 = ST6 & input = service28_RESET): ST0;
(service28 = ST6 & input = service28_UMS_ManagementTrip): ST7;
(service28 = ST2 & output = service28_UMS_concrete2): ST3;
(service28 = ST4 & output = service28_UMS_concrete4): ST5;
(service28 = ST1 & output = service28_UMS_concrete1): ST2;
1:service28;
esac;
TRANS ( input = service28_UMS_ManagementTrip) -> (service28 = ST6)
TRANS ( output = service28_UMS_concrete4) -> (service28 = ST4)
TRANS ( input = service28_UMS_TripManagementRequest) -> (service28 = ST0)
TRANS ( output = service28_UMS_concrete3) -> (service28 = ST3)
TRANS ( output = service28_UMS_concrete2) -> (service28 = ST2)
TRANS ( input = service28_RESET) -> (service28 = ST2 | service28 = ST1 | service28 = ST4 | service28 = ST3 | service28 = ST6 | service28 = ST5 | service28 = ST8 | service28 = ST7)
TRANS ( output = service28_UMS_concrete1) -> (service28 = ST1)
TRANS ( output = service28_UMS_TripManagementReply) -> (service28 = ST7)
TRANS ( output = service28_UMS_concrete5) -> (service28 = ST5)

--	 service29
VAR service29: {ST2, ST1, ST4, ST3, ST0};
ASSIGN
next(service29) :=
case
(service29 = ST1 & output = service29_UMS_concrete1): ST2;
(service29 = ST2 & output = service29_UMS_concrete2): ST3;
(service29 = ST0 & input = service29_UMS_TripRequest): ST1;
(service29 = ST3 & input = service29_RESET): ST0;
(service29 = ST4 & input = service29_RESET): ST0;
(service29 = ST2 & input = service29_RESET): ST0;
(service29 = ST1 & input = service29_RESET): ST0;
(service29 = ST3 & output = service29_UMS_TripAlternatives): ST4;
1:service29;
esac;
TRANS ( input = service29_UMS_TripRequest) -> (service29 = ST0)
TRANS ( output = service29_UMS_TripAlternatives) -> (service29 = ST3)
TRANS ( output = service29_UMS_concrete2) -> (service29 = ST2)
TRANS ( input = service29_RESET) -> (service29 = ST2 | service29 = ST1 | service29 = ST4 | service29 = ST3)
TRANS ( output = service29_UMS_concrete1) -> (service29 = ST1)

-----------------------
--   O B J E C T S   --
-----------------------
--	 SetupTrip
VAR SetupTrip: {ORGANIZED, INITIAL};
ASSIGN
next(SetupTrip) :=
case
(input = service28_UMS_ManagementTrip):{(ORGANIZED)};
1:SetupTrip;
esac;
--	 ProvideTrip
VAR ProvideTrip: {TRIP_REQUEST_ACCOMPLISHED, TRIP_MANAGEMENT_ACCOMPLISHED, TRIP_MANAGE_REQUEST_LOADED, TRIP_REQUEST_LOADED, INITIAL};
ASSIGN
next(ProvideTrip) :=
case
(ProvideTrip = TRIP_MANAGE_REQUEST_LOADED & output = service28_UMS_TripManagementReply): TRIP_MANAGEMENT_ACCOMPLISHED;
(ProvideTrip = TRIP_REQUEST_ACCOMPLISHED & input = service28_UMS_TripManagementRequest): TRIP_MANAGE_REQUEST_LOADED;
(ProvideTrip = TRIP_REQUEST_LOADED & output = service29_UMS_TripAlternatives): TRIP_REQUEST_ACCOMPLISHED;
(ProvideTrip = INITIAL & input = service29_UMS_TripRequest): TRIP_REQUEST_LOADED;
1:ProvideTrip;
esac;
-----------------------------------
--   P R E C O N D I T I O N S   --
-----------------------------------
TRANS (input = service28_UMS_TripManagementRequest) -> ((((ProvideTrip = TRIP_REQUEST_ACCOMPLISHED))))
TRANS (output = service28_UMS_TripManagementReply) -> ((((ProvideTrip = TRIP_MANAGE_REQUEST_LOADED))))
TRANS (input = service29_UMS_TripRequest) -> ((((ProvideTrip = INITIAL))))
TRANS (output = service29_UMS_TripAlternatives) -> ((((ProvideTrip = TRIP_REQUEST_LOADED))))
-----------------------------
--   I N V A R I A N T S   --
-----------------------------
--	 Synchronicity
TRANS (1-(input = UNDEF)) + (1-(output = UNDEF)) = 1

---------------------------------------
--   S Y N C H R O N I Z A T I O N   --
---------------------------------------
DEFINE SYNC0 := ((ProvideTrip in {TRIP_REQUEST_ACCOMPLISHED}));
--	 Synchronicity enforcement
TRANS (service28 in {ST2, ST1, ST4, ST3, ST5, ST7}) -> (next(service28) != service28)
TRANS (service29 in {ST2, ST1, ST3}) -> (next(service29) != service29)
-------------------------------
--   C O N T R O L L E R S   --
-------------------------------
--	 Fault Controller
VAR FAULT_CONTROLLER: { START, STOP };
INIT FAULT_CONTROLLER = START

ASSIGN
next(FAULT_CONTROLLER) := 
case
(FAULT_CONTROLLER = START & FALSE): STOP;
1: FAULT_CONTROLLER;
esac;

TRANS (output != UNDEF | input != UNDEF) -> (FAULT_CONTROLLER != STOP)

-------------------------------------
--   I N I T I A L I Z A T I O N   --
-------------------------------------
INIT ((SetupTrip = INITIAL) & (ProvideTrip = INITIAL))
INIT ((service28 = ST0) & (service29 = ST0))

-----------------
--   G O A L   --
-----------------
MODULE main
VAR dn_main : dn_main;

GOAL ONEOF(
(dn_main.SYNC0), 1;
(dn_main.FAULT_CONTROLLER = STOP), 0

)