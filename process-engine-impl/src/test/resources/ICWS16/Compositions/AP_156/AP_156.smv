-------------------------------
--   C O M P O S I T I O N   --
-------------------------------
MODULE dn_main

---------------------
--   I N P U T S   --
---------------------
IVAR input:
{
UNDEF, service331_BS_RegistrationRequest, service331_BS_UpdateSmartMobilityCard, service331_RESET, service332_BS_PlanBikeRoute, service332_BS_ExecuteRouteRequest, service332_RESET
};

-----------------------
--   O U T P U T S   --
-----------------------
IVAR output:
{
UNDEF, service331_BS_RegistrationAck, service332_BS_ExecuteRouteAck
};

-------------------------
--   S E R V I C E S   --
-------------------------
--	 service331
VAR service331: {ST2, ST1, ST3, ST0};
ASSIGN
next(service331) :=
case
(service331 = ST2 & output = service331_BS_RegistrationAck): ST3;
(service331 = ST1 & input = service331_RESET): ST0;
(service331 = ST3 & input = service331_RESET): ST0;
(service331 = ST1 & input = service331_BS_UpdateSmartMobilityCard): ST2;
(service331 = ST2 & input = service331_RESET): ST0;
(service331 = ST0 & input = service331_BS_RegistrationRequest): ST1;
1:service331;
esac;
TRANS ( input = service331_BS_RegistrationRequest) -> (service331 = ST0)
TRANS ( output = service331_BS_RegistrationAck) -> (service331 = ST2)
TRANS ( input = service331_BS_UpdateSmartMobilityCard) -> (service331 = ST1)
TRANS ( input = service331_RESET) -> (service331 = ST2 | service331 = ST1 | service331 = ST3)

--	 service332
VAR service332: {ST2, ST1, ST3, ST0};
ASSIGN
next(service332) :=
case
(service332 = ST3 & input = service332_RESET): ST0;
(service332 = ST1 & input = service332_RESET): ST0;
(service332 = ST2 & output = service332_BS_ExecuteRouteAck): ST3;
(service332 = ST1 & input = service332_BS_PlanBikeRoute): ST2;
(service332 = ST0 & input = service332_BS_ExecuteRouteRequest): ST1;
(service332 = ST2 & input = service332_RESET): ST0;
1:service332;
esac;
TRANS ( input = service332_BS_PlanBikeRoute) -> (service332 = ST1)
TRANS ( input = service332_BS_ExecuteRouteRequest) -> (service332 = ST0)
TRANS ( input = service332_RESET) -> (service332 = ST2 | service332 = ST1 | service332 = ST3)
TRANS ( output = service332_BS_ExecuteRouteAck) -> (service332 = ST2)

-----------------------
--   O B J E C T S   --
-----------------------
--	 BikeRoutePlan
VAR BikeRoutePlan: {ROUTE_PLANNED, INITIAL};
ASSIGN
next(BikeRoutePlan) :=
case
(input = service332_BS_PlanBikeRoute):{(ROUTE_PLANNED)};
1:BikeRoutePlan;
esac;
--	 BikeSharingTrip
VAR BikeSharingTrip: {RACK_REACHED, INITIAL, BIKE_ROUTE_PLANNED, USER_REGISTERED};
ASSIGN
next(BikeSharingTrip) :=
case
(BikeSharingTrip = USER_REGISTERED & input = service332_BS_PlanBikeRoute): BIKE_ROUTE_PLANNED;
(BikeSharingTrip = BIKE_ROUTE_PLANNED & output = service332_BS_ExecuteRouteAck): RACK_REACHED;
(BikeSharingTrip = INITIAL & output = service331_BS_RegistrationAck): USER_REGISTERED;
1:BikeSharingTrip;
esac;
--	 HandleSmartMobilityCard
VAR HandleSmartMobilityCard: {INITIAL, SUBSCRIPTION, CARD_RELEASED, SERVICE_ADDED};
ASSIGN
next(HandleSmartMobilityCard) :=
case
(HandleSmartMobilityCard = SERVICE_ADDED & input = service331_BS_UpdateSmartMobilityCard): CARD_RELEASED;
(input = service331_BS_UpdateSmartMobilityCard):{(SERVICE_ADDED)};
1:HandleSmartMobilityCard;
esac;
-----------------------------------
--   P R E C O N D I T I O N S   --
-----------------------------------
TRANS (input = service331_BS_RegistrationRequest) -> ((((BikeSharingTrip = INITIAL))))
TRANS (input = service332_BS_ExecuteRouteRequest) -> ((((BikeSharingTrip = USER_REGISTERED))))
-----------------------------
--   I N V A R I A N T S   --
-----------------------------
--	 Synchronicity
TRANS (1-(input = UNDEF)) + (1-(output = UNDEF)) = 1

---------------------------------------
--   S Y N C H R O N I Z A T I O N   --
---------------------------------------
DEFINE SYNC0 := ((BikeSharingTrip in {USER_REGISTERED}));
--	 Synchronicity enforcement
TRANS (service331 in {ST2}) -> (next(service331) != service331)
TRANS (service332 in {ST2}) -> (next(service332) != service332)
-------------------------------
--   C O N T R O L L E R S   --
-------------------------------
--	 Fault Controller
VAR FAULT_CONTROLLER: { START, STOP };
INIT FAULT_CONTROLLER = START

ASSIGN
next(FAULT_CONTROLLER) := 
case
(FAULT_CONTROLLER = START & FALSE): STOP;
1: FAULT_CONTROLLER;
esac;

TRANS (output != UNDEF | input != UNDEF) -> (FAULT_CONTROLLER != STOP)

-------------------------------------
--   I N I T I A L I Z A T I O N   --
-------------------------------------
INIT ((BikeRoutePlan = INITIAL) & (BikeSharingTrip = INITIAL) & (HandleSmartMobilityCard = INITIAL))
INIT ((service331 = ST0) & (service332 = ST0))

-----------------
--   G O A L   --
-----------------
MODULE main
VAR dn_main : dn_main;

GOAL ONEOF(
(dn_main.SYNC0), 1;
(dn_main.FAULT_CONTROLLER = STOP), 0

)