-------------------------------
--   C O M P O S I T I O N   --
-------------------------------
MODULE dn_main

---------------------
--   I N P U T S   --
---------------------
IVAR input:
{
UNDEF, service227_UMS_ManagementTrip, service227_UMS_TripManagementRequest, service227_RESET, service229_UMS_TripRequest, service229_RESET
};

-----------------------
--   O U T P U T S   --
-----------------------
IVAR output:
{
UNDEF, service227_UMS_concrete4, service227_UMS_concrete3, service227_UMS_concrete2, service227_UMS_concrete1, service227_UMS_TripManagementReply, service227_UMS_concrete5, service229_UMS_TripAlternatives, service229_UMS_concrete2, service229_UMS_concrete1
};

-------------------------
--   S E R V I C E S   --
-------------------------
--	 service227
VAR service227: {ST2, ST1, ST4, ST3, ST6, ST5, ST8, ST7, ST0};
ASSIGN
next(service227) :=
case
(service227 = ST5 & output = service227_UMS_concrete5): ST6;
(service227 = ST2 & output = service227_UMS_concrete2): ST3;
(service227 = ST2 & input = service227_RESET): ST0;
(service227 = ST7 & input = service227_RESET): ST0;
(service227 = ST1 & output = service227_UMS_concrete1): ST2;
(service227 = ST7 & output = service227_UMS_TripManagementReply): ST8;
(service227 = ST0 & input = service227_UMS_TripManagementRequest): ST1;
(service227 = ST6 & input = service227_RESET): ST0;
(service227 = ST1 & input = service227_RESET): ST0;
(service227 = ST4 & input = service227_RESET): ST0;
(service227 = ST3 & input = service227_RESET): ST0;
(service227 = ST4 & output = service227_UMS_concrete4): ST5;
(service227 = ST8 & input = service227_RESET): ST0;
(service227 = ST3 & output = service227_UMS_concrete3): ST4;
(service227 = ST5 & input = service227_RESET): ST0;
(service227 = ST6 & input = service227_UMS_ManagementTrip): ST7;
1:service227;
esac;
TRANS ( input = service227_UMS_ManagementTrip) -> (service227 = ST6)
TRANS ( output = service227_UMS_concrete4) -> (service227 = ST4)
TRANS ( input = service227_UMS_TripManagementRequest) -> (service227 = ST0)
TRANS ( output = service227_UMS_concrete3) -> (service227 = ST3)
TRANS ( output = service227_UMS_concrete2) -> (service227 = ST2)
TRANS ( input = service227_RESET) -> (service227 = ST2 | service227 = ST1 | service227 = ST4 | service227 = ST3 | service227 = ST6 | service227 = ST5 | service227 = ST8 | service227 = ST7)
TRANS ( output = service227_UMS_concrete1) -> (service227 = ST1)
TRANS ( output = service227_UMS_TripManagementReply) -> (service227 = ST7)
TRANS ( output = service227_UMS_concrete5) -> (service227 = ST5)

--	 service229
VAR service229: {ST2, ST1, ST4, ST3, ST0};
ASSIGN
next(service229) :=
case
(service229 = ST4 & input = service229_RESET): ST0;
(service229 = ST0 & input = service229_UMS_TripRequest): ST1;
(service229 = ST3 & output = service229_UMS_TripAlternatives): ST4;
(service229 = ST1 & output = service229_UMS_concrete1): ST2;
(service229 = ST2 & input = service229_RESET): ST0;
(service229 = ST1 & input = service229_RESET): ST0;
(service229 = ST3 & input = service229_RESET): ST0;
(service229 = ST2 & output = service229_UMS_concrete2): ST3;
1:service229;
esac;
TRANS ( input = service229_UMS_TripRequest) -> (service229 = ST0)
TRANS ( output = service229_UMS_TripAlternatives) -> (service229 = ST3)
TRANS ( output = service229_UMS_concrete2) -> (service229 = ST2)
TRANS ( input = service229_RESET) -> (service229 = ST2 | service229 = ST1 | service229 = ST4 | service229 = ST3)
TRANS ( output = service229_UMS_concrete1) -> (service229 = ST1)

-----------------------
--   O B J E C T S   --
-----------------------
--	 SetupTrip
VAR SetupTrip: {ORGANIZED, INITIAL};
ASSIGN
next(SetupTrip) :=
case
(input = service227_UMS_ManagementTrip):{(ORGANIZED)};
1:SetupTrip;
esac;
--	 ProvideTrip
VAR ProvideTrip: {TRIP_REQUEST_ACCOMPLISHED, TRIP_MANAGEMENT_ACCOMPLISHED, TRIP_MANAGE_REQUEST_LOADED, TRIP_REQUEST_LOADED, INITIAL};
ASSIGN
next(ProvideTrip) :=
case
(ProvideTrip = TRIP_MANAGE_REQUEST_LOADED & output = service227_UMS_TripManagementReply): TRIP_MANAGEMENT_ACCOMPLISHED;
(ProvideTrip = INITIAL & input = service229_UMS_TripRequest): TRIP_REQUEST_LOADED;
(ProvideTrip = TRIP_REQUEST_LOADED & output = service229_UMS_TripAlternatives): TRIP_REQUEST_ACCOMPLISHED;
(ProvideTrip = TRIP_REQUEST_ACCOMPLISHED & input = service227_UMS_TripManagementRequest): TRIP_MANAGE_REQUEST_LOADED;
1:ProvideTrip;
esac;
-----------------------------------
--   P R E C O N D I T I O N S   --
-----------------------------------
TRANS (input = service227_UMS_TripManagementRequest) -> ((((ProvideTrip = TRIP_REQUEST_ACCOMPLISHED))))
TRANS (output = service227_UMS_TripManagementReply) -> ((((ProvideTrip = TRIP_MANAGE_REQUEST_LOADED))))
TRANS (input = service229_UMS_TripRequest) -> ((((ProvideTrip = INITIAL))))
TRANS (output = service229_UMS_TripAlternatives) -> ((((ProvideTrip = TRIP_REQUEST_LOADED))))
-----------------------------
--   I N V A R I A N T S   --
-----------------------------
--	 Synchronicity
TRANS (1-(input = UNDEF)) + (1-(output = UNDEF)) = 1

---------------------------------------
--   S Y N C H R O N I Z A T I O N   --
---------------------------------------
DEFINE SYNC0 := ((ProvideTrip in {TRIP_MANAGEMENT_ACCOMPLISHED}));
--	 Synchronicity enforcement
TRANS (service227 in {ST2, ST1, ST4, ST3, ST5, ST7}) -> (next(service227) != service227)
TRANS (service229 in {ST2, ST1, ST3}) -> (next(service229) != service229)
-------------------------------
--   C O N T R O L L E R S   --
-------------------------------
--	 Fault Controller
VAR FAULT_CONTROLLER: { START, STOP };
INIT FAULT_CONTROLLER = START

ASSIGN
next(FAULT_CONTROLLER) := 
case
(FAULT_CONTROLLER = START & FALSE): STOP;
1: FAULT_CONTROLLER;
esac;

TRANS (output != UNDEF | input != UNDEF) -> (FAULT_CONTROLLER != STOP)

-------------------------------------
--   I N I T I A L I Z A T I O N   --
-------------------------------------
INIT ((SetupTrip = INITIAL) & (ProvideTrip = TRIP_REQUEST_ACCOMPLISHED))
INIT ((service227 = ST0) & (service229 = ST0))

-----------------
--   G O A L   --
-----------------
MODULE main
VAR dn_main : dn_main;

GOAL ONEOF(
(dn_main.SYNC0), 1;
(dn_main.FAULT_CONTROLLER = STOP), 0

)