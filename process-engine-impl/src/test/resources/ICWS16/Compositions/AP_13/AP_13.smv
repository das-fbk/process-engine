-------------------------------
--   C O M P O S I T I O N   --
-------------------------------
MODULE dn_main

---------------------
--   I N P U T S   --
---------------------
IVAR input:
{
UNDEF, service30_UMS_ManagementTrip, service30_UMS_TripManagementRequest, service30_RESET, service31_UMS_TripRequest, service31_RESET
};

-----------------------
--   O U T P U T S   --
-----------------------
IVAR output:
{
UNDEF, service30_UMS_concrete4, service30_UMS_concrete3, service30_UMS_concrete2, service30_UMS_concrete1, service30_UMS_TripManagementReply, service30_UMS_concrete5, service31_UMS_TripAlternatives, service31_UMS_concrete2, service31_UMS_concrete1
};

-------------------------
--   S E R V I C E S   --
-------------------------
--	 service30
VAR service30: {ST2, ST1, ST4, ST3, ST6, ST5, ST8, ST7, ST0};
ASSIGN
next(service30) :=
case
(service30 = ST0 & input = service30_UMS_TripManagementRequest): ST1;
(service30 = ST1 & output = service30_UMS_concrete1): ST2;
(service30 = ST3 & input = service30_RESET): ST0;
(service30 = ST1 & input = service30_RESET): ST0;
(service30 = ST2 & output = service30_UMS_concrete2): ST3;
(service30 = ST6 & input = service30_UMS_ManagementTrip): ST7;
(service30 = ST2 & input = service30_RESET): ST0;
(service30 = ST7 & output = service30_UMS_TripManagementReply): ST8;
(service30 = ST5 & output = service30_UMS_concrete5): ST6;
(service30 = ST5 & input = service30_RESET): ST0;
(service30 = ST4 & input = service30_RESET): ST0;
(service30 = ST7 & input = service30_RESET): ST0;
(service30 = ST8 & input = service30_RESET): ST0;
(service30 = ST3 & output = service30_UMS_concrete3): ST4;
(service30 = ST4 & output = service30_UMS_concrete4): ST5;
(service30 = ST6 & input = service30_RESET): ST0;
1:service30;
esac;
TRANS ( input = service30_UMS_ManagementTrip) -> (service30 = ST6)
TRANS ( output = service30_UMS_concrete4) -> (service30 = ST4)
TRANS ( input = service30_UMS_TripManagementRequest) -> (service30 = ST0)
TRANS ( output = service30_UMS_concrete3) -> (service30 = ST3)
TRANS ( output = service30_UMS_concrete2) -> (service30 = ST2)
TRANS ( input = service30_RESET) -> (service30 = ST2 | service30 = ST1 | service30 = ST4 | service30 = ST3 | service30 = ST6 | service30 = ST5 | service30 = ST8 | service30 = ST7)
TRANS ( output = service30_UMS_concrete1) -> (service30 = ST1)
TRANS ( output = service30_UMS_TripManagementReply) -> (service30 = ST7)
TRANS ( output = service30_UMS_concrete5) -> (service30 = ST5)

--	 service31
VAR service31: {ST2, ST1, ST4, ST3, ST0};
ASSIGN
next(service31) :=
case
(service31 = ST2 & output = service31_UMS_concrete2): ST3;
(service31 = ST2 & input = service31_RESET): ST0;
(service31 = ST4 & input = service31_RESET): ST0;
(service31 = ST1 & output = service31_UMS_concrete1): ST2;
(service31 = ST3 & input = service31_RESET): ST0;
(service31 = ST0 & input = service31_UMS_TripRequest): ST1;
(service31 = ST3 & output = service31_UMS_TripAlternatives): ST4;
(service31 = ST1 & input = service31_RESET): ST0;
1:service31;
esac;
TRANS ( input = service31_UMS_TripRequest) -> (service31 = ST0)
TRANS ( output = service31_UMS_TripAlternatives) -> (service31 = ST3)
TRANS ( output = service31_UMS_concrete2) -> (service31 = ST2)
TRANS ( input = service31_RESET) -> (service31 = ST2 | service31 = ST1 | service31 = ST4 | service31 = ST3)
TRANS ( output = service31_UMS_concrete1) -> (service31 = ST1)

-----------------------
--   O B J E C T S   --
-----------------------
--	 SetupTrip
VAR SetupTrip: {ORGANIZED, INITIAL};
ASSIGN
next(SetupTrip) :=
case
(input = service30_UMS_ManagementTrip):{(ORGANIZED)};
1:SetupTrip;
esac;
--	 ProvideTrip
VAR ProvideTrip: {TRIP_REQUEST_ACCOMPLISHED, TRIP_MANAGEMENT_ACCOMPLISHED, TRIP_MANAGE_REQUEST_LOADED, TRIP_REQUEST_LOADED, INITIAL};
ASSIGN
next(ProvideTrip) :=
case
(ProvideTrip = INITIAL & input = service31_UMS_TripRequest): TRIP_REQUEST_LOADED;
(ProvideTrip = TRIP_REQUEST_ACCOMPLISHED & input = service30_UMS_TripManagementRequest): TRIP_MANAGE_REQUEST_LOADED;
(ProvideTrip = TRIP_MANAGE_REQUEST_LOADED & output = service30_UMS_TripManagementReply): TRIP_MANAGEMENT_ACCOMPLISHED;
(ProvideTrip = TRIP_REQUEST_LOADED & output = service31_UMS_TripAlternatives): TRIP_REQUEST_ACCOMPLISHED;
1:ProvideTrip;
esac;
-----------------------------------
--   P R E C O N D I T I O N S   --
-----------------------------------
TRANS (input = service30_UMS_TripManagementRequest) -> ((((ProvideTrip = TRIP_REQUEST_ACCOMPLISHED))))
TRANS (output = service30_UMS_TripManagementReply) -> ((((ProvideTrip = TRIP_MANAGE_REQUEST_LOADED))))
TRANS (input = service31_UMS_TripRequest) -> ((((ProvideTrip = INITIAL))))
TRANS (output = service31_UMS_TripAlternatives) -> ((((ProvideTrip = TRIP_REQUEST_LOADED))))
-----------------------------
--   I N V A R I A N T S   --
-----------------------------
--	 Synchronicity
TRANS (1-(input = UNDEF)) + (1-(output = UNDEF)) = 1

---------------------------------------
--   S Y N C H R O N I Z A T I O N   --
---------------------------------------
DEFINE SYNC0 := ((ProvideTrip in {TRIP_REQUEST_ACCOMPLISHED}));
--	 Synchronicity enforcement
TRANS (service30 in {ST2, ST1, ST4, ST3, ST5, ST7}) -> (next(service30) != service30)
TRANS (service31 in {ST2, ST1, ST3}) -> (next(service31) != service31)
-------------------------------
--   C O N T R O L L E R S   --
-------------------------------
--	 Fault Controller
VAR FAULT_CONTROLLER: { START, STOP };
INIT FAULT_CONTROLLER = START

ASSIGN
next(FAULT_CONTROLLER) := 
case
(FAULT_CONTROLLER = START & FALSE): STOP;
1: FAULT_CONTROLLER;
esac;

TRANS (output != UNDEF | input != UNDEF) -> (FAULT_CONTROLLER != STOP)

-------------------------------------
--   I N I T I A L I Z A T I O N   --
-------------------------------------
INIT ((SetupTrip = INITIAL) & (ProvideTrip = INITIAL))
INIT ((service30 = ST0) & (service31 = ST0))

-----------------
--   G O A L   --
-----------------
MODULE main
VAR dn_main : dn_main;

GOAL ONEOF(
(dn_main.SYNC0), 1;
(dn_main.FAULT_CONTROLLER = STOP), 0

)