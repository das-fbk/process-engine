-------------------------------
--   C O M P O S I T I O N   --
-------------------------------
MODULE dn_main

---------------------
--   I N P U T S   --
---------------------
IVAR input:
{
UNDEF, service18_UMS_ManagementTrip, service18_UMS_TripManagementRequest, service18_RESET, service19_UMS_TripRequest, service19_RESET
};

-----------------------
--   O U T P U T S   --
-----------------------
IVAR output:
{
UNDEF, service18_UMS_concrete4, service18_UMS_concrete3, service18_UMS_concrete2, service18_UMS_concrete1, service18_UMS_TripManagementReply, service18_UMS_concrete5, service19_UMS_TripAlternatives, service19_UMS_concrete2, service19_UMS_concrete1
};

-------------------------
--   S E R V I C E S   --
-------------------------
--	 service18
VAR service18: {ST2, ST1, ST4, ST3, ST6, ST5, ST8, ST7, ST0};
ASSIGN
next(service18) :=
case
(service18 = ST7 & output = service18_UMS_TripManagementReply): ST8;
(service18 = ST7 & input = service18_RESET): ST0;
(service18 = ST5 & input = service18_RESET): ST0;
(service18 = ST5 & output = service18_UMS_concrete5): ST6;
(service18 = ST4 & input = service18_RESET): ST0;
(service18 = ST2 & input = service18_RESET): ST0;
(service18 = ST0 & input = service18_UMS_TripManagementRequest): ST1;
(service18 = ST1 & input = service18_RESET): ST0;
(service18 = ST2 & output = service18_UMS_concrete2): ST3;
(service18 = ST3 & input = service18_RESET): ST0;
(service18 = ST8 & input = service18_RESET): ST0;
(service18 = ST4 & output = service18_UMS_concrete4): ST5;
(service18 = ST1 & output = service18_UMS_concrete1): ST2;
(service18 = ST6 & input = service18_RESET): ST0;
(service18 = ST3 & output = service18_UMS_concrete3): ST4;
(service18 = ST6 & input = service18_UMS_ManagementTrip): ST7;
1:service18;
esac;
TRANS ( input = service18_UMS_ManagementTrip) -> (service18 = ST6)
TRANS ( output = service18_UMS_concrete4) -> (service18 = ST4)
TRANS ( input = service18_UMS_TripManagementRequest) -> (service18 = ST0)
TRANS ( output = service18_UMS_concrete3) -> (service18 = ST3)
TRANS ( output = service18_UMS_concrete2) -> (service18 = ST2)
TRANS ( input = service18_RESET) -> (service18 = ST2 | service18 = ST1 | service18 = ST4 | service18 = ST3 | service18 = ST6 | service18 = ST5 | service18 = ST8 | service18 = ST7)
TRANS ( output = service18_UMS_concrete1) -> (service18 = ST1)
TRANS ( output = service18_UMS_TripManagementReply) -> (service18 = ST7)
TRANS ( output = service18_UMS_concrete5) -> (service18 = ST5)

--	 service19
VAR service19: {ST2, ST1, ST4, ST3, ST0};
ASSIGN
next(service19) :=
case
(service19 = ST1 & output = service19_UMS_concrete1): ST2;
(service19 = ST0 & input = service19_UMS_TripRequest): ST1;
(service19 = ST2 & output = service19_UMS_concrete2): ST3;
(service19 = ST3 & output = service19_UMS_TripAlternatives): ST4;
(service19 = ST1 & input = service19_RESET): ST0;
(service19 = ST2 & input = service19_RESET): ST0;
(service19 = ST3 & input = service19_RESET): ST0;
(service19 = ST4 & input = service19_RESET): ST0;
1:service19;
esac;
TRANS ( input = service19_UMS_TripRequest) -> (service19 = ST0)
TRANS ( output = service19_UMS_TripAlternatives) -> (service19 = ST3)
TRANS ( output = service19_UMS_concrete2) -> (service19 = ST2)
TRANS ( input = service19_RESET) -> (service19 = ST2 | service19 = ST1 | service19 = ST4 | service19 = ST3)
TRANS ( output = service19_UMS_concrete1) -> (service19 = ST1)

-----------------------
--   O B J E C T S   --
-----------------------
--	 SetupTrip
VAR SetupTrip: {ORGANIZED, INITIAL};
ASSIGN
next(SetupTrip) :=
case
(input = service18_UMS_ManagementTrip):{(ORGANIZED)};
1:SetupTrip;
esac;
--	 ProvideTrip
VAR ProvideTrip: {TRIP_REQUEST_ACCOMPLISHED, TRIP_MANAGEMENT_ACCOMPLISHED, TRIP_MANAGE_REQUEST_LOADED, TRIP_REQUEST_LOADED, INITIAL};
ASSIGN
next(ProvideTrip) :=
case
(ProvideTrip = TRIP_MANAGE_REQUEST_LOADED & output = service18_UMS_TripManagementReply): TRIP_MANAGEMENT_ACCOMPLISHED;
(ProvideTrip = TRIP_REQUEST_ACCOMPLISHED & input = service18_UMS_TripManagementRequest): TRIP_MANAGE_REQUEST_LOADED;
(ProvideTrip = INITIAL & input = service19_UMS_TripRequest): TRIP_REQUEST_LOADED;
(ProvideTrip = TRIP_REQUEST_LOADED & output = service19_UMS_TripAlternatives): TRIP_REQUEST_ACCOMPLISHED;
1:ProvideTrip;
esac;
-----------------------------------
--   P R E C O N D I T I O N S   --
-----------------------------------
TRANS (input = service18_UMS_TripManagementRequest) -> ((((ProvideTrip = TRIP_REQUEST_ACCOMPLISHED))))
TRANS (output = service18_UMS_TripManagementReply) -> ((((ProvideTrip = TRIP_MANAGE_REQUEST_LOADED))))
TRANS (input = service19_UMS_TripRequest) -> ((((ProvideTrip = INITIAL))))
TRANS (output = service19_UMS_TripAlternatives) -> ((((ProvideTrip = TRIP_REQUEST_LOADED))))
-----------------------------
--   I N V A R I A N T S   --
-----------------------------
--	 Synchronicity
TRANS (1-(input = UNDEF)) + (1-(output = UNDEF)) = 1

---------------------------------------
--   S Y N C H R O N I Z A T I O N   --
---------------------------------------
DEFINE SYNC0 := ((ProvideTrip in {TRIP_REQUEST_ACCOMPLISHED}));
--	 Synchronicity enforcement
TRANS (service18 in {ST2, ST1, ST4, ST3, ST5, ST7}) -> (next(service18) != service18)
TRANS (service19 in {ST2, ST1, ST3}) -> (next(service19) != service19)
-------------------------------
--   C O N T R O L L E R S   --
-------------------------------
--	 Fault Controller
VAR FAULT_CONTROLLER: { START, STOP };
INIT FAULT_CONTROLLER = START

ASSIGN
next(FAULT_CONTROLLER) := 
case
(FAULT_CONTROLLER = START & FALSE): STOP;
1: FAULT_CONTROLLER;
esac;

TRANS (output != UNDEF | input != UNDEF) -> (FAULT_CONTROLLER != STOP)

-------------------------------------
--   I N I T I A L I Z A T I O N   --
-------------------------------------
INIT ((SetupTrip = INITIAL) & (ProvideTrip = INITIAL))
INIT ((service18 = ST0) & (service19 = ST0))

-----------------
--   G O A L   --
-----------------
MODULE main
VAR dn_main : dn_main;

GOAL ONEOF(
(dn_main.SYNC0), 1;
(dn_main.FAULT_CONTROLLER = STOP), 0

)