-------------------------------
--   C O M P O S I T I O N   --
-------------------------------
MODULE dn_main

---------------------
--   I N P U T S   --
---------------------
IVAR input:
{
UNDEF, service200_TO_HOAAorganizeTrip, service200_TO_ChosenTrip, service200_RESET
};

-----------------------
--   O U T P U T S   --
-----------------------
IVAR output:
{
UNDEF, service200_TO_DefineGoal, service200_TO_ChosenTripEnd
};

-------------------------
--   S E R V I C E S   --
-------------------------
--	 service200
VAR service200: {ST2, ST1, ST4, ST3, ST0};
ASSIGN
next(service200) :=
case
(service200 = ST0 & input = service200_TO_ChosenTrip): ST1;
(service200 = ST2 & input = service200_RESET): ST0;
(service200 = ST3 & input = service200_RESET): ST0;
(service200 = ST1 & output = service200_TO_DefineGoal): ST2;
(service200 = ST3 & output = service200_TO_ChosenTripEnd): ST4;
(service200 = ST1 & input = service200_RESET): ST0;
(service200 = ST4 & input = service200_RESET): ST0;
(service200 = ST2 & input = service200_TO_HOAAorganizeTrip): ST3;
1:service200;
esac;
TRANS ( input = service200_TO_HOAAorganizeTrip) -> (service200 = ST2)
TRANS ( input = service200_TO_ChosenTrip) -> (service200 = ST0)
TRANS ( input = service200_RESET) -> (service200 = ST2 | service200 = ST1 | service200 = ST4 | service200 = ST3)
TRANS ( output = service200_TO_DefineGoal) -> (service200 = ST1)
TRANS ( output = service200_TO_ChosenTripEnd) -> (service200 = ST3)

-----------------------
--   O B J E C T S   --
-----------------------
--	 SetupTrip
VAR SetupTrip: {ORGANIZED, INITIAL};
ASSIGN
next(SetupTrip) :=
case
(SetupTrip = INITIAL & output = service200_TO_ChosenTripEnd): ORGANIZED;
1:SetupTrip;
esac;
-----------------------------------
--   P R E C O N D I T I O N S   --
-----------------------------------
TRANS (input = service200_TO_ChosenTrip) -> ((((SetupTrip = INITIAL))))
-----------------------------
--   I N V A R I A N T S   --
-----------------------------
--	 Synchronicity
TRANS (1-(input = UNDEF)) + (1-(output = UNDEF)) = 1

---------------------------------------
--   S Y N C H R O N I Z A T I O N   --
---------------------------------------
DEFINE SYNC0 := ((SetupTrip in {ORGANIZED}));
--	 Synchronicity enforcement
TRANS (service200 in {ST1, ST3}) -> (next(service200) != service200)
-------------------------------
--   C O N T R O L L E R S   --
-------------------------------
--	 Fault Controller
VAR FAULT_CONTROLLER: { START, STOP };
INIT FAULT_CONTROLLER = START

ASSIGN
next(FAULT_CONTROLLER) := 
case
(FAULT_CONTROLLER = START & FALSE): STOP;
1: FAULT_CONTROLLER;
esac;

TRANS (output != UNDEF | input != UNDEF) -> (FAULT_CONTROLLER != STOP)

-------------------------------------
--   I N I T I A L I Z A T I O N   --
-------------------------------------
INIT ((SetupTrip = INITIAL))
INIT ((service200 = ST0))

-----------------
--   G O A L   --
-----------------
MODULE main
VAR dn_main : dn_main;

GOAL ONEOF(
(dn_main.SYNC0), 1;
(dn_main.FAULT_CONTROLLER = STOP), 0

)