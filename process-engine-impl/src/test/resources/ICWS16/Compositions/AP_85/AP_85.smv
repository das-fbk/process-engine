-------------------------------
--   C O M P O S I T I O N   --
-------------------------------
MODULE dn_main

---------------------
--   I N P U T S   --
---------------------
IVAR input:
{
UNDEF, service144_UMS_ManagementTrip, service144_UMS_TripManagementRequest, service144_RESET, service146_UMS_TripRequest, service146_RESET
};

-----------------------
--   O U T P U T S   --
-----------------------
IVAR output:
{
UNDEF, service144_UMS_concrete4, service144_UMS_concrete3, service144_UMS_concrete2, service144_UMS_concrete1, service144_UMS_TripManagementReply, service144_UMS_concrete5, service146_UMS_TripAlternatives, service146_UMS_concrete2, service146_UMS_concrete1
};

-------------------------
--   S E R V I C E S   --
-------------------------
--	 service144
VAR service144: {ST2, ST1, ST4, ST3, ST6, ST5, ST8, ST7, ST0};
ASSIGN
next(service144) :=
case
(service144 = ST5 & input = service144_RESET): ST0;
(service144 = ST4 & output = service144_UMS_concrete4): ST5;
(service144 = ST1 & input = service144_RESET): ST0;
(service144 = ST7 & output = service144_UMS_TripManagementReply): ST8;
(service144 = ST2 & output = service144_UMS_concrete2): ST3;
(service144 = ST6 & input = service144_RESET): ST0;
(service144 = ST7 & input = service144_RESET): ST0;
(service144 = ST3 & output = service144_UMS_concrete3): ST4;
(service144 = ST5 & output = service144_UMS_concrete5): ST6;
(service144 = ST6 & input = service144_UMS_ManagementTrip): ST7;
(service144 = ST4 & input = service144_RESET): ST0;
(service144 = ST3 & input = service144_RESET): ST0;
(service144 = ST0 & input = service144_UMS_TripManagementRequest): ST1;
(service144 = ST8 & input = service144_RESET): ST0;
(service144 = ST1 & output = service144_UMS_concrete1): ST2;
(service144 = ST2 & input = service144_RESET): ST0;
1:service144;
esac;
TRANS ( input = service144_UMS_ManagementTrip) -> (service144 = ST6)
TRANS ( output = service144_UMS_concrete4) -> (service144 = ST4)
TRANS ( input = service144_UMS_TripManagementRequest) -> (service144 = ST0)
TRANS ( output = service144_UMS_concrete3) -> (service144 = ST3)
TRANS ( output = service144_UMS_concrete2) -> (service144 = ST2)
TRANS ( input = service144_RESET) -> (service144 = ST2 | service144 = ST1 | service144 = ST4 | service144 = ST3 | service144 = ST6 | service144 = ST5 | service144 = ST8 | service144 = ST7)
TRANS ( output = service144_UMS_concrete1) -> (service144 = ST1)
TRANS ( output = service144_UMS_TripManagementReply) -> (service144 = ST7)
TRANS ( output = service144_UMS_concrete5) -> (service144 = ST5)

--	 service146
VAR service146: {ST2, ST1, ST4, ST3, ST0};
ASSIGN
next(service146) :=
case
(service146 = ST2 & output = service146_UMS_concrete2): ST3;
(service146 = ST1 & output = service146_UMS_concrete1): ST2;
(service146 = ST2 & input = service146_RESET): ST0;
(service146 = ST3 & output = service146_UMS_TripAlternatives): ST4;
(service146 = ST3 & input = service146_RESET): ST0;
(service146 = ST4 & input = service146_RESET): ST0;
(service146 = ST1 & input = service146_RESET): ST0;
(service146 = ST0 & input = service146_UMS_TripRequest): ST1;
1:service146;
esac;
TRANS ( input = service146_UMS_TripRequest) -> (service146 = ST0)
TRANS ( output = service146_UMS_TripAlternatives) -> (service146 = ST3)
TRANS ( output = service146_UMS_concrete2) -> (service146 = ST2)
TRANS ( input = service146_RESET) -> (service146 = ST2 | service146 = ST1 | service146 = ST4 | service146 = ST3)
TRANS ( output = service146_UMS_concrete1) -> (service146 = ST1)

-----------------------
--   O B J E C T S   --
-----------------------
--	 SetupTrip
VAR SetupTrip: {ORGANIZED, INITIAL};
ASSIGN
next(SetupTrip) :=
case
(input = service144_UMS_ManagementTrip):{(ORGANIZED)};
1:SetupTrip;
esac;
--	 ProvideTrip
VAR ProvideTrip: {TRIP_REQUEST_ACCOMPLISHED, TRIP_MANAGEMENT_ACCOMPLISHED, TRIP_MANAGE_REQUEST_LOADED, TRIP_REQUEST_LOADED, INITIAL};
ASSIGN
next(ProvideTrip) :=
case
(ProvideTrip = TRIP_MANAGE_REQUEST_LOADED & output = service144_UMS_TripManagementReply): TRIP_MANAGEMENT_ACCOMPLISHED;
(ProvideTrip = INITIAL & input = service146_UMS_TripRequest): TRIP_REQUEST_LOADED;
(ProvideTrip = TRIP_REQUEST_LOADED & output = service146_UMS_TripAlternatives): TRIP_REQUEST_ACCOMPLISHED;
(ProvideTrip = TRIP_REQUEST_ACCOMPLISHED & input = service144_UMS_TripManagementRequest): TRIP_MANAGE_REQUEST_LOADED;
1:ProvideTrip;
esac;
-----------------------------------
--   P R E C O N D I T I O N S   --
-----------------------------------
TRANS (input = service144_UMS_TripManagementRequest) -> ((((ProvideTrip = TRIP_REQUEST_ACCOMPLISHED))))
TRANS (output = service144_UMS_TripManagementReply) -> ((((ProvideTrip = TRIP_MANAGE_REQUEST_LOADED))))
TRANS (input = service146_UMS_TripRequest) -> ((((ProvideTrip = INITIAL))))
TRANS (output = service146_UMS_TripAlternatives) -> ((((ProvideTrip = TRIP_REQUEST_LOADED))))
-----------------------------
--   I N V A R I A N T S   --
-----------------------------
--	 Synchronicity
TRANS (1-(input = UNDEF)) + (1-(output = UNDEF)) = 1

---------------------------------------
--   S Y N C H R O N I Z A T I O N   --
---------------------------------------
DEFINE SYNC0 := ((ProvideTrip in {TRIP_MANAGEMENT_ACCOMPLISHED}));
--	 Synchronicity enforcement
TRANS (service144 in {ST2, ST1, ST4, ST3, ST5, ST7}) -> (next(service144) != service144)
TRANS (service146 in {ST2, ST1, ST3}) -> (next(service146) != service146)
-------------------------------
--   C O N T R O L L E R S   --
-------------------------------
--	 Fault Controller
VAR FAULT_CONTROLLER: { START, STOP };
INIT FAULT_CONTROLLER = START

ASSIGN
next(FAULT_CONTROLLER) := 
case
(FAULT_CONTROLLER = START & FALSE): STOP;
1: FAULT_CONTROLLER;
esac;

TRANS (output != UNDEF | input != UNDEF) -> (FAULT_CONTROLLER != STOP)

-------------------------------------
--   I N I T I A L I Z A T I O N   --
-------------------------------------
INIT ((SetupTrip = INITIAL) & (ProvideTrip = TRIP_REQUEST_ACCOMPLISHED))
INIT ((service144 = ST0) & (service146 = ST0))

-----------------
--   G O A L   --
-----------------
MODULE main
VAR dn_main : dn_main;

GOAL ONEOF(
(dn_main.SYNC0), 1;
(dn_main.FAULT_CONTROLLER = STOP), 0

)