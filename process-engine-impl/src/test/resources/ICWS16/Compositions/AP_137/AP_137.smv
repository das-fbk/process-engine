-------------------------------
--   C O M P O S I T I O N   --
-------------------------------
MODULE dn_main

---------------------
--   I N P U T S   --
---------------------
IVAR input:
{
UNDEF, service230_BS_RegistrationRequest, service230_BS_UpdateSmartMobilityCard, service230_RESET, service231_BS_PlanBikeRoute, service231_BS_ExecuteRouteRequest, service231_RESET
};

-----------------------
--   O U T P U T S   --
-----------------------
IVAR output:
{
UNDEF, service230_BS_RegistrationAck, service231_BS_ExecuteRouteAck
};

-------------------------
--   S E R V I C E S   --
-------------------------
--	 service230
VAR service230: {ST2, ST1, ST3, ST0};
ASSIGN
next(service230) :=
case
(service230 = ST3 & input = service230_RESET): ST0;
(service230 = ST0 & input = service230_BS_RegistrationRequest): ST1;
(service230 = ST1 & input = service230_BS_UpdateSmartMobilityCard): ST2;
(service230 = ST2 & output = service230_BS_RegistrationAck): ST3;
(service230 = ST2 & input = service230_RESET): ST0;
(service230 = ST1 & input = service230_RESET): ST0;
1:service230;
esac;
TRANS ( input = service230_BS_RegistrationRequest) -> (service230 = ST0)
TRANS ( output = service230_BS_RegistrationAck) -> (service230 = ST2)
TRANS ( input = service230_BS_UpdateSmartMobilityCard) -> (service230 = ST1)
TRANS ( input = service230_RESET) -> (service230 = ST2 | service230 = ST1 | service230 = ST3)

--	 service231
VAR service231: {ST2, ST1, ST3, ST0};
ASSIGN
next(service231) :=
case
(service231 = ST2 & input = service231_RESET): ST0;
(service231 = ST1 & input = service231_BS_PlanBikeRoute): ST2;
(service231 = ST1 & input = service231_RESET): ST0;
(service231 = ST3 & input = service231_RESET): ST0;
(service231 = ST0 & input = service231_BS_ExecuteRouteRequest): ST1;
(service231 = ST2 & output = service231_BS_ExecuteRouteAck): ST3;
1:service231;
esac;
TRANS ( input = service231_BS_PlanBikeRoute) -> (service231 = ST1)
TRANS ( input = service231_BS_ExecuteRouteRequest) -> (service231 = ST0)
TRANS ( input = service231_RESET) -> (service231 = ST2 | service231 = ST1 | service231 = ST3)
TRANS ( output = service231_BS_ExecuteRouteAck) -> (service231 = ST2)

-----------------------
--   O B J E C T S   --
-----------------------
--	 BikeRoutePlan
VAR BikeRoutePlan: {ROUTE_PLANNED, INITIAL};
ASSIGN
next(BikeRoutePlan) :=
case
(input = service231_BS_PlanBikeRoute):{(ROUTE_PLANNED)};
1:BikeRoutePlan;
esac;
--	 BikeSharingTrip
VAR BikeSharingTrip: {RACK_REACHED, INITIAL, BIKE_ROUTE_PLANNED, USER_REGISTERED};
ASSIGN
next(BikeSharingTrip) :=
case
(BikeSharingTrip = INITIAL & output = service230_BS_RegistrationAck): USER_REGISTERED;
(BikeSharingTrip = BIKE_ROUTE_PLANNED & output = service231_BS_ExecuteRouteAck): RACK_REACHED;
(BikeSharingTrip = USER_REGISTERED & input = service231_BS_PlanBikeRoute): BIKE_ROUTE_PLANNED;
1:BikeSharingTrip;
esac;
--	 HandleSmartMobilityCard
VAR HandleSmartMobilityCard: {INITIAL, SUBSCRIPTION, CARD_RELEASED, SERVICE_ADDED};
ASSIGN
next(HandleSmartMobilityCard) :=
case
(HandleSmartMobilityCard = SERVICE_ADDED & input = service230_BS_UpdateSmartMobilityCard): CARD_RELEASED;
(input = service230_BS_UpdateSmartMobilityCard):{(SERVICE_ADDED)};
1:HandleSmartMobilityCard;
esac;
-----------------------------------
--   P R E C O N D I T I O N S   --
-----------------------------------
TRANS (input = service230_BS_RegistrationRequest) -> ((((BikeSharingTrip = INITIAL))))
TRANS (input = service231_BS_ExecuteRouteRequest) -> ((((BikeSharingTrip = USER_REGISTERED))))
-----------------------------
--   I N V A R I A N T S   --
-----------------------------
--	 Synchronicity
TRANS (1-(input = UNDEF)) + (1-(output = UNDEF)) = 1

---------------------------------------
--   S Y N C H R O N I Z A T I O N   --
---------------------------------------
DEFINE SYNC0 := ((BikeSharingTrip in {USER_REGISTERED}));
--	 Synchronicity enforcement
TRANS (service230 in {ST2}) -> (next(service230) != service230)
TRANS (service231 in {ST2}) -> (next(service231) != service231)
-------------------------------
--   C O N T R O L L E R S   --
-------------------------------
--	 Fault Controller
VAR FAULT_CONTROLLER: { START, STOP };
INIT FAULT_CONTROLLER = START

ASSIGN
next(FAULT_CONTROLLER) := 
case
(FAULT_CONTROLLER = START & FALSE): STOP;
1: FAULT_CONTROLLER;
esac;

TRANS (output != UNDEF | input != UNDEF) -> (FAULT_CONTROLLER != STOP)

-------------------------------------
--   I N I T I A L I Z A T I O N   --
-------------------------------------
INIT ((BikeRoutePlan = INITIAL) & (BikeSharingTrip = INITIAL) & (HandleSmartMobilityCard = INITIAL))
INIT ((service230 = ST0) & (service231 = ST0))

-----------------
--   G O A L   --
-----------------
MODULE main
VAR dn_main : dn_main;

GOAL ONEOF(
(dn_main.SYNC0), 1;
(dn_main.FAULT_CONTROLLER = STOP), 0

)