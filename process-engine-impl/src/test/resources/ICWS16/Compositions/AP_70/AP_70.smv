-------------------------------
--   C O M P O S I T I O N   --
-------------------------------
MODULE dn_main

---------------------
--   I N P U T S   --
---------------------
IVAR input:
{
UNDEF, service115_UMS_ManagementTrip, service115_UMS_TripManagementRequest, service115_RESET, service116_UMS_TripRequest, service116_RESET
};

-----------------------
--   O U T P U T S   --
-----------------------
IVAR output:
{
UNDEF, service115_UMS_concrete4, service115_UMS_concrete3, service115_UMS_concrete2, service115_UMS_concrete1, service115_UMS_TripManagementReply, service115_UMS_concrete5, service116_UMS_TripAlternatives, service116_UMS_concrete2, service116_UMS_concrete1
};

-------------------------
--   S E R V I C E S   --
-------------------------
--	 service115
VAR service115: {ST2, ST1, ST4, ST3, ST6, ST5, ST8, ST7, ST0};
ASSIGN
next(service115) :=
case
(service115 = ST3 & output = service115_UMS_concrete3): ST4;
(service115 = ST4 & output = service115_UMS_concrete4): ST5;
(service115 = ST8 & input = service115_RESET): ST0;
(service115 = ST6 & input = service115_UMS_ManagementTrip): ST7;
(service115 = ST4 & input = service115_RESET): ST0;
(service115 = ST5 & output = service115_UMS_concrete5): ST6;
(service115 = ST0 & input = service115_UMS_TripManagementRequest): ST1;
(service115 = ST7 & output = service115_UMS_TripManagementReply): ST8;
(service115 = ST2 & input = service115_RESET): ST0;
(service115 = ST3 & input = service115_RESET): ST0;
(service115 = ST6 & input = service115_RESET): ST0;
(service115 = ST1 & input = service115_RESET): ST0;
(service115 = ST7 & input = service115_RESET): ST0;
(service115 = ST2 & output = service115_UMS_concrete2): ST3;
(service115 = ST1 & output = service115_UMS_concrete1): ST2;
(service115 = ST5 & input = service115_RESET): ST0;
1:service115;
esac;
TRANS ( input = service115_UMS_ManagementTrip) -> (service115 = ST6)
TRANS ( output = service115_UMS_concrete4) -> (service115 = ST4)
TRANS ( input = service115_UMS_TripManagementRequest) -> (service115 = ST0)
TRANS ( output = service115_UMS_concrete3) -> (service115 = ST3)
TRANS ( output = service115_UMS_concrete2) -> (service115 = ST2)
TRANS ( input = service115_RESET) -> (service115 = ST2 | service115 = ST1 | service115 = ST4 | service115 = ST3 | service115 = ST6 | service115 = ST5 | service115 = ST8 | service115 = ST7)
TRANS ( output = service115_UMS_concrete1) -> (service115 = ST1)
TRANS ( output = service115_UMS_TripManagementReply) -> (service115 = ST7)
TRANS ( output = service115_UMS_concrete5) -> (service115 = ST5)

--	 service116
VAR service116: {ST2, ST1, ST4, ST3, ST0};
ASSIGN
next(service116) :=
case
(service116 = ST3 & input = service116_RESET): ST0;
(service116 = ST1 & output = service116_UMS_concrete1): ST2;
(service116 = ST2 & output = service116_UMS_concrete2): ST3;
(service116 = ST3 & output = service116_UMS_TripAlternatives): ST4;
(service116 = ST0 & input = service116_UMS_TripRequest): ST1;
(service116 = ST4 & input = service116_RESET): ST0;
(service116 = ST2 & input = service116_RESET): ST0;
(service116 = ST1 & input = service116_RESET): ST0;
1:service116;
esac;
TRANS ( input = service116_UMS_TripRequest) -> (service116 = ST0)
TRANS ( output = service116_UMS_TripAlternatives) -> (service116 = ST3)
TRANS ( output = service116_UMS_concrete2) -> (service116 = ST2)
TRANS ( input = service116_RESET) -> (service116 = ST2 | service116 = ST1 | service116 = ST4 | service116 = ST3)
TRANS ( output = service116_UMS_concrete1) -> (service116 = ST1)

-----------------------
--   O B J E C T S   --
-----------------------
--	 SetupTrip
VAR SetupTrip: {ORGANIZED, INITIAL};
ASSIGN
next(SetupTrip) :=
case
(input = service115_UMS_ManagementTrip):{(ORGANIZED)};
1:SetupTrip;
esac;
--	 ProvideTrip
VAR ProvideTrip: {TRIP_REQUEST_ACCOMPLISHED, TRIP_MANAGEMENT_ACCOMPLISHED, TRIP_MANAGE_REQUEST_LOADED, TRIP_REQUEST_LOADED, INITIAL};
ASSIGN
next(ProvideTrip) :=
case
(ProvideTrip = TRIP_MANAGE_REQUEST_LOADED & output = service115_UMS_TripManagementReply): TRIP_MANAGEMENT_ACCOMPLISHED;
(ProvideTrip = TRIP_REQUEST_ACCOMPLISHED & input = service115_UMS_TripManagementRequest): TRIP_MANAGE_REQUEST_LOADED;
(ProvideTrip = TRIP_REQUEST_LOADED & output = service116_UMS_TripAlternatives): TRIP_REQUEST_ACCOMPLISHED;
(ProvideTrip = INITIAL & input = service116_UMS_TripRequest): TRIP_REQUEST_LOADED;
1:ProvideTrip;
esac;
-----------------------------------
--   P R E C O N D I T I O N S   --
-----------------------------------
TRANS (input = service115_UMS_TripManagementRequest) -> ((((ProvideTrip = TRIP_REQUEST_ACCOMPLISHED))))
TRANS (output = service115_UMS_TripManagementReply) -> ((((ProvideTrip = TRIP_MANAGE_REQUEST_LOADED))))
TRANS (input = service116_UMS_TripRequest) -> ((((ProvideTrip = INITIAL))))
TRANS (output = service116_UMS_TripAlternatives) -> ((((ProvideTrip = TRIP_REQUEST_LOADED))))
-----------------------------
--   I N V A R I A N T S   --
-----------------------------
--	 Synchronicity
TRANS (1-(input = UNDEF)) + (1-(output = UNDEF)) = 1

---------------------------------------
--   S Y N C H R O N I Z A T I O N   --
---------------------------------------
DEFINE SYNC0 := ((ProvideTrip in {TRIP_MANAGEMENT_ACCOMPLISHED}));
--	 Synchronicity enforcement
TRANS (service115 in {ST2, ST1, ST4, ST3, ST5, ST7}) -> (next(service115) != service115)
TRANS (service116 in {ST2, ST1, ST3}) -> (next(service116) != service116)
-------------------------------
--   C O N T R O L L E R S   --
-------------------------------
--	 Fault Controller
VAR FAULT_CONTROLLER: { START, STOP };
INIT FAULT_CONTROLLER = START

ASSIGN
next(FAULT_CONTROLLER) := 
case
(FAULT_CONTROLLER = START & FALSE): STOP;
1: FAULT_CONTROLLER;
esac;

TRANS (output != UNDEF | input != UNDEF) -> (FAULT_CONTROLLER != STOP)

-------------------------------------
--   I N I T I A L I Z A T I O N   --
-------------------------------------
INIT ((SetupTrip = INITIAL) & (ProvideTrip = TRIP_REQUEST_ACCOMPLISHED))
INIT ((service115 = ST0) & (service116 = ST0))

-----------------
--   G O A L   --
-----------------
MODULE main
VAR dn_main : dn_main;

GOAL ONEOF(
(dn_main.SYNC0), 1;
(dn_main.FAULT_CONTROLLER = STOP), 0

)