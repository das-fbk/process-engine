-------------------------------
--   C O M P O S I T I O N   --
-------------------------------
MODULE dn_main

---------------------
--   I N P U T S   --
---------------------
IVAR input:
{
UNDEF, service16_UMS_ManagementTrip, service16_UMS_TripManagementRequest, service16_RESET, service17_UMS_TripRequest, service17_RESET
};

-----------------------
--   O U T P U T S   --
-----------------------
IVAR output:
{
UNDEF, service16_UMS_concrete4, service16_UMS_concrete3, service16_UMS_concrete2, service16_UMS_concrete1, service16_UMS_TripManagementReply, service16_UMS_concrete5, service17_UMS_TripAlternatives, service17_UMS_concrete2, service17_UMS_concrete1
};

-------------------------
--   S E R V I C E S   --
-------------------------
--	 service16
VAR service16: {ST2, ST1, ST4, ST3, ST6, ST5, ST8, ST7, ST0};
ASSIGN
next(service16) :=
case
(service16 = ST6 & input = service16_RESET): ST0;
(service16 = ST7 & input = service16_RESET): ST0;
(service16 = ST1 & input = service16_RESET): ST0;
(service16 = ST0 & input = service16_UMS_TripManagementRequest): ST1;
(service16 = ST1 & output = service16_UMS_concrete1): ST2;
(service16 = ST7 & output = service16_UMS_TripManagementReply): ST8;
(service16 = ST8 & input = service16_RESET): ST0;
(service16 = ST4 & output = service16_UMS_concrete4): ST5;
(service16 = ST5 & input = service16_RESET): ST0;
(service16 = ST2 & output = service16_UMS_concrete2): ST3;
(service16 = ST3 & output = service16_UMS_concrete3): ST4;
(service16 = ST5 & output = service16_UMS_concrete5): ST6;
(service16 = ST2 & input = service16_RESET): ST0;
(service16 = ST4 & input = service16_RESET): ST0;
(service16 = ST3 & input = service16_RESET): ST0;
(service16 = ST6 & input = service16_UMS_ManagementTrip): ST7;
1:service16;
esac;
TRANS ( input = service16_UMS_ManagementTrip) -> (service16 = ST6)
TRANS ( output = service16_UMS_concrete4) -> (service16 = ST4)
TRANS ( input = service16_UMS_TripManagementRequest) -> (service16 = ST0)
TRANS ( output = service16_UMS_concrete3) -> (service16 = ST3)
TRANS ( output = service16_UMS_concrete2) -> (service16 = ST2)
TRANS ( input = service16_RESET) -> (service16 = ST2 | service16 = ST1 | service16 = ST4 | service16 = ST3 | service16 = ST6 | service16 = ST5 | service16 = ST8 | service16 = ST7)
TRANS ( output = service16_UMS_concrete1) -> (service16 = ST1)
TRANS ( output = service16_UMS_TripManagementReply) -> (service16 = ST7)
TRANS ( output = service16_UMS_concrete5) -> (service16 = ST5)

--	 service17
VAR service17: {ST2, ST1, ST4, ST3, ST0};
ASSIGN
next(service17) :=
case
(service17 = ST0 & input = service17_UMS_TripRequest): ST1;
(service17 = ST2 & input = service17_RESET): ST0;
(service17 = ST1 & input = service17_RESET): ST0;
(service17 = ST3 & input = service17_RESET): ST0;
(service17 = ST4 & input = service17_RESET): ST0;
(service17 = ST3 & output = service17_UMS_TripAlternatives): ST4;
(service17 = ST1 & output = service17_UMS_concrete1): ST2;
(service17 = ST2 & output = service17_UMS_concrete2): ST3;
1:service17;
esac;
TRANS ( input = service17_UMS_TripRequest) -> (service17 = ST0)
TRANS ( output = service17_UMS_TripAlternatives) -> (service17 = ST3)
TRANS ( output = service17_UMS_concrete2) -> (service17 = ST2)
TRANS ( input = service17_RESET) -> (service17 = ST2 | service17 = ST1 | service17 = ST4 | service17 = ST3)
TRANS ( output = service17_UMS_concrete1) -> (service17 = ST1)

-----------------------
--   O B J E C T S   --
-----------------------
--	 SetupTrip
VAR SetupTrip: {ORGANIZED, INITIAL};
ASSIGN
next(SetupTrip) :=
case
(input = service16_UMS_ManagementTrip):{(ORGANIZED)};
1:SetupTrip;
esac;
--	 ProvideTrip
VAR ProvideTrip: {TRIP_REQUEST_ACCOMPLISHED, TRIP_MANAGEMENT_ACCOMPLISHED, TRIP_MANAGE_REQUEST_LOADED, TRIP_REQUEST_LOADED, INITIAL};
ASSIGN
next(ProvideTrip) :=
case
(ProvideTrip = TRIP_REQUEST_LOADED & output = service17_UMS_TripAlternatives): TRIP_REQUEST_ACCOMPLISHED;
(ProvideTrip = TRIP_REQUEST_ACCOMPLISHED & input = service16_UMS_TripManagementRequest): TRIP_MANAGE_REQUEST_LOADED;
(ProvideTrip = INITIAL & input = service17_UMS_TripRequest): TRIP_REQUEST_LOADED;
(ProvideTrip = TRIP_MANAGE_REQUEST_LOADED & output = service16_UMS_TripManagementReply): TRIP_MANAGEMENT_ACCOMPLISHED;
1:ProvideTrip;
esac;
-----------------------------------
--   P R E C O N D I T I O N S   --
-----------------------------------
TRANS (input = service16_UMS_TripManagementRequest) -> ((((ProvideTrip = TRIP_REQUEST_ACCOMPLISHED))))
TRANS (output = service16_UMS_TripManagementReply) -> ((((ProvideTrip = TRIP_MANAGE_REQUEST_LOADED))))
TRANS (input = service17_UMS_TripRequest) -> ((((ProvideTrip = INITIAL))))
TRANS (output = service17_UMS_TripAlternatives) -> ((((ProvideTrip = TRIP_REQUEST_LOADED))))
-----------------------------
--   I N V A R I A N T S   --
-----------------------------
--	 Synchronicity
TRANS (1-(input = UNDEF)) + (1-(output = UNDEF)) = 1

---------------------------------------
--   S Y N C H R O N I Z A T I O N   --
---------------------------------------
DEFINE SYNC0 := ((ProvideTrip in {TRIP_REQUEST_ACCOMPLISHED}));
--	 Synchronicity enforcement
TRANS (service16 in {ST2, ST1, ST4, ST3, ST5, ST7}) -> (next(service16) != service16)
TRANS (service17 in {ST2, ST1, ST3}) -> (next(service17) != service17)
-------------------------------
--   C O N T R O L L E R S   --
-------------------------------
--	 Fault Controller
VAR FAULT_CONTROLLER: { START, STOP };
INIT FAULT_CONTROLLER = START

ASSIGN
next(FAULT_CONTROLLER) := 
case
(FAULT_CONTROLLER = START & FALSE): STOP;
1: FAULT_CONTROLLER;
esac;

TRANS (output != UNDEF | input != UNDEF) -> (FAULT_CONTROLLER != STOP)

-------------------------------------
--   I N I T I A L I Z A T I O N   --
-------------------------------------
INIT ((SetupTrip = INITIAL) & (ProvideTrip = INITIAL))
INIT ((service16 = ST0) & (service17 = ST0))

-----------------
--   G O A L   --
-----------------
MODULE main
VAR dn_main : dn_main;

GOAL ONEOF(
(dn_main.SYNC0), 1;
(dn_main.FAULT_CONTROLLER = STOP), 0

)