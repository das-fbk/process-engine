<?xml version="1.0" encoding="UTF-8"?>
<tns:domainProperty id="HelperDP" xmlns:tns="http://das.fbk.eu/Process"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

	<tns:state isInitial="true">INITIAL</tns:state>
	<tns:state>HELPER_COMMITED</tns:state>

	<tns:event>helpercommited</tns:event>

	<tns:transition isControllable="true">
		<tns:from>INITIAL</tns:from>
		<tns:to>HELPER_COMMITED</tns:to>
		<tns:event>helpercommited</tns:event>
	</tns:transition>

</tns:domainProperty>
