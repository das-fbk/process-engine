-------------------------------
--   C O M P O S I T I O N   --
-------------------------------
MODULE dn_main

---------------------
--   I N P U T S   --
---------------------
IVAR input:
{
UNDEF, service120_UMS_ManagementTrip, service120_UMS_TripManagementRequest, service120_RESET, service122_UMS_TripRequest, service122_RESET
};

-----------------------
--   O U T P U T S   --
-----------------------
IVAR output:
{
UNDEF, service120_UMS_concrete4, service120_UMS_concrete3, service120_UMS_concrete2, service120_UMS_concrete1, service120_UMS_TripManagementReply, service120_UMS_concrete5, service122_UMS_TripAlternatives, service122_UMS_concrete2, service122_UMS_concrete1
};

-------------------------
--   S E R V I C E S   --
-------------------------
--	 service120
VAR service120: {ST2, ST1, ST4, ST3, ST6, ST5, ST8, ST7, ST0};
ASSIGN
next(service120) :=
case
(service120 = ST4 & output = service120_UMS_concrete4): ST5;
(service120 = ST3 & input = service120_RESET): ST0;
(service120 = ST0 & input = service120_UMS_TripManagementRequest): ST1;
(service120 = ST1 & output = service120_UMS_concrete1): ST2;
(service120 = ST4 & input = service120_RESET): ST0;
(service120 = ST7 & input = service120_RESET): ST0;
(service120 = ST1 & input = service120_RESET): ST0;
(service120 = ST5 & input = service120_RESET): ST0;
(service120 = ST3 & output = service120_UMS_concrete3): ST4;
(service120 = ST5 & output = service120_UMS_concrete5): ST6;
(service120 = ST8 & input = service120_RESET): ST0;
(service120 = ST2 & output = service120_UMS_concrete2): ST3;
(service120 = ST6 & input = service120_UMS_ManagementTrip): ST7;
(service120 = ST6 & input = service120_RESET): ST0;
(service120 = ST7 & output = service120_UMS_TripManagementReply): ST8;
(service120 = ST2 & input = service120_RESET): ST0;
1:service120;
esac;
TRANS ( input = service120_UMS_ManagementTrip) -> (service120 = ST6)
TRANS ( output = service120_UMS_concrete4) -> (service120 = ST4)
TRANS ( input = service120_UMS_TripManagementRequest) -> (service120 = ST0)
TRANS ( output = service120_UMS_concrete3) -> (service120 = ST3)
TRANS ( output = service120_UMS_concrete2) -> (service120 = ST2)
TRANS ( input = service120_RESET) -> (service120 = ST2 | service120 = ST1 | service120 = ST4 | service120 = ST3 | service120 = ST6 | service120 = ST5 | service120 = ST8 | service120 = ST7)
TRANS ( output = service120_UMS_concrete1) -> (service120 = ST1)
TRANS ( output = service120_UMS_TripManagementReply) -> (service120 = ST7)
TRANS ( output = service120_UMS_concrete5) -> (service120 = ST5)

--	 service122
VAR service122: {ST2, ST1, ST4, ST3, ST0};
ASSIGN
next(service122) :=
case
(service122 = ST4 & input = service122_RESET): ST0;
(service122 = ST3 & output = service122_UMS_TripAlternatives): ST4;
(service122 = ST1 & output = service122_UMS_concrete1): ST2;
(service122 = ST2 & output = service122_UMS_concrete2): ST3;
(service122 = ST1 & input = service122_RESET): ST0;
(service122 = ST0 & input = service122_UMS_TripRequest): ST1;
(service122 = ST3 & input = service122_RESET): ST0;
(service122 = ST2 & input = service122_RESET): ST0;
1:service122;
esac;
TRANS ( input = service122_UMS_TripRequest) -> (service122 = ST0)
TRANS ( output = service122_UMS_TripAlternatives) -> (service122 = ST3)
TRANS ( output = service122_UMS_concrete2) -> (service122 = ST2)
TRANS ( input = service122_RESET) -> (service122 = ST2 | service122 = ST1 | service122 = ST4 | service122 = ST3)
TRANS ( output = service122_UMS_concrete1) -> (service122 = ST1)

-----------------------
--   O B J E C T S   --
-----------------------
--	 SetupTrip
VAR SetupTrip: {ORGANIZED, INITIAL};
ASSIGN
next(SetupTrip) :=
case
(input = service120_UMS_ManagementTrip):{(ORGANIZED)};
1:SetupTrip;
esac;
--	 ProvideTrip
VAR ProvideTrip: {TRIP_REQUEST_ACCOMPLISHED, TRIP_MANAGEMENT_ACCOMPLISHED, TRIP_MANAGE_REQUEST_LOADED, TRIP_REQUEST_LOADED, INITIAL};
ASSIGN
next(ProvideTrip) :=
case
(ProvideTrip = TRIP_MANAGE_REQUEST_LOADED & output = service120_UMS_TripManagementReply): TRIP_MANAGEMENT_ACCOMPLISHED;
(ProvideTrip = INITIAL & input = service122_UMS_TripRequest): TRIP_REQUEST_LOADED;
(ProvideTrip = TRIP_REQUEST_LOADED & output = service122_UMS_TripAlternatives): TRIP_REQUEST_ACCOMPLISHED;
(ProvideTrip = TRIP_REQUEST_ACCOMPLISHED & input = service120_UMS_TripManagementRequest): TRIP_MANAGE_REQUEST_LOADED;
1:ProvideTrip;
esac;
-----------------------------------
--   P R E C O N D I T I O N S   --
-----------------------------------
TRANS (input = service120_UMS_TripManagementRequest) -> ((((ProvideTrip = TRIP_REQUEST_ACCOMPLISHED))))
TRANS (output = service120_UMS_TripManagementReply) -> ((((ProvideTrip = TRIP_MANAGE_REQUEST_LOADED))))
TRANS (input = service122_UMS_TripRequest) -> ((((ProvideTrip = INITIAL))))
TRANS (output = service122_UMS_TripAlternatives) -> ((((ProvideTrip = TRIP_REQUEST_LOADED))))
-----------------------------
--   I N V A R I A N T S   --
-----------------------------
--	 Synchronicity
TRANS (1-(input = UNDEF)) + (1-(output = UNDEF)) = 1

---------------------------------------
--   S Y N C H R O N I Z A T I O N   --
---------------------------------------
DEFINE SYNC0 := ((ProvideTrip in {TRIP_MANAGEMENT_ACCOMPLISHED}));
--	 Synchronicity enforcement
TRANS (service120 in {ST2, ST1, ST4, ST3, ST5, ST7}) -> (next(service120) != service120)
TRANS (service122 in {ST2, ST1, ST3}) -> (next(service122) != service122)
-------------------------------
--   C O N T R O L L E R S   --
-------------------------------
--	 Fault Controller
VAR FAULT_CONTROLLER: { START, STOP };
INIT FAULT_CONTROLLER = START

ASSIGN
next(FAULT_CONTROLLER) := 
case
(FAULT_CONTROLLER = START & FALSE): STOP;
1: FAULT_CONTROLLER;
esac;

TRANS (output != UNDEF | input != UNDEF) -> (FAULT_CONTROLLER != STOP)

-------------------------------------
--   I N I T I A L I Z A T I O N   --
-------------------------------------
INIT ((SetupTrip = INITIAL) & (ProvideTrip = TRIP_REQUEST_ACCOMPLISHED))
INIT ((service120 = ST0) & (service122 = ST0))

-----------------
--   G O A L   --
-----------------
MODULE main
VAR dn_main : dn_main;

GOAL ONEOF(
(dn_main.SYNC0), 1;
(dn_main.FAULT_CONTROLLER = STOP), 0

)