-------------------------------
--   C O M P O S I T I O N   --
-------------------------------
MODULE dn_main

---------------------
--   I N P U T S   --
---------------------
IVAR input:
{
UNDEF, service124_UMS_ManagementTrip, service124_UMS_TripManagementRequest, service124_RESET, service125_UMS_TripRequest, service125_RESET
};

-----------------------
--   O U T P U T S   --
-----------------------
IVAR output:
{
UNDEF, service124_UMS_concrete4, service124_UMS_concrete3, service124_UMS_concrete2, service124_UMS_concrete1, service124_UMS_TripManagementReply, service124_UMS_concrete5, service125_UMS_TripAlternatives, service125_UMS_concrete2, service125_UMS_concrete1
};

-------------------------
--   S E R V I C E S   --
-------------------------
--	 service124
VAR service124: {ST2, ST1, ST4, ST3, ST6, ST5, ST8, ST7, ST0};
ASSIGN
next(service124) :=
case
(service124 = ST1 & output = service124_UMS_concrete1): ST2;
(service124 = ST4 & output = service124_UMS_concrete4): ST5;
(service124 = ST5 & output = service124_UMS_concrete5): ST6;
(service124 = ST7 & input = service124_RESET): ST0;
(service124 = ST2 & output = service124_UMS_concrete2): ST3;
(service124 = ST7 & output = service124_UMS_TripManagementReply): ST8;
(service124 = ST5 & input = service124_RESET): ST0;
(service124 = ST4 & input = service124_RESET): ST0;
(service124 = ST6 & input = service124_UMS_ManagementTrip): ST7;
(service124 = ST8 & input = service124_RESET): ST0;
(service124 = ST3 & output = service124_UMS_concrete3): ST4;
(service124 = ST2 & input = service124_RESET): ST0;
(service124 = ST1 & input = service124_RESET): ST0;
(service124 = ST3 & input = service124_RESET): ST0;
(service124 = ST0 & input = service124_UMS_TripManagementRequest): ST1;
(service124 = ST6 & input = service124_RESET): ST0;
1:service124;
esac;
TRANS ( input = service124_UMS_ManagementTrip) -> (service124 = ST6)
TRANS ( output = service124_UMS_concrete4) -> (service124 = ST4)
TRANS ( input = service124_UMS_TripManagementRequest) -> (service124 = ST0)
TRANS ( output = service124_UMS_concrete3) -> (service124 = ST3)
TRANS ( output = service124_UMS_concrete2) -> (service124 = ST2)
TRANS ( input = service124_RESET) -> (service124 = ST2 | service124 = ST1 | service124 = ST4 | service124 = ST3 | service124 = ST6 | service124 = ST5 | service124 = ST8 | service124 = ST7)
TRANS ( output = service124_UMS_concrete1) -> (service124 = ST1)
TRANS ( output = service124_UMS_TripManagementReply) -> (service124 = ST7)
TRANS ( output = service124_UMS_concrete5) -> (service124 = ST5)

--	 service125
VAR service125: {ST2, ST1, ST4, ST3, ST0};
ASSIGN
next(service125) :=
case
(service125 = ST4 & input = service125_RESET): ST0;
(service125 = ST1 & output = service125_UMS_concrete1): ST2;
(service125 = ST1 & input = service125_RESET): ST0;
(service125 = ST3 & output = service125_UMS_TripAlternatives): ST4;
(service125 = ST0 & input = service125_UMS_TripRequest): ST1;
(service125 = ST2 & input = service125_RESET): ST0;
(service125 = ST2 & output = service125_UMS_concrete2): ST3;
(service125 = ST3 & input = service125_RESET): ST0;
1:service125;
esac;
TRANS ( input = service125_UMS_TripRequest) -> (service125 = ST0)
TRANS ( output = service125_UMS_TripAlternatives) -> (service125 = ST3)
TRANS ( output = service125_UMS_concrete2) -> (service125 = ST2)
TRANS ( input = service125_RESET) -> (service125 = ST2 | service125 = ST1 | service125 = ST4 | service125 = ST3)
TRANS ( output = service125_UMS_concrete1) -> (service125 = ST1)

-----------------------
--   O B J E C T S   --
-----------------------
--	 SetupTrip
VAR SetupTrip: {ORGANIZED, INITIAL};
ASSIGN
next(SetupTrip) :=
case
(input = service124_UMS_ManagementTrip):{(ORGANIZED)};
1:SetupTrip;
esac;
--	 ProvideTrip
VAR ProvideTrip: {TRIP_REQUEST_ACCOMPLISHED, TRIP_MANAGEMENT_ACCOMPLISHED, TRIP_MANAGE_REQUEST_LOADED, TRIP_REQUEST_LOADED, INITIAL};
ASSIGN
next(ProvideTrip) :=
case
(ProvideTrip = TRIP_REQUEST_LOADED & output = service125_UMS_TripAlternatives): TRIP_REQUEST_ACCOMPLISHED;
(ProvideTrip = INITIAL & input = service125_UMS_TripRequest): TRIP_REQUEST_LOADED;
(ProvideTrip = TRIP_REQUEST_ACCOMPLISHED & input = service124_UMS_TripManagementRequest): TRIP_MANAGE_REQUEST_LOADED;
(ProvideTrip = TRIP_MANAGE_REQUEST_LOADED & output = service124_UMS_TripManagementReply): TRIP_MANAGEMENT_ACCOMPLISHED;
1:ProvideTrip;
esac;
-----------------------------------
--   P R E C O N D I T I O N S   --
-----------------------------------
TRANS (input = service124_UMS_TripManagementRequest) -> ((((ProvideTrip = TRIP_REQUEST_ACCOMPLISHED))))
TRANS (output = service124_UMS_TripManagementReply) -> ((((ProvideTrip = TRIP_MANAGE_REQUEST_LOADED))))
TRANS (input = service125_UMS_TripRequest) -> ((((ProvideTrip = INITIAL))))
TRANS (output = service125_UMS_TripAlternatives) -> ((((ProvideTrip = TRIP_REQUEST_LOADED))))
-----------------------------
--   I N V A R I A N T S   --
-----------------------------
--	 Synchronicity
TRANS (1-(input = UNDEF)) + (1-(output = UNDEF)) = 1

---------------------------------------
--   S Y N C H R O N I Z A T I O N   --
---------------------------------------
DEFINE SYNC0 := ((ProvideTrip in {TRIP_MANAGEMENT_ACCOMPLISHED}));
--	 Synchronicity enforcement
TRANS (service124 in {ST2, ST1, ST4, ST3, ST5, ST7}) -> (next(service124) != service124)
TRANS (service125 in {ST2, ST1, ST3}) -> (next(service125) != service125)
-------------------------------
--   C O N T R O L L E R S   --
-------------------------------
--	 Fault Controller
VAR FAULT_CONTROLLER: { START, STOP };
INIT FAULT_CONTROLLER = START

ASSIGN
next(FAULT_CONTROLLER) := 
case
(FAULT_CONTROLLER = START & FALSE): STOP;
1: FAULT_CONTROLLER;
esac;

TRANS (output != UNDEF | input != UNDEF) -> (FAULT_CONTROLLER != STOP)

-------------------------------------
--   I N I T I A L I Z A T I O N   --
-------------------------------------
INIT ((SetupTrip = INITIAL) & (ProvideTrip = TRIP_REQUEST_ACCOMPLISHED))
INIT ((service124 = ST0) & (service125 = ST0))

-----------------
--   G O A L   --
-----------------
MODULE main
VAR dn_main : dn_main;

GOAL ONEOF(
(dn_main.SYNC0), 1;
(dn_main.FAULT_CONTROLLER = STOP), 0

)