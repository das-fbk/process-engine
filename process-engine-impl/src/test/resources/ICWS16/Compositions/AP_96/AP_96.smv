-------------------------------
--   C O M P O S I T I O N   --
-------------------------------
MODULE dn_main

---------------------
--   I N P U T S   --
---------------------
IVAR input:
{
UNDEF, service167_UMS_ManagementTrip, service167_UMS_TripManagementRequest, service167_RESET, service169_UMS_TripRequest, service169_RESET
};

-----------------------
--   O U T P U T S   --
-----------------------
IVAR output:
{
UNDEF, service167_UMS_concrete4, service167_UMS_concrete3, service167_UMS_concrete2, service167_UMS_concrete1, service167_UMS_TripManagementReply, service167_UMS_concrete5, service169_UMS_TripAlternatives, service169_UMS_concrete2, service169_UMS_concrete1
};

-------------------------
--   S E R V I C E S   --
-------------------------
--	 service167
VAR service167: {ST2, ST1, ST4, ST3, ST6, ST5, ST8, ST7, ST0};
ASSIGN
next(service167) :=
case
(service167 = ST2 & output = service167_UMS_concrete2): ST3;
(service167 = ST3 & output = service167_UMS_concrete3): ST4;
(service167 = ST2 & input = service167_RESET): ST0;
(service167 = ST7 & input = service167_RESET): ST0;
(service167 = ST4 & input = service167_RESET): ST0;
(service167 = ST0 & input = service167_UMS_TripManagementRequest): ST1;
(service167 = ST5 & output = service167_UMS_concrete5): ST6;
(service167 = ST7 & output = service167_UMS_TripManagementReply): ST8;
(service167 = ST3 & input = service167_RESET): ST0;
(service167 = ST1 & output = service167_UMS_concrete1): ST2;
(service167 = ST4 & output = service167_UMS_concrete4): ST5;
(service167 = ST1 & input = service167_RESET): ST0;
(service167 = ST6 & input = service167_UMS_ManagementTrip): ST7;
(service167 = ST8 & input = service167_RESET): ST0;
(service167 = ST6 & input = service167_RESET): ST0;
(service167 = ST5 & input = service167_RESET): ST0;
1:service167;
esac;
TRANS ( input = service167_UMS_ManagementTrip) -> (service167 = ST6)
TRANS ( output = service167_UMS_concrete4) -> (service167 = ST4)
TRANS ( input = service167_UMS_TripManagementRequest) -> (service167 = ST0)
TRANS ( output = service167_UMS_concrete3) -> (service167 = ST3)
TRANS ( output = service167_UMS_concrete2) -> (service167 = ST2)
TRANS ( input = service167_RESET) -> (service167 = ST2 | service167 = ST1 | service167 = ST4 | service167 = ST3 | service167 = ST6 | service167 = ST5 | service167 = ST8 | service167 = ST7)
TRANS ( output = service167_UMS_concrete1) -> (service167 = ST1)
TRANS ( output = service167_UMS_TripManagementReply) -> (service167 = ST7)
TRANS ( output = service167_UMS_concrete5) -> (service167 = ST5)

--	 service169
VAR service169: {ST2, ST1, ST4, ST3, ST0};
ASSIGN
next(service169) :=
case
(service169 = ST3 & input = service169_RESET): ST0;
(service169 = ST3 & output = service169_UMS_TripAlternatives): ST4;
(service169 = ST2 & input = service169_RESET): ST0;
(service169 = ST1 & output = service169_UMS_concrete1): ST2;
(service169 = ST2 & output = service169_UMS_concrete2): ST3;
(service169 = ST0 & input = service169_UMS_TripRequest): ST1;
(service169 = ST4 & input = service169_RESET): ST0;
(service169 = ST1 & input = service169_RESET): ST0;
1:service169;
esac;
TRANS ( input = service169_UMS_TripRequest) -> (service169 = ST0)
TRANS ( output = service169_UMS_TripAlternatives) -> (service169 = ST3)
TRANS ( output = service169_UMS_concrete2) -> (service169 = ST2)
TRANS ( input = service169_RESET) -> (service169 = ST2 | service169 = ST1 | service169 = ST4 | service169 = ST3)
TRANS ( output = service169_UMS_concrete1) -> (service169 = ST1)

-----------------------
--   O B J E C T S   --
-----------------------
--	 SetupTrip
VAR SetupTrip: {ORGANIZED, INITIAL};
ASSIGN
next(SetupTrip) :=
case
(input = service167_UMS_ManagementTrip):{(ORGANIZED)};
1:SetupTrip;
esac;
--	 ProvideTrip
VAR ProvideTrip: {TRIP_REQUEST_ACCOMPLISHED, TRIP_MANAGEMENT_ACCOMPLISHED, TRIP_MANAGE_REQUEST_LOADED, TRIP_REQUEST_LOADED, INITIAL};
ASSIGN
next(ProvideTrip) :=
case
(ProvideTrip = TRIP_MANAGE_REQUEST_LOADED & output = service167_UMS_TripManagementReply): TRIP_MANAGEMENT_ACCOMPLISHED;
(ProvideTrip = TRIP_REQUEST_LOADED & output = service169_UMS_TripAlternatives): TRIP_REQUEST_ACCOMPLISHED;
(ProvideTrip = INITIAL & input = service169_UMS_TripRequest): TRIP_REQUEST_LOADED;
(ProvideTrip = TRIP_REQUEST_ACCOMPLISHED & input = service167_UMS_TripManagementRequest): TRIP_MANAGE_REQUEST_LOADED;
1:ProvideTrip;
esac;
-----------------------------------
--   P R E C O N D I T I O N S   --
-----------------------------------
TRANS (input = service167_UMS_TripManagementRequest) -> ((((ProvideTrip = TRIP_REQUEST_ACCOMPLISHED))))
TRANS (output = service167_UMS_TripManagementReply) -> ((((ProvideTrip = TRIP_MANAGE_REQUEST_LOADED))))
TRANS (input = service169_UMS_TripRequest) -> ((((ProvideTrip = INITIAL))))
TRANS (output = service169_UMS_TripAlternatives) -> ((((ProvideTrip = TRIP_REQUEST_LOADED))))
-----------------------------
--   I N V A R I A N T S   --
-----------------------------
--	 Synchronicity
TRANS (1-(input = UNDEF)) + (1-(output = UNDEF)) = 1

---------------------------------------
--   S Y N C H R O N I Z A T I O N   --
---------------------------------------
DEFINE SYNC0 := ((ProvideTrip in {TRIP_MANAGEMENT_ACCOMPLISHED}));
--	 Synchronicity enforcement
TRANS (service167 in {ST2, ST1, ST4, ST3, ST5, ST7}) -> (next(service167) != service167)
TRANS (service169 in {ST2, ST1, ST3}) -> (next(service169) != service169)
-------------------------------
--   C O N T R O L L E R S   --
-------------------------------
--	 Fault Controller
VAR FAULT_CONTROLLER: { START, STOP };
INIT FAULT_CONTROLLER = START

ASSIGN
next(FAULT_CONTROLLER) := 
case
(FAULT_CONTROLLER = START & FALSE): STOP;
1: FAULT_CONTROLLER;
esac;

TRANS (output != UNDEF | input != UNDEF) -> (FAULT_CONTROLLER != STOP)

-------------------------------------
--   I N I T I A L I Z A T I O N   --
-------------------------------------
INIT ((SetupTrip = INITIAL) & (ProvideTrip = TRIP_REQUEST_ACCOMPLISHED))
INIT ((service167 = ST0) & (service169 = ST0))

-----------------
--   G O A L   --
-----------------
MODULE main
VAR dn_main : dn_main;

GOAL ONEOF(
(dn_main.SYNC0), 1;
(dn_main.FAULT_CONTROLLER = STOP), 0

)