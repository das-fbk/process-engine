<?xml version="1.0" encoding="UTF-8"?>
<tns:process name="PROC_RobotAssistant" xmlns:tns="http://das.fbk.eu/Process"
	xmlns:tns1="http://das.fbk.eu/Annotation" xmlns:tns2="http://das.fbk.eu/DataType"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://das.fbk.eu/Process ../../../schema/allow-ensembles/Process.xsd ">

	<tns:receive name="RA_MoveRequest">
		<tns:precondition>
			<tns1:point>
				<tns1:domainProperty dp_name="RobotStatus">
					<tns1:state>INITIAL</tns1:state>
				</tns1:domainProperty>
			</tns1:point>
		</tns:precondition>
	</tns:receive>

	<tns:concrete name="RA_CalculateStep">
	</tns:concrete>

	<tns:invoke name="RA_MoveResponse">
		<tns:effect>
			<tns1:event dpName="RobotStatus" eventName="doStep"></tns1:event>
		</tns:effect>
	</tns:invoke>
</tns:process>
