-------------------------------
--   C O M P O S I T I O N   --
-------------------------------
MODULE dn_main

---------------------
--   I N P U T S   --
---------------------
IVAR input:
{
UNDEF, service168_UMS_ManagementTrip, service168_UMS_TripManagementRequest, service168_RESET, service170_UMS_TripRequest, service170_RESET
};

-----------------------
--   O U T P U T S   --
-----------------------
IVAR output:
{
UNDEF, service168_UMS_concrete4, service168_UMS_concrete3, service168_UMS_concrete2, service168_UMS_concrete1, service168_UMS_TripManagementReply, service168_UMS_concrete5, service170_UMS_TripAlternatives, service170_UMS_concrete2, service170_UMS_concrete1
};

-------------------------
--   S E R V I C E S   --
-------------------------
--	 service168
VAR service168: {ST2, ST1, ST4, ST3, ST6, ST5, ST8, ST7, ST0};
ASSIGN
next(service168) :=
case
(service168 = ST6 & input = service168_RESET): ST0;
(service168 = ST6 & input = service168_UMS_ManagementTrip): ST7;
(service168 = ST8 & input = service168_RESET): ST0;
(service168 = ST5 & input = service168_RESET): ST0;
(service168 = ST1 & input = service168_RESET): ST0;
(service168 = ST7 & output = service168_UMS_TripManagementReply): ST8;
(service168 = ST5 & output = service168_UMS_concrete5): ST6;
(service168 = ST2 & input = service168_RESET): ST0;
(service168 = ST7 & input = service168_RESET): ST0;
(service168 = ST3 & input = service168_RESET): ST0;
(service168 = ST4 & output = service168_UMS_concrete4): ST5;
(service168 = ST4 & input = service168_RESET): ST0;
(service168 = ST1 & output = service168_UMS_concrete1): ST2;
(service168 = ST3 & output = service168_UMS_concrete3): ST4;
(service168 = ST2 & output = service168_UMS_concrete2): ST3;
(service168 = ST0 & input = service168_UMS_TripManagementRequest): ST1;
1:service168;
esac;
TRANS ( input = service168_UMS_ManagementTrip) -> (service168 = ST6)
TRANS ( output = service168_UMS_concrete4) -> (service168 = ST4)
TRANS ( input = service168_UMS_TripManagementRequest) -> (service168 = ST0)
TRANS ( output = service168_UMS_concrete3) -> (service168 = ST3)
TRANS ( output = service168_UMS_concrete2) -> (service168 = ST2)
TRANS ( input = service168_RESET) -> (service168 = ST2 | service168 = ST1 | service168 = ST4 | service168 = ST3 | service168 = ST6 | service168 = ST5 | service168 = ST8 | service168 = ST7)
TRANS ( output = service168_UMS_concrete1) -> (service168 = ST1)
TRANS ( output = service168_UMS_TripManagementReply) -> (service168 = ST7)
TRANS ( output = service168_UMS_concrete5) -> (service168 = ST5)

--	 service170
VAR service170: {ST2, ST1, ST4, ST3, ST0};
ASSIGN
next(service170) :=
case
(service170 = ST3 & output = service170_UMS_TripAlternatives): ST4;
(service170 = ST2 & input = service170_RESET): ST0;
(service170 = ST4 & input = service170_RESET): ST0;
(service170 = ST0 & input = service170_UMS_TripRequest): ST1;
(service170 = ST1 & output = service170_UMS_concrete1): ST2;
(service170 = ST1 & input = service170_RESET): ST0;
(service170 = ST2 & output = service170_UMS_concrete2): ST3;
(service170 = ST3 & input = service170_RESET): ST0;
1:service170;
esac;
TRANS ( input = service170_UMS_TripRequest) -> (service170 = ST0)
TRANS ( output = service170_UMS_TripAlternatives) -> (service170 = ST3)
TRANS ( output = service170_UMS_concrete2) -> (service170 = ST2)
TRANS ( input = service170_RESET) -> (service170 = ST2 | service170 = ST1 | service170 = ST4 | service170 = ST3)
TRANS ( output = service170_UMS_concrete1) -> (service170 = ST1)

-----------------------
--   O B J E C T S   --
-----------------------
--	 SetupTrip
VAR SetupTrip: {ORGANIZED, INITIAL};
ASSIGN
next(SetupTrip) :=
case
(input = service168_UMS_ManagementTrip):{(ORGANIZED)};
1:SetupTrip;
esac;
--	 ProvideTrip
VAR ProvideTrip: {TRIP_REQUEST_ACCOMPLISHED, TRIP_MANAGEMENT_ACCOMPLISHED, TRIP_MANAGE_REQUEST_LOADED, TRIP_REQUEST_LOADED, INITIAL};
ASSIGN
next(ProvideTrip) :=
case
(ProvideTrip = INITIAL & input = service170_UMS_TripRequest): TRIP_REQUEST_LOADED;
(ProvideTrip = TRIP_REQUEST_ACCOMPLISHED & input = service168_UMS_TripManagementRequest): TRIP_MANAGE_REQUEST_LOADED;
(ProvideTrip = TRIP_MANAGE_REQUEST_LOADED & output = service168_UMS_TripManagementReply): TRIP_MANAGEMENT_ACCOMPLISHED;
(ProvideTrip = TRIP_REQUEST_LOADED & output = service170_UMS_TripAlternatives): TRIP_REQUEST_ACCOMPLISHED;
1:ProvideTrip;
esac;
-----------------------------------
--   P R E C O N D I T I O N S   --
-----------------------------------
TRANS (input = service168_UMS_TripManagementRequest) -> ((((ProvideTrip = TRIP_REQUEST_ACCOMPLISHED))))
TRANS (output = service168_UMS_TripManagementReply) -> ((((ProvideTrip = TRIP_MANAGE_REQUEST_LOADED))))
TRANS (input = service170_UMS_TripRequest) -> ((((ProvideTrip = INITIAL))))
TRANS (output = service170_UMS_TripAlternatives) -> ((((ProvideTrip = TRIP_REQUEST_LOADED))))
-----------------------------
--   I N V A R I A N T S   --
-----------------------------
--	 Synchronicity
TRANS (1-(input = UNDEF)) + (1-(output = UNDEF)) = 1

---------------------------------------
--   S Y N C H R O N I Z A T I O N   --
---------------------------------------
DEFINE SYNC0 := ((ProvideTrip in {TRIP_MANAGEMENT_ACCOMPLISHED}));
--	 Synchronicity enforcement
TRANS (service168 in {ST2, ST1, ST4, ST3, ST5, ST7}) -> (next(service168) != service168)
TRANS (service170 in {ST2, ST1, ST3}) -> (next(service170) != service170)
-------------------------------
--   C O N T R O L L E R S   --
-------------------------------
--	 Fault Controller
VAR FAULT_CONTROLLER: { START, STOP };
INIT FAULT_CONTROLLER = START

ASSIGN
next(FAULT_CONTROLLER) := 
case
(FAULT_CONTROLLER = START & FALSE): STOP;
1: FAULT_CONTROLLER;
esac;

TRANS (output != UNDEF | input != UNDEF) -> (FAULT_CONTROLLER != STOP)

-------------------------------------
--   I N I T I A L I Z A T I O N   --
-------------------------------------
INIT ((SetupTrip = INITIAL) & (ProvideTrip = TRIP_REQUEST_ACCOMPLISHED))
INIT ((service168 = ST0) & (service170 = ST0))

-----------------
--   G O A L   --
-----------------
MODULE main
VAR dn_main : dn_main;

GOAL ONEOF(
(dn_main.SYNC0), 1;
(dn_main.FAULT_CONTROLLER = STOP), 0

)