-------------------------------
--   C O M P O S I T I O N   --
-------------------------------
MODULE dn_main

---------------------
--   I N P U T S   --
---------------------
IVAR input:
{
UNDEF, service59_UMS_ManagementTrip, service59_UMS_TripManagementRequest, service59_RESET, service60_UMS_TripRequest, service60_RESET
};

-----------------------
--   O U T P U T S   --
-----------------------
IVAR output:
{
UNDEF, service59_UMS_concrete4, service59_UMS_concrete3, service59_UMS_concrete2, service59_UMS_concrete1, service59_UMS_TripManagementReply, service59_UMS_concrete5, service60_UMS_TripAlternatives, service60_UMS_concrete2, service60_UMS_concrete1
};

-------------------------
--   S E R V I C E S   --
-------------------------
--	 service59
VAR service59: {ST2, ST1, ST4, ST3, ST6, ST5, ST8, ST7, ST0};
ASSIGN
next(service59) :=
case
(service59 = ST1 & input = service59_RESET): ST0;
(service59 = ST7 & input = service59_RESET): ST0;
(service59 = ST6 & input = service59_UMS_ManagementTrip): ST7;
(service59 = ST5 & input = service59_RESET): ST0;
(service59 = ST3 & input = service59_RESET): ST0;
(service59 = ST7 & output = service59_UMS_TripManagementReply): ST8;
(service59 = ST0 & input = service59_UMS_TripManagementRequest): ST1;
(service59 = ST2 & output = service59_UMS_concrete2): ST3;
(service59 = ST1 & output = service59_UMS_concrete1): ST2;
(service59 = ST3 & output = service59_UMS_concrete3): ST4;
(service59 = ST6 & input = service59_RESET): ST0;
(service59 = ST4 & input = service59_RESET): ST0;
(service59 = ST8 & input = service59_RESET): ST0;
(service59 = ST2 & input = service59_RESET): ST0;
(service59 = ST5 & output = service59_UMS_concrete5): ST6;
(service59 = ST4 & output = service59_UMS_concrete4): ST5;
1:service59;
esac;
TRANS ( input = service59_UMS_ManagementTrip) -> (service59 = ST6)
TRANS ( output = service59_UMS_concrete4) -> (service59 = ST4)
TRANS ( input = service59_UMS_TripManagementRequest) -> (service59 = ST0)
TRANS ( output = service59_UMS_concrete3) -> (service59 = ST3)
TRANS ( output = service59_UMS_concrete2) -> (service59 = ST2)
TRANS ( input = service59_RESET) -> (service59 = ST2 | service59 = ST1 | service59 = ST4 | service59 = ST3 | service59 = ST6 | service59 = ST5 | service59 = ST8 | service59 = ST7)
TRANS ( output = service59_UMS_concrete1) -> (service59 = ST1)
TRANS ( output = service59_UMS_TripManagementReply) -> (service59 = ST7)
TRANS ( output = service59_UMS_concrete5) -> (service59 = ST5)

--	 service60
VAR service60: {ST2, ST1, ST4, ST3, ST0};
ASSIGN
next(service60) :=
case
(service60 = ST1 & input = service60_RESET): ST0;
(service60 = ST0 & input = service60_UMS_TripRequest): ST1;
(service60 = ST1 & output = service60_UMS_concrete1): ST2;
(service60 = ST3 & input = service60_RESET): ST0;
(service60 = ST2 & input = service60_RESET): ST0;
(service60 = ST4 & input = service60_RESET): ST0;
(service60 = ST3 & output = service60_UMS_TripAlternatives): ST4;
(service60 = ST2 & output = service60_UMS_concrete2): ST3;
1:service60;
esac;
TRANS ( input = service60_UMS_TripRequest) -> (service60 = ST0)
TRANS ( output = service60_UMS_TripAlternatives) -> (service60 = ST3)
TRANS ( output = service60_UMS_concrete2) -> (service60 = ST2)
TRANS ( input = service60_RESET) -> (service60 = ST2 | service60 = ST1 | service60 = ST4 | service60 = ST3)
TRANS ( output = service60_UMS_concrete1) -> (service60 = ST1)

-----------------------
--   O B J E C T S   --
-----------------------
--	 SetupTrip
VAR SetupTrip: {ORGANIZED, INITIAL};
ASSIGN
next(SetupTrip) :=
case
(input = service59_UMS_ManagementTrip):{(ORGANIZED)};
1:SetupTrip;
esac;
--	 ProvideTrip
VAR ProvideTrip: {TRIP_REQUEST_ACCOMPLISHED, TRIP_MANAGEMENT_ACCOMPLISHED, TRIP_MANAGE_REQUEST_LOADED, TRIP_REQUEST_LOADED, INITIAL};
ASSIGN
next(ProvideTrip) :=
case
(ProvideTrip = TRIP_REQUEST_LOADED & output = service60_UMS_TripAlternatives): TRIP_REQUEST_ACCOMPLISHED;
(ProvideTrip = TRIP_REQUEST_ACCOMPLISHED & input = service59_UMS_TripManagementRequest): TRIP_MANAGE_REQUEST_LOADED;
(ProvideTrip = TRIP_MANAGE_REQUEST_LOADED & output = service59_UMS_TripManagementReply): TRIP_MANAGEMENT_ACCOMPLISHED;
(ProvideTrip = INITIAL & input = service60_UMS_TripRequest): TRIP_REQUEST_LOADED;
1:ProvideTrip;
esac;
-----------------------------------
--   P R E C O N D I T I O N S   --
-----------------------------------
TRANS (input = service59_UMS_TripManagementRequest) -> ((((ProvideTrip = TRIP_REQUEST_ACCOMPLISHED))))
TRANS (output = service59_UMS_TripManagementReply) -> ((((ProvideTrip = TRIP_MANAGE_REQUEST_LOADED))))
TRANS (input = service60_UMS_TripRequest) -> ((((ProvideTrip = INITIAL))))
TRANS (output = service60_UMS_TripAlternatives) -> ((((ProvideTrip = TRIP_REQUEST_LOADED))))
-----------------------------
--   I N V A R I A N T S   --
-----------------------------
--	 Synchronicity
TRANS (1-(input = UNDEF)) + (1-(output = UNDEF)) = 1

---------------------------------------
--   S Y N C H R O N I Z A T I O N   --
---------------------------------------
DEFINE SYNC0 := ((ProvideTrip in {TRIP_REQUEST_ACCOMPLISHED}));
--	 Synchronicity enforcement
TRANS (service59 in {ST2, ST1, ST4, ST3, ST5, ST7}) -> (next(service59) != service59)
TRANS (service60 in {ST2, ST1, ST3}) -> (next(service60) != service60)
-------------------------------
--   C O N T R O L L E R S   --
-------------------------------
--	 Fault Controller
VAR FAULT_CONTROLLER: { START, STOP };
INIT FAULT_CONTROLLER = START

ASSIGN
next(FAULT_CONTROLLER) := 
case
(FAULT_CONTROLLER = START & FALSE): STOP;
1: FAULT_CONTROLLER;
esac;

TRANS (output != UNDEF | input != UNDEF) -> (FAULT_CONTROLLER != STOP)

-------------------------------------
--   I N I T I A L I Z A T I O N   --
-------------------------------------
INIT ((SetupTrip = INITIAL) & (ProvideTrip = INITIAL))
INIT ((service59 = ST0) & (service60 = ST0))

-----------------
--   G O A L   --
-----------------
MODULE main
VAR dn_main : dn_main;

GOAL ONEOF(
(dn_main.SYNC0), 1;
(dn_main.FAULT_CONTROLLER = STOP), 0

)