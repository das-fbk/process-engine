-------------------------------
--   C O M P O S I T I O N   --
-------------------------------
MODULE dn_main

---------------------
--   I N P U T S   --
---------------------
IVAR input:
{
UNDEF, service155_UMS_ManagementTrip, service155_UMS_TripManagementRequest, service155_RESET, service157_UMS_TripRequest, service157_RESET
};

-----------------------
--   O U T P U T S   --
-----------------------
IVAR output:
{
UNDEF, service155_UMS_concrete4, service155_UMS_concrete3, service155_UMS_concrete2, service155_UMS_concrete1, service155_UMS_TripManagementReply, service155_UMS_concrete5, service157_UMS_TripAlternatives, service157_UMS_concrete2, service157_UMS_concrete1
};

-------------------------
--   S E R V I C E S   --
-------------------------
--	 service155
VAR service155: {ST2, ST1, ST4, ST3, ST6, ST5, ST8, ST7, ST0};
ASSIGN
next(service155) :=
case
(service155 = ST7 & output = service155_UMS_TripManagementReply): ST8;
(service155 = ST6 & input = service155_UMS_ManagementTrip): ST7;
(service155 = ST1 & output = service155_UMS_concrete1): ST2;
(service155 = ST5 & output = service155_UMS_concrete5): ST6;
(service155 = ST4 & output = service155_UMS_concrete4): ST5;
(service155 = ST0 & input = service155_UMS_TripManagementRequest): ST1;
(service155 = ST3 & input = service155_RESET): ST0;
(service155 = ST3 & output = service155_UMS_concrete3): ST4;
(service155 = ST4 & input = service155_RESET): ST0;
(service155 = ST1 & input = service155_RESET): ST0;
(service155 = ST8 & input = service155_RESET): ST0;
(service155 = ST7 & input = service155_RESET): ST0;
(service155 = ST2 & output = service155_UMS_concrete2): ST3;
(service155 = ST2 & input = service155_RESET): ST0;
(service155 = ST6 & input = service155_RESET): ST0;
(service155 = ST5 & input = service155_RESET): ST0;
1:service155;
esac;
TRANS ( input = service155_UMS_ManagementTrip) -> (service155 = ST6)
TRANS ( output = service155_UMS_concrete4) -> (service155 = ST4)
TRANS ( input = service155_UMS_TripManagementRequest) -> (service155 = ST0)
TRANS ( output = service155_UMS_concrete3) -> (service155 = ST3)
TRANS ( output = service155_UMS_concrete2) -> (service155 = ST2)
TRANS ( input = service155_RESET) -> (service155 = ST2 | service155 = ST1 | service155 = ST4 | service155 = ST3 | service155 = ST6 | service155 = ST5 | service155 = ST8 | service155 = ST7)
TRANS ( output = service155_UMS_concrete1) -> (service155 = ST1)
TRANS ( output = service155_UMS_TripManagementReply) -> (service155 = ST7)
TRANS ( output = service155_UMS_concrete5) -> (service155 = ST5)

--	 service157
VAR service157: {ST2, ST1, ST4, ST3, ST0};
ASSIGN
next(service157) :=
case
(service157 = ST1 & output = service157_UMS_concrete1): ST2;
(service157 = ST3 & input = service157_RESET): ST0;
(service157 = ST4 & input = service157_RESET): ST0;
(service157 = ST2 & output = service157_UMS_concrete2): ST3;
(service157 = ST1 & input = service157_RESET): ST0;
(service157 = ST0 & input = service157_UMS_TripRequest): ST1;
(service157 = ST2 & input = service157_RESET): ST0;
(service157 = ST3 & output = service157_UMS_TripAlternatives): ST4;
1:service157;
esac;
TRANS ( input = service157_UMS_TripRequest) -> (service157 = ST0)
TRANS ( output = service157_UMS_TripAlternatives) -> (service157 = ST3)
TRANS ( output = service157_UMS_concrete2) -> (service157 = ST2)
TRANS ( input = service157_RESET) -> (service157 = ST2 | service157 = ST1 | service157 = ST4 | service157 = ST3)
TRANS ( output = service157_UMS_concrete1) -> (service157 = ST1)

-----------------------
--   O B J E C T S   --
-----------------------
--	 SetupTrip
VAR SetupTrip: {ORGANIZED, INITIAL};
ASSIGN
next(SetupTrip) :=
case
(input = service155_UMS_ManagementTrip):{(ORGANIZED)};
1:SetupTrip;
esac;
--	 ProvideTrip
VAR ProvideTrip: {TRIP_REQUEST_ACCOMPLISHED, TRIP_MANAGEMENT_ACCOMPLISHED, TRIP_MANAGE_REQUEST_LOADED, TRIP_REQUEST_LOADED, INITIAL};
ASSIGN
next(ProvideTrip) :=
case
(ProvideTrip = TRIP_REQUEST_LOADED & output = service157_UMS_TripAlternatives): TRIP_REQUEST_ACCOMPLISHED;
(ProvideTrip = INITIAL & input = service157_UMS_TripRequest): TRIP_REQUEST_LOADED;
(ProvideTrip = TRIP_MANAGE_REQUEST_LOADED & output = service155_UMS_TripManagementReply): TRIP_MANAGEMENT_ACCOMPLISHED;
(ProvideTrip = TRIP_REQUEST_ACCOMPLISHED & input = service155_UMS_TripManagementRequest): TRIP_MANAGE_REQUEST_LOADED;
1:ProvideTrip;
esac;
-----------------------------------
--   P R E C O N D I T I O N S   --
-----------------------------------
TRANS (input = service155_UMS_TripManagementRequest) -> ((((ProvideTrip = TRIP_REQUEST_ACCOMPLISHED))))
TRANS (output = service155_UMS_TripManagementReply) -> ((((ProvideTrip = TRIP_MANAGE_REQUEST_LOADED))))
TRANS (input = service157_UMS_TripRequest) -> ((((ProvideTrip = INITIAL))))
TRANS (output = service157_UMS_TripAlternatives) -> ((((ProvideTrip = TRIP_REQUEST_LOADED))))
-----------------------------
--   I N V A R I A N T S   --
-----------------------------
--	 Synchronicity
TRANS (1-(input = UNDEF)) + (1-(output = UNDEF)) = 1

---------------------------------------
--   S Y N C H R O N I Z A T I O N   --
---------------------------------------
DEFINE SYNC0 := ((ProvideTrip in {TRIP_MANAGEMENT_ACCOMPLISHED}));
--	 Synchronicity enforcement
TRANS (service155 in {ST2, ST1, ST4, ST3, ST5, ST7}) -> (next(service155) != service155)
TRANS (service157 in {ST2, ST1, ST3}) -> (next(service157) != service157)
-------------------------------
--   C O N T R O L L E R S   --
-------------------------------
--	 Fault Controller
VAR FAULT_CONTROLLER: { START, STOP };
INIT FAULT_CONTROLLER = START

ASSIGN
next(FAULT_CONTROLLER) := 
case
(FAULT_CONTROLLER = START & FALSE): STOP;
1: FAULT_CONTROLLER;
esac;

TRANS (output != UNDEF | input != UNDEF) -> (FAULT_CONTROLLER != STOP)

-------------------------------------
--   I N I T I A L I Z A T I O N   --
-------------------------------------
INIT ((SetupTrip = INITIAL) & (ProvideTrip = TRIP_REQUEST_ACCOMPLISHED))
INIT ((service155 = ST0) & (service157 = ST0))

-----------------
--   G O A L   --
-----------------
MODULE main
VAR dn_main : dn_main;

GOAL ONEOF(
(dn_main.SYNC0), 1;
(dn_main.FAULT_CONTROLLER = STOP), 0

)