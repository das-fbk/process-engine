-------------------------------
--   C O M P O S I T I O N   --
-------------------------------
MODULE dn_main

---------------------
--   I N P U T S   --
---------------------
IVAR input:
{
UNDEF, service287_BS_RegistrationRequest, service287_BS_UpdateSmartMobilityCard, service287_RESET, service288_BS_PlanBikeRoute, service288_BS_ExecuteRouteRequest, service288_RESET
};

-----------------------
--   O U T P U T S   --
-----------------------
IVAR output:
{
UNDEF, service287_BS_RegistrationAck, service288_BS_ExecuteRouteAck
};

-------------------------
--   S E R V I C E S   --
-------------------------
--	 service287
VAR service287: {ST2, ST1, ST3, ST0};
ASSIGN
next(service287) :=
case
(service287 = ST2 & input = service287_RESET): ST0;
(service287 = ST1 & input = service287_BS_UpdateSmartMobilityCard): ST2;
(service287 = ST3 & input = service287_RESET): ST0;
(service287 = ST0 & input = service287_BS_RegistrationRequest): ST1;
(service287 = ST1 & input = service287_RESET): ST0;
(service287 = ST2 & output = service287_BS_RegistrationAck): ST3;
1:service287;
esac;
TRANS ( input = service287_BS_RegistrationRequest) -> (service287 = ST0)
TRANS ( output = service287_BS_RegistrationAck) -> (service287 = ST2)
TRANS ( input = service287_BS_UpdateSmartMobilityCard) -> (service287 = ST1)
TRANS ( input = service287_RESET) -> (service287 = ST2 | service287 = ST1 | service287 = ST3)

--	 service288
VAR service288: {ST2, ST1, ST3, ST0};
ASSIGN
next(service288) :=
case
(service288 = ST1 & input = service288_RESET): ST0;
(service288 = ST0 & input = service288_BS_ExecuteRouteRequest): ST1;
(service288 = ST1 & input = service288_BS_PlanBikeRoute): ST2;
(service288 = ST2 & output = service288_BS_ExecuteRouteAck): ST3;
(service288 = ST3 & input = service288_RESET): ST0;
(service288 = ST2 & input = service288_RESET): ST0;
1:service288;
esac;
TRANS ( input = service288_BS_PlanBikeRoute) -> (service288 = ST1)
TRANS ( input = service288_BS_ExecuteRouteRequest) -> (service288 = ST0)
TRANS ( input = service288_RESET) -> (service288 = ST2 | service288 = ST1 | service288 = ST3)
TRANS ( output = service288_BS_ExecuteRouteAck) -> (service288 = ST2)

-----------------------
--   O B J E C T S   --
-----------------------
--	 BikeRoutePlan
VAR BikeRoutePlan: {ROUTE_PLANNED, INITIAL};
ASSIGN
next(BikeRoutePlan) :=
case
(input = service288_BS_PlanBikeRoute):{(ROUTE_PLANNED)};
1:BikeRoutePlan;
esac;
--	 BikeSharingTrip
VAR BikeSharingTrip: {RACK_REACHED, INITIAL, BIKE_ROUTE_PLANNED, USER_REGISTERED};
ASSIGN
next(BikeSharingTrip) :=
case
(BikeSharingTrip = BIKE_ROUTE_PLANNED & output = service288_BS_ExecuteRouteAck): RACK_REACHED;
(BikeSharingTrip = INITIAL & output = service287_BS_RegistrationAck): USER_REGISTERED;
(BikeSharingTrip = USER_REGISTERED & input = service288_BS_PlanBikeRoute): BIKE_ROUTE_PLANNED;
1:BikeSharingTrip;
esac;
--	 HandleSmartMobilityCard
VAR HandleSmartMobilityCard: {INITIAL, SUBSCRIPTION, CARD_RELEASED, SERVICE_ADDED};
ASSIGN
next(HandleSmartMobilityCard) :=
case
(HandleSmartMobilityCard = SERVICE_ADDED & input = service287_BS_UpdateSmartMobilityCard): CARD_RELEASED;
(input = service287_BS_UpdateSmartMobilityCard):{(SERVICE_ADDED)};
1:HandleSmartMobilityCard;
esac;
-----------------------------------
--   P R E C O N D I T I O N S   --
-----------------------------------
TRANS (input = service287_BS_RegistrationRequest) -> ((((BikeSharingTrip = INITIAL))))
TRANS (input = service288_BS_ExecuteRouteRequest) -> ((((BikeSharingTrip = USER_REGISTERED))))
-----------------------------
--   I N V A R I A N T S   --
-----------------------------
--	 Synchronicity
TRANS (1-(input = UNDEF)) + (1-(output = UNDEF)) = 1

---------------------------------------
--   S Y N C H R O N I Z A T I O N   --
---------------------------------------
DEFINE SYNC0 := ((BikeSharingTrip in {USER_REGISTERED}));
--	 Synchronicity enforcement
TRANS (service287 in {ST2}) -> (next(service287) != service287)
TRANS (service288 in {ST2}) -> (next(service288) != service288)
-------------------------------
--   C O N T R O L L E R S   --
-------------------------------
--	 Fault Controller
VAR FAULT_CONTROLLER: { START, STOP };
INIT FAULT_CONTROLLER = START

ASSIGN
next(FAULT_CONTROLLER) := 
case
(FAULT_CONTROLLER = START & FALSE): STOP;
1: FAULT_CONTROLLER;
esac;

TRANS (output != UNDEF | input != UNDEF) -> (FAULT_CONTROLLER != STOP)

-------------------------------------
--   I N I T I A L I Z A T I O N   --
-------------------------------------
INIT ((BikeRoutePlan = INITIAL) & (BikeSharingTrip = INITIAL) & (HandleSmartMobilityCard = INITIAL))
INIT ((service287 = ST0) & (service288 = ST0))

-----------------
--   G O A L   --
-----------------
MODULE main
VAR dn_main : dn_main;

GOAL ONEOF(
(dn_main.SYNC0), 1;
(dn_main.FAULT_CONTROLLER = STOP), 0

)