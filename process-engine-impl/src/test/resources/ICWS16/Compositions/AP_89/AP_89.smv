-------------------------------
--   C O M P O S I T I O N   --
-------------------------------
MODULE dn_main

---------------------
--   I N P U T S   --
---------------------
IVAR input:
{
UNDEF, service153_UMS_ManagementTrip, service153_UMS_TripManagementRequest, service153_RESET, service154_UMS_TripRequest, service154_RESET
};

-----------------------
--   O U T P U T S   --
-----------------------
IVAR output:
{
UNDEF, service153_UMS_concrete4, service153_UMS_concrete3, service153_UMS_concrete2, service153_UMS_concrete1, service153_UMS_TripManagementReply, service153_UMS_concrete5, service154_UMS_TripAlternatives, service154_UMS_concrete2, service154_UMS_concrete1
};

-------------------------
--   S E R V I C E S   --
-------------------------
--	 service153
VAR service153: {ST2, ST1, ST4, ST3, ST6, ST5, ST8, ST7, ST0};
ASSIGN
next(service153) :=
case
(service153 = ST1 & output = service153_UMS_concrete1): ST2;
(service153 = ST3 & output = service153_UMS_concrete3): ST4;
(service153 = ST4 & output = service153_UMS_concrete4): ST5;
(service153 = ST6 & input = service153_RESET): ST0;
(service153 = ST1 & input = service153_RESET): ST0;
(service153 = ST2 & input = service153_RESET): ST0;
(service153 = ST0 & input = service153_UMS_TripManagementRequest): ST1;
(service153 = ST5 & input = service153_RESET): ST0;
(service153 = ST8 & input = service153_RESET): ST0;
(service153 = ST6 & input = service153_UMS_ManagementTrip): ST7;
(service153 = ST4 & input = service153_RESET): ST0;
(service153 = ST3 & input = service153_RESET): ST0;
(service153 = ST7 & input = service153_RESET): ST0;
(service153 = ST5 & output = service153_UMS_concrete5): ST6;
(service153 = ST2 & output = service153_UMS_concrete2): ST3;
(service153 = ST7 & output = service153_UMS_TripManagementReply): ST8;
1:service153;
esac;
TRANS ( input = service153_UMS_ManagementTrip) -> (service153 = ST6)
TRANS ( output = service153_UMS_concrete4) -> (service153 = ST4)
TRANS ( input = service153_UMS_TripManagementRequest) -> (service153 = ST0)
TRANS ( output = service153_UMS_concrete3) -> (service153 = ST3)
TRANS ( output = service153_UMS_concrete2) -> (service153 = ST2)
TRANS ( input = service153_RESET) -> (service153 = ST2 | service153 = ST1 | service153 = ST4 | service153 = ST3 | service153 = ST6 | service153 = ST5 | service153 = ST8 | service153 = ST7)
TRANS ( output = service153_UMS_concrete1) -> (service153 = ST1)
TRANS ( output = service153_UMS_TripManagementReply) -> (service153 = ST7)
TRANS ( output = service153_UMS_concrete5) -> (service153 = ST5)

--	 service154
VAR service154: {ST2, ST1, ST4, ST3, ST0};
ASSIGN
next(service154) :=
case
(service154 = ST0 & input = service154_UMS_TripRequest): ST1;
(service154 = ST1 & input = service154_RESET): ST0;
(service154 = ST2 & output = service154_UMS_concrete2): ST3;
(service154 = ST3 & output = service154_UMS_TripAlternatives): ST4;
(service154 = ST1 & output = service154_UMS_concrete1): ST2;
(service154 = ST2 & input = service154_RESET): ST0;
(service154 = ST3 & input = service154_RESET): ST0;
(service154 = ST4 & input = service154_RESET): ST0;
1:service154;
esac;
TRANS ( input = service154_UMS_TripRequest) -> (service154 = ST0)
TRANS ( output = service154_UMS_TripAlternatives) -> (service154 = ST3)
TRANS ( output = service154_UMS_concrete2) -> (service154 = ST2)
TRANS ( input = service154_RESET) -> (service154 = ST2 | service154 = ST1 | service154 = ST4 | service154 = ST3)
TRANS ( output = service154_UMS_concrete1) -> (service154 = ST1)

-----------------------
--   O B J E C T S   --
-----------------------
--	 SetupTrip
VAR SetupTrip: {ORGANIZED, INITIAL};
ASSIGN
next(SetupTrip) :=
case
(input = service153_UMS_ManagementTrip):{(ORGANIZED)};
1:SetupTrip;
esac;
--	 ProvideTrip
VAR ProvideTrip: {TRIP_REQUEST_ACCOMPLISHED, TRIP_MANAGEMENT_ACCOMPLISHED, TRIP_MANAGE_REQUEST_LOADED, TRIP_REQUEST_LOADED, INITIAL};
ASSIGN
next(ProvideTrip) :=
case
(ProvideTrip = TRIP_REQUEST_ACCOMPLISHED & input = service153_UMS_TripManagementRequest): TRIP_MANAGE_REQUEST_LOADED;
(ProvideTrip = TRIP_MANAGE_REQUEST_LOADED & output = service153_UMS_TripManagementReply): TRIP_MANAGEMENT_ACCOMPLISHED;
(ProvideTrip = TRIP_REQUEST_LOADED & output = service154_UMS_TripAlternatives): TRIP_REQUEST_ACCOMPLISHED;
(ProvideTrip = INITIAL & input = service154_UMS_TripRequest): TRIP_REQUEST_LOADED;
1:ProvideTrip;
esac;
-----------------------------------
--   P R E C O N D I T I O N S   --
-----------------------------------
TRANS (input = service153_UMS_TripManagementRequest) -> ((((ProvideTrip = TRIP_REQUEST_ACCOMPLISHED))))
TRANS (output = service153_UMS_TripManagementReply) -> ((((ProvideTrip = TRIP_MANAGE_REQUEST_LOADED))))
TRANS (input = service154_UMS_TripRequest) -> ((((ProvideTrip = INITIAL))))
TRANS (output = service154_UMS_TripAlternatives) -> ((((ProvideTrip = TRIP_REQUEST_LOADED))))
-----------------------------
--   I N V A R I A N T S   --
-----------------------------
--	 Synchronicity
TRANS (1-(input = UNDEF)) + (1-(output = UNDEF)) = 1

---------------------------------------
--   S Y N C H R O N I Z A T I O N   --
---------------------------------------
DEFINE SYNC0 := ((ProvideTrip in {TRIP_MANAGEMENT_ACCOMPLISHED}));
--	 Synchronicity enforcement
TRANS (service153 in {ST2, ST1, ST4, ST3, ST5, ST7}) -> (next(service153) != service153)
TRANS (service154 in {ST2, ST1, ST3}) -> (next(service154) != service154)
-------------------------------
--   C O N T R O L L E R S   --
-------------------------------
--	 Fault Controller
VAR FAULT_CONTROLLER: { START, STOP };
INIT FAULT_CONTROLLER = START

ASSIGN
next(FAULT_CONTROLLER) := 
case
(FAULT_CONTROLLER = START & FALSE): STOP;
1: FAULT_CONTROLLER;
esac;

TRANS (output != UNDEF | input != UNDEF) -> (FAULT_CONTROLLER != STOP)

-------------------------------------
--   I N I T I A L I Z A T I O N   --
-------------------------------------
INIT ((SetupTrip = INITIAL) & (ProvideTrip = TRIP_REQUEST_ACCOMPLISHED))
INIT ((service153 = ST0) & (service154 = ST0))

-----------------
--   G O A L   --
-----------------
MODULE main
VAR dn_main : dn_main;

GOAL ONEOF(
(dn_main.SYNC0), 1;
(dn_main.FAULT_CONTROLLER = STOP), 0

)