<?xml version="1.0" encoding="UTF-8"?>
<tns:fragment id="TA_userAssistant" xmlns:tns="http://das.fbk.eu/Fragment"
	xmlns:tns1="http://das.fbk.eu/Annotation" xmlns:tns2="http://das.fbk.eu/DataType"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://das.fbk.eu/Fragment Fragment.xsd ">

	<tns:state isInitial="true" name="ST0" />
	<tns:state name="ST1" />
	<tns:state name="ST2" />
	<tns:state name="ST3" />
	<tns:state name="ST4" />
	<tns:state name="ST5" />
	<tns:state name="ST6" />
	<tns:state name="ST7" />

	<tns:action name="StartInteraction" actionType="input" />

	<tns:action name="StartChatbot" actionType="concrete" />
	<tns:action name="InsertSource" actionType="concrete" />
	<tns:action name="InsertDestination" actionType="concrete" />

	<tns:action name="AssistantRequest" actionType="input">
		<tns:actionVariable name="From" type="string">
			<tns2:content type="anyType"></tns2:content>
		</tns:actionVariable>
		<tns:actionVariable name="To" type="string">
			<tns2:content type="anyType"></tns2:content>
		</tns:actionVariable>
	</tns:action>

	<tns:action name="AssistantResponse" actionType="output">
<!-- 		<tns:effect> -->
<!-- 		<tns1:event dpName="TravelAssistant" eventName="assistUser"></tns1:event> -->
<!-- 		</tns:effect> -->
		<tns:actionVariable name="Plan" type="string">
			<tns2:content type="anyType"></tns2:content>
		</tns:actionVariable>
	</tns:action>

	<tns:action name="ShowResults" actionType="concrete" >
		<tns:effect>
			<tns1:event dpName="TravelAssistant" eventName="assistUser"></tns1:event>
		</tns:effect>
	</tns:action>


	<tns:transition>
		<tns:initialState>ST0</tns:initialState>
		<tns:finalState>ST1</tns:finalState>
		<tns:action name="StartInteraction" actionType="input" />
	</tns:transition>

	<tns:transition>
		<tns:initialState>ST1</tns:initialState>
		<tns:finalState>ST2</tns:finalState>
		<tns:action name="StartChatbot" actionType="concrete" />
	</tns:transition>

	<tns:transition>
		<tns:initialState>ST2</tns:initialState>
		<tns:finalState>ST3</tns:finalState>
		<tns:action name="InsertSource" actionType="concrete" />
	</tns:transition>

	<tns:transition>
		<tns:initialState>ST3</tns:initialState>
		<tns:finalState>ST4</tns:finalState>
		<tns:action name="InsertDestination" actionType="concrete" />
	</tns:transition>

	<tns:transition>
		<tns:initialState>ST4</tns:initialState>
		<tns:finalState>ST5</tns:finalState>
		<tns:action name="AssistantRequest" actionType="input" />
	</tns:transition>

	<tns:transition>
		<tns:initialState>ST5</tns:initialState>
		<tns:finalState>ST6</tns:finalState>
		<tns:action name="AssistantResponse" actionType="output" />
	</tns:transition>

	<tns:transition>
		<tns:initialState>ST6</tns:initialState>
		<tns:finalState>ST7</tns:finalState>
		<tns:action name="ShowResults" actionType="concrete" />
	</tns:transition>

</tns:fragment>

