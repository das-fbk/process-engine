-------------------------------
--   C O M P O S I T I O N   --
-------------------------------
MODULE dn_main

---------------------
--   I N P U T S   --
---------------------
IVAR input:
{
UNDEF, service107_UMS_ManagementTrip, service107_UMS_TripManagementRequest, service107_RESET, service109_UMS_TripRequest, service109_RESET
};

-----------------------
--   O U T P U T S   --
-----------------------
IVAR output:
{
UNDEF, service107_UMS_concrete4, service107_UMS_concrete3, service107_UMS_concrete2, service107_UMS_concrete1, service107_UMS_TripManagementReply, service107_UMS_concrete5, service109_UMS_TripAlternatives, service109_UMS_concrete2, service109_UMS_concrete1
};

-------------------------
--   S E R V I C E S   --
-------------------------
--	 service107
VAR service107: {ST2, ST1, ST4, ST3, ST6, ST5, ST8, ST7, ST0};
ASSIGN
next(service107) :=
case
(service107 = ST1 & input = service107_RESET): ST0;
(service107 = ST7 & input = service107_RESET): ST0;
(service107 = ST5 & output = service107_UMS_concrete5): ST6;
(service107 = ST3 & output = service107_UMS_concrete3): ST4;
(service107 = ST4 & output = service107_UMS_concrete4): ST5;
(service107 = ST4 & input = service107_RESET): ST0;
(service107 = ST3 & input = service107_RESET): ST0;
(service107 = ST0 & input = service107_UMS_TripManagementRequest): ST1;
(service107 = ST6 & input = service107_UMS_ManagementTrip): ST7;
(service107 = ST7 & output = service107_UMS_TripManagementReply): ST8;
(service107 = ST6 & input = service107_RESET): ST0;
(service107 = ST2 & input = service107_RESET): ST0;
(service107 = ST5 & input = service107_RESET): ST0;
(service107 = ST2 & output = service107_UMS_concrete2): ST3;
(service107 = ST1 & output = service107_UMS_concrete1): ST2;
(service107 = ST8 & input = service107_RESET): ST0;
1:service107;
esac;
TRANS ( input = service107_UMS_ManagementTrip) -> (service107 = ST6)
TRANS ( output = service107_UMS_concrete4) -> (service107 = ST4)
TRANS ( input = service107_UMS_TripManagementRequest) -> (service107 = ST0)
TRANS ( output = service107_UMS_concrete3) -> (service107 = ST3)
TRANS ( output = service107_UMS_concrete2) -> (service107 = ST2)
TRANS ( input = service107_RESET) -> (service107 = ST2 | service107 = ST1 | service107 = ST4 | service107 = ST3 | service107 = ST6 | service107 = ST5 | service107 = ST8 | service107 = ST7)
TRANS ( output = service107_UMS_concrete1) -> (service107 = ST1)
TRANS ( output = service107_UMS_TripManagementReply) -> (service107 = ST7)
TRANS ( output = service107_UMS_concrete5) -> (service107 = ST5)

--	 service109
VAR service109: {ST2, ST1, ST4, ST3, ST0};
ASSIGN
next(service109) :=
case
(service109 = ST2 & input = service109_RESET): ST0;
(service109 = ST1 & input = service109_RESET): ST0;
(service109 = ST2 & output = service109_UMS_concrete2): ST3;
(service109 = ST0 & input = service109_UMS_TripRequest): ST1;
(service109 = ST1 & output = service109_UMS_concrete1): ST2;
(service109 = ST4 & input = service109_RESET): ST0;
(service109 = ST3 & input = service109_RESET): ST0;
(service109 = ST3 & output = service109_UMS_TripAlternatives): ST4;
1:service109;
esac;
TRANS ( input = service109_UMS_TripRequest) -> (service109 = ST0)
TRANS ( output = service109_UMS_TripAlternatives) -> (service109 = ST3)
TRANS ( output = service109_UMS_concrete2) -> (service109 = ST2)
TRANS ( input = service109_RESET) -> (service109 = ST2 | service109 = ST1 | service109 = ST4 | service109 = ST3)
TRANS ( output = service109_UMS_concrete1) -> (service109 = ST1)

-----------------------
--   O B J E C T S   --
-----------------------
--	 SetupTrip
VAR SetupTrip: {ORGANIZED, INITIAL};
ASSIGN
next(SetupTrip) :=
case
(input = service107_UMS_ManagementTrip):{(ORGANIZED)};
1:SetupTrip;
esac;
--	 ProvideTrip
VAR ProvideTrip: {TRIP_REQUEST_ACCOMPLISHED, TRIP_MANAGEMENT_ACCOMPLISHED, TRIP_MANAGE_REQUEST_LOADED, TRIP_REQUEST_LOADED, INITIAL};
ASSIGN
next(ProvideTrip) :=
case
(ProvideTrip = TRIP_MANAGE_REQUEST_LOADED & output = service107_UMS_TripManagementReply): TRIP_MANAGEMENT_ACCOMPLISHED;
(ProvideTrip = INITIAL & input = service109_UMS_TripRequest): TRIP_REQUEST_LOADED;
(ProvideTrip = TRIP_REQUEST_LOADED & output = service109_UMS_TripAlternatives): TRIP_REQUEST_ACCOMPLISHED;
(ProvideTrip = TRIP_REQUEST_ACCOMPLISHED & input = service107_UMS_TripManagementRequest): TRIP_MANAGE_REQUEST_LOADED;
1:ProvideTrip;
esac;
-----------------------------------
--   P R E C O N D I T I O N S   --
-----------------------------------
TRANS (input = service107_UMS_TripManagementRequest) -> ((((ProvideTrip = TRIP_REQUEST_ACCOMPLISHED))))
TRANS (output = service107_UMS_TripManagementReply) -> ((((ProvideTrip = TRIP_MANAGE_REQUEST_LOADED))))
TRANS (input = service109_UMS_TripRequest) -> ((((ProvideTrip = INITIAL))))
TRANS (output = service109_UMS_TripAlternatives) -> ((((ProvideTrip = TRIP_REQUEST_LOADED))))
-----------------------------
--   I N V A R I A N T S   --
-----------------------------
--	 Synchronicity
TRANS (1-(input = UNDEF)) + (1-(output = UNDEF)) = 1

---------------------------------------
--   S Y N C H R O N I Z A T I O N   --
---------------------------------------
DEFINE SYNC0 := ((ProvideTrip in {TRIP_MANAGEMENT_ACCOMPLISHED}));
--	 Synchronicity enforcement
TRANS (service107 in {ST2, ST1, ST4, ST3, ST5, ST7}) -> (next(service107) != service107)
TRANS (service109 in {ST2, ST1, ST3}) -> (next(service109) != service109)
-------------------------------
--   C O N T R O L L E R S   --
-------------------------------
--	 Fault Controller
VAR FAULT_CONTROLLER: { START, STOP };
INIT FAULT_CONTROLLER = START

ASSIGN
next(FAULT_CONTROLLER) := 
case
(FAULT_CONTROLLER = START & FALSE): STOP;
1: FAULT_CONTROLLER;
esac;

TRANS (output != UNDEF | input != UNDEF) -> (FAULT_CONTROLLER != STOP)

-------------------------------------
--   I N I T I A L I Z A T I O N   --
-------------------------------------
INIT ((SetupTrip = INITIAL) & (ProvideTrip = TRIP_REQUEST_ACCOMPLISHED))
INIT ((service107 = ST0) & (service109 = ST0))

-----------------
--   G O A L   --
-----------------
MODULE main
VAR dn_main : dn_main;

GOAL ONEOF(
(dn_main.SYNC0), 1;
(dn_main.FAULT_CONTROLLER = STOP), 0

)