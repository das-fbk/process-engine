-------------------------------
--   C O M P O S I T I O N   --
-------------------------------
MODULE dn_main

---------------------
--   I N P U T S   --
---------------------
IVAR input:
{
UNDEF, service163_UMS_ManagementTrip, service163_UMS_TripManagementRequest, service163_RESET, service165_UMS_TripRequest, service165_RESET
};

-----------------------
--   O U T P U T S   --
-----------------------
IVAR output:
{
UNDEF, service163_UMS_concrete4, service163_UMS_concrete3, service163_UMS_concrete2, service163_UMS_concrete1, service163_UMS_TripManagementReply, service163_UMS_concrete5, service165_UMS_TripAlternatives, service165_UMS_concrete2, service165_UMS_concrete1
};

-------------------------
--   S E R V I C E S   --
-------------------------
--	 service163
VAR service163: {ST2, ST1, ST4, ST3, ST6, ST5, ST8, ST7, ST0};
ASSIGN
next(service163) :=
case
(service163 = ST5 & input = service163_RESET): ST0;
(service163 = ST8 & input = service163_RESET): ST0;
(service163 = ST1 & output = service163_UMS_concrete1): ST2;
(service163 = ST7 & input = service163_RESET): ST0;
(service163 = ST5 & output = service163_UMS_concrete5): ST6;
(service163 = ST4 & input = service163_RESET): ST0;
(service163 = ST7 & output = service163_UMS_TripManagementReply): ST8;
(service163 = ST2 & input = service163_RESET): ST0;
(service163 = ST2 & output = service163_UMS_concrete2): ST3;
(service163 = ST3 & output = service163_UMS_concrete3): ST4;
(service163 = ST0 & input = service163_UMS_TripManagementRequest): ST1;
(service163 = ST3 & input = service163_RESET): ST0;
(service163 = ST6 & input = service163_RESET): ST0;
(service163 = ST6 & input = service163_UMS_ManagementTrip): ST7;
(service163 = ST4 & output = service163_UMS_concrete4): ST5;
(service163 = ST1 & input = service163_RESET): ST0;
1:service163;
esac;
TRANS ( input = service163_UMS_ManagementTrip) -> (service163 = ST6)
TRANS ( output = service163_UMS_concrete4) -> (service163 = ST4)
TRANS ( input = service163_UMS_TripManagementRequest) -> (service163 = ST0)
TRANS ( output = service163_UMS_concrete3) -> (service163 = ST3)
TRANS ( output = service163_UMS_concrete2) -> (service163 = ST2)
TRANS ( input = service163_RESET) -> (service163 = ST2 | service163 = ST1 | service163 = ST4 | service163 = ST3 | service163 = ST6 | service163 = ST5 | service163 = ST8 | service163 = ST7)
TRANS ( output = service163_UMS_concrete1) -> (service163 = ST1)
TRANS ( output = service163_UMS_TripManagementReply) -> (service163 = ST7)
TRANS ( output = service163_UMS_concrete5) -> (service163 = ST5)

--	 service165
VAR service165: {ST2, ST1, ST4, ST3, ST0};
ASSIGN
next(service165) :=
case
(service165 = ST1 & input = service165_RESET): ST0;
(service165 = ST0 & input = service165_UMS_TripRequest): ST1;
(service165 = ST1 & output = service165_UMS_concrete1): ST2;
(service165 = ST3 & output = service165_UMS_TripAlternatives): ST4;
(service165 = ST4 & input = service165_RESET): ST0;
(service165 = ST2 & input = service165_RESET): ST0;
(service165 = ST3 & input = service165_RESET): ST0;
(service165 = ST2 & output = service165_UMS_concrete2): ST3;
1:service165;
esac;
TRANS ( input = service165_UMS_TripRequest) -> (service165 = ST0)
TRANS ( output = service165_UMS_TripAlternatives) -> (service165 = ST3)
TRANS ( output = service165_UMS_concrete2) -> (service165 = ST2)
TRANS ( input = service165_RESET) -> (service165 = ST2 | service165 = ST1 | service165 = ST4 | service165 = ST3)
TRANS ( output = service165_UMS_concrete1) -> (service165 = ST1)

-----------------------
--   O B J E C T S   --
-----------------------
--	 SetupTrip
VAR SetupTrip: {ORGANIZED, INITIAL};
ASSIGN
next(SetupTrip) :=
case
(input = service163_UMS_ManagementTrip):{(ORGANIZED)};
1:SetupTrip;
esac;
--	 ProvideTrip
VAR ProvideTrip: {TRIP_REQUEST_ACCOMPLISHED, TRIP_MANAGEMENT_ACCOMPLISHED, TRIP_MANAGE_REQUEST_LOADED, TRIP_REQUEST_LOADED, INITIAL};
ASSIGN
next(ProvideTrip) :=
case
(ProvideTrip = TRIP_REQUEST_ACCOMPLISHED & input = service163_UMS_TripManagementRequest): TRIP_MANAGE_REQUEST_LOADED;
(ProvideTrip = TRIP_MANAGE_REQUEST_LOADED & output = service163_UMS_TripManagementReply): TRIP_MANAGEMENT_ACCOMPLISHED;
(ProvideTrip = TRIP_REQUEST_LOADED & output = service165_UMS_TripAlternatives): TRIP_REQUEST_ACCOMPLISHED;
(ProvideTrip = INITIAL & input = service165_UMS_TripRequest): TRIP_REQUEST_LOADED;
1:ProvideTrip;
esac;
-----------------------------------
--   P R E C O N D I T I O N S   --
-----------------------------------
TRANS (input = service163_UMS_TripManagementRequest) -> ((((ProvideTrip = TRIP_REQUEST_ACCOMPLISHED))))
TRANS (output = service163_UMS_TripManagementReply) -> ((((ProvideTrip = TRIP_MANAGE_REQUEST_LOADED))))
TRANS (input = service165_UMS_TripRequest) -> ((((ProvideTrip = INITIAL))))
TRANS (output = service165_UMS_TripAlternatives) -> ((((ProvideTrip = TRIP_REQUEST_LOADED))))
-----------------------------
--   I N V A R I A N T S   --
-----------------------------
--	 Synchronicity
TRANS (1-(input = UNDEF)) + (1-(output = UNDEF)) = 1

---------------------------------------
--   S Y N C H R O N I Z A T I O N   --
---------------------------------------
DEFINE SYNC0 := ((ProvideTrip in {TRIP_MANAGEMENT_ACCOMPLISHED}));
--	 Synchronicity enforcement
TRANS (service163 in {ST2, ST1, ST4, ST3, ST5, ST7}) -> (next(service163) != service163)
TRANS (service165 in {ST2, ST1, ST3}) -> (next(service165) != service165)
-------------------------------
--   C O N T R O L L E R S   --
-------------------------------
--	 Fault Controller
VAR FAULT_CONTROLLER: { START, STOP };
INIT FAULT_CONTROLLER = START

ASSIGN
next(FAULT_CONTROLLER) := 
case
(FAULT_CONTROLLER = START & FALSE): STOP;
1: FAULT_CONTROLLER;
esac;

TRANS (output != UNDEF | input != UNDEF) -> (FAULT_CONTROLLER != STOP)

-------------------------------------
--   I N I T I A L I Z A T I O N   --
-------------------------------------
INIT ((SetupTrip = INITIAL) & (ProvideTrip = TRIP_REQUEST_ACCOMPLISHED))
INIT ((service163 = ST0) & (service165 = ST0))

-----------------
--   G O A L   --
-----------------
MODULE main
VAR dn_main : dn_main;

GOAL ONEOF(
(dn_main.SYNC0), 1;
(dn_main.FAULT_CONTROLLER = STOP), 0

)