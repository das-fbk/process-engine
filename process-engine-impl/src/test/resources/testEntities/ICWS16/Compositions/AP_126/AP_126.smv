-------------------------------
--   C O M P O S I T I O N   --
-------------------------------
MODULE dn_main

---------------------
--   I N P U T S   --
---------------------
IVAR input:
{
UNDEF, service226_UMS_ManagementTrip, service226_UMS_TripManagementRequest, service226_RESET, service228_UMS_TripRequest, service228_RESET
};

-----------------------
--   O U T P U T S   --
-----------------------
IVAR output:
{
UNDEF, service226_UMS_concrete4, service226_UMS_concrete3, service226_UMS_concrete2, service226_UMS_concrete1, service226_UMS_TripManagementReply, service226_UMS_concrete5, service228_UMS_TripAlternatives, service228_UMS_concrete2, service228_UMS_concrete1
};

-------------------------
--   S E R V I C E S   --
-------------------------
--	 service226
VAR service226: {ST2, ST1, ST4, ST3, ST6, ST5, ST8, ST7, ST0};
ASSIGN
next(service226) :=
case
(service226 = ST6 & input = service226_UMS_ManagementTrip): ST7;
(service226 = ST3 & input = service226_RESET): ST0;
(service226 = ST2 & output = service226_UMS_concrete2): ST3;
(service226 = ST2 & input = service226_RESET): ST0;
(service226 = ST6 & input = service226_RESET): ST0;
(service226 = ST5 & input = service226_RESET): ST0;
(service226 = ST7 & output = service226_UMS_TripManagementReply): ST8;
(service226 = ST1 & input = service226_RESET): ST0;
(service226 = ST7 & input = service226_RESET): ST0;
(service226 = ST5 & output = service226_UMS_concrete5): ST6;
(service226 = ST8 & input = service226_RESET): ST0;
(service226 = ST3 & output = service226_UMS_concrete3): ST4;
(service226 = ST1 & output = service226_UMS_concrete1): ST2;
(service226 = ST4 & output = service226_UMS_concrete4): ST5;
(service226 = ST4 & input = service226_RESET): ST0;
(service226 = ST0 & input = service226_UMS_TripManagementRequest): ST1;
1:service226;
esac;
TRANS ( input = service226_UMS_ManagementTrip) -> (service226 = ST6)
TRANS ( output = service226_UMS_concrete4) -> (service226 = ST4)
TRANS ( input = service226_UMS_TripManagementRequest) -> (service226 = ST0)
TRANS ( output = service226_UMS_concrete3) -> (service226 = ST3)
TRANS ( output = service226_UMS_concrete2) -> (service226 = ST2)
TRANS ( input = service226_RESET) -> (service226 = ST2 | service226 = ST1 | service226 = ST4 | service226 = ST3 | service226 = ST6 | service226 = ST5 | service226 = ST8 | service226 = ST7)
TRANS ( output = service226_UMS_concrete1) -> (service226 = ST1)
TRANS ( output = service226_UMS_TripManagementReply) -> (service226 = ST7)
TRANS ( output = service226_UMS_concrete5) -> (service226 = ST5)

--	 service228
VAR service228: {ST2, ST1, ST4, ST3, ST0};
ASSIGN
next(service228) :=
case
(service228 = ST0 & input = service228_UMS_TripRequest): ST1;
(service228 = ST1 & input = service228_RESET): ST0;
(service228 = ST2 & output = service228_UMS_concrete2): ST3;
(service228 = ST2 & input = service228_RESET): ST0;
(service228 = ST3 & output = service228_UMS_TripAlternatives): ST4;
(service228 = ST3 & input = service228_RESET): ST0;
(service228 = ST1 & output = service228_UMS_concrete1): ST2;
(service228 = ST4 & input = service228_RESET): ST0;
1:service228;
esac;
TRANS ( input = service228_UMS_TripRequest) -> (service228 = ST0)
TRANS ( output = service228_UMS_TripAlternatives) -> (service228 = ST3)
TRANS ( output = service228_UMS_concrete2) -> (service228 = ST2)
TRANS ( input = service228_RESET) -> (service228 = ST2 | service228 = ST1 | service228 = ST4 | service228 = ST3)
TRANS ( output = service228_UMS_concrete1) -> (service228 = ST1)

-----------------------
--   O B J E C T S   --
-----------------------
--	 SetupTrip
VAR SetupTrip: {ORGANIZED, INITIAL};
ASSIGN
next(SetupTrip) :=
case
(input = service226_UMS_ManagementTrip):{(ORGANIZED)};
1:SetupTrip;
esac;
--	 ProvideTrip
VAR ProvideTrip: {TRIP_REQUEST_ACCOMPLISHED, TRIP_MANAGEMENT_ACCOMPLISHED, TRIP_MANAGE_REQUEST_LOADED, TRIP_REQUEST_LOADED, INITIAL};
ASSIGN
next(ProvideTrip) :=
case
(ProvideTrip = TRIP_REQUEST_LOADED & output = service228_UMS_TripAlternatives): TRIP_REQUEST_ACCOMPLISHED;
(ProvideTrip = INITIAL & input = service228_UMS_TripRequest): TRIP_REQUEST_LOADED;
(ProvideTrip = TRIP_MANAGE_REQUEST_LOADED & output = service226_UMS_TripManagementReply): TRIP_MANAGEMENT_ACCOMPLISHED;
(ProvideTrip = TRIP_REQUEST_ACCOMPLISHED & input = service226_UMS_TripManagementRequest): TRIP_MANAGE_REQUEST_LOADED;
1:ProvideTrip;
esac;
-----------------------------------
--   P R E C O N D I T I O N S   --
-----------------------------------
TRANS (input = service226_UMS_TripManagementRequest) -> ((((ProvideTrip = TRIP_REQUEST_ACCOMPLISHED))))
TRANS (output = service226_UMS_TripManagementReply) -> ((((ProvideTrip = TRIP_MANAGE_REQUEST_LOADED))))
TRANS (input = service228_UMS_TripRequest) -> ((((ProvideTrip = INITIAL))))
TRANS (output = service228_UMS_TripAlternatives) -> ((((ProvideTrip = TRIP_REQUEST_LOADED))))
-----------------------------
--   I N V A R I A N T S   --
-----------------------------
--	 Synchronicity
TRANS (1-(input = UNDEF)) + (1-(output = UNDEF)) = 1

---------------------------------------
--   S Y N C H R O N I Z A T I O N   --
---------------------------------------
DEFINE SYNC0 := ((ProvideTrip in {TRIP_MANAGEMENT_ACCOMPLISHED}));
--	 Synchronicity enforcement
TRANS (service226 in {ST2, ST1, ST4, ST3, ST5, ST7}) -> (next(service226) != service226)
TRANS (service228 in {ST2, ST1, ST3}) -> (next(service228) != service228)
-------------------------------
--   C O N T R O L L E R S   --
-------------------------------
--	 Fault Controller
VAR FAULT_CONTROLLER: { START, STOP };
INIT FAULT_CONTROLLER = START

ASSIGN
next(FAULT_CONTROLLER) := 
case
(FAULT_CONTROLLER = START & FALSE): STOP;
1: FAULT_CONTROLLER;
esac;

TRANS (output != UNDEF | input != UNDEF) -> (FAULT_CONTROLLER != STOP)

-------------------------------------
--   I N I T I A L I Z A T I O N   --
-------------------------------------
INIT ((SetupTrip = INITIAL) & (ProvideTrip = TRIP_REQUEST_ACCOMPLISHED))
INIT ((service226 = ST0) & (service228 = ST0))

-----------------
--   G O A L   --
-----------------
MODULE main
VAR dn_main : dn_main;

GOAL ONEOF(
(dn_main.SYNC0), 1;
(dn_main.FAULT_CONTROLLER = STOP), 0

)