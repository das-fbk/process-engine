-------------------------------
--   C O M P O S I T I O N   --
-------------------------------
MODULE dn_main

---------------------
--   I N P U T S   --
---------------------
IVAR input:
{
UNDEF, service113_UMS_ManagementTrip, service113_UMS_TripManagementRequest, service113_RESET, service114_UMS_TripRequest, service114_RESET
};

-----------------------
--   O U T P U T S   --
-----------------------
IVAR output:
{
UNDEF, service113_UMS_concrete4, service113_UMS_concrete3, service113_UMS_concrete2, service113_UMS_concrete1, service113_UMS_TripManagementReply, service113_UMS_concrete5, service114_UMS_TripAlternatives, service114_UMS_concrete2, service114_UMS_concrete1
};

-------------------------
--   S E R V I C E S   --
-------------------------
--	 service113
VAR service113: {ST2, ST1, ST4, ST3, ST6, ST5, ST8, ST7, ST0};
ASSIGN
next(service113) :=
case
(service113 = ST5 & input = service113_RESET): ST0;
(service113 = ST7 & input = service113_RESET): ST0;
(service113 = ST4 & output = service113_UMS_concrete4): ST5;
(service113 = ST1 & output = service113_UMS_concrete1): ST2;
(service113 = ST3 & input = service113_RESET): ST0;
(service113 = ST4 & input = service113_RESET): ST0;
(service113 = ST7 & output = service113_UMS_TripManagementReply): ST8;
(service113 = ST6 & input = service113_RESET): ST0;
(service113 = ST1 & input = service113_RESET): ST0;
(service113 = ST2 & input = service113_RESET): ST0;
(service113 = ST0 & input = service113_UMS_TripManagementRequest): ST1;
(service113 = ST3 & output = service113_UMS_concrete3): ST4;
(service113 = ST5 & output = service113_UMS_concrete5): ST6;
(service113 = ST6 & input = service113_UMS_ManagementTrip): ST7;
(service113 = ST8 & input = service113_RESET): ST0;
(service113 = ST2 & output = service113_UMS_concrete2): ST3;
1:service113;
esac;
TRANS ( input = service113_UMS_ManagementTrip) -> (service113 = ST6)
TRANS ( output = service113_UMS_concrete4) -> (service113 = ST4)
TRANS ( input = service113_UMS_TripManagementRequest) -> (service113 = ST0)
TRANS ( output = service113_UMS_concrete3) -> (service113 = ST3)
TRANS ( output = service113_UMS_concrete2) -> (service113 = ST2)
TRANS ( input = service113_RESET) -> (service113 = ST2 | service113 = ST1 | service113 = ST4 | service113 = ST3 | service113 = ST6 | service113 = ST5 | service113 = ST8 | service113 = ST7)
TRANS ( output = service113_UMS_concrete1) -> (service113 = ST1)
TRANS ( output = service113_UMS_TripManagementReply) -> (service113 = ST7)
TRANS ( output = service113_UMS_concrete5) -> (service113 = ST5)

--	 service114
VAR service114: {ST2, ST1, ST4, ST3, ST0};
ASSIGN
next(service114) :=
case
(service114 = ST3 & output = service114_UMS_TripAlternatives): ST4;
(service114 = ST2 & output = service114_UMS_concrete2): ST3;
(service114 = ST2 & input = service114_RESET): ST0;
(service114 = ST3 & input = service114_RESET): ST0;
(service114 = ST1 & input = service114_RESET): ST0;
(service114 = ST0 & input = service114_UMS_TripRequest): ST1;
(service114 = ST1 & output = service114_UMS_concrete1): ST2;
(service114 = ST4 & input = service114_RESET): ST0;
1:service114;
esac;
TRANS ( input = service114_UMS_TripRequest) -> (service114 = ST0)
TRANS ( output = service114_UMS_TripAlternatives) -> (service114 = ST3)
TRANS ( output = service114_UMS_concrete2) -> (service114 = ST2)
TRANS ( input = service114_RESET) -> (service114 = ST2 | service114 = ST1 | service114 = ST4 | service114 = ST3)
TRANS ( output = service114_UMS_concrete1) -> (service114 = ST1)

-----------------------
--   O B J E C T S   --
-----------------------
--	 SetupTrip
VAR SetupTrip: {ORGANIZED, INITIAL};
ASSIGN
next(SetupTrip) :=
case
(input = service113_UMS_ManagementTrip):{(ORGANIZED)};
1:SetupTrip;
esac;
--	 ProvideTrip
VAR ProvideTrip: {TRIP_REQUEST_ACCOMPLISHED, TRIP_MANAGEMENT_ACCOMPLISHED, TRIP_MANAGE_REQUEST_LOADED, TRIP_REQUEST_LOADED, INITIAL};
ASSIGN
next(ProvideTrip) :=
case
(ProvideTrip = TRIP_MANAGE_REQUEST_LOADED & output = service113_UMS_TripManagementReply): TRIP_MANAGEMENT_ACCOMPLISHED;
(ProvideTrip = INITIAL & input = service114_UMS_TripRequest): TRIP_REQUEST_LOADED;
(ProvideTrip = TRIP_REQUEST_ACCOMPLISHED & input = service113_UMS_TripManagementRequest): TRIP_MANAGE_REQUEST_LOADED;
(ProvideTrip = TRIP_REQUEST_LOADED & output = service114_UMS_TripAlternatives): TRIP_REQUEST_ACCOMPLISHED;
1:ProvideTrip;
esac;
-----------------------------------
--   P R E C O N D I T I O N S   --
-----------------------------------
TRANS (input = service113_UMS_TripManagementRequest) -> ((((ProvideTrip = TRIP_REQUEST_ACCOMPLISHED))))
TRANS (output = service113_UMS_TripManagementReply) -> ((((ProvideTrip = TRIP_MANAGE_REQUEST_LOADED))))
TRANS (input = service114_UMS_TripRequest) -> ((((ProvideTrip = INITIAL))))
TRANS (output = service114_UMS_TripAlternatives) -> ((((ProvideTrip = TRIP_REQUEST_LOADED))))
-----------------------------
--   I N V A R I A N T S   --
-----------------------------
--	 Synchronicity
TRANS (1-(input = UNDEF)) + (1-(output = UNDEF)) = 1

---------------------------------------
--   S Y N C H R O N I Z A T I O N   --
---------------------------------------
DEFINE SYNC0 := ((ProvideTrip in {TRIP_MANAGEMENT_ACCOMPLISHED}));
--	 Synchronicity enforcement
TRANS (service113 in {ST2, ST1, ST4, ST3, ST5, ST7}) -> (next(service113) != service113)
TRANS (service114 in {ST2, ST1, ST3}) -> (next(service114) != service114)
-------------------------------
--   C O N T R O L L E R S   --
-------------------------------
--	 Fault Controller
VAR FAULT_CONTROLLER: { START, STOP };
INIT FAULT_CONTROLLER = START

ASSIGN
next(FAULT_CONTROLLER) := 
case
(FAULT_CONTROLLER = START & FALSE): STOP;
1: FAULT_CONTROLLER;
esac;

TRANS (output != UNDEF | input != UNDEF) -> (FAULT_CONTROLLER != STOP)

-------------------------------------
--   I N I T I A L I Z A T I O N   --
-------------------------------------
INIT ((SetupTrip = INITIAL) & (ProvideTrip = TRIP_REQUEST_ACCOMPLISHED))
INIT ((service113 = ST0) & (service114 = ST0))

-----------------
--   G O A L   --
-----------------
MODULE main
VAR dn_main : dn_main;

GOAL ONEOF(
(dn_main.SYNC0), 1;
(dn_main.FAULT_CONTROLLER = STOP), 0

)