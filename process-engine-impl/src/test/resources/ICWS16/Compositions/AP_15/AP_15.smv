-------------------------------
--   C O M P O S I T I O N   --
-------------------------------
MODULE dn_main

---------------------
--   I N P U T S   --
---------------------
IVAR input:
{
UNDEF, service34_UMS_ManagementTrip, service34_UMS_TripManagementRequest, service34_RESET, service35_UMS_TripRequest, service35_RESET
};

-----------------------
--   O U T P U T S   --
-----------------------
IVAR output:
{
UNDEF, service34_UMS_concrete4, service34_UMS_concrete3, service34_UMS_concrete2, service34_UMS_concrete1, service34_UMS_TripManagementReply, service34_UMS_concrete5, service35_UMS_TripAlternatives, service35_UMS_concrete2, service35_UMS_concrete1
};

-------------------------
--   S E R V I C E S   --
-------------------------
--	 service34
VAR service34: {ST2, ST1, ST4, ST3, ST6, ST5, ST8, ST7, ST0};
ASSIGN
next(service34) :=
case
(service34 = ST5 & output = service34_UMS_concrete5): ST6;
(service34 = ST5 & input = service34_RESET): ST0;
(service34 = ST6 & input = service34_UMS_ManagementTrip): ST7;
(service34 = ST0 & input = service34_UMS_TripManagementRequest): ST1;
(service34 = ST1 & input = service34_RESET): ST0;
(service34 = ST4 & output = service34_UMS_concrete4): ST5;
(service34 = ST4 & input = service34_RESET): ST0;
(service34 = ST7 & output = service34_UMS_TripManagementReply): ST8;
(service34 = ST6 & input = service34_RESET): ST0;
(service34 = ST2 & output = service34_UMS_concrete2): ST3;
(service34 = ST8 & input = service34_RESET): ST0;
(service34 = ST3 & input = service34_RESET): ST0;
(service34 = ST3 & output = service34_UMS_concrete3): ST4;
(service34 = ST2 & input = service34_RESET): ST0;
(service34 = ST1 & output = service34_UMS_concrete1): ST2;
(service34 = ST7 & input = service34_RESET): ST0;
1:service34;
esac;
TRANS ( input = service34_UMS_ManagementTrip) -> (service34 = ST6)
TRANS ( output = service34_UMS_concrete4) -> (service34 = ST4)
TRANS ( input = service34_UMS_TripManagementRequest) -> (service34 = ST0)
TRANS ( output = service34_UMS_concrete3) -> (service34 = ST3)
TRANS ( output = service34_UMS_concrete2) -> (service34 = ST2)
TRANS ( input = service34_RESET) -> (service34 = ST2 | service34 = ST1 | service34 = ST4 | service34 = ST3 | service34 = ST6 | service34 = ST5 | service34 = ST8 | service34 = ST7)
TRANS ( output = service34_UMS_concrete1) -> (service34 = ST1)
TRANS ( output = service34_UMS_TripManagementReply) -> (service34 = ST7)
TRANS ( output = service34_UMS_concrete5) -> (service34 = ST5)

--	 service35
VAR service35: {ST2, ST1, ST4, ST3, ST0};
ASSIGN
next(service35) :=
case
(service35 = ST2 & input = service35_RESET): ST0;
(service35 = ST1 & input = service35_RESET): ST0;
(service35 = ST3 & output = service35_UMS_TripAlternatives): ST4;
(service35 = ST3 & input = service35_RESET): ST0;
(service35 = ST0 & input = service35_UMS_TripRequest): ST1;
(service35 = ST4 & input = service35_RESET): ST0;
(service35 = ST1 & output = service35_UMS_concrete1): ST2;
(service35 = ST2 & output = service35_UMS_concrete2): ST3;
1:service35;
esac;
TRANS ( input = service35_UMS_TripRequest) -> (service35 = ST0)
TRANS ( output = service35_UMS_TripAlternatives) -> (service35 = ST3)
TRANS ( output = service35_UMS_concrete2) -> (service35 = ST2)
TRANS ( input = service35_RESET) -> (service35 = ST2 | service35 = ST1 | service35 = ST4 | service35 = ST3)
TRANS ( output = service35_UMS_concrete1) -> (service35 = ST1)

-----------------------
--   O B J E C T S   --
-----------------------
--	 SetupTrip
VAR SetupTrip: {ORGANIZED, INITIAL};
ASSIGN
next(SetupTrip) :=
case
(input = service34_UMS_ManagementTrip):{(ORGANIZED)};
1:SetupTrip;
esac;
--	 ProvideTrip
VAR ProvideTrip: {TRIP_REQUEST_ACCOMPLISHED, TRIP_MANAGEMENT_ACCOMPLISHED, TRIP_MANAGE_REQUEST_LOADED, TRIP_REQUEST_LOADED, INITIAL};
ASSIGN
next(ProvideTrip) :=
case
(ProvideTrip = INITIAL & input = service35_UMS_TripRequest): TRIP_REQUEST_LOADED;
(ProvideTrip = TRIP_MANAGE_REQUEST_LOADED & output = service34_UMS_TripManagementReply): TRIP_MANAGEMENT_ACCOMPLISHED;
(ProvideTrip = TRIP_REQUEST_LOADED & output = service35_UMS_TripAlternatives): TRIP_REQUEST_ACCOMPLISHED;
(ProvideTrip = TRIP_REQUEST_ACCOMPLISHED & input = service34_UMS_TripManagementRequest): TRIP_MANAGE_REQUEST_LOADED;
1:ProvideTrip;
esac;
-----------------------------------
--   P R E C O N D I T I O N S   --
-----------------------------------
TRANS (input = service34_UMS_TripManagementRequest) -> ((((ProvideTrip = TRIP_REQUEST_ACCOMPLISHED))))
TRANS (output = service34_UMS_TripManagementReply) -> ((((ProvideTrip = TRIP_MANAGE_REQUEST_LOADED))))
TRANS (input = service35_UMS_TripRequest) -> ((((ProvideTrip = INITIAL))))
TRANS (output = service35_UMS_TripAlternatives) -> ((((ProvideTrip = TRIP_REQUEST_LOADED))))
-----------------------------
--   I N V A R I A N T S   --
-----------------------------
--	 Synchronicity
TRANS (1-(input = UNDEF)) + (1-(output = UNDEF)) = 1

---------------------------------------
--   S Y N C H R O N I Z A T I O N   --
---------------------------------------
DEFINE SYNC0 := ((ProvideTrip in {TRIP_REQUEST_ACCOMPLISHED}));
--	 Synchronicity enforcement
TRANS (service34 in {ST2, ST1, ST4, ST3, ST5, ST7}) -> (next(service34) != service34)
TRANS (service35 in {ST2, ST1, ST3}) -> (next(service35) != service35)
-------------------------------
--   C O N T R O L L E R S   --
-------------------------------
--	 Fault Controller
VAR FAULT_CONTROLLER: { START, STOP };
INIT FAULT_CONTROLLER = START

ASSIGN
next(FAULT_CONTROLLER) := 
case
(FAULT_CONTROLLER = START & FALSE): STOP;
1: FAULT_CONTROLLER;
esac;

TRANS (output != UNDEF | input != UNDEF) -> (FAULT_CONTROLLER != STOP)

-------------------------------------
--   I N I T I A L I Z A T I O N   --
-------------------------------------
INIT ((SetupTrip = INITIAL) & (ProvideTrip = INITIAL))
INIT ((service34 = ST0) & (service35 = ST0))

-----------------
--   G O A L   --
-----------------
MODULE main
VAR dn_main : dn_main;

GOAL ONEOF(
(dn_main.SYNC0), 1;
(dn_main.FAULT_CONTROLLER = STOP), 0

)