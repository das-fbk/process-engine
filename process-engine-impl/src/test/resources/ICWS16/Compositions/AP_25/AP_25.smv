-------------------------------
--   C O M P O S I T I O N   --
-------------------------------
MODULE dn_main

---------------------
--   I N P U T S   --
---------------------
IVAR input:
{
UNDEF, service53_UMS_ManagementTrip, service53_UMS_TripManagementRequest, service53_RESET, service54_UMS_TripRequest, service54_RESET
};

-----------------------
--   O U T P U T S   --
-----------------------
IVAR output:
{
UNDEF, service53_UMS_concrete4, service53_UMS_concrete3, service53_UMS_concrete2, service53_UMS_concrete1, service53_UMS_TripManagementReply, service53_UMS_concrete5, service54_UMS_TripAlternatives, service54_UMS_concrete2, service54_UMS_concrete1
};

-------------------------
--   S E R V I C E S   --
-------------------------
--	 service53
VAR service53: {ST2, ST1, ST4, ST3, ST6, ST5, ST8, ST7, ST0};
ASSIGN
next(service53) :=
case
(service53 = ST8 & input = service53_RESET): ST0;
(service53 = ST1 & output = service53_UMS_concrete1): ST2;
(service53 = ST1 & input = service53_RESET): ST0;
(service53 = ST7 & output = service53_UMS_TripManagementReply): ST8;
(service53 = ST2 & input = service53_RESET): ST0;
(service53 = ST4 & input = service53_RESET): ST0;
(service53 = ST6 & input = service53_RESET): ST0;
(service53 = ST5 & input = service53_RESET): ST0;
(service53 = ST0 & input = service53_UMS_TripManagementRequest): ST1;
(service53 = ST3 & output = service53_UMS_concrete3): ST4;
(service53 = ST5 & output = service53_UMS_concrete5): ST6;
(service53 = ST7 & input = service53_RESET): ST0;
(service53 = ST3 & input = service53_RESET): ST0;
(service53 = ST6 & input = service53_UMS_ManagementTrip): ST7;
(service53 = ST4 & output = service53_UMS_concrete4): ST5;
(service53 = ST2 & output = service53_UMS_concrete2): ST3;
1:service53;
esac;
TRANS ( input = service53_UMS_ManagementTrip) -> (service53 = ST6)
TRANS ( output = service53_UMS_concrete4) -> (service53 = ST4)
TRANS ( input = service53_UMS_TripManagementRequest) -> (service53 = ST0)
TRANS ( output = service53_UMS_concrete3) -> (service53 = ST3)
TRANS ( output = service53_UMS_concrete2) -> (service53 = ST2)
TRANS ( input = service53_RESET) -> (service53 = ST2 | service53 = ST1 | service53 = ST4 | service53 = ST3 | service53 = ST6 | service53 = ST5 | service53 = ST8 | service53 = ST7)
TRANS ( output = service53_UMS_concrete1) -> (service53 = ST1)
TRANS ( output = service53_UMS_TripManagementReply) -> (service53 = ST7)
TRANS ( output = service53_UMS_concrete5) -> (service53 = ST5)

--	 service54
VAR service54: {ST2, ST1, ST4, ST3, ST0};
ASSIGN
next(service54) :=
case
(service54 = ST2 & input = service54_RESET): ST0;
(service54 = ST3 & input = service54_RESET): ST0;
(service54 = ST1 & output = service54_UMS_concrete1): ST2;
(service54 = ST0 & input = service54_UMS_TripRequest): ST1;
(service54 = ST1 & input = service54_RESET): ST0;
(service54 = ST2 & output = service54_UMS_concrete2): ST3;
(service54 = ST3 & output = service54_UMS_TripAlternatives): ST4;
(service54 = ST4 & input = service54_RESET): ST0;
1:service54;
esac;
TRANS ( input = service54_UMS_TripRequest) -> (service54 = ST0)
TRANS ( output = service54_UMS_TripAlternatives) -> (service54 = ST3)
TRANS ( output = service54_UMS_concrete2) -> (service54 = ST2)
TRANS ( input = service54_RESET) -> (service54 = ST2 | service54 = ST1 | service54 = ST4 | service54 = ST3)
TRANS ( output = service54_UMS_concrete1) -> (service54 = ST1)

-----------------------
--   O B J E C T S   --
-----------------------
--	 SetupTrip
VAR SetupTrip: {ORGANIZED, INITIAL};
ASSIGN
next(SetupTrip) :=
case
(input = service53_UMS_ManagementTrip):{(ORGANIZED)};
1:SetupTrip;
esac;
--	 ProvideTrip
VAR ProvideTrip: {TRIP_REQUEST_ACCOMPLISHED, TRIP_MANAGEMENT_ACCOMPLISHED, TRIP_MANAGE_REQUEST_LOADED, TRIP_REQUEST_LOADED, INITIAL};
ASSIGN
next(ProvideTrip) :=
case
(ProvideTrip = INITIAL & input = service54_UMS_TripRequest): TRIP_REQUEST_LOADED;
(ProvideTrip = TRIP_REQUEST_ACCOMPLISHED & input = service53_UMS_TripManagementRequest): TRIP_MANAGE_REQUEST_LOADED;
(ProvideTrip = TRIP_MANAGE_REQUEST_LOADED & output = service53_UMS_TripManagementReply): TRIP_MANAGEMENT_ACCOMPLISHED;
(ProvideTrip = TRIP_REQUEST_LOADED & output = service54_UMS_TripAlternatives): TRIP_REQUEST_ACCOMPLISHED;
1:ProvideTrip;
esac;
-----------------------------------
--   P R E C O N D I T I O N S   --
-----------------------------------
TRANS (input = service53_UMS_TripManagementRequest) -> ((((ProvideTrip = TRIP_REQUEST_ACCOMPLISHED))))
TRANS (output = service53_UMS_TripManagementReply) -> ((((ProvideTrip = TRIP_MANAGE_REQUEST_LOADED))))
TRANS (input = service54_UMS_TripRequest) -> ((((ProvideTrip = INITIAL))))
TRANS (output = service54_UMS_TripAlternatives) -> ((((ProvideTrip = TRIP_REQUEST_LOADED))))
-----------------------------
--   I N V A R I A N T S   --
-----------------------------
--	 Synchronicity
TRANS (1-(input = UNDEF)) + (1-(output = UNDEF)) = 1

---------------------------------------
--   S Y N C H R O N I Z A T I O N   --
---------------------------------------
DEFINE SYNC0 := ((ProvideTrip in {TRIP_REQUEST_ACCOMPLISHED}));
--	 Synchronicity enforcement
TRANS (service53 in {ST2, ST1, ST4, ST3, ST5, ST7}) -> (next(service53) != service53)
TRANS (service54 in {ST2, ST1, ST3}) -> (next(service54) != service54)
-------------------------------
--   C O N T R O L L E R S   --
-------------------------------
--	 Fault Controller
VAR FAULT_CONTROLLER: { START, STOP };
INIT FAULT_CONTROLLER = START

ASSIGN
next(FAULT_CONTROLLER) := 
case
(FAULT_CONTROLLER = START & FALSE): STOP;
1: FAULT_CONTROLLER;
esac;

TRANS (output != UNDEF | input != UNDEF) -> (FAULT_CONTROLLER != STOP)

-------------------------------------
--   I N I T I A L I Z A T I O N   --
-------------------------------------
INIT ((SetupTrip = INITIAL) & (ProvideTrip = INITIAL))
INIT ((service53 = ST0) & (service54 = ST0))

-----------------
--   G O A L   --
-----------------
MODULE main
VAR dn_main : dn_main;

GOAL ONEOF(
(dn_main.SYNC0), 1;
(dn_main.FAULT_CONTROLLER = STOP), 0

)