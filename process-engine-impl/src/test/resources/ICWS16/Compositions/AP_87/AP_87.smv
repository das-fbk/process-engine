-------------------------------
--   C O M P O S I T I O N   --
-------------------------------
MODULE dn_main

---------------------
--   I N P U T S   --
---------------------
IVAR input:
{
UNDEF, service148_UMS_ManagementTrip, service148_UMS_TripManagementRequest, service148_RESET, service149_UMS_TripRequest, service149_RESET
};

-----------------------
--   O U T P U T S   --
-----------------------
IVAR output:
{
UNDEF, service148_UMS_concrete4, service148_UMS_concrete3, service148_UMS_concrete2, service148_UMS_concrete1, service148_UMS_TripManagementReply, service148_UMS_concrete5, service149_UMS_TripAlternatives, service149_UMS_concrete2, service149_UMS_concrete1
};

-------------------------
--   S E R V I C E S   --
-------------------------
--	 service148
VAR service148: {ST2, ST1, ST4, ST3, ST6, ST5, ST8, ST7, ST0};
ASSIGN
next(service148) :=
case
(service148 = ST2 & output = service148_UMS_concrete2): ST3;
(service148 = ST3 & output = service148_UMS_concrete3): ST4;
(service148 = ST6 & input = service148_UMS_ManagementTrip): ST7;
(service148 = ST5 & input = service148_RESET): ST0;
(service148 = ST7 & input = service148_RESET): ST0;
(service148 = ST1 & input = service148_RESET): ST0;
(service148 = ST5 & output = service148_UMS_concrete5): ST6;
(service148 = ST7 & output = service148_UMS_TripManagementReply): ST8;
(service148 = ST4 & input = service148_RESET): ST0;
(service148 = ST3 & input = service148_RESET): ST0;
(service148 = ST0 & input = service148_UMS_TripManagementRequest): ST1;
(service148 = ST6 & input = service148_RESET): ST0;
(service148 = ST8 & input = service148_RESET): ST0;
(service148 = ST2 & input = service148_RESET): ST0;
(service148 = ST1 & output = service148_UMS_concrete1): ST2;
(service148 = ST4 & output = service148_UMS_concrete4): ST5;
1:service148;
esac;
TRANS ( input = service148_UMS_ManagementTrip) -> (service148 = ST6)
TRANS ( output = service148_UMS_concrete4) -> (service148 = ST4)
TRANS ( input = service148_UMS_TripManagementRequest) -> (service148 = ST0)
TRANS ( output = service148_UMS_concrete3) -> (service148 = ST3)
TRANS ( output = service148_UMS_concrete2) -> (service148 = ST2)
TRANS ( input = service148_RESET) -> (service148 = ST2 | service148 = ST1 | service148 = ST4 | service148 = ST3 | service148 = ST6 | service148 = ST5 | service148 = ST8 | service148 = ST7)
TRANS ( output = service148_UMS_concrete1) -> (service148 = ST1)
TRANS ( output = service148_UMS_TripManagementReply) -> (service148 = ST7)
TRANS ( output = service148_UMS_concrete5) -> (service148 = ST5)

--	 service149
VAR service149: {ST2, ST1, ST4, ST3, ST0};
ASSIGN
next(service149) :=
case
(service149 = ST1 & input = service149_RESET): ST0;
(service149 = ST0 & input = service149_UMS_TripRequest): ST1;
(service149 = ST3 & output = service149_UMS_TripAlternatives): ST4;
(service149 = ST2 & input = service149_RESET): ST0;
(service149 = ST2 & output = service149_UMS_concrete2): ST3;
(service149 = ST3 & input = service149_RESET): ST0;
(service149 = ST1 & output = service149_UMS_concrete1): ST2;
(service149 = ST4 & input = service149_RESET): ST0;
1:service149;
esac;
TRANS ( input = service149_UMS_TripRequest) -> (service149 = ST0)
TRANS ( output = service149_UMS_TripAlternatives) -> (service149 = ST3)
TRANS ( output = service149_UMS_concrete2) -> (service149 = ST2)
TRANS ( input = service149_RESET) -> (service149 = ST2 | service149 = ST1 | service149 = ST4 | service149 = ST3)
TRANS ( output = service149_UMS_concrete1) -> (service149 = ST1)

-----------------------
--   O B J E C T S   --
-----------------------
--	 SetupTrip
VAR SetupTrip: {ORGANIZED, INITIAL};
ASSIGN
next(SetupTrip) :=
case
(input = service148_UMS_ManagementTrip):{(ORGANIZED)};
1:SetupTrip;
esac;
--	 ProvideTrip
VAR ProvideTrip: {TRIP_REQUEST_ACCOMPLISHED, TRIP_MANAGEMENT_ACCOMPLISHED, TRIP_MANAGE_REQUEST_LOADED, TRIP_REQUEST_LOADED, INITIAL};
ASSIGN
next(ProvideTrip) :=
case
(ProvideTrip = TRIP_REQUEST_LOADED & output = service149_UMS_TripAlternatives): TRIP_REQUEST_ACCOMPLISHED;
(ProvideTrip = INITIAL & input = service149_UMS_TripRequest): TRIP_REQUEST_LOADED;
(ProvideTrip = TRIP_MANAGE_REQUEST_LOADED & output = service148_UMS_TripManagementReply): TRIP_MANAGEMENT_ACCOMPLISHED;
(ProvideTrip = TRIP_REQUEST_ACCOMPLISHED & input = service148_UMS_TripManagementRequest): TRIP_MANAGE_REQUEST_LOADED;
1:ProvideTrip;
esac;
-----------------------------------
--   P R E C O N D I T I O N S   --
-----------------------------------
TRANS (input = service148_UMS_TripManagementRequest) -> ((((ProvideTrip = TRIP_REQUEST_ACCOMPLISHED))))
TRANS (output = service148_UMS_TripManagementReply) -> ((((ProvideTrip = TRIP_MANAGE_REQUEST_LOADED))))
TRANS (input = service149_UMS_TripRequest) -> ((((ProvideTrip = INITIAL))))
TRANS (output = service149_UMS_TripAlternatives) -> ((((ProvideTrip = TRIP_REQUEST_LOADED))))
-----------------------------
--   I N V A R I A N T S   --
-----------------------------
--	 Synchronicity
TRANS (1-(input = UNDEF)) + (1-(output = UNDEF)) = 1

---------------------------------------
--   S Y N C H R O N I Z A T I O N   --
---------------------------------------
DEFINE SYNC0 := ((ProvideTrip in {TRIP_MANAGEMENT_ACCOMPLISHED}));
--	 Synchronicity enforcement
TRANS (service148 in {ST2, ST1, ST4, ST3, ST5, ST7}) -> (next(service148) != service148)
TRANS (service149 in {ST2, ST1, ST3}) -> (next(service149) != service149)
-------------------------------
--   C O N T R O L L E R S   --
-------------------------------
--	 Fault Controller
VAR FAULT_CONTROLLER: { START, STOP };
INIT FAULT_CONTROLLER = START

ASSIGN
next(FAULT_CONTROLLER) := 
case
(FAULT_CONTROLLER = START & FALSE): STOP;
1: FAULT_CONTROLLER;
esac;

TRANS (output != UNDEF | input != UNDEF) -> (FAULT_CONTROLLER != STOP)

-------------------------------------
--   I N I T I A L I Z A T I O N   --
-------------------------------------
INIT ((SetupTrip = INITIAL) & (ProvideTrip = TRIP_REQUEST_ACCOMPLISHED))
INIT ((service148 = ST0) & (service149 = ST0))

-----------------
--   G O A L   --
-----------------
MODULE main
VAR dn_main : dn_main;

GOAL ONEOF(
(dn_main.SYNC0), 1;
(dn_main.FAULT_CONTROLLER = STOP), 0

)