-------------------------------
--   C O M P O S I T I O N   --
-------------------------------
MODULE dn_main

---------------------
--   I N P U T S   --
---------------------
IVAR input:
{
UNDEF, service39_UMS_ManagementTrip, service39_UMS_TripManagementRequest, service39_RESET, service41_UMS_TripRequest, service41_RESET
};

-----------------------
--   O U T P U T S   --
-----------------------
IVAR output:
{
UNDEF, service39_UMS_concrete4, service39_UMS_concrete3, service39_UMS_concrete2, service39_UMS_concrete1, service39_UMS_TripManagementReply, service39_UMS_concrete5, service41_UMS_TripAlternatives, service41_UMS_concrete2, service41_UMS_concrete1
};

-------------------------
--   S E R V I C E S   --
-------------------------
--	 service39
VAR service39: {ST2, ST1, ST4, ST3, ST6, ST5, ST8, ST7, ST0};
ASSIGN
next(service39) :=
case
(service39 = ST4 & output = service39_UMS_concrete4): ST5;
(service39 = ST2 & input = service39_RESET): ST0;
(service39 = ST5 & input = service39_RESET): ST0;
(service39 = ST6 & input = service39_UMS_ManagementTrip): ST7;
(service39 = ST4 & input = service39_RESET): ST0;
(service39 = ST1 & output = service39_UMS_concrete1): ST2;
(service39 = ST2 & output = service39_UMS_concrete2): ST3;
(service39 = ST6 & input = service39_RESET): ST0;
(service39 = ST1 & input = service39_RESET): ST0;
(service39 = ST7 & input = service39_RESET): ST0;
(service39 = ST0 & input = service39_UMS_TripManagementRequest): ST1;
(service39 = ST3 & input = service39_RESET): ST0;
(service39 = ST8 & input = service39_RESET): ST0;
(service39 = ST5 & output = service39_UMS_concrete5): ST6;
(service39 = ST7 & output = service39_UMS_TripManagementReply): ST8;
(service39 = ST3 & output = service39_UMS_concrete3): ST4;
1:service39;
esac;
TRANS ( input = service39_UMS_ManagementTrip) -> (service39 = ST6)
TRANS ( output = service39_UMS_concrete4) -> (service39 = ST4)
TRANS ( input = service39_UMS_TripManagementRequest) -> (service39 = ST0)
TRANS ( output = service39_UMS_concrete3) -> (service39 = ST3)
TRANS ( output = service39_UMS_concrete2) -> (service39 = ST2)
TRANS ( input = service39_RESET) -> (service39 = ST2 | service39 = ST1 | service39 = ST4 | service39 = ST3 | service39 = ST6 | service39 = ST5 | service39 = ST8 | service39 = ST7)
TRANS ( output = service39_UMS_concrete1) -> (service39 = ST1)
TRANS ( output = service39_UMS_TripManagementReply) -> (service39 = ST7)
TRANS ( output = service39_UMS_concrete5) -> (service39 = ST5)

--	 service41
VAR service41: {ST2, ST1, ST4, ST3, ST0};
ASSIGN
next(service41) :=
case
(service41 = ST2 & output = service41_UMS_concrete2): ST3;
(service41 = ST1 & output = service41_UMS_concrete1): ST2;
(service41 = ST3 & output = service41_UMS_TripAlternatives): ST4;
(service41 = ST2 & input = service41_RESET): ST0;
(service41 = ST1 & input = service41_RESET): ST0;
(service41 = ST3 & input = service41_RESET): ST0;
(service41 = ST0 & input = service41_UMS_TripRequest): ST1;
(service41 = ST4 & input = service41_RESET): ST0;
1:service41;
esac;
TRANS ( input = service41_UMS_TripRequest) -> (service41 = ST0)
TRANS ( output = service41_UMS_TripAlternatives) -> (service41 = ST3)
TRANS ( output = service41_UMS_concrete2) -> (service41 = ST2)
TRANS ( input = service41_RESET) -> (service41 = ST2 | service41 = ST1 | service41 = ST4 | service41 = ST3)
TRANS ( output = service41_UMS_concrete1) -> (service41 = ST1)

-----------------------
--   O B J E C T S   --
-----------------------
--	 SetupTrip
VAR SetupTrip: {ORGANIZED, INITIAL};
ASSIGN
next(SetupTrip) :=
case
(input = service39_UMS_ManagementTrip):{(ORGANIZED)};
1:SetupTrip;
esac;
--	 ProvideTrip
VAR ProvideTrip: {TRIP_REQUEST_ACCOMPLISHED, TRIP_MANAGEMENT_ACCOMPLISHED, TRIP_MANAGE_REQUEST_LOADED, TRIP_REQUEST_LOADED, INITIAL};
ASSIGN
next(ProvideTrip) :=
case
(ProvideTrip = TRIP_MANAGE_REQUEST_LOADED & output = service39_UMS_TripManagementReply): TRIP_MANAGEMENT_ACCOMPLISHED;
(ProvideTrip = TRIP_REQUEST_LOADED & output = service41_UMS_TripAlternatives): TRIP_REQUEST_ACCOMPLISHED;
(ProvideTrip = INITIAL & input = service41_UMS_TripRequest): TRIP_REQUEST_LOADED;
(ProvideTrip = TRIP_REQUEST_ACCOMPLISHED & input = service39_UMS_TripManagementRequest): TRIP_MANAGE_REQUEST_LOADED;
1:ProvideTrip;
esac;
-----------------------------------
--   P R E C O N D I T I O N S   --
-----------------------------------
TRANS (input = service39_UMS_TripManagementRequest) -> ((((ProvideTrip = TRIP_REQUEST_ACCOMPLISHED))))
TRANS (output = service39_UMS_TripManagementReply) -> ((((ProvideTrip = TRIP_MANAGE_REQUEST_LOADED))))
TRANS (input = service41_UMS_TripRequest) -> ((((ProvideTrip = INITIAL))))
TRANS (output = service41_UMS_TripAlternatives) -> ((((ProvideTrip = TRIP_REQUEST_LOADED))))
-----------------------------
--   I N V A R I A N T S   --
-----------------------------
--	 Synchronicity
TRANS (1-(input = UNDEF)) + (1-(output = UNDEF)) = 1

---------------------------------------
--   S Y N C H R O N I Z A T I O N   --
---------------------------------------
DEFINE SYNC0 := ((ProvideTrip in {TRIP_REQUEST_ACCOMPLISHED}));
--	 Synchronicity enforcement
TRANS (service39 in {ST2, ST1, ST4, ST3, ST5, ST7}) -> (next(service39) != service39)
TRANS (service41 in {ST2, ST1, ST3}) -> (next(service41) != service41)
-------------------------------
--   C O N T R O L L E R S   --
-------------------------------
--	 Fault Controller
VAR FAULT_CONTROLLER: { START, STOP };
INIT FAULT_CONTROLLER = START

ASSIGN
next(FAULT_CONTROLLER) := 
case
(FAULT_CONTROLLER = START & FALSE): STOP;
1: FAULT_CONTROLLER;
esac;

TRANS (output != UNDEF | input != UNDEF) -> (FAULT_CONTROLLER != STOP)

-------------------------------------
--   I N I T I A L I Z A T I O N   --
-------------------------------------
INIT ((SetupTrip = INITIAL) & (ProvideTrip = INITIAL))
INIT ((service39 = ST0) & (service41 = ST0))

-----------------
--   G O A L   --
-----------------
MODULE main
VAR dn_main : dn_main;

GOAL ONEOF(
(dn_main.SYNC0), 1;
(dn_main.FAULT_CONTROLLER = STOP), 0

)