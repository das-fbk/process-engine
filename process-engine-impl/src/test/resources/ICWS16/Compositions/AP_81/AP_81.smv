-------------------------------
--   C O M P O S I T I O N   --
-------------------------------
MODULE dn_main

---------------------
--   I N P U T S   --
---------------------
IVAR input:
{
UNDEF, service137_UMS_ManagementTrip, service137_UMS_TripManagementRequest, service137_RESET, service138_UMS_TripRequest, service138_RESET
};

-----------------------
--   O U T P U T S   --
-----------------------
IVAR output:
{
UNDEF, service137_UMS_concrete4, service137_UMS_concrete3, service137_UMS_concrete2, service137_UMS_concrete1, service137_UMS_TripManagementReply, service137_UMS_concrete5, service138_UMS_TripAlternatives, service138_UMS_concrete2, service138_UMS_concrete1
};

-------------------------
--   S E R V I C E S   --
-------------------------
--	 service137
VAR service137: {ST2, ST1, ST4, ST3, ST6, ST5, ST8, ST7, ST0};
ASSIGN
next(service137) :=
case
(service137 = ST0 & input = service137_UMS_TripManagementRequest): ST1;
(service137 = ST2 & input = service137_RESET): ST0;
(service137 = ST3 & output = service137_UMS_concrete3): ST4;
(service137 = ST5 & output = service137_UMS_concrete5): ST6;
(service137 = ST2 & output = service137_UMS_concrete2): ST3;
(service137 = ST4 & output = service137_UMS_concrete4): ST5;
(service137 = ST4 & input = service137_RESET): ST0;
(service137 = ST6 & input = service137_UMS_ManagementTrip): ST7;
(service137 = ST6 & input = service137_RESET): ST0;
(service137 = ST7 & input = service137_RESET): ST0;
(service137 = ST5 & input = service137_RESET): ST0;
(service137 = ST1 & input = service137_RESET): ST0;
(service137 = ST3 & input = service137_RESET): ST0;
(service137 = ST1 & output = service137_UMS_concrete1): ST2;
(service137 = ST7 & output = service137_UMS_TripManagementReply): ST8;
(service137 = ST8 & input = service137_RESET): ST0;
1:service137;
esac;
TRANS ( input = service137_UMS_ManagementTrip) -> (service137 = ST6)
TRANS ( output = service137_UMS_concrete4) -> (service137 = ST4)
TRANS ( input = service137_UMS_TripManagementRequest) -> (service137 = ST0)
TRANS ( output = service137_UMS_concrete3) -> (service137 = ST3)
TRANS ( output = service137_UMS_concrete2) -> (service137 = ST2)
TRANS ( input = service137_RESET) -> (service137 = ST2 | service137 = ST1 | service137 = ST4 | service137 = ST3 | service137 = ST6 | service137 = ST5 | service137 = ST8 | service137 = ST7)
TRANS ( output = service137_UMS_concrete1) -> (service137 = ST1)
TRANS ( output = service137_UMS_TripManagementReply) -> (service137 = ST7)
TRANS ( output = service137_UMS_concrete5) -> (service137 = ST5)

--	 service138
VAR service138: {ST2, ST1, ST4, ST3, ST0};
ASSIGN
next(service138) :=
case
(service138 = ST4 & input = service138_RESET): ST0;
(service138 = ST3 & output = service138_UMS_TripAlternatives): ST4;
(service138 = ST3 & input = service138_RESET): ST0;
(service138 = ST2 & output = service138_UMS_concrete2): ST3;
(service138 = ST2 & input = service138_RESET): ST0;
(service138 = ST0 & input = service138_UMS_TripRequest): ST1;
(service138 = ST1 & input = service138_RESET): ST0;
(service138 = ST1 & output = service138_UMS_concrete1): ST2;
1:service138;
esac;
TRANS ( input = service138_UMS_TripRequest) -> (service138 = ST0)
TRANS ( output = service138_UMS_TripAlternatives) -> (service138 = ST3)
TRANS ( output = service138_UMS_concrete2) -> (service138 = ST2)
TRANS ( input = service138_RESET) -> (service138 = ST2 | service138 = ST1 | service138 = ST4 | service138 = ST3)
TRANS ( output = service138_UMS_concrete1) -> (service138 = ST1)

-----------------------
--   O B J E C T S   --
-----------------------
--	 SetupTrip
VAR SetupTrip: {ORGANIZED, INITIAL};
ASSIGN
next(SetupTrip) :=
case
(input = service137_UMS_ManagementTrip):{(ORGANIZED)};
1:SetupTrip;
esac;
--	 ProvideTrip
VAR ProvideTrip: {TRIP_REQUEST_ACCOMPLISHED, TRIP_MANAGEMENT_ACCOMPLISHED, TRIP_MANAGE_REQUEST_LOADED, TRIP_REQUEST_LOADED, INITIAL};
ASSIGN
next(ProvideTrip) :=
case
(ProvideTrip = TRIP_REQUEST_LOADED & output = service138_UMS_TripAlternatives): TRIP_REQUEST_ACCOMPLISHED;
(ProvideTrip = TRIP_REQUEST_ACCOMPLISHED & input = service137_UMS_TripManagementRequest): TRIP_MANAGE_REQUEST_LOADED;
(ProvideTrip = TRIP_MANAGE_REQUEST_LOADED & output = service137_UMS_TripManagementReply): TRIP_MANAGEMENT_ACCOMPLISHED;
(ProvideTrip = INITIAL & input = service138_UMS_TripRequest): TRIP_REQUEST_LOADED;
1:ProvideTrip;
esac;
-----------------------------------
--   P R E C O N D I T I O N S   --
-----------------------------------
TRANS (input = service137_UMS_TripManagementRequest) -> ((((ProvideTrip = TRIP_REQUEST_ACCOMPLISHED))))
TRANS (output = service137_UMS_TripManagementReply) -> ((((ProvideTrip = TRIP_MANAGE_REQUEST_LOADED))))
TRANS (input = service138_UMS_TripRequest) -> ((((ProvideTrip = INITIAL))))
TRANS (output = service138_UMS_TripAlternatives) -> ((((ProvideTrip = TRIP_REQUEST_LOADED))))
-----------------------------
--   I N V A R I A N T S   --
-----------------------------
--	 Synchronicity
TRANS (1-(input = UNDEF)) + (1-(output = UNDEF)) = 1

---------------------------------------
--   S Y N C H R O N I Z A T I O N   --
---------------------------------------
DEFINE SYNC0 := ((ProvideTrip in {TRIP_MANAGEMENT_ACCOMPLISHED}));
--	 Synchronicity enforcement
TRANS (service137 in {ST2, ST1, ST4, ST3, ST5, ST7}) -> (next(service137) != service137)
TRANS (service138 in {ST2, ST1, ST3}) -> (next(service138) != service138)
-------------------------------
--   C O N T R O L L E R S   --
-------------------------------
--	 Fault Controller
VAR FAULT_CONTROLLER: { START, STOP };
INIT FAULT_CONTROLLER = START

ASSIGN
next(FAULT_CONTROLLER) := 
case
(FAULT_CONTROLLER = START & FALSE): STOP;
1: FAULT_CONTROLLER;
esac;

TRANS (output != UNDEF | input != UNDEF) -> (FAULT_CONTROLLER != STOP)

-------------------------------------
--   I N I T I A L I Z A T I O N   --
-------------------------------------
INIT ((SetupTrip = INITIAL) & (ProvideTrip = TRIP_REQUEST_ACCOMPLISHED))
INIT ((service137 = ST0) & (service138 = ST0))

-----------------
--   G O A L   --
-----------------
MODULE main
VAR dn_main : dn_main;

GOAL ONEOF(
(dn_main.SYNC0), 1;
(dn_main.FAULT_CONTROLLER = STOP), 0

)