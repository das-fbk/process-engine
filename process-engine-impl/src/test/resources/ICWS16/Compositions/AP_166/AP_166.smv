-------------------------------
--   C O M P O S I T I O N   --
-------------------------------
MODULE dn_main

---------------------
--   I N P U T S   --
---------------------
IVAR input:
{
UNDEF, service372_SMC_ActivateServiceRequest, service372_RESET, service373_SMC_SubscribeAndPay, service373_RESET, service373_SMC_MobilityCardRequest
};

-----------------------
--   O U T P U T S   --
-----------------------
IVAR output:
{
UNDEF, service372_SMC_ActivateServiceReply, service373_SMC_MobilityCardReply
};

-------------------------
--   S E R V I C E S   --
-------------------------
--	 service372
VAR service372: {ST2, ST1, ST0};
ASSIGN
next(service372) :=
case
(service372 = ST2 & input = service372_RESET): ST0;
(service372 = ST0 & input = service372_SMC_ActivateServiceRequest): ST1;
(service372 = ST1 & output = service372_SMC_ActivateServiceReply): ST2;
(service372 = ST1 & input = service372_RESET): ST0;
1:service372;
esac;
TRANS ( input = service372_SMC_ActivateServiceRequest) -> (service372 = ST0)
TRANS ( input = service372_RESET) -> (service372 = ST2 | service372 = ST1)
TRANS ( output = service372_SMC_ActivateServiceReply) -> (service372 = ST1)

--	 service373
VAR service373: {ST2, ST1, ST3, ST0};
ASSIGN
next(service373) :=
case
(service373 = ST2 & output = service373_SMC_MobilityCardReply): ST3;
(service373 = ST2 & input = service373_RESET): ST0;
(service373 = ST1 & input = service373_RESET): ST0;
(service373 = ST0 & input = service373_SMC_MobilityCardRequest): ST1;
(service373 = ST1 & input = service373_SMC_SubscribeAndPay): ST2;
(service373 = ST3 & input = service373_RESET): ST0;
1:service373;
esac;
TRANS ( input = service373_SMC_SubscribeAndPay) -> (service373 = ST1)
TRANS ( output = service373_SMC_MobilityCardReply) -> (service373 = ST2)
TRANS ( input = service373_RESET) -> (service373 = ST2 | service373 = ST1 | service373 = ST3)
TRANS ( input = service373_SMC_MobilityCardRequest) -> (service373 = ST0)

-----------------------
--   O B J E C T S   --
-----------------------
--	 HandlePayment
VAR HandlePayment: {PAYMENT_EXECUTED, INITIAL};
ASSIGN
next(HandlePayment) :=
case
(input = service373_SMC_SubscribeAndPay):{(PAYMENT_EXECUTED)};
1:HandlePayment;
esac;
--	 HandleSmartMobilityCard
VAR HandleSmartMobilityCard: {INITIAL, SUBSCRIPTION, CARD_RELEASED, SERVICE_ADDED};
ASSIGN
next(HandleSmartMobilityCard) :=
case
(HandleSmartMobilityCard = CARD_RELEASED & output = service372_SMC_ActivateServiceReply): SERVICE_ADDED;
(HandleSmartMobilityCard = INITIAL & input = service373_SMC_SubscribeAndPay): SUBSCRIPTION;
(HandleSmartMobilityCard = SUBSCRIPTION & output = service373_SMC_MobilityCardReply): CARD_RELEASED;
1:HandleSmartMobilityCard;
esac;
-----------------------------------
--   P R E C O N D I T I O N S   --
-----------------------------------
TRANS (input = service372_SMC_ActivateServiceRequest) -> ((((HandleSmartMobilityCard = CARD_RELEASED))))
TRANS (input = service373_SMC_MobilityCardRequest) -> ((((HandleSmartMobilityCard = INITIAL))))
TRANS (output = service373_SMC_MobilityCardReply) -> ((((HandleSmartMobilityCard = SUBSCRIPTION))))
-----------------------------
--   I N V A R I A N T S   --
-----------------------------
--	 Synchronicity
TRANS (1-(input = UNDEF)) + (1-(output = UNDEF)) = 1

---------------------------------------
--   S Y N C H R O N I Z A T I O N   --
---------------------------------------
DEFINE SYNC0 := ((HandleSmartMobilityCard in {SERVICE_ADDED}));
--	 Synchronicity enforcement
TRANS (service372 in {ST1}) -> (next(service372) != service372)
TRANS (service373 in {ST2}) -> (next(service373) != service373)
-------------------------------
--   C O N T R O L L E R S   --
-------------------------------
--	 Fault Controller
VAR FAULT_CONTROLLER: { START, STOP };
INIT FAULT_CONTROLLER = START

ASSIGN
next(FAULT_CONTROLLER) := 
case
(FAULT_CONTROLLER = START & FALSE): STOP;
1: FAULT_CONTROLLER;
esac;

TRANS (output != UNDEF | input != UNDEF) -> (FAULT_CONTROLLER != STOP)

-------------------------------------
--   I N I T I A L I Z A T I O N   --
-------------------------------------
INIT ((HandlePayment = INITIAL) & (HandleSmartMobilityCard = INITIAL))
INIT ((service372 = ST0) & (service373 = ST0))

-----------------
--   G O A L   --
-----------------
MODULE main
VAR dn_main : dn_main;

GOAL ONEOF(
(dn_main.SYNC0), 1;
(dn_main.FAULT_CONTROLLER = STOP), 0

)