-------------------------------
--   C O M P O S I T I O N   --
-------------------------------
MODULE dn_main

---------------------
--   I N P U T S   --
---------------------
IVAR input:
{
UNDEF, service44_UMS_ManagementTrip, service44_UMS_TripManagementRequest, service44_RESET, service45_UMS_TripRequest, service45_RESET
};

-----------------------
--   O U T P U T S   --
-----------------------
IVAR output:
{
UNDEF, service44_UMS_concrete4, service44_UMS_concrete3, service44_UMS_concrete2, service44_UMS_concrete1, service44_UMS_TripManagementReply, service44_UMS_concrete5, service45_UMS_TripAlternatives, service45_UMS_concrete2, service45_UMS_concrete1
};

-------------------------
--   S E R V I C E S   --
-------------------------
--	 service44
VAR service44: {ST2, ST1, ST4, ST3, ST6, ST5, ST8, ST7, ST0};
ASSIGN
next(service44) :=
case
(service44 = ST1 & input = service44_RESET): ST0;
(service44 = ST4 & input = service44_RESET): ST0;
(service44 = ST0 & input = service44_UMS_TripManagementRequest): ST1;
(service44 = ST6 & input = service44_RESET): ST0;
(service44 = ST3 & input = service44_RESET): ST0;
(service44 = ST1 & output = service44_UMS_concrete1): ST2;
(service44 = ST2 & output = service44_UMS_concrete2): ST3;
(service44 = ST5 & output = service44_UMS_concrete5): ST6;
(service44 = ST2 & input = service44_RESET): ST0;
(service44 = ST4 & output = service44_UMS_concrete4): ST5;
(service44 = ST3 & output = service44_UMS_concrete3): ST4;
(service44 = ST6 & input = service44_UMS_ManagementTrip): ST7;
(service44 = ST7 & output = service44_UMS_TripManagementReply): ST8;
(service44 = ST7 & input = service44_RESET): ST0;
(service44 = ST5 & input = service44_RESET): ST0;
(service44 = ST8 & input = service44_RESET): ST0;
1:service44;
esac;
TRANS ( input = service44_UMS_ManagementTrip) -> (service44 = ST6)
TRANS ( output = service44_UMS_concrete4) -> (service44 = ST4)
TRANS ( input = service44_UMS_TripManagementRequest) -> (service44 = ST0)
TRANS ( output = service44_UMS_concrete3) -> (service44 = ST3)
TRANS ( output = service44_UMS_concrete2) -> (service44 = ST2)
TRANS ( input = service44_RESET) -> (service44 = ST2 | service44 = ST1 | service44 = ST4 | service44 = ST3 | service44 = ST6 | service44 = ST5 | service44 = ST8 | service44 = ST7)
TRANS ( output = service44_UMS_concrete1) -> (service44 = ST1)
TRANS ( output = service44_UMS_TripManagementReply) -> (service44 = ST7)
TRANS ( output = service44_UMS_concrete5) -> (service44 = ST5)

--	 service45
VAR service45: {ST2, ST1, ST4, ST3, ST0};
ASSIGN
next(service45) :=
case
(service45 = ST1 & output = service45_UMS_concrete1): ST2;
(service45 = ST1 & input = service45_RESET): ST0;
(service45 = ST0 & input = service45_UMS_TripRequest): ST1;
(service45 = ST2 & output = service45_UMS_concrete2): ST3;
(service45 = ST3 & input = service45_RESET): ST0;
(service45 = ST3 & output = service45_UMS_TripAlternatives): ST4;
(service45 = ST2 & input = service45_RESET): ST0;
(service45 = ST4 & input = service45_RESET): ST0;
1:service45;
esac;
TRANS ( input = service45_UMS_TripRequest) -> (service45 = ST0)
TRANS ( output = service45_UMS_TripAlternatives) -> (service45 = ST3)
TRANS ( output = service45_UMS_concrete2) -> (service45 = ST2)
TRANS ( input = service45_RESET) -> (service45 = ST2 | service45 = ST1 | service45 = ST4 | service45 = ST3)
TRANS ( output = service45_UMS_concrete1) -> (service45 = ST1)

-----------------------
--   O B J E C T S   --
-----------------------
--	 SetupTrip
VAR SetupTrip: {ORGANIZED, INITIAL};
ASSIGN
next(SetupTrip) :=
case
(input = service44_UMS_ManagementTrip):{(ORGANIZED)};
1:SetupTrip;
esac;
--	 ProvideTrip
VAR ProvideTrip: {TRIP_REQUEST_ACCOMPLISHED, TRIP_MANAGEMENT_ACCOMPLISHED, TRIP_MANAGE_REQUEST_LOADED, TRIP_REQUEST_LOADED, INITIAL};
ASSIGN
next(ProvideTrip) :=
case
(ProvideTrip = TRIP_REQUEST_LOADED & output = service45_UMS_TripAlternatives): TRIP_REQUEST_ACCOMPLISHED;
(ProvideTrip = TRIP_REQUEST_ACCOMPLISHED & input = service44_UMS_TripManagementRequest): TRIP_MANAGE_REQUEST_LOADED;
(ProvideTrip = TRIP_MANAGE_REQUEST_LOADED & output = service44_UMS_TripManagementReply): TRIP_MANAGEMENT_ACCOMPLISHED;
(ProvideTrip = INITIAL & input = service45_UMS_TripRequest): TRIP_REQUEST_LOADED;
1:ProvideTrip;
esac;
-----------------------------------
--   P R E C O N D I T I O N S   --
-----------------------------------
TRANS (input = service44_UMS_TripManagementRequest) -> ((((ProvideTrip = TRIP_REQUEST_ACCOMPLISHED))))
TRANS (output = service44_UMS_TripManagementReply) -> ((((ProvideTrip = TRIP_MANAGE_REQUEST_LOADED))))
TRANS (input = service45_UMS_TripRequest) -> ((((ProvideTrip = INITIAL))))
TRANS (output = service45_UMS_TripAlternatives) -> ((((ProvideTrip = TRIP_REQUEST_LOADED))))
-----------------------------
--   I N V A R I A N T S   --
-----------------------------
--	 Synchronicity
TRANS (1-(input = UNDEF)) + (1-(output = UNDEF)) = 1

---------------------------------------
--   S Y N C H R O N I Z A T I O N   --
---------------------------------------
DEFINE SYNC0 := ((ProvideTrip in {TRIP_REQUEST_ACCOMPLISHED}));
--	 Synchronicity enforcement
TRANS (service44 in {ST2, ST1, ST4, ST3, ST5, ST7}) -> (next(service44) != service44)
TRANS (service45 in {ST2, ST1, ST3}) -> (next(service45) != service45)
-------------------------------
--   C O N T R O L L E R S   --
-------------------------------
--	 Fault Controller
VAR FAULT_CONTROLLER: { START, STOP };
INIT FAULT_CONTROLLER = START

ASSIGN
next(FAULT_CONTROLLER) := 
case
(FAULT_CONTROLLER = START & FALSE): STOP;
1: FAULT_CONTROLLER;
esac;

TRANS (output != UNDEF | input != UNDEF) -> (FAULT_CONTROLLER != STOP)

-------------------------------------
--   I N I T I A L I Z A T I O N   --
-------------------------------------
INIT ((SetupTrip = INITIAL) & (ProvideTrip = INITIAL))
INIT ((service44 = ST0) & (service45 = ST0))

-----------------
--   G O A L   --
-----------------
MODULE main
VAR dn_main : dn_main;

GOAL ONEOF(
(dn_main.SYNC0), 1;
(dn_main.FAULT_CONTROLLER = STOP), 0

)