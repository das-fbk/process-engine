-------------------------------
--   C O M P O S I T I O N   --
-------------------------------
MODULE dn_main

---------------------
--   I N P U T S   --
---------------------
IVAR input:
{
UNDEF, service159_UMS_ManagementTrip, service159_UMS_TripManagementRequest, service159_RESET, service161_UMS_TripRequest, service161_RESET
};

-----------------------
--   O U T P U T S   --
-----------------------
IVAR output:
{
UNDEF, service159_UMS_concrete4, service159_UMS_concrete3, service159_UMS_concrete2, service159_UMS_concrete1, service159_UMS_TripManagementReply, service159_UMS_concrete5, service161_UMS_TripAlternatives, service161_UMS_concrete2, service161_UMS_concrete1
};

-------------------------
--   S E R V I C E S   --
-------------------------
--	 service159
VAR service159: {ST2, ST1, ST4, ST3, ST6, ST5, ST8, ST7, ST0};
ASSIGN
next(service159) :=
case
(service159 = ST4 & output = service159_UMS_concrete4): ST5;
(service159 = ST4 & input = service159_RESET): ST0;
(service159 = ST2 & output = service159_UMS_concrete2): ST3;
(service159 = ST8 & input = service159_RESET): ST0;
(service159 = ST5 & input = service159_RESET): ST0;
(service159 = ST0 & input = service159_UMS_TripManagementRequest): ST1;
(service159 = ST3 & output = service159_UMS_concrete3): ST4;
(service159 = ST6 & input = service159_UMS_ManagementTrip): ST7;
(service159 = ST3 & input = service159_RESET): ST0;
(service159 = ST7 & input = service159_RESET): ST0;
(service159 = ST7 & output = service159_UMS_TripManagementReply): ST8;
(service159 = ST5 & output = service159_UMS_concrete5): ST6;
(service159 = ST2 & input = service159_RESET): ST0;
(service159 = ST6 & input = service159_RESET): ST0;
(service159 = ST1 & output = service159_UMS_concrete1): ST2;
(service159 = ST1 & input = service159_RESET): ST0;
1:service159;
esac;
TRANS ( input = service159_UMS_ManagementTrip) -> (service159 = ST6)
TRANS ( output = service159_UMS_concrete4) -> (service159 = ST4)
TRANS ( input = service159_UMS_TripManagementRequest) -> (service159 = ST0)
TRANS ( output = service159_UMS_concrete3) -> (service159 = ST3)
TRANS ( output = service159_UMS_concrete2) -> (service159 = ST2)
TRANS ( input = service159_RESET) -> (service159 = ST2 | service159 = ST1 | service159 = ST4 | service159 = ST3 | service159 = ST6 | service159 = ST5 | service159 = ST8 | service159 = ST7)
TRANS ( output = service159_UMS_concrete1) -> (service159 = ST1)
TRANS ( output = service159_UMS_TripManagementReply) -> (service159 = ST7)
TRANS ( output = service159_UMS_concrete5) -> (service159 = ST5)

--	 service161
VAR service161: {ST2, ST1, ST4, ST3, ST0};
ASSIGN
next(service161) :=
case
(service161 = ST1 & output = service161_UMS_concrete1): ST2;
(service161 = ST4 & input = service161_RESET): ST0;
(service161 = ST1 & input = service161_RESET): ST0;
(service161 = ST3 & output = service161_UMS_TripAlternatives): ST4;
(service161 = ST2 & output = service161_UMS_concrete2): ST3;
(service161 = ST3 & input = service161_RESET): ST0;
(service161 = ST2 & input = service161_RESET): ST0;
(service161 = ST0 & input = service161_UMS_TripRequest): ST1;
1:service161;
esac;
TRANS ( input = service161_UMS_TripRequest) -> (service161 = ST0)
TRANS ( output = service161_UMS_TripAlternatives) -> (service161 = ST3)
TRANS ( output = service161_UMS_concrete2) -> (service161 = ST2)
TRANS ( input = service161_RESET) -> (service161 = ST2 | service161 = ST1 | service161 = ST4 | service161 = ST3)
TRANS ( output = service161_UMS_concrete1) -> (service161 = ST1)

-----------------------
--   O B J E C T S   --
-----------------------
--	 SetupTrip
VAR SetupTrip: {ORGANIZED, INITIAL};
ASSIGN
next(SetupTrip) :=
case
(input = service159_UMS_ManagementTrip):{(ORGANIZED)};
1:SetupTrip;
esac;
--	 ProvideTrip
VAR ProvideTrip: {TRIP_REQUEST_ACCOMPLISHED, TRIP_MANAGEMENT_ACCOMPLISHED, TRIP_MANAGE_REQUEST_LOADED, TRIP_REQUEST_LOADED, INITIAL};
ASSIGN
next(ProvideTrip) :=
case
(ProvideTrip = INITIAL & input = service161_UMS_TripRequest): TRIP_REQUEST_LOADED;
(ProvideTrip = TRIP_MANAGE_REQUEST_LOADED & output = service159_UMS_TripManagementReply): TRIP_MANAGEMENT_ACCOMPLISHED;
(ProvideTrip = TRIP_REQUEST_LOADED & output = service161_UMS_TripAlternatives): TRIP_REQUEST_ACCOMPLISHED;
(ProvideTrip = TRIP_REQUEST_ACCOMPLISHED & input = service159_UMS_TripManagementRequest): TRIP_MANAGE_REQUEST_LOADED;
1:ProvideTrip;
esac;
-----------------------------------
--   P R E C O N D I T I O N S   --
-----------------------------------
TRANS (input = service159_UMS_TripManagementRequest) -> ((((ProvideTrip = TRIP_REQUEST_ACCOMPLISHED))))
TRANS (output = service159_UMS_TripManagementReply) -> ((((ProvideTrip = TRIP_MANAGE_REQUEST_LOADED))))
TRANS (input = service161_UMS_TripRequest) -> ((((ProvideTrip = INITIAL))))
TRANS (output = service161_UMS_TripAlternatives) -> ((((ProvideTrip = TRIP_REQUEST_LOADED))))
-----------------------------
--   I N V A R I A N T S   --
-----------------------------
--	 Synchronicity
TRANS (1-(input = UNDEF)) + (1-(output = UNDEF)) = 1

---------------------------------------
--   S Y N C H R O N I Z A T I O N   --
---------------------------------------
DEFINE SYNC0 := ((ProvideTrip in {TRIP_MANAGEMENT_ACCOMPLISHED}));
--	 Synchronicity enforcement
TRANS (service159 in {ST2, ST1, ST4, ST3, ST5, ST7}) -> (next(service159) != service159)
TRANS (service161 in {ST2, ST1, ST3}) -> (next(service161) != service161)
-------------------------------
--   C O N T R O L L E R S   --
-------------------------------
--	 Fault Controller
VAR FAULT_CONTROLLER: { START, STOP };
INIT FAULT_CONTROLLER = START

ASSIGN
next(FAULT_CONTROLLER) := 
case
(FAULT_CONTROLLER = START & FALSE): STOP;
1: FAULT_CONTROLLER;
esac;

TRANS (output != UNDEF | input != UNDEF) -> (FAULT_CONTROLLER != STOP)

-------------------------------------
--   I N I T I A L I Z A T I O N   --
-------------------------------------
INIT ((SetupTrip = INITIAL) & (ProvideTrip = TRIP_REQUEST_ACCOMPLISHED))
INIT ((service159 = ST0) & (service161 = ST0))

-----------------
--   G O A L   --
-----------------
MODULE main
VAR dn_main : dn_main;

GOAL ONEOF(
(dn_main.SYNC0), 1;
(dn_main.FAULT_CONTROLLER = STOP), 0

)