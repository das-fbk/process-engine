-------------------------------
--   C O M P O S I T I O N   --
-------------------------------
MODULE dn_main

---------------------
--   I N P U T S   --
---------------------
IVAR input:
{
UNDEF, service123_UMS_ManagementTrip, service123_UMS_TripManagementRequest, service123_RESET, service126_UMS_TripRequest, service126_RESET
};

-----------------------
--   O U T P U T S   --
-----------------------
IVAR output:
{
UNDEF, service123_UMS_concrete4, service123_UMS_concrete3, service123_UMS_concrete2, service123_UMS_concrete1, service123_UMS_TripManagementReply, service123_UMS_concrete5, service126_UMS_TripAlternatives, service126_UMS_concrete2, service126_UMS_concrete1
};

-------------------------
--   S E R V I C E S   --
-------------------------
--	 service123
VAR service123: {ST2, ST1, ST4, ST3, ST6, ST5, ST8, ST7, ST0};
ASSIGN
next(service123) :=
case
(service123 = ST4 & output = service123_UMS_concrete4): ST5;
(service123 = ST7 & output = service123_UMS_TripManagementReply): ST8;
(service123 = ST3 & output = service123_UMS_concrete3): ST4;
(service123 = ST1 & input = service123_RESET): ST0;
(service123 = ST5 & input = service123_RESET): ST0;
(service123 = ST5 & output = service123_UMS_concrete5): ST6;
(service123 = ST6 & input = service123_RESET): ST0;
(service123 = ST1 & output = service123_UMS_concrete1): ST2;
(service123 = ST2 & input = service123_RESET): ST0;
(service123 = ST4 & input = service123_RESET): ST0;
(service123 = ST7 & input = service123_RESET): ST0;
(service123 = ST8 & input = service123_RESET): ST0;
(service123 = ST6 & input = service123_UMS_ManagementTrip): ST7;
(service123 = ST3 & input = service123_RESET): ST0;
(service123 = ST0 & input = service123_UMS_TripManagementRequest): ST1;
(service123 = ST2 & output = service123_UMS_concrete2): ST3;
1:service123;
esac;
TRANS ( input = service123_UMS_ManagementTrip) -> (service123 = ST6)
TRANS ( output = service123_UMS_concrete4) -> (service123 = ST4)
TRANS ( input = service123_UMS_TripManagementRequest) -> (service123 = ST0)
TRANS ( output = service123_UMS_concrete3) -> (service123 = ST3)
TRANS ( output = service123_UMS_concrete2) -> (service123 = ST2)
TRANS ( input = service123_RESET) -> (service123 = ST2 | service123 = ST1 | service123 = ST4 | service123 = ST3 | service123 = ST6 | service123 = ST5 | service123 = ST8 | service123 = ST7)
TRANS ( output = service123_UMS_concrete1) -> (service123 = ST1)
TRANS ( output = service123_UMS_TripManagementReply) -> (service123 = ST7)
TRANS ( output = service123_UMS_concrete5) -> (service123 = ST5)

--	 service126
VAR service126: {ST2, ST1, ST4, ST3, ST0};
ASSIGN
next(service126) :=
case
(service126 = ST4 & input = service126_RESET): ST0;
(service126 = ST1 & output = service126_UMS_concrete1): ST2;
(service126 = ST2 & output = service126_UMS_concrete2): ST3;
(service126 = ST3 & input = service126_RESET): ST0;
(service126 = ST1 & input = service126_RESET): ST0;
(service126 = ST2 & input = service126_RESET): ST0;
(service126 = ST0 & input = service126_UMS_TripRequest): ST1;
(service126 = ST3 & output = service126_UMS_TripAlternatives): ST4;
1:service126;
esac;
TRANS ( input = service126_UMS_TripRequest) -> (service126 = ST0)
TRANS ( output = service126_UMS_TripAlternatives) -> (service126 = ST3)
TRANS ( output = service126_UMS_concrete2) -> (service126 = ST2)
TRANS ( input = service126_RESET) -> (service126 = ST2 | service126 = ST1 | service126 = ST4 | service126 = ST3)
TRANS ( output = service126_UMS_concrete1) -> (service126 = ST1)

-----------------------
--   O B J E C T S   --
-----------------------
--	 SetupTrip
VAR SetupTrip: {ORGANIZED, INITIAL};
ASSIGN
next(SetupTrip) :=
case
(input = service123_UMS_ManagementTrip):{(ORGANIZED)};
1:SetupTrip;
esac;
--	 ProvideTrip
VAR ProvideTrip: {TRIP_REQUEST_ACCOMPLISHED, TRIP_MANAGEMENT_ACCOMPLISHED, TRIP_MANAGE_REQUEST_LOADED, TRIP_REQUEST_LOADED, INITIAL};
ASSIGN
next(ProvideTrip) :=
case
(ProvideTrip = INITIAL & input = service126_UMS_TripRequest): TRIP_REQUEST_LOADED;
(ProvideTrip = TRIP_REQUEST_LOADED & output = service126_UMS_TripAlternatives): TRIP_REQUEST_ACCOMPLISHED;
(ProvideTrip = TRIP_REQUEST_ACCOMPLISHED & input = service123_UMS_TripManagementRequest): TRIP_MANAGE_REQUEST_LOADED;
(ProvideTrip = TRIP_MANAGE_REQUEST_LOADED & output = service123_UMS_TripManagementReply): TRIP_MANAGEMENT_ACCOMPLISHED;
1:ProvideTrip;
esac;
-----------------------------------
--   P R E C O N D I T I O N S   --
-----------------------------------
TRANS (input = service123_UMS_TripManagementRequest) -> ((((ProvideTrip = TRIP_REQUEST_ACCOMPLISHED))))
TRANS (output = service123_UMS_TripManagementReply) -> ((((ProvideTrip = TRIP_MANAGE_REQUEST_LOADED))))
TRANS (input = service126_UMS_TripRequest) -> ((((ProvideTrip = INITIAL))))
TRANS (output = service126_UMS_TripAlternatives) -> ((((ProvideTrip = TRIP_REQUEST_LOADED))))
-----------------------------
--   I N V A R I A N T S   --
-----------------------------
--	 Synchronicity
TRANS (1-(input = UNDEF)) + (1-(output = UNDEF)) = 1

---------------------------------------
--   S Y N C H R O N I Z A T I O N   --
---------------------------------------
DEFINE SYNC0 := ((ProvideTrip in {TRIP_MANAGEMENT_ACCOMPLISHED}));
--	 Synchronicity enforcement
TRANS (service123 in {ST2, ST1, ST4, ST3, ST5, ST7}) -> (next(service123) != service123)
TRANS (service126 in {ST2, ST1, ST3}) -> (next(service126) != service126)
-------------------------------
--   C O N T R O L L E R S   --
-------------------------------
--	 Fault Controller
VAR FAULT_CONTROLLER: { START, STOP };
INIT FAULT_CONTROLLER = START

ASSIGN
next(FAULT_CONTROLLER) := 
case
(FAULT_CONTROLLER = START & FALSE): STOP;
1: FAULT_CONTROLLER;
esac;

TRANS (output != UNDEF | input != UNDEF) -> (FAULT_CONTROLLER != STOP)

-------------------------------------
--   I N I T I A L I Z A T I O N   --
-------------------------------------
INIT ((SetupTrip = INITIAL) & (ProvideTrip = TRIP_REQUEST_ACCOMPLISHED))
INIT ((service123 = ST0) & (service126 = ST0))

-----------------
--   G O A L   --
-----------------
MODULE main
VAR dn_main : dn_main;

GOAL ONEOF(
(dn_main.SYNC0), 1;
(dn_main.FAULT_CONTROLLER = STOP), 0

)