-------------------------------
--   C O M P O S I T I O N   --
-------------------------------
MODULE dn_main

---------------------
--   I N P U T S   --
---------------------
IVAR input:
{
UNDEF, service135_UMS_ManagementTrip, service135_UMS_TripManagementRequest, service135_RESET, service136_UMS_TripRequest, service136_RESET
};

-----------------------
--   O U T P U T S   --
-----------------------
IVAR output:
{
UNDEF, service135_UMS_concrete4, service135_UMS_concrete3, service135_UMS_concrete2, service135_UMS_concrete1, service135_UMS_TripManagementReply, service135_UMS_concrete5, service136_UMS_TripAlternatives, service136_UMS_concrete2, service136_UMS_concrete1
};

-------------------------
--   S E R V I C E S   --
-------------------------
--	 service135
VAR service135: {ST2, ST1, ST4, ST3, ST6, ST5, ST8, ST7, ST0};
ASSIGN
next(service135) :=
case
(service135 = ST8 & input = service135_RESET): ST0;
(service135 = ST0 & input = service135_UMS_TripManagementRequest): ST1;
(service135 = ST3 & output = service135_UMS_concrete3): ST4;
(service135 = ST4 & input = service135_RESET): ST0;
(service135 = ST7 & output = service135_UMS_TripManagementReply): ST8;
(service135 = ST2 & output = service135_UMS_concrete2): ST3;
(service135 = ST5 & output = service135_UMS_concrete5): ST6;
(service135 = ST1 & output = service135_UMS_concrete1): ST2;
(service135 = ST5 & input = service135_RESET): ST0;
(service135 = ST7 & input = service135_RESET): ST0;
(service135 = ST6 & input = service135_UMS_ManagementTrip): ST7;
(service135 = ST3 & input = service135_RESET): ST0;
(service135 = ST6 & input = service135_RESET): ST0;
(service135 = ST4 & output = service135_UMS_concrete4): ST5;
(service135 = ST1 & input = service135_RESET): ST0;
(service135 = ST2 & input = service135_RESET): ST0;
1:service135;
esac;
TRANS ( input = service135_UMS_ManagementTrip) -> (service135 = ST6)
TRANS ( output = service135_UMS_concrete4) -> (service135 = ST4)
TRANS ( input = service135_UMS_TripManagementRequest) -> (service135 = ST0)
TRANS ( output = service135_UMS_concrete3) -> (service135 = ST3)
TRANS ( output = service135_UMS_concrete2) -> (service135 = ST2)
TRANS ( input = service135_RESET) -> (service135 = ST2 | service135 = ST1 | service135 = ST4 | service135 = ST3 | service135 = ST6 | service135 = ST5 | service135 = ST8 | service135 = ST7)
TRANS ( output = service135_UMS_concrete1) -> (service135 = ST1)
TRANS ( output = service135_UMS_TripManagementReply) -> (service135 = ST7)
TRANS ( output = service135_UMS_concrete5) -> (service135 = ST5)

--	 service136
VAR service136: {ST2, ST1, ST4, ST3, ST0};
ASSIGN
next(service136) :=
case
(service136 = ST3 & input = service136_RESET): ST0;
(service136 = ST2 & output = service136_UMS_concrete2): ST3;
(service136 = ST2 & input = service136_RESET): ST0;
(service136 = ST1 & input = service136_RESET): ST0;
(service136 = ST0 & input = service136_UMS_TripRequest): ST1;
(service136 = ST1 & output = service136_UMS_concrete1): ST2;
(service136 = ST4 & input = service136_RESET): ST0;
(service136 = ST3 & output = service136_UMS_TripAlternatives): ST4;
1:service136;
esac;
TRANS ( input = service136_UMS_TripRequest) -> (service136 = ST0)
TRANS ( output = service136_UMS_TripAlternatives) -> (service136 = ST3)
TRANS ( output = service136_UMS_concrete2) -> (service136 = ST2)
TRANS ( input = service136_RESET) -> (service136 = ST2 | service136 = ST1 | service136 = ST4 | service136 = ST3)
TRANS ( output = service136_UMS_concrete1) -> (service136 = ST1)

-----------------------
--   O B J E C T S   --
-----------------------
--	 SetupTrip
VAR SetupTrip: {ORGANIZED, INITIAL};
ASSIGN
next(SetupTrip) :=
case
(input = service135_UMS_ManagementTrip):{(ORGANIZED)};
1:SetupTrip;
esac;
--	 ProvideTrip
VAR ProvideTrip: {TRIP_REQUEST_ACCOMPLISHED, TRIP_MANAGEMENT_ACCOMPLISHED, TRIP_MANAGE_REQUEST_LOADED, TRIP_REQUEST_LOADED, INITIAL};
ASSIGN
next(ProvideTrip) :=
case
(ProvideTrip = INITIAL & input = service136_UMS_TripRequest): TRIP_REQUEST_LOADED;
(ProvideTrip = TRIP_REQUEST_LOADED & output = service136_UMS_TripAlternatives): TRIP_REQUEST_ACCOMPLISHED;
(ProvideTrip = TRIP_REQUEST_ACCOMPLISHED & input = service135_UMS_TripManagementRequest): TRIP_MANAGE_REQUEST_LOADED;
(ProvideTrip = TRIP_MANAGE_REQUEST_LOADED & output = service135_UMS_TripManagementReply): TRIP_MANAGEMENT_ACCOMPLISHED;
1:ProvideTrip;
esac;
-----------------------------------
--   P R E C O N D I T I O N S   --
-----------------------------------
TRANS (input = service135_UMS_TripManagementRequest) -> ((((ProvideTrip = TRIP_REQUEST_ACCOMPLISHED))))
TRANS (output = service135_UMS_TripManagementReply) -> ((((ProvideTrip = TRIP_MANAGE_REQUEST_LOADED))))
TRANS (input = service136_UMS_TripRequest) -> ((((ProvideTrip = INITIAL))))
TRANS (output = service136_UMS_TripAlternatives) -> ((((ProvideTrip = TRIP_REQUEST_LOADED))))
-----------------------------
--   I N V A R I A N T S   --
-----------------------------
--	 Synchronicity
TRANS (1-(input = UNDEF)) + (1-(output = UNDEF)) = 1

---------------------------------------
--   S Y N C H R O N I Z A T I O N   --
---------------------------------------
DEFINE SYNC0 := ((ProvideTrip in {TRIP_MANAGEMENT_ACCOMPLISHED}));
--	 Synchronicity enforcement
TRANS (service135 in {ST2, ST1, ST4, ST3, ST5, ST7}) -> (next(service135) != service135)
TRANS (service136 in {ST2, ST1, ST3}) -> (next(service136) != service136)
-------------------------------
--   C O N T R O L L E R S   --
-------------------------------
--	 Fault Controller
VAR FAULT_CONTROLLER: { START, STOP };
INIT FAULT_CONTROLLER = START

ASSIGN
next(FAULT_CONTROLLER) := 
case
(FAULT_CONTROLLER = START & FALSE): STOP;
1: FAULT_CONTROLLER;
esac;

TRANS (output != UNDEF | input != UNDEF) -> (FAULT_CONTROLLER != STOP)

-------------------------------------
--   I N I T I A L I Z A T I O N   --
-------------------------------------
INIT ((SetupTrip = INITIAL) & (ProvideTrip = TRIP_REQUEST_ACCOMPLISHED))
INIT ((service135 = ST0) & (service136 = ST0))

-----------------
--   G O A L   --
-----------------
MODULE main
VAR dn_main : dn_main;

GOAL ONEOF(
(dn_main.SYNC0), 1;
(dn_main.FAULT_CONTROLLER = STOP), 0

)