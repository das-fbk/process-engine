-------------------------------
--   C O M P O S I T I O N   --
-------------------------------
MODULE dn_main

---------------------
--   I N P U T S   --
---------------------
IVAR input:
{
UNDEF, service385_SMC_ActivateServiceRequest, service385_RESET, service386_SMC_SubscribeAndPay, service386_RESET, service386_SMC_MobilityCardRequest
};

-----------------------
--   O U T P U T S   --
-----------------------
IVAR output:
{
UNDEF, service385_SMC_ActivateServiceReply, service386_SMC_MobilityCardReply
};

-------------------------
--   S E R V I C E S   --
-------------------------
--	 service385
VAR service385: {ST2, ST1, ST0};
ASSIGN
next(service385) :=
case
(service385 = ST1 & input = service385_RESET): ST0;
(service385 = ST0 & input = service385_SMC_ActivateServiceRequest): ST1;
(service385 = ST2 & input = service385_RESET): ST0;
(service385 = ST1 & output = service385_SMC_ActivateServiceReply): ST2;
1:service385;
esac;
TRANS ( input = service385_SMC_ActivateServiceRequest) -> (service385 = ST0)
TRANS ( input = service385_RESET) -> (service385 = ST2 | service385 = ST1)
TRANS ( output = service385_SMC_ActivateServiceReply) -> (service385 = ST1)

--	 service386
VAR service386: {ST2, ST1, ST3, ST0};
ASSIGN
next(service386) :=
case
(service386 = ST1 & input = service386_SMC_SubscribeAndPay): ST2;
(service386 = ST3 & input = service386_RESET): ST0;
(service386 = ST1 & input = service386_RESET): ST0;
(service386 = ST0 & input = service386_SMC_MobilityCardRequest): ST1;
(service386 = ST2 & output = service386_SMC_MobilityCardReply): ST3;
(service386 = ST2 & input = service386_RESET): ST0;
1:service386;
esac;
TRANS ( input = service386_SMC_SubscribeAndPay) -> (service386 = ST1)
TRANS ( output = service386_SMC_MobilityCardReply) -> (service386 = ST2)
TRANS ( input = service386_RESET) -> (service386 = ST2 | service386 = ST1 | service386 = ST3)
TRANS ( input = service386_SMC_MobilityCardRequest) -> (service386 = ST0)

-----------------------
--   O B J E C T S   --
-----------------------
--	 HandlePayment
VAR HandlePayment: {PAYMENT_EXECUTED, INITIAL};
ASSIGN
next(HandlePayment) :=
case
(input = service386_SMC_SubscribeAndPay):{(PAYMENT_EXECUTED)};
1:HandlePayment;
esac;
--	 HandleSmartMobilityCard
VAR HandleSmartMobilityCard: {INITIAL, SUBSCRIPTION, CARD_RELEASED, SERVICE_ADDED};
ASSIGN
next(HandleSmartMobilityCard) :=
case
(HandleSmartMobilityCard = CARD_RELEASED & output = service385_SMC_ActivateServiceReply): SERVICE_ADDED;
(HandleSmartMobilityCard = SUBSCRIPTION & output = service386_SMC_MobilityCardReply): CARD_RELEASED;
(HandleSmartMobilityCard = INITIAL & input = service386_SMC_SubscribeAndPay): SUBSCRIPTION;
1:HandleSmartMobilityCard;
esac;
-----------------------------------
--   P R E C O N D I T I O N S   --
-----------------------------------
TRANS (input = service385_SMC_ActivateServiceRequest) -> ((((HandleSmartMobilityCard = CARD_RELEASED))))
TRANS (input = service386_SMC_MobilityCardRequest) -> ((((HandleSmartMobilityCard = INITIAL))))
TRANS (output = service386_SMC_MobilityCardReply) -> ((((HandleSmartMobilityCard = SUBSCRIPTION))))
-----------------------------
--   I N V A R I A N T S   --
-----------------------------
--	 Synchronicity
TRANS (1-(input = UNDEF)) + (1-(output = UNDEF)) = 1

---------------------------------------
--   S Y N C H R O N I Z A T I O N   --
---------------------------------------
DEFINE SYNC0 := ((HandleSmartMobilityCard in {SERVICE_ADDED}));
--	 Synchronicity enforcement
TRANS (service385 in {ST1}) -> (next(service385) != service385)
TRANS (service386 in {ST2}) -> (next(service386) != service386)
-------------------------------
--   C O N T R O L L E R S   --
-------------------------------
--	 Fault Controller
VAR FAULT_CONTROLLER: { START, STOP };
INIT FAULT_CONTROLLER = START

ASSIGN
next(FAULT_CONTROLLER) := 
case
(FAULT_CONTROLLER = START & FALSE): STOP;
1: FAULT_CONTROLLER;
esac;

TRANS (output != UNDEF | input != UNDEF) -> (FAULT_CONTROLLER != STOP)

-------------------------------------
--   I N I T I A L I Z A T I O N   --
-------------------------------------
INIT ((HandlePayment = INITIAL) & (HandleSmartMobilityCard = INITIAL))
INIT ((service385 = ST0) & (service386 = ST0))

-----------------
--   G O A L   --
-----------------
MODULE main
VAR dn_main : dn_main;

GOAL ONEOF(
(dn_main.SYNC0), 1;
(dn_main.FAULT_CONTROLLER = STOP), 0

)