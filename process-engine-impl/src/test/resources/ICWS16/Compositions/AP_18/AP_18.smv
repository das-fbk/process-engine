-------------------------------
--   C O M P O S I T I O N   --
-------------------------------
MODULE dn_main

---------------------
--   I N P U T S   --
---------------------
IVAR input:
{
UNDEF, service38_UMS_ManagementTrip, service38_UMS_TripManagementRequest, service38_RESET, service40_UMS_TripRequest, service40_RESET
};

-----------------------
--   O U T P U T S   --
-----------------------
IVAR output:
{
UNDEF, service38_UMS_concrete4, service38_UMS_concrete3, service38_UMS_concrete2, service38_UMS_concrete1, service38_UMS_TripManagementReply, service38_UMS_concrete5, service40_UMS_TripAlternatives, service40_UMS_concrete2, service40_UMS_concrete1
};

-------------------------
--   S E R V I C E S   --
-------------------------
--	 service38
VAR service38: {ST2, ST1, ST4, ST3, ST6, ST5, ST8, ST7, ST0};
ASSIGN
next(service38) :=
case
(service38 = ST6 & input = service38_UMS_ManagementTrip): ST7;
(service38 = ST1 & input = service38_RESET): ST0;
(service38 = ST2 & output = service38_UMS_concrete2): ST3;
(service38 = ST7 & output = service38_UMS_TripManagementReply): ST8;
(service38 = ST5 & input = service38_RESET): ST0;
(service38 = ST5 & output = service38_UMS_concrete5): ST6;
(service38 = ST6 & input = service38_RESET): ST0;
(service38 = ST2 & input = service38_RESET): ST0;
(service38 = ST7 & input = service38_RESET): ST0;
(service38 = ST1 & output = service38_UMS_concrete1): ST2;
(service38 = ST3 & output = service38_UMS_concrete3): ST4;
(service38 = ST4 & input = service38_RESET): ST0;
(service38 = ST8 & input = service38_RESET): ST0;
(service38 = ST0 & input = service38_UMS_TripManagementRequest): ST1;
(service38 = ST3 & input = service38_RESET): ST0;
(service38 = ST4 & output = service38_UMS_concrete4): ST5;
1:service38;
esac;
TRANS ( input = service38_UMS_ManagementTrip) -> (service38 = ST6)
TRANS ( output = service38_UMS_concrete4) -> (service38 = ST4)
TRANS ( input = service38_UMS_TripManagementRequest) -> (service38 = ST0)
TRANS ( output = service38_UMS_concrete3) -> (service38 = ST3)
TRANS ( output = service38_UMS_concrete2) -> (service38 = ST2)
TRANS ( input = service38_RESET) -> (service38 = ST2 | service38 = ST1 | service38 = ST4 | service38 = ST3 | service38 = ST6 | service38 = ST5 | service38 = ST8 | service38 = ST7)
TRANS ( output = service38_UMS_concrete1) -> (service38 = ST1)
TRANS ( output = service38_UMS_TripManagementReply) -> (service38 = ST7)
TRANS ( output = service38_UMS_concrete5) -> (service38 = ST5)

--	 service40
VAR service40: {ST2, ST1, ST4, ST3, ST0};
ASSIGN
next(service40) :=
case
(service40 = ST4 & input = service40_RESET): ST0;
(service40 = ST1 & input = service40_RESET): ST0;
(service40 = ST0 & input = service40_UMS_TripRequest): ST1;
(service40 = ST1 & output = service40_UMS_concrete1): ST2;
(service40 = ST3 & output = service40_UMS_TripAlternatives): ST4;
(service40 = ST2 & input = service40_RESET): ST0;
(service40 = ST2 & output = service40_UMS_concrete2): ST3;
(service40 = ST3 & input = service40_RESET): ST0;
1:service40;
esac;
TRANS ( input = service40_UMS_TripRequest) -> (service40 = ST0)
TRANS ( output = service40_UMS_TripAlternatives) -> (service40 = ST3)
TRANS ( output = service40_UMS_concrete2) -> (service40 = ST2)
TRANS ( input = service40_RESET) -> (service40 = ST2 | service40 = ST1 | service40 = ST4 | service40 = ST3)
TRANS ( output = service40_UMS_concrete1) -> (service40 = ST1)

-----------------------
--   O B J E C T S   --
-----------------------
--	 SetupTrip
VAR SetupTrip: {ORGANIZED, INITIAL};
ASSIGN
next(SetupTrip) :=
case
(input = service38_UMS_ManagementTrip):{(ORGANIZED)};
1:SetupTrip;
esac;
--	 ProvideTrip
VAR ProvideTrip: {TRIP_REQUEST_ACCOMPLISHED, TRIP_MANAGEMENT_ACCOMPLISHED, TRIP_MANAGE_REQUEST_LOADED, TRIP_REQUEST_LOADED, INITIAL};
ASSIGN
next(ProvideTrip) :=
case
(ProvideTrip = TRIP_REQUEST_LOADED & output = service40_UMS_TripAlternatives): TRIP_REQUEST_ACCOMPLISHED;
(ProvideTrip = INITIAL & input = service40_UMS_TripRequest): TRIP_REQUEST_LOADED;
(ProvideTrip = TRIP_MANAGE_REQUEST_LOADED & output = service38_UMS_TripManagementReply): TRIP_MANAGEMENT_ACCOMPLISHED;
(ProvideTrip = TRIP_REQUEST_ACCOMPLISHED & input = service38_UMS_TripManagementRequest): TRIP_MANAGE_REQUEST_LOADED;
1:ProvideTrip;
esac;
-----------------------------------
--   P R E C O N D I T I O N S   --
-----------------------------------
TRANS (input = service38_UMS_TripManagementRequest) -> ((((ProvideTrip = TRIP_REQUEST_ACCOMPLISHED))))
TRANS (output = service38_UMS_TripManagementReply) -> ((((ProvideTrip = TRIP_MANAGE_REQUEST_LOADED))))
TRANS (input = service40_UMS_TripRequest) -> ((((ProvideTrip = INITIAL))))
TRANS (output = service40_UMS_TripAlternatives) -> ((((ProvideTrip = TRIP_REQUEST_LOADED))))
-----------------------------
--   I N V A R I A N T S   --
-----------------------------
--	 Synchronicity
TRANS (1-(input = UNDEF)) + (1-(output = UNDEF)) = 1

---------------------------------------
--   S Y N C H R O N I Z A T I O N   --
---------------------------------------
DEFINE SYNC0 := ((ProvideTrip in {TRIP_REQUEST_ACCOMPLISHED}));
--	 Synchronicity enforcement
TRANS (service38 in {ST2, ST1, ST4, ST3, ST5, ST7}) -> (next(service38) != service38)
TRANS (service40 in {ST2, ST1, ST3}) -> (next(service40) != service40)
-------------------------------
--   C O N T R O L L E R S   --
-------------------------------
--	 Fault Controller
VAR FAULT_CONTROLLER: { START, STOP };
INIT FAULT_CONTROLLER = START

ASSIGN
next(FAULT_CONTROLLER) := 
case
(FAULT_CONTROLLER = START & FALSE): STOP;
1: FAULT_CONTROLLER;
esac;

TRANS (output != UNDEF | input != UNDEF) -> (FAULT_CONTROLLER != STOP)

-------------------------------------
--   I N I T I A L I Z A T I O N   --
-------------------------------------
INIT ((SetupTrip = INITIAL) & (ProvideTrip = INITIAL))
INIT ((service38 = ST0) & (service40 = ST0))

-----------------
--   G O A L   --
-----------------
MODULE main
VAR dn_main : dn_main;

GOAL ONEOF(
(dn_main.SYNC0), 1;
(dn_main.FAULT_CONTROLLER = STOP), 0

)