-------------------------------
--   C O M P O S I T I O N   --
-------------------------------
MODULE dn_main

---------------------
--   I N P U T S   --
---------------------
IVAR input:
{
UNDEF, service12_UMS_ManagementTrip, service12_UMS_TripManagementRequest, service12_RESET, service13_UMS_TripRequest, service13_RESET
};

-----------------------
--   O U T P U T S   --
-----------------------
IVAR output:
{
UNDEF, service12_UMS_concrete4, service12_UMS_concrete3, service12_UMS_concrete2, service12_UMS_concrete1, service12_UMS_TripManagementReply, service12_UMS_concrete5, service13_UMS_TripAlternatives, service13_UMS_concrete2, service13_UMS_concrete1
};

-------------------------
--   S E R V I C E S   --
-------------------------
--	 service12
VAR service12: {ST2, ST1, ST4, ST3, ST6, ST5, ST8, ST7, ST0};
ASSIGN
next(service12) :=
case
(service12 = ST6 & input = service12_UMS_ManagementTrip): ST7;
(service12 = ST4 & input = service12_RESET): ST0;
(service12 = ST6 & input = service12_RESET): ST0;
(service12 = ST5 & input = service12_RESET): ST0;
(service12 = ST0 & input = service12_UMS_TripManagementRequest): ST1;
(service12 = ST3 & output = service12_UMS_concrete3): ST4;
(service12 = ST4 & output = service12_UMS_concrete4): ST5;
(service12 = ST7 & output = service12_UMS_TripManagementReply): ST8;
(service12 = ST8 & input = service12_RESET): ST0;
(service12 = ST1 & output = service12_UMS_concrete1): ST2;
(service12 = ST5 & output = service12_UMS_concrete5): ST6;
(service12 = ST2 & input = service12_RESET): ST0;
(service12 = ST1 & input = service12_RESET): ST0;
(service12 = ST7 & input = service12_RESET): ST0;
(service12 = ST2 & output = service12_UMS_concrete2): ST3;
(service12 = ST3 & input = service12_RESET): ST0;
1:service12;
esac;
TRANS ( input = service12_UMS_ManagementTrip) -> (service12 = ST6)
TRANS ( output = service12_UMS_concrete4) -> (service12 = ST4)
TRANS ( input = service12_UMS_TripManagementRequest) -> (service12 = ST0)
TRANS ( output = service12_UMS_concrete3) -> (service12 = ST3)
TRANS ( output = service12_UMS_concrete2) -> (service12 = ST2)
TRANS ( input = service12_RESET) -> (service12 = ST2 | service12 = ST1 | service12 = ST4 | service12 = ST3 | service12 = ST6 | service12 = ST5 | service12 = ST8 | service12 = ST7)
TRANS ( output = service12_UMS_concrete1) -> (service12 = ST1)
TRANS ( output = service12_UMS_TripManagementReply) -> (service12 = ST7)
TRANS ( output = service12_UMS_concrete5) -> (service12 = ST5)

--	 service13
VAR service13: {ST2, ST1, ST4, ST3, ST0};
ASSIGN
next(service13) :=
case
(service13 = ST4 & input = service13_RESET): ST0;
(service13 = ST0 & input = service13_UMS_TripRequest): ST1;
(service13 = ST3 & output = service13_UMS_TripAlternatives): ST4;
(service13 = ST1 & output = service13_UMS_concrete1): ST2;
(service13 = ST1 & input = service13_RESET): ST0;
(service13 = ST2 & input = service13_RESET): ST0;
(service13 = ST2 & output = service13_UMS_concrete2): ST3;
(service13 = ST3 & input = service13_RESET): ST0;
1:service13;
esac;
TRANS ( input = service13_UMS_TripRequest) -> (service13 = ST0)
TRANS ( output = service13_UMS_TripAlternatives) -> (service13 = ST3)
TRANS ( output = service13_UMS_concrete2) -> (service13 = ST2)
TRANS ( input = service13_RESET) -> (service13 = ST2 | service13 = ST1 | service13 = ST4 | service13 = ST3)
TRANS ( output = service13_UMS_concrete1) -> (service13 = ST1)

-----------------------
--   O B J E C T S   --
-----------------------
--	 SetupTrip
VAR SetupTrip: {ORGANIZED, INITIAL};
ASSIGN
next(SetupTrip) :=
case
(input = service12_UMS_ManagementTrip):{(ORGANIZED)};
1:SetupTrip;
esac;
--	 ProvideTrip
VAR ProvideTrip: {TRIP_REQUEST_ACCOMPLISHED, TRIP_MANAGEMENT_ACCOMPLISHED, TRIP_MANAGE_REQUEST_LOADED, TRIP_REQUEST_LOADED, INITIAL};
ASSIGN
next(ProvideTrip) :=
case
(ProvideTrip = TRIP_MANAGE_REQUEST_LOADED & output = service12_UMS_TripManagementReply): TRIP_MANAGEMENT_ACCOMPLISHED;
(ProvideTrip = INITIAL & input = service13_UMS_TripRequest): TRIP_REQUEST_LOADED;
(ProvideTrip = TRIP_REQUEST_ACCOMPLISHED & input = service12_UMS_TripManagementRequest): TRIP_MANAGE_REQUEST_LOADED;
(ProvideTrip = TRIP_REQUEST_LOADED & output = service13_UMS_TripAlternatives): TRIP_REQUEST_ACCOMPLISHED;
1:ProvideTrip;
esac;
-----------------------------------
--   P R E C O N D I T I O N S   --
-----------------------------------
TRANS (input = service12_UMS_TripManagementRequest) -> ((((ProvideTrip = TRIP_REQUEST_ACCOMPLISHED))))
TRANS (output = service12_UMS_TripManagementReply) -> ((((ProvideTrip = TRIP_MANAGE_REQUEST_LOADED))))
TRANS (input = service13_UMS_TripRequest) -> ((((ProvideTrip = INITIAL))))
TRANS (output = service13_UMS_TripAlternatives) -> ((((ProvideTrip = TRIP_REQUEST_LOADED))))
-----------------------------
--   I N V A R I A N T S   --
-----------------------------
--	 Synchronicity
TRANS (1-(input = UNDEF)) + (1-(output = UNDEF)) = 1

---------------------------------------
--   S Y N C H R O N I Z A T I O N   --
---------------------------------------
DEFINE SYNC0 := ((ProvideTrip in {TRIP_REQUEST_ACCOMPLISHED}));
--	 Synchronicity enforcement
TRANS (service12 in {ST2, ST1, ST4, ST3, ST5, ST7}) -> (next(service12) != service12)
TRANS (service13 in {ST2, ST1, ST3}) -> (next(service13) != service13)
-------------------------------
--   C O N T R O L L E R S   --
-------------------------------
--	 Fault Controller
VAR FAULT_CONTROLLER: { START, STOP };
INIT FAULT_CONTROLLER = START

ASSIGN
next(FAULT_CONTROLLER) := 
case
(FAULT_CONTROLLER = START & FALSE): STOP;
1: FAULT_CONTROLLER;
esac;

TRANS (output != UNDEF | input != UNDEF) -> (FAULT_CONTROLLER != STOP)

-------------------------------------
--   I N I T I A L I Z A T I O N   --
-------------------------------------
INIT ((SetupTrip = INITIAL) & (ProvideTrip = INITIAL))
INIT ((service12 = ST0) & (service13 = ST0))

-----------------
--   G O A L   --
-----------------
MODULE main
VAR dn_main : dn_main;

GOAL ONEOF(
(dn_main.SYNC0), 1;
(dn_main.FAULT_CONTROLLER = STOP), 0

)