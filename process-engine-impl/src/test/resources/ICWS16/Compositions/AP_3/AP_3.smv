-------------------------------
--   C O M P O S I T I O N   --
-------------------------------
MODULE dn_main

---------------------
--   I N P U T S   --
---------------------
IVAR input:
{
UNDEF, service10_UMS_ManagementTrip, service10_UMS_TripManagementRequest, service10_RESET, service11_UMS_TripRequest, service11_RESET
};

-----------------------
--   O U T P U T S   --
-----------------------
IVAR output:
{
UNDEF, service10_UMS_concrete4, service10_UMS_concrete3, service10_UMS_concrete2, service10_UMS_concrete1, service10_UMS_TripManagementReply, service10_UMS_concrete5, service11_UMS_TripAlternatives, service11_UMS_concrete2, service11_UMS_concrete1
};

-------------------------
--   S E R V I C E S   --
-------------------------
--	 service10
VAR service10: {ST2, ST1, ST4, ST3, ST6, ST5, ST8, ST7, ST0};
ASSIGN
next(service10) :=
case
(service10 = ST0 & input = service10_UMS_TripManagementRequest): ST1;
(service10 = ST5 & output = service10_UMS_concrete5): ST6;
(service10 = ST8 & input = service10_RESET): ST0;
(service10 = ST2 & input = service10_RESET): ST0;
(service10 = ST7 & input = service10_RESET): ST0;
(service10 = ST4 & input = service10_RESET): ST0;
(service10 = ST6 & input = service10_UMS_ManagementTrip): ST7;
(service10 = ST1 & input = service10_RESET): ST0;
(service10 = ST3 & input = service10_RESET): ST0;
(service10 = ST1 & output = service10_UMS_concrete1): ST2;
(service10 = ST2 & output = service10_UMS_concrete2): ST3;
(service10 = ST4 & output = service10_UMS_concrete4): ST5;
(service10 = ST7 & output = service10_UMS_TripManagementReply): ST8;
(service10 = ST6 & input = service10_RESET): ST0;
(service10 = ST5 & input = service10_RESET): ST0;
(service10 = ST3 & output = service10_UMS_concrete3): ST4;
1:service10;
esac;
TRANS ( input = service10_UMS_ManagementTrip) -> (service10 = ST6)
TRANS ( output = service10_UMS_concrete4) -> (service10 = ST4)
TRANS ( input = service10_UMS_TripManagementRequest) -> (service10 = ST0)
TRANS ( output = service10_UMS_concrete3) -> (service10 = ST3)
TRANS ( output = service10_UMS_concrete2) -> (service10 = ST2)
TRANS ( input = service10_RESET) -> (service10 = ST2 | service10 = ST1 | service10 = ST4 | service10 = ST3 | service10 = ST6 | service10 = ST5 | service10 = ST8 | service10 = ST7)
TRANS ( output = service10_UMS_concrete1) -> (service10 = ST1)
TRANS ( output = service10_UMS_TripManagementReply) -> (service10 = ST7)
TRANS ( output = service10_UMS_concrete5) -> (service10 = ST5)

--	 service11
VAR service11: {ST2, ST1, ST4, ST3, ST0};
ASSIGN
next(service11) :=
case
(service11 = ST0 & input = service11_UMS_TripRequest): ST1;
(service11 = ST2 & input = service11_RESET): ST0;
(service11 = ST1 & output = service11_UMS_concrete1): ST2;
(service11 = ST2 & output = service11_UMS_concrete2): ST3;
(service11 = ST1 & input = service11_RESET): ST0;
(service11 = ST4 & input = service11_RESET): ST0;
(service11 = ST3 & input = service11_RESET): ST0;
(service11 = ST3 & output = service11_UMS_TripAlternatives): ST4;
1:service11;
esac;
TRANS ( input = service11_UMS_TripRequest) -> (service11 = ST0)
TRANS ( output = service11_UMS_TripAlternatives) -> (service11 = ST3)
TRANS ( output = service11_UMS_concrete2) -> (service11 = ST2)
TRANS ( input = service11_RESET) -> (service11 = ST2 | service11 = ST1 | service11 = ST4 | service11 = ST3)
TRANS ( output = service11_UMS_concrete1) -> (service11 = ST1)

-----------------------
--   O B J E C T S   --
-----------------------
--	 SetupTrip
VAR SetupTrip: {ORGANIZED, INITIAL};
ASSIGN
next(SetupTrip) :=
case
(input = service10_UMS_ManagementTrip):{(ORGANIZED)};
1:SetupTrip;
esac;
--	 ProvideTrip
VAR ProvideTrip: {TRIP_REQUEST_ACCOMPLISHED, TRIP_MANAGEMENT_ACCOMPLISHED, TRIP_MANAGE_REQUEST_LOADED, TRIP_REQUEST_LOADED, INITIAL};
ASSIGN
next(ProvideTrip) :=
case
(ProvideTrip = INITIAL & input = service11_UMS_TripRequest): TRIP_REQUEST_LOADED;
(ProvideTrip = TRIP_REQUEST_ACCOMPLISHED & input = service10_UMS_TripManagementRequest): TRIP_MANAGE_REQUEST_LOADED;
(ProvideTrip = TRIP_REQUEST_LOADED & output = service11_UMS_TripAlternatives): TRIP_REQUEST_ACCOMPLISHED;
(ProvideTrip = TRIP_MANAGE_REQUEST_LOADED & output = service10_UMS_TripManagementReply): TRIP_MANAGEMENT_ACCOMPLISHED;
1:ProvideTrip;
esac;
-----------------------------------
--   P R E C O N D I T I O N S   --
-----------------------------------
TRANS (input = service10_UMS_TripManagementRequest) -> ((((ProvideTrip = TRIP_REQUEST_ACCOMPLISHED))))
TRANS (output = service10_UMS_TripManagementReply) -> ((((ProvideTrip = TRIP_MANAGE_REQUEST_LOADED))))
TRANS (input = service11_UMS_TripRequest) -> ((((ProvideTrip = INITIAL))))
TRANS (output = service11_UMS_TripAlternatives) -> ((((ProvideTrip = TRIP_REQUEST_LOADED))))
-----------------------------
--   I N V A R I A N T S   --
-----------------------------
--	 Synchronicity
TRANS (1-(input = UNDEF)) + (1-(output = UNDEF)) = 1

---------------------------------------
--   S Y N C H R O N I Z A T I O N   --
---------------------------------------
DEFINE SYNC0 := ((ProvideTrip in {TRIP_REQUEST_ACCOMPLISHED}));
--	 Synchronicity enforcement
TRANS (service10 in {ST2, ST1, ST4, ST3, ST5, ST7}) -> (next(service10) != service10)
TRANS (service11 in {ST2, ST1, ST3}) -> (next(service11) != service11)
-------------------------------
--   C O N T R O L L E R S   --
-------------------------------
--	 Fault Controller
VAR FAULT_CONTROLLER: { START, STOP };
INIT FAULT_CONTROLLER = START

ASSIGN
next(FAULT_CONTROLLER) := 
case
(FAULT_CONTROLLER = START & FALSE): STOP;
1: FAULT_CONTROLLER;
esac;

TRANS (output != UNDEF | input != UNDEF) -> (FAULT_CONTROLLER != STOP)

-------------------------------------
--   I N I T I A L I Z A T I O N   --
-------------------------------------
INIT ((SetupTrip = INITIAL) & (ProvideTrip = INITIAL))
INIT ((service10 = ST0) & (service11 = ST0))

-----------------
--   G O A L   --
-----------------
MODULE main
VAR dn_main : dn_main;

GOAL ONEOF(
(dn_main.SYNC0), 1;
(dn_main.FAULT_CONTROLLER = STOP), 0

)