<?xml version="1.0" encoding="UTF-8"?>
<tns:process name="PROC_TravelAssistant" xmlns:tns="http://das.fbk.eu/Process"
	xmlns:tns1="http://das.fbk.eu/Annotation" xmlns:tns2="http://das.fbk.eu/DataType"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://das.fbk.eu/Process ../../../schema/allow-ensembles/Process.xsd ">

	<tns:receive name="StartInteraction" />
	
	<tns:receive name="AssistantRequest">
		<tns:actionVariable name="From" type="string">
			<tns2:content type="anyType"></tns2:content>
		</tns:actionVariable>
		<tns:actionVariable name="To" type="string">
			<tns2:content type="anyType"></tns2:content>
		</tns:actionVariable>
	</tns:receive>

	<tns:abstract name="TA_calculate">
		<tns:goal>
			<tns1:point>
				<tns1:domainProperty dp_name="GlobalPlanner">
					<tns1:state>
						ALTERNATIVES_SENT
					</tns1:state>
				</tns1:domainProperty>
			</tns1:point>
		</tns:goal>
	</tns:abstract>

	<tns:invoke name="AssistantResponse">
		<tns:actionVariable name="Plan" type="string">
			<tns2:content type="anyType"></tns2:content>
		</tns:actionVariable>
	</tns:invoke>

</tns:process>
