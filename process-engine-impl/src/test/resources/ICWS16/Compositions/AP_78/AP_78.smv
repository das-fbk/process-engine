-------------------------------
--   C O M P O S I T I O N   --
-------------------------------
MODULE dn_main

---------------------
--   I N P U T S   --
---------------------
IVAR input:
{
UNDEF, service131_UMS_ManagementTrip, service131_UMS_TripManagementRequest, service131_RESET, service133_UMS_TripRequest, service133_RESET
};

-----------------------
--   O U T P U T S   --
-----------------------
IVAR output:
{
UNDEF, service131_UMS_concrete4, service131_UMS_concrete3, service131_UMS_concrete2, service131_UMS_concrete1, service131_UMS_TripManagementReply, service131_UMS_concrete5, service133_UMS_TripAlternatives, service133_UMS_concrete2, service133_UMS_concrete1
};

-------------------------
--   S E R V I C E S   --
-------------------------
--	 service131
VAR service131: {ST2, ST1, ST4, ST3, ST6, ST5, ST8, ST7, ST0};
ASSIGN
next(service131) :=
case
(service131 = ST0 & input = service131_UMS_TripManagementRequest): ST1;
(service131 = ST7 & input = service131_RESET): ST0;
(service131 = ST3 & input = service131_RESET): ST0;
(service131 = ST1 & output = service131_UMS_concrete1): ST2;
(service131 = ST4 & input = service131_RESET): ST0;
(service131 = ST7 & output = service131_UMS_TripManagementReply): ST8;
(service131 = ST2 & output = service131_UMS_concrete2): ST3;
(service131 = ST8 & input = service131_RESET): ST0;
(service131 = ST4 & output = service131_UMS_concrete4): ST5;
(service131 = ST3 & output = service131_UMS_concrete3): ST4;
(service131 = ST1 & input = service131_RESET): ST0;
(service131 = ST5 & input = service131_RESET): ST0;
(service131 = ST5 & output = service131_UMS_concrete5): ST6;
(service131 = ST6 & input = service131_UMS_ManagementTrip): ST7;
(service131 = ST2 & input = service131_RESET): ST0;
(service131 = ST6 & input = service131_RESET): ST0;
1:service131;
esac;
TRANS ( input = service131_UMS_ManagementTrip) -> (service131 = ST6)
TRANS ( output = service131_UMS_concrete4) -> (service131 = ST4)
TRANS ( input = service131_UMS_TripManagementRequest) -> (service131 = ST0)
TRANS ( output = service131_UMS_concrete3) -> (service131 = ST3)
TRANS ( output = service131_UMS_concrete2) -> (service131 = ST2)
TRANS ( input = service131_RESET) -> (service131 = ST2 | service131 = ST1 | service131 = ST4 | service131 = ST3 | service131 = ST6 | service131 = ST5 | service131 = ST8 | service131 = ST7)
TRANS ( output = service131_UMS_concrete1) -> (service131 = ST1)
TRANS ( output = service131_UMS_TripManagementReply) -> (service131 = ST7)
TRANS ( output = service131_UMS_concrete5) -> (service131 = ST5)

--	 service133
VAR service133: {ST2, ST1, ST4, ST3, ST0};
ASSIGN
next(service133) :=
case
(service133 = ST3 & input = service133_RESET): ST0;
(service133 = ST3 & output = service133_UMS_TripAlternatives): ST4;
(service133 = ST2 & output = service133_UMS_concrete2): ST3;
(service133 = ST1 & input = service133_RESET): ST0;
(service133 = ST1 & output = service133_UMS_concrete1): ST2;
(service133 = ST4 & input = service133_RESET): ST0;
(service133 = ST2 & input = service133_RESET): ST0;
(service133 = ST0 & input = service133_UMS_TripRequest): ST1;
1:service133;
esac;
TRANS ( input = service133_UMS_TripRequest) -> (service133 = ST0)
TRANS ( output = service133_UMS_TripAlternatives) -> (service133 = ST3)
TRANS ( output = service133_UMS_concrete2) -> (service133 = ST2)
TRANS ( input = service133_RESET) -> (service133 = ST2 | service133 = ST1 | service133 = ST4 | service133 = ST3)
TRANS ( output = service133_UMS_concrete1) -> (service133 = ST1)

-----------------------
--   O B J E C T S   --
-----------------------
--	 SetupTrip
VAR SetupTrip: {ORGANIZED, INITIAL};
ASSIGN
next(SetupTrip) :=
case
(input = service131_UMS_ManagementTrip):{(ORGANIZED)};
1:SetupTrip;
esac;
--	 ProvideTrip
VAR ProvideTrip: {TRIP_REQUEST_ACCOMPLISHED, TRIP_MANAGEMENT_ACCOMPLISHED, TRIP_MANAGE_REQUEST_LOADED, TRIP_REQUEST_LOADED, INITIAL};
ASSIGN
next(ProvideTrip) :=
case
(ProvideTrip = INITIAL & input = service133_UMS_TripRequest): TRIP_REQUEST_LOADED;
(ProvideTrip = TRIP_MANAGE_REQUEST_LOADED & output = service131_UMS_TripManagementReply): TRIP_MANAGEMENT_ACCOMPLISHED;
(ProvideTrip = TRIP_REQUEST_LOADED & output = service133_UMS_TripAlternatives): TRIP_REQUEST_ACCOMPLISHED;
(ProvideTrip = TRIP_REQUEST_ACCOMPLISHED & input = service131_UMS_TripManagementRequest): TRIP_MANAGE_REQUEST_LOADED;
1:ProvideTrip;
esac;
-----------------------------------
--   P R E C O N D I T I O N S   --
-----------------------------------
TRANS (input = service131_UMS_TripManagementRequest) -> ((((ProvideTrip = TRIP_REQUEST_ACCOMPLISHED))))
TRANS (output = service131_UMS_TripManagementReply) -> ((((ProvideTrip = TRIP_MANAGE_REQUEST_LOADED))))
TRANS (input = service133_UMS_TripRequest) -> ((((ProvideTrip = INITIAL))))
TRANS (output = service133_UMS_TripAlternatives) -> ((((ProvideTrip = TRIP_REQUEST_LOADED))))
-----------------------------
--   I N V A R I A N T S   --
-----------------------------
--	 Synchronicity
TRANS (1-(input = UNDEF)) + (1-(output = UNDEF)) = 1

---------------------------------------
--   S Y N C H R O N I Z A T I O N   --
---------------------------------------
DEFINE SYNC0 := ((ProvideTrip in {TRIP_MANAGEMENT_ACCOMPLISHED}));
--	 Synchronicity enforcement
TRANS (service131 in {ST2, ST1, ST4, ST3, ST5, ST7}) -> (next(service131) != service131)
TRANS (service133 in {ST2, ST1, ST3}) -> (next(service133) != service133)
-------------------------------
--   C O N T R O L L E R S   --
-------------------------------
--	 Fault Controller
VAR FAULT_CONTROLLER: { START, STOP };
INIT FAULT_CONTROLLER = START

ASSIGN
next(FAULT_CONTROLLER) := 
case
(FAULT_CONTROLLER = START & FALSE): STOP;
1: FAULT_CONTROLLER;
esac;

TRANS (output != UNDEF | input != UNDEF) -> (FAULT_CONTROLLER != STOP)

-------------------------------------
--   I N I T I A L I Z A T I O N   --
-------------------------------------
INIT ((SetupTrip = INITIAL) & (ProvideTrip = TRIP_REQUEST_ACCOMPLISHED))
INIT ((service131 = ST0) & (service133 = ST0))

-----------------
--   G O A L   --
-----------------
MODULE main
VAR dn_main : dn_main;

GOAL ONEOF(
(dn_main.SYNC0), 1;
(dn_main.FAULT_CONTROLLER = STOP), 0

)