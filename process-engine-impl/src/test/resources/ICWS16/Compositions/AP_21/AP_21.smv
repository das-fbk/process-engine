-------------------------------
--   C O M P O S I T I O N   --
-------------------------------
MODULE dn_main

---------------------
--   I N P U T S   --
---------------------
IVAR input:
{
UNDEF, service46_UMS_ManagementTrip, service46_UMS_TripManagementRequest, service46_RESET, service47_UMS_TripRequest, service47_RESET
};

-----------------------
--   O U T P U T S   --
-----------------------
IVAR output:
{
UNDEF, service46_UMS_concrete4, service46_UMS_concrete3, service46_UMS_concrete2, service46_UMS_concrete1, service46_UMS_TripManagementReply, service46_UMS_concrete5, service47_UMS_TripAlternatives, service47_UMS_concrete2, service47_UMS_concrete1
};

-------------------------
--   S E R V I C E S   --
-------------------------
--	 service46
VAR service46: {ST2, ST1, ST4, ST3, ST6, ST5, ST8, ST7, ST0};
ASSIGN
next(service46) :=
case
(service46 = ST3 & input = service46_RESET): ST0;
(service46 = ST6 & input = service46_UMS_ManagementTrip): ST7;
(service46 = ST1 & input = service46_RESET): ST0;
(service46 = ST1 & output = service46_UMS_concrete1): ST2;
(service46 = ST2 & output = service46_UMS_concrete2): ST3;
(service46 = ST7 & input = service46_RESET): ST0;
(service46 = ST5 & input = service46_RESET): ST0;
(service46 = ST4 & input = service46_RESET): ST0;
(service46 = ST0 & input = service46_UMS_TripManagementRequest): ST1;
(service46 = ST6 & input = service46_RESET): ST0;
(service46 = ST3 & output = service46_UMS_concrete3): ST4;
(service46 = ST4 & output = service46_UMS_concrete4): ST5;
(service46 = ST7 & output = service46_UMS_TripManagementReply): ST8;
(service46 = ST5 & output = service46_UMS_concrete5): ST6;
(service46 = ST8 & input = service46_RESET): ST0;
(service46 = ST2 & input = service46_RESET): ST0;
1:service46;
esac;
TRANS ( input = service46_UMS_ManagementTrip) -> (service46 = ST6)
TRANS ( output = service46_UMS_concrete4) -> (service46 = ST4)
TRANS ( input = service46_UMS_TripManagementRequest) -> (service46 = ST0)
TRANS ( output = service46_UMS_concrete3) -> (service46 = ST3)
TRANS ( output = service46_UMS_concrete2) -> (service46 = ST2)
TRANS ( input = service46_RESET) -> (service46 = ST2 | service46 = ST1 | service46 = ST4 | service46 = ST3 | service46 = ST6 | service46 = ST5 | service46 = ST8 | service46 = ST7)
TRANS ( output = service46_UMS_concrete1) -> (service46 = ST1)
TRANS ( output = service46_UMS_TripManagementReply) -> (service46 = ST7)
TRANS ( output = service46_UMS_concrete5) -> (service46 = ST5)

--	 service47
VAR service47: {ST2, ST1, ST4, ST3, ST0};
ASSIGN
next(service47) :=
case
(service47 = ST1 & output = service47_UMS_concrete1): ST2;
(service47 = ST2 & output = service47_UMS_concrete2): ST3;
(service47 = ST3 & input = service47_RESET): ST0;
(service47 = ST0 & input = service47_UMS_TripRequest): ST1;
(service47 = ST1 & input = service47_RESET): ST0;
(service47 = ST2 & input = service47_RESET): ST0;
(service47 = ST4 & input = service47_RESET): ST0;
(service47 = ST3 & output = service47_UMS_TripAlternatives): ST4;
1:service47;
esac;
TRANS ( input = service47_UMS_TripRequest) -> (service47 = ST0)
TRANS ( output = service47_UMS_TripAlternatives) -> (service47 = ST3)
TRANS ( output = service47_UMS_concrete2) -> (service47 = ST2)
TRANS ( input = service47_RESET) -> (service47 = ST2 | service47 = ST1 | service47 = ST4 | service47 = ST3)
TRANS ( output = service47_UMS_concrete1) -> (service47 = ST1)

-----------------------
--   O B J E C T S   --
-----------------------
--	 SetupTrip
VAR SetupTrip: {ORGANIZED, INITIAL};
ASSIGN
next(SetupTrip) :=
case
(input = service46_UMS_ManagementTrip):{(ORGANIZED)};
1:SetupTrip;
esac;
--	 ProvideTrip
VAR ProvideTrip: {TRIP_REQUEST_ACCOMPLISHED, TRIP_MANAGEMENT_ACCOMPLISHED, TRIP_MANAGE_REQUEST_LOADED, TRIP_REQUEST_LOADED, INITIAL};
ASSIGN
next(ProvideTrip) :=
case
(ProvideTrip = TRIP_MANAGE_REQUEST_LOADED & output = service46_UMS_TripManagementReply): TRIP_MANAGEMENT_ACCOMPLISHED;
(ProvideTrip = TRIP_REQUEST_ACCOMPLISHED & input = service46_UMS_TripManagementRequest): TRIP_MANAGE_REQUEST_LOADED;
(ProvideTrip = TRIP_REQUEST_LOADED & output = service47_UMS_TripAlternatives): TRIP_REQUEST_ACCOMPLISHED;
(ProvideTrip = INITIAL & input = service47_UMS_TripRequest): TRIP_REQUEST_LOADED;
1:ProvideTrip;
esac;
-----------------------------------
--   P R E C O N D I T I O N S   --
-----------------------------------
TRANS (input = service46_UMS_TripManagementRequest) -> ((((ProvideTrip = TRIP_REQUEST_ACCOMPLISHED))))
TRANS (output = service46_UMS_TripManagementReply) -> ((((ProvideTrip = TRIP_MANAGE_REQUEST_LOADED))))
TRANS (input = service47_UMS_TripRequest) -> ((((ProvideTrip = INITIAL))))
TRANS (output = service47_UMS_TripAlternatives) -> ((((ProvideTrip = TRIP_REQUEST_LOADED))))
-----------------------------
--   I N V A R I A N T S   --
-----------------------------
--	 Synchronicity
TRANS (1-(input = UNDEF)) + (1-(output = UNDEF)) = 1

---------------------------------------
--   S Y N C H R O N I Z A T I O N   --
---------------------------------------
DEFINE SYNC0 := ((ProvideTrip in {TRIP_REQUEST_ACCOMPLISHED}));
--	 Synchronicity enforcement
TRANS (service46 in {ST2, ST1, ST4, ST3, ST5, ST7}) -> (next(service46) != service46)
TRANS (service47 in {ST2, ST1, ST3}) -> (next(service47) != service47)
-------------------------------
--   C O N T R O L L E R S   --
-------------------------------
--	 Fault Controller
VAR FAULT_CONTROLLER: { START, STOP };
INIT FAULT_CONTROLLER = START

ASSIGN
next(FAULT_CONTROLLER) := 
case
(FAULT_CONTROLLER = START & FALSE): STOP;
1: FAULT_CONTROLLER;
esac;

TRANS (output != UNDEF | input != UNDEF) -> (FAULT_CONTROLLER != STOP)

-------------------------------------
--   I N I T I A L I Z A T I O N   --
-------------------------------------
INIT ((SetupTrip = INITIAL) & (ProvideTrip = INITIAL))
INIT ((service46 = ST0) & (service47 = ST0))

-----------------
--   G O A L   --
-----------------
MODULE main
VAR dn_main : dn_main;

GOAL ONEOF(
(dn_main.SYNC0), 1;
(dn_main.FAULT_CONTROLLER = STOP), 0

)