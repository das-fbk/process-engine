-------------------------------
--   C O M P O S I T I O N   --
-------------------------------
MODULE dn_main

---------------------
--   I N P U T S   --
---------------------
IVAR input:
{
UNDEF, service104_FD_TakeFlexibusRequest, service104_RESET, service105_RESET, service105_FD_ExecuteRouteRequest
};

-----------------------
--   O U T P U T S   --
-----------------------
IVAR output:
{
UNDEF, service104_FD_TakeFlexibusReply, service105_FD_AllPickupPointsCoveredNotice, service105_FD_RouteStartedNotice, service105_FD_ExecuteRouteReply
};

-------------------------
--   S E R V I C E S   --
-------------------------
--	 service104
VAR service104: {ST2, ST1, ST0};
ASSIGN
next(service104) :=
case
(service104 = ST2 & input = service104_RESET): ST0;
(service104 = ST1 & input = service104_RESET): ST0;
(service104 = ST0 & input = service104_FD_TakeFlexibusRequest): ST1;
(service104 = ST1 & output = service104_FD_TakeFlexibusReply): ST2;
1:service104;
esac;
TRANS ( input = service104_FD_TakeFlexibusRequest) -> (service104 = ST0)
TRANS ( output = service104_FD_TakeFlexibusReply) -> (service104 = ST1)
TRANS ( input = service104_RESET) -> (service104 = ST2 | service104 = ST1)

--	 service105
VAR service105: {ST2, ST1, ST4, ST3, ST0};
ASSIGN
next(service105) :=
case
(service105 = ST0 & input = service105_FD_ExecuteRouteRequest): ST1;
(service105 = ST2 & input = service105_RESET): ST0;
(service105 = ST2 & output = service105_FD_AllPickupPointsCoveredNotice): ST3;
(service105 = ST1 & input = service105_RESET): ST0;
(service105 = ST3 & output = service105_FD_ExecuteRouteReply): ST4;
(service105 = ST4 & input = service105_RESET): ST0;
(service105 = ST1 & output = service105_FD_RouteStartedNotice): ST2;
(service105 = ST3 & input = service105_RESET): ST0;
1:service105;
esac;
TRANS ( output = service105_FD_AllPickupPointsCoveredNotice) -> (service105 = ST2)
TRANS ( input = service105_RESET) -> (service105 = ST2 | service105 = ST1 | service105 = ST4 | service105 = ST3)
TRANS ( input = service105_FD_ExecuteRouteRequest) -> (service105 = ST0)
TRANS ( output = service105_FD_RouteStartedNotice) -> (service105 = ST1)
TRANS ( output = service105_FD_ExecuteRouteReply) -> (service105 = ST3)

-----------------------
--   O B J E C T S   --
-----------------------
--	 FlexibusDriverStatus
VAR FlexibusDriverStatus: {ROUTE_STARTED, PASSENGERS_LOADED, AT_PICKUP_POINT, ROUTE_ENDED, INITIAL, ROUTE_CLOSED, FLEXIBUS_TAKEN, TAKING_PASSENGER};
ASSIGN
next(FlexibusDriverStatus) :=
case
(FlexibusDriverStatus = ROUTE_STARTED & output = service105_FD_AllPickupPointsCoveredNotice): PASSENGERS_LOADED;
(FlexibusDriverStatus = FLEXIBUS_TAKEN & output = service105_FD_RouteStartedNotice): ROUTE_STARTED;
(FlexibusDriverStatus = PASSENGERS_LOADED & output = service105_FD_ExecuteRouteReply): ROUTE_ENDED;
(FlexibusDriverStatus = INITIAL & output = service104_FD_TakeFlexibusReply): FLEXIBUS_TAKEN;
1:FlexibusDriverStatus;
esac;
-----------------------------------
--   P R E C O N D I T I O N S   --
-----------------------------------
TRANS (input = service104_FD_TakeFlexibusRequest) -> ((((FlexibusDriverStatus = INITIAL))))
TRANS (input = service105_FD_ExecuteRouteRequest) -> ((((FlexibusDriverStatus = FLEXIBUS_TAKEN))))
-----------------------------
--   I N V A R I A N T S   --
-----------------------------
--	 Synchronicity
TRANS (1-(input = UNDEF)) + (1-(output = UNDEF)) = 1

---------------------------------------
--   S Y N C H R O N I Z A T I O N   --
---------------------------------------
DEFINE SYNC0 := ((FlexibusDriverStatus in {ROUTE_ENDED}));
--	 Synchronicity enforcement
TRANS (service104 in {ST1}) -> (next(service104) != service104)
TRANS (service105 in {ST2, ST1, ST3}) -> (next(service105) != service105)
-------------------------------
--   C O N T R O L L E R S   --
-------------------------------
--	 Fault Controller
VAR FAULT_CONTROLLER: { START, STOP };
INIT FAULT_CONTROLLER = START

ASSIGN
next(FAULT_CONTROLLER) := 
case
(FAULT_CONTROLLER = START & FALSE): STOP;
1: FAULT_CONTROLLER;
esac;

TRANS (output != UNDEF | input != UNDEF) -> (FAULT_CONTROLLER != STOP)

-------------------------------------
--   I N I T I A L I Z A T I O N   --
-------------------------------------
INIT ((FlexibusDriverStatus = FLEXIBUS_TAKEN))
INIT ((service104 = ST0) & (service105 = ST0))

-----------------
--   G O A L   --
-----------------
MODULE main
VAR dn_main : dn_main;

GOAL ONEOF(
(dn_main.SYNC0), 1;
(dn_main.FAULT_CONTROLLER = STOP), 0

)