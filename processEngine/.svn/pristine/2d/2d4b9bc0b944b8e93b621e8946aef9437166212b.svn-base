<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<tns:process name="MakerProcess" xmlns:tns="http://das.fbk.eu/Process"
	xmlns:tns1="http://das.fbk.eu/Annotation" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<tns:receive name="InvokeMaker" />
	
	<tns:abstract name="HelperAbstract">
		<tns:goal>
			<tns1:point>
				<tns1:domainProperty dp_name="HelperDP">
					<tns1:state>
						HELPER_COMMITED
					</tns1:state>
				</tns1:domainProperty>
			</tns1:point>
		</tns:goal>
	</tns:abstract>
	
	
	<tns:invoke name="ResponseMaker" />
</tns:process>