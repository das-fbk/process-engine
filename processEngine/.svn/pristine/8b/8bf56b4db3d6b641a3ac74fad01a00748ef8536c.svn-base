<?xml version="1.0" encoding="UTF-8"?>
<tns:fragment id="UMS_provideTripAlternatives" xmlns:tns="http://das.fbk.eu/Fragment"
	xmlns:tns1="http://das.fbk.eu/Annotation" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://das.fbk.eu/Fragment ../../../schema/allow-ensembles/Fragment.xsd ">

	<tns:state isInitial="true" name="ST0" />
	<tns:state name="ST1" />
	<tns:state name="ST2" />
	<tns:state name="ST3" />

	<tns:action name="testAstrazioneInput" actionType="input">
		<tns:precondition>
			<tns1:point>
				<tns1:domainProperty dp_name="TripProvider">
					<tns1:state>RINIT</tns1:state>
				</tns1:domainProperty>
			</tns1:point>
		</tns:precondition>
	</tns:action>

	<!-- attivitÃ  astratta di test -->
	<tns:action name="testAstrazione" actionType="abstract">
		<tns:goal>
			<tns1:point>
				<tns1:domainProperty dp_name="TripProvider">
					<tns1:state>
						REQUESTLOADED
					</tns1:state>
				</tns1:domainProperty>
			</tns1:point>
		</tns:goal>
	</tns:action>

	<tns:action name="testAstrazioneOutput" actionType="output">
		<tns:precondition>
			<tns1:point>
				<tns1:domainProperty dp_name="TripProvider">
					<tns1:state>REQUESTLOADED</tns1:state>
				</tns1:domainProperty>
			</tns1:point>
		</tns:precondition>
		<tns:effect>
			<tns1:event dpName="TripProvider" eventName="replyReceived"></tns1:event>
		</tns:effect>
	</tns:action>

	<tns:transition>
		<tns:initialState>ST0</tns:initialState>
		<tns:finalState>ST1</tns:finalState>
		<tns:action name="testAstrazioneInput" actionType="input" />
	</tns:transition>
	
	<tns:transition>
		<tns:initialState>ST1</tns:initialState>
		<tns:finalState>ST2</tns:finalState>
		<tns:action name="testAstrazione" actionType="abstract" />
	</tns:transition>
	
		<tns:transition>
		<tns:initialState>ST2</tns:initialState>
		<tns:finalState>ST3</tns:finalState>
		<tns:action name="testAstrazioneOutput" actionType="output" />
	</tns:transition>

	<tns:consumerEntityType>User</tns:consumerEntityType>
</tns:fragment>
