<?xml version="1.0" encoding="UTF-8"?>
<tns:fragment id="F_Supporter_Second" xmlns:tns="http://das.fbk.eu/Fragment"
	xmlns:tns1="http://das.fbk.eu/Annotation" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<tns:state isInitial="true" name="ST0" />
	<tns:state name="ST1" />
	<tns:state name="ST2" />

	<tns:action actionType="input" name="InvokeSupporterSecond">
		<tns:precondition>
			<tns1:point>
				<tns1:domainProperty dp_name="TestDP">
					<tns1:state>SECOND_INITIAL</tns1:state>
				</tns1:domainProperty>
			</tns1:point>
		</tns:precondition>
<!-- 		<tns:effect> -->
<!-- 			<tns1:event dpName="TestDP" eventName="userRegistration" /> -->
<!-- 		</tns:effect>		 -->
	</tns:action>
	
	<tns:action actionType="output" name="ResponseSupporterSecond">
		<tns:effect>
			<tns1:event dpName="TestDP" eventName="secondCompleted" />
		</tns:effect>		
	</tns:action>
	
	<tns:transition>
		<tns:initialState>ST0</tns:initialState>
		<tns:finalState>ST1</tns:finalState>
		<tns:action actionType="input" name="InvokeSupporterSecond" />
	</tns:transition>
		<tns:transition>
		<tns:initialState>ST1</tns:initialState>
		<tns:finalState>ST2</tns:finalState>
		<tns:action actionType="output" name="ResponseSupporterSecond" />
	</tns:transition>
	<tns:consumerEntityType>Supporter</tns:consumerEntityType>
</tns:fragment>