-------------------------------
--   C O M P O S I T I O N   --
-------------------------------
MODULE dn_main

---------------------
--   I N P U T S   --
---------------------
IVAR input:
{
UNDEF, service14_UMS_ManagementTrip, service14_UMS_TripManagementRequest, service14_RESET, service15_UMS_TripRequest, service15_RESET
};

-----------------------
--   O U T P U T S   --
-----------------------
IVAR output:
{
UNDEF, service14_UMS_concrete4, service14_UMS_concrete3, service14_UMS_concrete2, service14_UMS_concrete1, service14_UMS_TripManagementReply, service14_UMS_concrete5, service15_UMS_TripAlternatives, service15_UMS_concrete2, service15_UMS_concrete1
};

-------------------------
--   S E R V I C E S   --
-------------------------
--	 service14
VAR service14: {ST2, ST1, ST4, ST3, ST6, ST5, ST8, ST7, ST0};
ASSIGN
next(service14) :=
case
(service14 = ST5 & output = service14_UMS_concrete5): ST6;
(service14 = ST4 & input = service14_RESET): ST0;
(service14 = ST1 & output = service14_UMS_concrete1): ST2;
(service14 = ST5 & input = service14_RESET): ST0;
(service14 = ST1 & input = service14_RESET): ST0;
(service14 = ST2 & input = service14_RESET): ST0;
(service14 = ST2 & output = service14_UMS_concrete2): ST3;
(service14 = ST8 & input = service14_RESET): ST0;
(service14 = ST6 & input = service14_UMS_ManagementTrip): ST7;
(service14 = ST3 & input = service14_RESET): ST0;
(service14 = ST3 & output = service14_UMS_concrete3): ST4;
(service14 = ST7 & input = service14_RESET): ST0;
(service14 = ST6 & input = service14_RESET): ST0;
(service14 = ST0 & input = service14_UMS_TripManagementRequest): ST1;
(service14 = ST7 & output = service14_UMS_TripManagementReply): ST8;
(service14 = ST4 & output = service14_UMS_concrete4): ST5;
1:service14;
esac;
TRANS ( input = service14_UMS_ManagementTrip) -> (service14 = ST6)
TRANS ( output = service14_UMS_concrete4) -> (service14 = ST4)
TRANS ( input = service14_UMS_TripManagementRequest) -> (service14 = ST0)
TRANS ( output = service14_UMS_concrete3) -> (service14 = ST3)
TRANS ( output = service14_UMS_concrete2) -> (service14 = ST2)
TRANS ( input = service14_RESET) -> (service14 = ST2 | service14 = ST1 | service14 = ST4 | service14 = ST3 | service14 = ST6 | service14 = ST5 | service14 = ST8 | service14 = ST7)
TRANS ( output = service14_UMS_concrete1) -> (service14 = ST1)
TRANS ( output = service14_UMS_TripManagementReply) -> (service14 = ST7)
TRANS ( output = service14_UMS_concrete5) -> (service14 = ST5)

--	 service15
VAR service15: {ST2, ST1, ST4, ST3, ST0};
ASSIGN
next(service15) :=
case
(service15 = ST4 & input = service15_RESET): ST0;
(service15 = ST2 & output = service15_UMS_concrete2): ST3;
(service15 = ST3 & input = service15_RESET): ST0;
(service15 = ST0 & input = service15_UMS_TripRequest): ST1;
(service15 = ST2 & input = service15_RESET): ST0;
(service15 = ST1 & input = service15_RESET): ST0;
(service15 = ST1 & output = service15_UMS_concrete1): ST2;
(service15 = ST3 & output = service15_UMS_TripAlternatives): ST4;
1:service15;
esac;
TRANS ( input = service15_UMS_TripRequest) -> (service15 = ST0)
TRANS ( output = service15_UMS_TripAlternatives) -> (service15 = ST3)
TRANS ( output = service15_UMS_concrete2) -> (service15 = ST2)
TRANS ( input = service15_RESET) -> (service15 = ST2 | service15 = ST1 | service15 = ST4 | service15 = ST3)
TRANS ( output = service15_UMS_concrete1) -> (service15 = ST1)

-----------------------
--   O B J E C T S   --
-----------------------
--	 SetupTrip
VAR SetupTrip: {ORGANIZED, INITIAL};
ASSIGN
next(SetupTrip) :=
case
(input = service14_UMS_ManagementTrip):{(ORGANIZED)};
1:SetupTrip;
esac;
--	 ProvideTrip
VAR ProvideTrip: {TRIP_REQUEST_ACCOMPLISHED, TRIP_MANAGEMENT_ACCOMPLISHED, TRIP_MANAGE_REQUEST_LOADED, TRIP_REQUEST_LOADED, INITIAL};
ASSIGN
next(ProvideTrip) :=
case
(ProvideTrip = TRIP_REQUEST_LOADED & output = service15_UMS_TripAlternatives): TRIP_REQUEST_ACCOMPLISHED;
(ProvideTrip = TRIP_MANAGE_REQUEST_LOADED & output = service14_UMS_TripManagementReply): TRIP_MANAGEMENT_ACCOMPLISHED;
(ProvideTrip = INITIAL & input = service15_UMS_TripRequest): TRIP_REQUEST_LOADED;
(ProvideTrip = TRIP_REQUEST_ACCOMPLISHED & input = service14_UMS_TripManagementRequest): TRIP_MANAGE_REQUEST_LOADED;
1:ProvideTrip;
esac;
-----------------------------------
--   P R E C O N D I T I O N S   --
-----------------------------------
TRANS (input = service14_UMS_TripManagementRequest) -> ((((ProvideTrip = TRIP_REQUEST_ACCOMPLISHED))))
TRANS (output = service14_UMS_TripManagementReply) -> ((((ProvideTrip = TRIP_MANAGE_REQUEST_LOADED))))
TRANS (input = service15_UMS_TripRequest) -> ((((ProvideTrip = INITIAL))))
TRANS (output = service15_UMS_TripAlternatives) -> ((((ProvideTrip = TRIP_REQUEST_LOADED))))
-----------------------------
--   I N V A R I A N T S   --
-----------------------------
--	 Synchronicity
TRANS (1-(input = UNDEF)) + (1-(output = UNDEF)) = 1

---------------------------------------
--   S Y N C H R O N I Z A T I O N   --
---------------------------------------
DEFINE SYNC0 := ((ProvideTrip in {TRIP_REQUEST_ACCOMPLISHED}));
--	 Synchronicity enforcement
TRANS (service14 in {ST2, ST1, ST4, ST3, ST5, ST7}) -> (next(service14) != service14)
TRANS (service15 in {ST2, ST1, ST3}) -> (next(service15) != service15)
-------------------------------
--   C O N T R O L L E R S   --
-------------------------------
--	 Fault Controller
VAR FAULT_CONTROLLER: { START, STOP };
INIT FAULT_CONTROLLER = START

ASSIGN
next(FAULT_CONTROLLER) := 
case
(FAULT_CONTROLLER = START & FALSE): STOP;
1: FAULT_CONTROLLER;
esac;

TRANS (output != UNDEF | input != UNDEF) -> (FAULT_CONTROLLER != STOP)

-------------------------------------
--   I N I T I A L I Z A T I O N   --
-------------------------------------
INIT ((SetupTrip = INITIAL) & (ProvideTrip = INITIAL))
INIT ((service14 = ST0) & (service15 = ST0))

-----------------
--   G O A L   --
-----------------
MODULE main
VAR dn_main : dn_main;

GOAL ONEOF(
(dn_main.SYNC0), 1;
(dn_main.FAULT_CONTROLLER = STOP), 0

)