-------------------------------
--   C O M P O S I T I O N   --
-------------------------------
MODULE dn_main

---------------------
--   I N P U T S   --
---------------------
IVAR input:
{
UNDEF, service171_UMS_ManagementTrip, service171_UMS_TripManagementRequest, service171_RESET, service173_UMS_TripRequest, service173_RESET
};

-----------------------
--   O U T P U T S   --
-----------------------
IVAR output:
{
UNDEF, service171_UMS_concrete4, service171_UMS_concrete3, service171_UMS_concrete2, service171_UMS_concrete1, service171_UMS_TripManagementReply, service171_UMS_concrete5, service173_UMS_TripAlternatives, service173_UMS_concrete2, service173_UMS_concrete1
};

-------------------------
--   S E R V I C E S   --
-------------------------
--	 service171
VAR service171: {ST2, ST1, ST4, ST3, ST6, ST5, ST8, ST7, ST0};
ASSIGN
next(service171) :=
case
(service171 = ST6 & input = service171_UMS_ManagementTrip): ST7;
(service171 = ST4 & output = service171_UMS_concrete4): ST5;
(service171 = ST8 & input = service171_RESET): ST0;
(service171 = ST3 & output = service171_UMS_concrete3): ST4;
(service171 = ST4 & input = service171_RESET): ST0;
(service171 = ST5 & output = service171_UMS_concrete5): ST6;
(service171 = ST5 & input = service171_RESET): ST0;
(service171 = ST2 & output = service171_UMS_concrete2): ST3;
(service171 = ST7 & output = service171_UMS_TripManagementReply): ST8;
(service171 = ST2 & input = service171_RESET): ST0;
(service171 = ST3 & input = service171_RESET): ST0;
(service171 = ST0 & input = service171_UMS_TripManagementRequest): ST1;
(service171 = ST1 & output = service171_UMS_concrete1): ST2;
(service171 = ST1 & input = service171_RESET): ST0;
(service171 = ST7 & input = service171_RESET): ST0;
(service171 = ST6 & input = service171_RESET): ST0;
1:service171;
esac;
TRANS ( input = service171_UMS_ManagementTrip) -> (service171 = ST6)
TRANS ( output = service171_UMS_concrete4) -> (service171 = ST4)
TRANS ( input = service171_UMS_TripManagementRequest) -> (service171 = ST0)
TRANS ( output = service171_UMS_concrete3) -> (service171 = ST3)
TRANS ( output = service171_UMS_concrete2) -> (service171 = ST2)
TRANS ( input = service171_RESET) -> (service171 = ST2 | service171 = ST1 | service171 = ST4 | service171 = ST3 | service171 = ST6 | service171 = ST5 | service171 = ST8 | service171 = ST7)
TRANS ( output = service171_UMS_concrete1) -> (service171 = ST1)
TRANS ( output = service171_UMS_TripManagementReply) -> (service171 = ST7)
TRANS ( output = service171_UMS_concrete5) -> (service171 = ST5)

--	 service173
VAR service173: {ST2, ST1, ST4, ST3, ST0};
ASSIGN
next(service173) :=
case
(service173 = ST1 & output = service173_UMS_concrete1): ST2;
(service173 = ST2 & input = service173_RESET): ST0;
(service173 = ST3 & output = service173_UMS_TripAlternatives): ST4;
(service173 = ST3 & input = service173_RESET): ST0;
(service173 = ST1 & input = service173_RESET): ST0;
(service173 = ST2 & output = service173_UMS_concrete2): ST3;
(service173 = ST4 & input = service173_RESET): ST0;
(service173 = ST0 & input = service173_UMS_TripRequest): ST1;
1:service173;
esac;
TRANS ( input = service173_UMS_TripRequest) -> (service173 = ST0)
TRANS ( output = service173_UMS_TripAlternatives) -> (service173 = ST3)
TRANS ( output = service173_UMS_concrete2) -> (service173 = ST2)
TRANS ( input = service173_RESET) -> (service173 = ST2 | service173 = ST1 | service173 = ST4 | service173 = ST3)
TRANS ( output = service173_UMS_concrete1) -> (service173 = ST1)

-----------------------
--   O B J E C T S   --
-----------------------
--	 SetupTrip
VAR SetupTrip: {ORGANIZED, INITIAL};
ASSIGN
next(SetupTrip) :=
case
(input = service171_UMS_ManagementTrip):{(ORGANIZED)};
1:SetupTrip;
esac;
--	 ProvideTrip
VAR ProvideTrip: {TRIP_REQUEST_ACCOMPLISHED, TRIP_MANAGEMENT_ACCOMPLISHED, TRIP_MANAGE_REQUEST_LOADED, TRIP_REQUEST_LOADED, INITIAL};
ASSIGN
next(ProvideTrip) :=
case
(ProvideTrip = TRIP_MANAGE_REQUEST_LOADED & output = service171_UMS_TripManagementReply): TRIP_MANAGEMENT_ACCOMPLISHED;
(ProvideTrip = INITIAL & input = service173_UMS_TripRequest): TRIP_REQUEST_LOADED;
(ProvideTrip = TRIP_REQUEST_ACCOMPLISHED & input = service171_UMS_TripManagementRequest): TRIP_MANAGE_REQUEST_LOADED;
(ProvideTrip = TRIP_REQUEST_LOADED & output = service173_UMS_TripAlternatives): TRIP_REQUEST_ACCOMPLISHED;
1:ProvideTrip;
esac;
-----------------------------------
--   P R E C O N D I T I O N S   --
-----------------------------------
TRANS (input = service171_UMS_TripManagementRequest) -> ((((ProvideTrip = TRIP_REQUEST_ACCOMPLISHED))))
TRANS (output = service171_UMS_TripManagementReply) -> ((((ProvideTrip = TRIP_MANAGE_REQUEST_LOADED))))
TRANS (input = service173_UMS_TripRequest) -> ((((ProvideTrip = INITIAL))))
TRANS (output = service173_UMS_TripAlternatives) -> ((((ProvideTrip = TRIP_REQUEST_LOADED))))
-----------------------------
--   I N V A R I A N T S   --
-----------------------------
--	 Synchronicity
TRANS (1-(input = UNDEF)) + (1-(output = UNDEF)) = 1

---------------------------------------
--   S Y N C H R O N I Z A T I O N   --
---------------------------------------
DEFINE SYNC0 := ((ProvideTrip in {TRIP_MANAGEMENT_ACCOMPLISHED}));
--	 Synchronicity enforcement
TRANS (service171 in {ST2, ST1, ST4, ST3, ST5, ST7}) -> (next(service171) != service171)
TRANS (service173 in {ST2, ST1, ST3}) -> (next(service173) != service173)
-------------------------------
--   C O N T R O L L E R S   --
-------------------------------
--	 Fault Controller
VAR FAULT_CONTROLLER: { START, STOP };
INIT FAULT_CONTROLLER = START

ASSIGN
next(FAULT_CONTROLLER) := 
case
(FAULT_CONTROLLER = START & FALSE): STOP;
1: FAULT_CONTROLLER;
esac;

TRANS (output != UNDEF | input != UNDEF) -> (FAULT_CONTROLLER != STOP)

-------------------------------------
--   I N I T I A L I Z A T I O N   --
-------------------------------------
INIT ((SetupTrip = INITIAL) & (ProvideTrip = TRIP_REQUEST_ACCOMPLISHED))
INIT ((service171 = ST0) & (service173 = ST0))

-----------------
--   G O A L   --
-----------------
MODULE main
VAR dn_main : dn_main;

GOAL ONEOF(
(dn_main.SYNC0), 1;
(dn_main.FAULT_CONTROLLER = STOP), 0

)