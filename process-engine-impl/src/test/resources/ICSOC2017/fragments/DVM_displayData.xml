<?xml version="1.0" encoding="UTF-8"?>
<tns:fragment id="DVM_displayData" xmlns:tns="http://das.fbk.eu/Fragment"
	xmlns:tns1="http://das.fbk.eu/Annotation" xmlns:tns2="http://das.fbk.eu/DataType"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://das.fbk.eu/Fragment Fragment.xsd ">

	<tns:state isInitial="true" name="ST0" />
	<tns:state name="ST1" />
	<tns:state name="ST2" />

	<tns:action name="DVM_DataPatternRequest" actionType="input">
		<tns:precondition>
			<tns1:point>
				<tns1:domainProperty dp_name="DataViewer">
					<tns1:state>INITIAL</tns1:state>
				</tns1:domainProperty>
			</tns1:point>
		</tns:precondition>
		<tns:effect>
			<tns1:event dpName="DataViewer" eventName="receiveData"></tns1:event>
		</tns:effect>
		<tns:actionVariable name="PlannerOutput" type="string">
			<tns2:content type="anyType"></tns2:content>
		</tns:actionVariable>
	</tns:action>

	<tns:action name="DVM_DataPatternResponse" actionType="output">
		<tns:precondition>
			<tns1:point>
				<tns1:domainProperty dp_name="DataViewer">
					<tns1:state>DATA_RECEIVED</tns1:state>
				</tns1:domainProperty>
			</tns1:point>
		</tns:precondition>
		<tns:effect>
			<tns1:event dpName="DataViewer" eventName="sendPattern"></tns1:event>
		</tns:effect>
		<tns:actionVariable name="DataPattern" type="string">
			<tns2:content type="anyType"></tns2:content>
		</tns:actionVariable>
		<tns:actionVariable name="PlanList" type="string">
			<tns2:content type="anyType"></tns2:content>
		</tns:actionVariable>
	</tns:action>


	<tns:transition>
		<tns:initialState>ST0</tns:initialState>
		<tns:finalState>ST1</tns:finalState>
		<tns:action name="DVM_DataPatternRequest" actionType="input" />
	</tns:transition>

	<tns:transition>
		<tns:initialState>ST1</tns:initialState>
		<tns:finalState>ST2</tns:finalState>
		<tns:action name="DVM_DataPatternResponse" actionType="output" />
	</tns:transition>

</tns:fragment>

