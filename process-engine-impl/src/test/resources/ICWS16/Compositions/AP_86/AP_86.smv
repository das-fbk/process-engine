-------------------------------
--   C O M P O S I T I O N   --
-------------------------------
MODULE dn_main

---------------------
--   I N P U T S   --
---------------------
IVAR input:
{
UNDEF, service147_UMS_ManagementTrip, service147_UMS_TripManagementRequest, service147_RESET, service150_UMS_TripRequest, service150_RESET
};

-----------------------
--   O U T P U T S   --
-----------------------
IVAR output:
{
UNDEF, service147_UMS_concrete4, service147_UMS_concrete3, service147_UMS_concrete2, service147_UMS_concrete1, service147_UMS_TripManagementReply, service147_UMS_concrete5, service150_UMS_TripAlternatives, service150_UMS_concrete2, service150_UMS_concrete1
};

-------------------------
--   S E R V I C E S   --
-------------------------
--	 service147
VAR service147: {ST2, ST1, ST4, ST3, ST6, ST5, ST8, ST7, ST0};
ASSIGN
next(service147) :=
case
(service147 = ST0 & input = service147_UMS_TripManagementRequest): ST1;
(service147 = ST5 & output = service147_UMS_concrete5): ST6;
(service147 = ST7 & output = service147_UMS_TripManagementReply): ST8;
(service147 = ST6 & input = service147_RESET): ST0;
(service147 = ST6 & input = service147_UMS_ManagementTrip): ST7;
(service147 = ST8 & input = service147_RESET): ST0;
(service147 = ST1 & input = service147_RESET): ST0;
(service147 = ST7 & input = service147_RESET): ST0;
(service147 = ST3 & input = service147_RESET): ST0;
(service147 = ST2 & output = service147_UMS_concrete2): ST3;
(service147 = ST3 & output = service147_UMS_concrete3): ST4;
(service147 = ST1 & output = service147_UMS_concrete1): ST2;
(service147 = ST2 & input = service147_RESET): ST0;
(service147 = ST4 & output = service147_UMS_concrete4): ST5;
(service147 = ST5 & input = service147_RESET): ST0;
(service147 = ST4 & input = service147_RESET): ST0;
1:service147;
esac;
TRANS ( input = service147_UMS_ManagementTrip) -> (service147 = ST6)
TRANS ( output = service147_UMS_concrete4) -> (service147 = ST4)
TRANS ( input = service147_UMS_TripManagementRequest) -> (service147 = ST0)
TRANS ( output = service147_UMS_concrete3) -> (service147 = ST3)
TRANS ( output = service147_UMS_concrete2) -> (service147 = ST2)
TRANS ( input = service147_RESET) -> (service147 = ST2 | service147 = ST1 | service147 = ST4 | service147 = ST3 | service147 = ST6 | service147 = ST5 | service147 = ST8 | service147 = ST7)
TRANS ( output = service147_UMS_concrete1) -> (service147 = ST1)
TRANS ( output = service147_UMS_TripManagementReply) -> (service147 = ST7)
TRANS ( output = service147_UMS_concrete5) -> (service147 = ST5)

--	 service150
VAR service150: {ST2, ST1, ST4, ST3, ST0};
ASSIGN
next(service150) :=
case
(service150 = ST1 & input = service150_RESET): ST0;
(service150 = ST3 & input = service150_RESET): ST0;
(service150 = ST2 & input = service150_RESET): ST0;
(service150 = ST2 & output = service150_UMS_concrete2): ST3;
(service150 = ST1 & output = service150_UMS_concrete1): ST2;
(service150 = ST0 & input = service150_UMS_TripRequest): ST1;
(service150 = ST3 & output = service150_UMS_TripAlternatives): ST4;
(service150 = ST4 & input = service150_RESET): ST0;
1:service150;
esac;
TRANS ( input = service150_UMS_TripRequest) -> (service150 = ST0)
TRANS ( output = service150_UMS_TripAlternatives) -> (service150 = ST3)
TRANS ( output = service150_UMS_concrete2) -> (service150 = ST2)
TRANS ( input = service150_RESET) -> (service150 = ST2 | service150 = ST1 | service150 = ST4 | service150 = ST3)
TRANS ( output = service150_UMS_concrete1) -> (service150 = ST1)

-----------------------
--   O B J E C T S   --
-----------------------
--	 SetupTrip
VAR SetupTrip: {ORGANIZED, INITIAL};
ASSIGN
next(SetupTrip) :=
case
(input = service147_UMS_ManagementTrip):{(ORGANIZED)};
1:SetupTrip;
esac;
--	 ProvideTrip
VAR ProvideTrip: {TRIP_REQUEST_ACCOMPLISHED, TRIP_MANAGEMENT_ACCOMPLISHED, TRIP_MANAGE_REQUEST_LOADED, TRIP_REQUEST_LOADED, INITIAL};
ASSIGN
next(ProvideTrip) :=
case
(ProvideTrip = TRIP_REQUEST_ACCOMPLISHED & input = service147_UMS_TripManagementRequest): TRIP_MANAGE_REQUEST_LOADED;
(ProvideTrip = INITIAL & input = service150_UMS_TripRequest): TRIP_REQUEST_LOADED;
(ProvideTrip = TRIP_MANAGE_REQUEST_LOADED & output = service147_UMS_TripManagementReply): TRIP_MANAGEMENT_ACCOMPLISHED;
(ProvideTrip = TRIP_REQUEST_LOADED & output = service150_UMS_TripAlternatives): TRIP_REQUEST_ACCOMPLISHED;
1:ProvideTrip;
esac;
-----------------------------------
--   P R E C O N D I T I O N S   --
-----------------------------------
TRANS (input = service147_UMS_TripManagementRequest) -> ((((ProvideTrip = TRIP_REQUEST_ACCOMPLISHED))))
TRANS (output = service147_UMS_TripManagementReply) -> ((((ProvideTrip = TRIP_MANAGE_REQUEST_LOADED))))
TRANS (input = service150_UMS_TripRequest) -> ((((ProvideTrip = INITIAL))))
TRANS (output = service150_UMS_TripAlternatives) -> ((((ProvideTrip = TRIP_REQUEST_LOADED))))
-----------------------------
--   I N V A R I A N T S   --
-----------------------------
--	 Synchronicity
TRANS (1-(input = UNDEF)) + (1-(output = UNDEF)) = 1

---------------------------------------
--   S Y N C H R O N I Z A T I O N   --
---------------------------------------
DEFINE SYNC0 := ((ProvideTrip in {TRIP_MANAGEMENT_ACCOMPLISHED}));
--	 Synchronicity enforcement
TRANS (service147 in {ST2, ST1, ST4, ST3, ST5, ST7}) -> (next(service147) != service147)
TRANS (service150 in {ST2, ST1, ST3}) -> (next(service150) != service150)
-------------------------------
--   C O N T R O L L E R S   --
-------------------------------
--	 Fault Controller
VAR FAULT_CONTROLLER: { START, STOP };
INIT FAULT_CONTROLLER = START

ASSIGN
next(FAULT_CONTROLLER) := 
case
(FAULT_CONTROLLER = START & FALSE): STOP;
1: FAULT_CONTROLLER;
esac;

TRANS (output != UNDEF | input != UNDEF) -> (FAULT_CONTROLLER != STOP)

-------------------------------------
--   I N I T I A L I Z A T I O N   --
-------------------------------------
INIT ((SetupTrip = INITIAL) & (ProvideTrip = TRIP_REQUEST_ACCOMPLISHED))
INIT ((service147 = ST0) & (service150 = ST0))

-----------------
--   G O A L   --
-----------------
MODULE main
VAR dn_main : dn_main;

GOAL ONEOF(
(dn_main.SYNC0), 1;
(dn_main.FAULT_CONTROLLER = STOP), 0

)